    {
        if (_dataSourceCtx.name() == dataEntityDataSourceStr(SMAServiceObjectEntity, InventDim))
        {
            // Read only datasource was handled by mapEntityToDataSource(...)
            return true;
        }

        boolean ret = super(_entityCtx, _dataSourceCtx);

        if (ret)
        {
            if (_dataSourceCtx.name() == dataEntityDataSourceStr(SMAServiceObjectEntity, SMAServiceObjectTable))
            {
                // Need to re-apply inventory dimensions as defaulting logic may have change them (e.g. set warehouse based on site + item):
                SMAServiceObjectTable smaServiceObjectTable = _dataSourceCtx.getBuffer();
                this.InventInventoryDimensionEntityFieldsMapping::initFromInventDim(this.inventDim(smaServiceObjectTable));
            }
        }

        return ret;
    }

