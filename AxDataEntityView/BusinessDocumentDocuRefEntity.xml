				<Source><![CDATA[
    public void postLoad()
    {
        DocuRef docuRef = DocuRef::findRecId(this.DocuRefRecId);
        
        this.Content = docuRef.getFileContentAsContainer();
        this.ContentBase64 = docuRef.getFileContentAsBase64String();
        this.FileName = docuRef.originalFileName();
        this.FileType = docuRef.fileType();
        this.URI = docuRef.url();
        this.RestrictionStr = enum2Symbol(enumNum(DocuRestriction), this.Restriction);
        
        super();
    }

]]></Source>
