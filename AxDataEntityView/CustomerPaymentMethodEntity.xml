    /// <summary>
    /// Creates or deletes records in the related table <c>CustPaymMethodVal</c>.
    /// </summary>
    protected void updatePaymentControl()
    {
        CustomerPaymentMethodEntity orig = this.orig();

        this.processCustPaymMethodVal(this.ValidateTransactionTypeIsBank, orig.ValidateTransactionTypeIsBank, CustPaymentValidate::BankPostType);
        this.processCustPaymMethodVal(this.ValidateOffsetTransactionTypeIsBank, orig.ValidateOffsetTransactionTypeIsBank, CustPaymentValidate::OffsetAccountIsBank);
        this.processCustPaymMethodVal(this.ValidateCheckNumberIsMandatory, orig.ValidateCheckNumberIsMandatory, CustPaymentValidate::CheckNo);
        this.processCustPaymMethodVal(this.ValidatePaymentReferenceIsMandatory, orig.ValidatePaymentReferenceIsMandatory, CustPaymentValidate::PaymReference);
        this.processCustPaymMethodVal(this.ValidateDepositSlipIsMandatory, orig.ValidateDepositSlipIsMandatory, CustPaymentValidate::DepositSlip);
    }

]]></Source>
