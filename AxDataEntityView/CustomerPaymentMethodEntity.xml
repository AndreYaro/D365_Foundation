			<Method>
				<Name>resolvePaymentFileClassIds</Name>
				<Source><![CDATA[
    private void resolvePaymentFileClassIds(CustPaymModeTable _paymentMethod)
    {
        if (this.ExportFormatClassName != this.orig().ExportFormatClassName)
        {
            _paymentMethod.ClassId = className2Id(this.ExportFormatClassName);

            if (_paymentMethod.ClassId
                && SysDictClass::isSuperclass(_paymentMethod.classId, classnum(CustOutPaym)))
            {
                CustPaymFormat::addFormats(BankFormat::Export, [_paymentMethod.ClassId]);
            }
        }

        if (this.ImportFormatClassName != this.orig().ImportFormatClassName)
        {
            _paymentMethod.ClassIdIn = className2Id(this.ImportFormatClassName);

            if (_paymentMethod.ClassIdIn
                && SysDictClass::isSuperclass(_paymentMethod.ClassIdIn, classnum(CustInPaym)))
            {
                CustPaymFormat::addFormats(BankFormat::Import, [_paymentMethod.ClassIdIn]);
            }
        }

        if (this.RemittanceFormatClassName != this.orig().RemittanceFormatClassName)
        {
            _paymentMethod.ClassIdRemittance = className2Id(this.RemittanceFormatClassName);

            if (_paymentMethod.ClassIdRemittance
                && SysDictClass::isSuperclass(_paymentMethod.ClassIdRemittance, classnum(CustOutPaymRemittance)))
            {
                CustPaymFormat::addFormats(BankFormat::Remittance, [_paymentMethod.ClassIdRemittance]);
            }
        }

        if (this.ReturnFormatClassName != this.orig().ReturnFormatClassName)
        {
            _paymentMethod.ClassIdReturn = className2Id(this.ReturnFormatClassName);

            if (_paymentMethod.ClassIdReturn
                && SysDictClass::isSuperclass(_paymentMethod.ClassIdReturn, classnum(CustPaymReconciliationImport)))
            {
                CustPaymFormat::addFormats(BankFormat::ReturnFormat, [_paymentMethod.ClassIdReturn]);
            }
        }
    }

]]></Source>
