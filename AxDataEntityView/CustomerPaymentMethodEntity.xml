			<Method>
				<Name>processCustPaymMethodVal</Name>
				<Source><![CDATA[
    protected void processCustPaymMethodVal(NoYes _validationIsMandatory, NoYes _origValidationIsMandatory, CustPaymentValidate _custPaymentValidate)
    {
        if (_validationIsMandatory != _origValidationIsMandatory)
        {
            if (_validationIsMandatory == NoYes::Yes)
            {
                CustPaymMethodVal::add(this.Name, _custPaymentValidate);
            }
            else
            {
                CustPaymMethodVal::remove(this.Name, _custPaymentValidate);
            }
        }
    }

]]></Source>
