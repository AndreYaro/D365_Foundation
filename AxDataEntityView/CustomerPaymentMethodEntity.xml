				<Source><![CDATA[
    private static str computeValidateColumn(CustPaymentValidate _custPaymentValidate)
    {
        str custPaymentValidateSqlValue = queryValue(enum2int(_custPaymentValidate));

        str sql = 'SELECT COUNT(*) FROM ' + tableStr(CustPaymMethodVal) + ' CPMV ';
        sql += 'WHERE CPMV.' + fieldStr(CustPaymMethodVal, CustPaymMode) + '='
            + SysComputedColumn::returnField(tablestr(CustomerPaymentMethodEntity), dataEntityDataSourceStr(CustomerPaymentMethodEntity, CustPaymModeTable), fieldstr(CustPaymModeTable, PaymMode));
        sql += ' AND CPMV.' + fieldStr(CustPaymMethodVal, CustPaymValidate) + '=' +  custPaymentValidateSqlValue;

        // Add ranges for Partition and DataAreaId
        sql += ' AND CPMV.'+ fieldStr(CustPaymMethodVal, Partition) + '='
            + SysComputedColumn::returnField(tablestr(CustomerPaymentMethodEntity), dataEntityDataSourceStr(CustomerPaymentMethodEntity, CustPaymModeTable), fieldstr(CustPaymModeTable, Partition));
        sql += ' AND CPMV.' + fieldStr(CustPaymMethodVal, DATAAREAID) + '='
            + SysComputedColumn::returnField(tablestr(CustomerPaymentMethodEntity), dataEntityDataSourceStr(CustomerPaymentMethodEntity, CustPaymModeTable), fieldstr(CustPaymModeTable, dataAreaId));

        return sql;
    }

]]></Source>
