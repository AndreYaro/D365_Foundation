    /// <summary>
    /// Adds to standard framework mapping functionality by processing change control.
    /// </summary>
    /// <param name = "_entityCtx">The entity context.</param>
    /// <param name = "_dataSourceCtx">The datasource context.</param>
    public void mapEntityToDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        if (_dataSourceCtx.name() == dataEntityDataSourceStr(CustCustomerBaseEntity, CustTable))
        {
            this.processChangesForApproval(_entityCtx);
        }
        else if (_dataSourceCtx.name() == dataEntityDataSourceStr(CustCustomerBaseEntity, DirPartyBaseEntity))
        {
            this.processChangesForApproval(_entityCtx);
        }

        super(_entityCtx, _dataSourceCtx);
    }

