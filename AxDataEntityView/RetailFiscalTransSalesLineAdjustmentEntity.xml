    /// </param>
    /// <returns>True, if the insert or update was successful; Otherwise, false.</returns>
    public boolean insertEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        boolean ret;

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(RetailFiscalTransSalesLineAdjustmentEntity, RetailFiscalTransSalesTransAdjustment))
        {
            RetailFiscalTransSalesTransAdjustment fiscalTransAdjustment = _dataSourceCtx.getBuffer();

            // Store field is mandatory on the table level, but is not exposed on the entity.
            fiscalTransAdjustment.Store = RetailStoreTable::findRecId(fiscalTransAdjustment.channel).StoreNumber;
        }

        ret = super(_entityCtx, _dataSourceCtx);
    
        return ret;
    }

]]></Source>
