		<Methods>
			<Method>
				<Name>mapEntityToDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Maps the entity to the backing datasources.
    /// </summary>
    /// <param name = "entityCtx">
    /// The context of the entity.
    /// </param>
    /// <param name = "dataSourceCtx">
    /// The context of the datasource being mapped.
    /// </param>
    [Hookable(false)]
    public void mapEntityToDataSource(DataEntityRuntimeContext entityCtx, DataEntityDataSourceRuntimeContext dataSourceCtx)
    {
        if (entityCtx.getDatabaseOperation() == DataEntityDatabaseOperation::Insert
            || entityCtx.getDatabaseOperation() == DataEntityDatabaseOperation::Update)
        {
            switch (dataSourceCtx.name())
            {
                case dataEntityDataSourceStr(LedgerReportingCurrencyAdjustmentJournalLineEntity, LedgerJournalTable):
                    dataSourceCtx.setBuffer(LedgerJournalEntityBase::findOrCreateLedgerJournalHeader(this));
                    break;

                case dataEntityDataSourceStr(LedgerReportingCurrencyAdjustmentJournalLineEntity, LedgerJournalTrans):
                    this.CurrencyCode = Ledger::reportingCurrency();
                    LedgerJournalTrans ledgerJournalTrans = dataSourceCtx.getBuffer();
                    ledgerJournalTrans.TransactionType = LedgerTransType::ReportingCurrencyAdjustment;

                    // Defaulting the exchange rate value to 100.00 as the transaction and reporting currency will be the same always in reporting currency adjustment journal.
                    ledgerJournalTrans.ExchRate = 100.00;
                    break;
            }

            super(entityCtx, dataSourceCtx);
        }
    }

]]></Source>
			</Method>
		</Methods>
