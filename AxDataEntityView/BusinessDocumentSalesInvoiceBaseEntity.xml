		<Methods>
			<Method>
				<Name>calcCompanyTaxRegNumber</Name>
				<Source><![CDATA[
    private CompanyCoRegNum calcCompanyTaxRegNumber()
    {
        return SysCountryRegionCode::isLegalEntityInCountryRegion([#isoMX], this.DataArea ? this.DataArea : curExt())
            ? this.Rfc_MX 
            : this.CoRegNum;
    }

]]></Source>
			</Method>
			<Method>
				<Name>postLoad</Name>
				<Source><![CDATA[
    public void postLoad()
    {
        this.CustInvoiceJourTableId = tableNum(CustInvoiceJour);
        this.CompanyInfoTableId = tableNum(CompanyInfo);
        this.CustTableTableId = tableNum(CustTable);

        changecompany(this.DataArea ? this.DataArea : curExt())
        {
            this.CompanyTaxRegNumber = this.calcCompanyTaxRegNumber();
            this.CustomerEmail = CustTable::find(this.CustomerOrderAccountNum).email();
        
            CustPaymModeTable custPaymModeTable = CustPaymModeTable::find(this.CustTransPaymMode);
            this.IsBillOfExchange =
                custPaymModeTable &&
                custPaymModeTable.PaymentType == CustPaymentType::BillOfExchange;
            this.PaymentAccountType = enum2Symbol(enumNum(LedgerJournalACType), custPaymModeTable.AccountType);

            BankAccountTable bankAccountTable = custPaymModeTable.bankAccountTable();
            if (bankAccountTable)
            {
                this.BankName = bankAccountTable.Name;
                this.BankIBAN = bankAccountTable.IBAN;
                this.BankSWIFT = bankAccountTable.SWIFTNo;
            }

            CompanyInfo companyInfo = CompanyInfo::findRecId(this.CompanyInfoRecId);
        
            LogisticsPostalAddress postalAddress = companyInfo.postalAddress();
            this.CompanyAddress = postalAddress.Address;
            this.CompanyCity = postalAddress.City;
            this.CompanyCountryISOCode = LogisticsAddressCountryRegion::find(postalAddress.CountryRegionId).ISOcode;
            this.CompanyCounty = postalAddress.County;
            this.CompanyState = postalAddress.State;
            this.CompanyStreet = postalAddress.Street;
            this.CompanyZipCode = postalAddress.ZipCode;

            postalAddress = DirParty::findPostalAddressByRole(companyInfo.RecId, LogisticsLocationRoleType::StableOrganization_IT);
            this.CompanyAddressAdditional = postalAddress.Address;
            this.CompanyCityAdditional = postalAddress.City;
            this.CompanyCountryISOCodeAdditional = LogisticsAddressCountryRegion::find(postalAddress.CountryRegionId).ISOcode;
            this.CompanyCountyAdditional = postalAddress.County;
            this.CompanyStateAdditional = postalAddress.State;
            this.CompanyStreetAdditional = postalAddress.Street;
            this.CompanyZipCodeAdditional = postalAddress.ZipCode;

            this.ParametersITArtLiquidationStr = enum2Symbol(enumNum(Liquidation_IT), this.ParametersITArtLiquidation);
            this.ParametersITArtSoleShareholderStr = enum2Symbol(enumNum(SoleShareholder_IT), this.ParametersITArtSoleShareholder);

            EInvoice_IT eInvoice;
            EInvoiceRefCust_IT eInvoiceRefCust;

            select EInvoiceTransmissionName from eInvoice
                exists join eInvoiceRefCust
            where eInvoiceRefCust.EInvoice == eInvoice.RecId
               && eInvoiceRefCust.Invoice == this.CustInvoiceJourRecId;
  
            this.ParametersITTrnEInvoiceTransmissionName = eInvoice.EInvoiceTransmissionName;

            if (this.SalesId)
            {
                SalesTable salesTable = SalesTable::find(this.SalesId);
                DlvReason dlvReason = DlvReason::find(salesTable.DlvReason);
   
                this.DevileryReason = dlvReason.Txt;
                this.GoodsForFree = dlvReason.Free_IT;
                this.EndDiscPercent = salesTable.DiscPercent;
                this.SelfInvoiceAccountId = dlvReason.InvoiceAccount_IT;
            }

            CustTable custTable = CustTable::findRecId(this.CustTableRecId);
            DirPartyTable dirPartyTable = DirPartyTable::findRec(custTable.Party);
            DirPerson dirPerson = DirPerson::find(custTable.Party);
            DirPersonName dirPersonName;
            var dateNow = today();

            select firstonly validtimestate(dateNow) dirPersonName
                where dirPersonName.Person == dirPerson.RecId;
  
            this.InvoiceAccountFirstName = dirPersonName.FirstName;
            this.InvoiceAccountLastName = dirPersonName.LastName;
            this.InvoiceAccountMiddleName = dirPersonName.MiddleName;
            this.InvoiceAccountName = dirPartyTable.Name;
            this.InvoiceAccountVATRegistrationNumber = TaxTransactionTaxIDHelper::getTaxID(
                this.CustInvoiceJourTableId, 
                this.CustInvoiceJourRecId, 
                this.InvoiceDate)
                .getCounterPartyTaxID();
            this.IsOrganization = dirPartyTable.type() != DirPartyType::Person;
            this.IsSelfInvoice = companyInfo.RecId == custTable.Party;

            postalAddress = custTable.postalAddress();
            this.InvoiceAccountAddress = postalAddress.Address;
            this.InvoiceAccountCity = postalAddress.City;
            this.InvoiceAccountCountryISOCode = LogisticsAddressCountryRegion::find(postalAddress.CountryRegionId).ISOcode;
            this.InvoiceAccountCounty = postalAddress.County;
            this.InvoiceAccountState = postalAddress.State;
            this.InvoiceAccountStreet = postalAddress.Street;
            this.InvoiceAccountZipCode = postalAddress.ZipCode;

            postalAddress = DirParty::findPostalAddressByRole(custTable.Party, LogisticsLocationRoleType::StableOrganization_IT);
            this.InvoiceAccountAddressAdditional = postalAddress.Address;
            this.InvoiceAccountCityAdditional = postalAddress.City;
            this.InvoiceAccountCountryISOCodeAdditional = LogisticsAddressCountryRegion::find(postalAddress.CountryRegionId).ISOcode;
            this.InvoiceAccountCountyAdditional = postalAddress.County;
            this.InvoiceAccountStateAdditional = postalAddress.State;
            this.InvoiceAccountStreetAdditional = postalAddress.Street;
            this.InvoiceAccountZipCodeAdditional = postalAddress.ZipCode;

            CustTrans custTrans;
            CustTrans custTransOffsetRecId;
            CustSettlement custSettlement;

            select firstonly custTrans
                exists join custSettlement
                exists join custTransOffsetRecId
                where  custTrans.Invoice == this.InvoiceId
                    && custTrans.AccountNum == this.CustomerAccountNum
                    && custTrans.TransDate == this.InvoiceDate
                    && custTrans.Voucher == this.LedgerVoucher
                    && custSettlement.TransCompany == custTrans.DataAreaId
                    && custSettlement.TransRecId == custTrans.RecId
                    && custSettlement.AccountNum == custTrans.AccountNum
                    && custSettlement.OffsetRecId == custTransOffsetRecId.RecId
                    && custTransOffsetRecId.Prepayment == NoYes::Yes;

            this.Prepayment = custTrans.RecId
                ? NoYes::Yes
                : NoYes::No;

        }
        
        super();
    }

]]></Source>
			</Method>
