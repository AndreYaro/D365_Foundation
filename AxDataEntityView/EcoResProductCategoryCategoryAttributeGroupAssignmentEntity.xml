				<Name>jumpRefMethod</Name>
				<Source><![CDATA[
    /// <summary>
    /// Opens the form referenced by the FormRef based on the selected record in the staging table.
    /// </summary>
    /// <param name="_menuItemName">
    /// Name of the menu item to open.
    /// </param>
    /// <param name="_buffer">
    /// Staging record selected.
    /// </param>
    /// <param name="_caller">
    /// Caller object.
    /// </param>
    /// <param name="_dataArea">
    /// The legal entity Id of the caller context.
    /// </param>
    public static void jumpRefMethod(MenuItemName _menuItemName, EcoResProductCategoryCategoryAttributeGroupAssignmentStaging _buffer, object _caller, DataAreaId _dataArea)
    {
        changeCompany(_dataArea)
        {
            EcoResCategory category = EcoResCategory::findCategoryByNameAndHierarchy(_buffer.ProductCategoryName, _buffer.ProductCategoryHierarchyName);

            Args args = new Args();

            args.record(category);
            args.caller(_caller);
            args.lookupTable(tablenum(EcoResCategory));
            args.lookupValue(int642Str(category.RecId));

            MenuFunction::runCalled(_menuItemName, MenuItemType::Display, false, args);
        }
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
