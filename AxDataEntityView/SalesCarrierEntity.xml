				<Source><![CDATA[
    private void savePostalAddress(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        if (_dataSourceCtx.name() == dataEntityDataSourceStr(SalesCarrierEntity, SalesCarrier))
        {
            LogisticsLocationRecId locationRecId = LogisticsPostalAddressBaseEntity::saveAddress(_entityCtx, dataEntityDataSourceStr(SalesCarrierEntity, LogisticsPostalAddressBaseEntity));
            if (locationRecId)
            {
                SalesCarrier salesCarrier = _dataSourceCtx.getBuffer();
                salesCarrier.Location = locationRecId;
            }
        }
    }

]]></Source>
