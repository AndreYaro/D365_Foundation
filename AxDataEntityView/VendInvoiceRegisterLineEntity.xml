			<Method>
				<Name>resolvePartyNumber</Name>
				<Source><![CDATA[
    private void resolvePartyNumber(DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        if (this.AccountType == LedgerJournalACType::Vend)
        {
            DirPartyLocationPostalAddressV2Entity addressEntity = _dataSourceCtx.getBuffer();
            addressEntity.PartyNumber = DirPartyTable::findRec(VendTable::find(this.AccountDisplayValue).Party).PartyNumber;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>insert</Name>
				<Source><![CDATA[
    public void insert()
    {
        LedgerJournalTable ledgerJournalTable = LedgerJournalTable::find(this.JournalBatchNumber);

        if (ledgerJournalTable == null || ledgerJournalTable.JournalType == LedgerJournalType::PurchaseLedger)
        {
            // call super because the LedgerJournalTrans strategy handles the case where the LedgerJournalTable is not found
            super();
        }
        else
        {
            throw error(strFmt("@SYS22977", LedgerJournalType::PurchaseLedger));
        }
    }

]]></Source>
