				<Name>isRequiredAddressFieldSet</Name>
				<Source><![CDATA[
    private boolean isRequiredAddressFieldSet()
    {
        return this.isFieldSet(fieldNum(VendInvoiceRegisterLineEntity, RemittanceAddressLocationId))
            || this.isFieldSet(fieldNum(VendInvoiceRegisterLineEntity, RemittanceAddressDescription))
            || this.isFieldSet(fieldNum(VendInvoiceRegisterLineEntity, RemittanceAddressCountry));
    }

]]></Source>
