			<Method>
				<Name>updateEntityDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Generates an audit log and then updates the underlying data sources.
    /// </summary>
    /// <param name = "_entityCtx">The entity context.</param>
    /// <param name = "_dataSourceCtx">The data source context.</param>
    /// <returns>true if saved successfully; otherwise, false.</returns>
    public boolean updateEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        boolean ret;

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(RetailTransactionAuditableSafeDropEntity, RetailTransactionSafeTenderTrans))
        {
            RetailTransactionAuditLogController::construct(_dataSourceCtx.getBuffer()).auditChanges();
        }

        ret = super(_entityCtx, _dataSourceCtx);

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>findBySafeTenderTrans</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds the corresponding <c>RetailTransactionAuditableSafeDropEntity</c> for the given <c>RetailTransactionSafeTenderTrans</c> record.
    /// </summary>
    /// <param name = "_safeTenderTrans">The safe drop line.</param>
    /// <returns>A <c>RetailTransactionAuditableSafeDropEntity</c> record.</returns>
    public static RetailTransactionAuditableSafeDropEntity findBySafeTenderTrans(RetailTransactionSafeTenderTrans _safeTenderTrans)
    {
        RetailTransactionAuditableSafeDropEntity entity;

        select firstonly entity
            where entity.Channel         == _safeTenderTrans.Channel &&
                  entity.StoreNumber     == _safeTenderTrans.store &&
                  entity.RegisterNumber  == _safeTenderTrans.terminal &&
                  entity.TransactionId   == _safeTenderTrans.transactionId &&
                  entity.LineNumber      == _safeTenderTrans.LineNum;

        return entity;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateField</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates changes to the following fields:
    /// * TransactionStatus
    /// </summary>
    /// <param name = "_fieldIdToCheck">The field being checked.</param>
    /// <returns>true if allowed; false, otherwise.</returns>
    public boolean validateField(FieldId _fieldIdToCheck)
    {
        boolean ret;

        ret = super(_fieldIdToCheck);

        if (_fieldIdToCheck == fieldNum(RetailTransactionAuditableSafeDropEntity, TransactionStatus))
        {
            ret = ret && this.validateTransactionStatusChange();
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateTransactionStatusChange</Name>
				<Source><![CDATA[
    private boolean validateTransactionStatusChange()
    {
        RetailTransactionAuditableSafeDropEntity origEntity = this.orig();

        if ((origEntity.TransactionStatus == this.TransactionStatus)
        ||  (origEntity.TransactionStatus == RetailEntryStatus::Voided && this.TransactionStatus == RetailEntryStatus::None)
        ||  (origEntity.TransactionStatus == RetailEntryStatus::None && this.TransactionStatus == RetailEntryStatus::Voided))
        {
            return true;
        }

        return checkFailed(strFmt("@RetailAudit:EntryStatusChangeNotAllowed", fieldPName(RetailTransactionAuditableSafeDropEntity, TransactionStatus), enum2Str(RetailEntryStatus::None), enum2Str(RetailEntryStatus::Voided)));
    }

]]></Source>
			</Method>
