			<Method>
				<Name>updateEntityDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Generates an audit log and then updates the underlying data sources.
    /// </summary>
    /// <param name = "_entityCtx">The entity context.</param>
    /// <param name = "_dataSourceCtx">The data source context.</param>
    /// <returns>true if saved successfully; otherwise, false.</returns>
    public boolean updateEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        boolean ret;

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(RetailTransactionAuditableAddressLineEntity, RetailTransactionAddressTrans))
        {
            RetailTransactionAuditLogController::construct(_dataSourceCtx.getBuffer()).auditChanges();
        }

        ret = super(_entityCtx, _dataSourceCtx);

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates if the changes to the entity can be performed.
    /// </summary>
    /// <returns>true if allowed; false, otherwise.</returns>
    public boolean validateWrite()
    {
        boolean ret = super();
            
        ret = ret && this.RetailTransactionAuditableValidationsMap::mappedValidateWrite();

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>findByAddressTrans</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds the corresponding <c>RetailTransactionAuditableAddressLineEntity</c> for the given <c>RetailTransactionAddressTrans</c> record.
    /// </summary>
    /// <param name = "_addressTrans">The transaction address line.</param>
    /// <returns>A <c>RetailTransactionAuditableAddressLineEntity</c> record.</returns>
    public static RetailTransactionAuditableAddressLineEntity findByAddressTrans(RetailTransactionAddressTrans _addressTrans)
    {
        RetailTransactionAuditableAddressLineEntity entity;

        select firstonly entity
            where entity.Channel         == _addressTrans.Channel &&
                  entity.StoreNumber     == _addressTrans.store &&
                  entity.RegisterNumber  == _addressTrans.terminal &&
                  entity.TransactionId   == _addressTrans.transactionId &&
                  entity.SalesLineNumber == _addressTrans.SaleLineNum;

        return entity;
    }

]]></Source>
