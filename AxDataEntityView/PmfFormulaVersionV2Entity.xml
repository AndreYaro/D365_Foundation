				<Name>mapEntityToDataSource</Name>
				<Source><![CDATA[
    public void mapEntityToDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        // Approving and Activating requires an action in the UI. The entity allows these fields to be imported.
        this.skipDataSourceValidateField(fieldNum(PmfFormulaVersionV2Entity, IsApproved), true);
        this.skipDataSourceValidateField(fieldNum(PmfFormulaVersionV2Entity, ApproverId), true);
        this.skipDataSourceValidateField(fieldNum(PmfFormulaVersionV2Entity, IsActive), true);

        super(_entityCtx, _dataSourceCtx);

        switch (_dataSourceCtx.name())
        {
            case dataEntityDataSourceStr(PmfFormulaVersionV2Entity, FormulaVersion):
                switch (_entityCtx.getDatabaseOperation())
                {
                    case DataEntityDatabaseOperation::Insert:
                    case DataEntityDatabaseOperation::Update:
                        // Set the correct inventDimId on the table buffer.
                        BOMVersion version = _dataSourceCtx.getBuffer();
                        version.InventDimId = this.InventInventoryDimensionEntityFieldsMapping::resolveInventDimId();
                }
                break;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>findEntityDataSource</Name>
				<Source><![CDATA[
    public Common findEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        Common ret;

        switch (_dataSourceCtx.name())
        {
            case dataEntityDataSourceStr(PmfFormulaVersionV2Entity, InventDim):
                var formulaVersionContext = _entityCtx.getRuntimeContextByName(dataEntityDataSourceStr(PmfFormulaVersionV2Entity, FormulaVersion));
                BOMVersion version = formulaVersionContext.getBuffer();

                ret = InventDim::find(version.InventDimId);
                break;

            default:
                ret = super(_entityCtx, _dataSourceCtx);
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>mapInventDim</Name>
				<Source><![CDATA[
    private void mapInventDim(DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        if (_dataSourceCtx.name() == dataEntityDataSourceStr(PmfFormulaVersionV2Entity, FormulaVersion))
        {
            // Need to re-apply inventory dimensions as defaulting logic may have changed them:
            BOMVersion version = _dataSourceCtx.getBuffer();
            this.InventInventoryDimensionEntityFieldsMapping::initFromInventDim(version.inventDim());
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateEntityDataSource</Name>
				<Source><![CDATA[
    public boolean updateEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        boolean ret = super(_entityCtx, _dataSourceCtx);

        if (ret)
        {
            this.mapInventDim(_dataSourceCtx);
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>insertEntityDataSource</Name>
				<Source><![CDATA[
    public boolean insertEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
