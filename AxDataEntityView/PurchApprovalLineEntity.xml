		<Methods>
			<Method>
				<Name>getEntityDetails</Name>
				<Source><![CDATA[
    [SysODataActionAttribute("getEntityDetails", false)]
    public static str getEntityDetails(str _purchId, int _lineNum, str _legalEntity, str _vendorAccountNumber)
    {
        SysInstrumentationEventDataJsonSerializer jsonSerializer = new SysInstrumentationEventDataJsonSerializer();

        using (var activityContext = WorkflowApprovalHelper::getLoggerActivities(tableStr(PurchApprovalLineEntity)).PurchLineEntity_getEntityDetails())
        {
            changecompany(_legalEntity)
            {
                jsonSerializer.setValue('ProductName', PurchLine::find(_purchId, _lineNum).itemName());
                jsonSerializer.setValue('VendorName', _vendorAccountNumber == "" ? "" :  VendTable::find(_vendorAccountNumber).name());
            }
        }

        return jsonSerializer.ToString();
    }

]]></Source>
			</Method>
