			<Method>
				<Name>insertEntityDataSource</Name>
				<Source><![CDATA[
    public boolean insertEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        switch (_dataSourceCtx.name())
        {
            case dataEntityDataSourceStr(InventTransferOrderHeaderEntity, InventTransferTable):
                InventTransferTable inventTransferTable =  _dataSourceCtx.getBuffer();
                
                if (!this.isFieldSet(fieldNum(InventTransferOrderHeaderEntity, RequestedShippingDate))
                    && !this.isFieldSet(fieldNum(InventTransferOrderHeaderEntity, RequestedReceiptDate)))
                {
                    SalesCalcAvailableDlvDates::initDateFieldsOrderEntryDeadline(inventTransferTable);
                }

                this.InventTransferOrderEntityMap::setDeliveryDateControlFields(inventTransferTable, fieldNum(InventTransferTable, DeliveryDateControlType));
                
                if (this.DeliveryModeCode
                    && this.isFieldModified(fieldNum(InventTransferOrderHeaderEntity, DeliveryModeCode)))
                {
                    inventTransferTable.modifiedField(fieldNum(InventTransferTable, DlvModeId));
                }
                
                break;
        }

        boolean ret = super(_entityCtx, _dataSourceCtx);

        return ret;
    }

]]></Source>
