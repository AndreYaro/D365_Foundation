			<Method>
				<Name>defaultCTQuery</Name>
				<Source><![CDATA[
    /// <summary>
    /// Returns the default change tracking query.
    /// </summary>
    /// <returns>The default change tracking query.</returns>
    public static Query defaultCTQuery()
    {
        Query transferOrderHeaderQuery = new Query();

        var transferTableDS = transferOrderHeaderQuery.addDataSource(tableNum(InventTransferTable));

        var shippingContactPersonDS = transferTableDS.addDataSource(tableNum(HcmWorker));
        shippingContactPersonDS.joinMode(JoinMode::OuterJoin);
        shippingContactPersonDS.addLink(fieldNum(InventTransferTable, FromContactPerson), fieldNum(HcmWorker, RecId));

        var receivingContactPersonDS = transferTableDS.addDataSource(tableNum(HcmWorker));
        receivingContactPersonDS.joinMode(JoinMode::OuterJoin);
        receivingContactPersonDS.addLink(fieldNum(InventTransferTable, ToContactPerson), fieldNum(HcmWorker, RecId));

        var tmsTransferTableDS = transferTableDS.addDataSource(tableNum(TMSInventTransferTable));
        tmsTransferTableDS.joinMode(JoinMode::OuterJoin);
        tmsTransferTableDS.addLink(fieldNum(InventTransferTable, TransferId), fieldNum(TMSInventTransferTable, TransferId));

        LogisticsPostalAddressBaseEntityChangeTrackingHelper::addPostalAddressBasedOnAddressRecIdLink(transferTableDS, fieldNum(InventTransferTable, FromPostalAddress));

        LogisticsPostalAddressBaseEntityChangeTrackingHelper::addPostalAddressBasedOnAddressRecIdLink(transferTableDS, fieldNum(InventTransferTable, ToPostalAddress));

        return transferOrderHeaderQuery;
    }

]]></Source>
