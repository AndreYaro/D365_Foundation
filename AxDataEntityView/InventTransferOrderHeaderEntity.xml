            this.TradeEntitySpecificReceivingAddress::mapSavedPostalAddressToEntity(_entityCtx,  inventTransferTable.FromPostalAddress, 'ReceivingLogisticsPostalAddressBaseEntity');
        }

        super(_entityCtx, _dataSourceCtx);
    }

]]></Source>
