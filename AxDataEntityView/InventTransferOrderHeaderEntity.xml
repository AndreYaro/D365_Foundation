				<Source><![CDATA[
    public Common findEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        Common ret;

        switch (_dataSourceCtx.name())
        {
            case dataEntityDataSourceStr(InventTransferOrderHeaderEntity, ShippingLogisticsPostalAddressBaseEntity):
                ret = TradeEntitySpecificShippingAddress::findLogisticsPostalAddressBaseEntityDataSource();
                break;

            case dataEntityDataSourceStr(InventTransferOrderHeaderEntity, ReceivingLogisticsPostalAddressBaseEntity):
                ret = TradeEntitySpecificReceivingAddress::findLogisticsPostalAddressBaseEntityDataSource();
                break;

            default:
                ret = super(_entityCtx, _dataSourceCtx);
        }

        return ret;
    }

]]></Source>
