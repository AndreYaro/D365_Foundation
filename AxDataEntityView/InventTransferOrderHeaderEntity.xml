                }
                break;

            case dataEntityDataSourceStr(InventTransferOrderHeaderEntity, ReceivingLogisticsPostalAddressBaseEntity):
                if (_dataSourceCtx.isAnyEntityFieldSet(this))
                {
                    this.TradeEntitySpecificReceivingAddress::mapLogisticsPostalAddressBaseEntityDataSourceToEntity(_dataSourceCtx);
                }
                break;
        }

        super(_entityCtx, _dataSourceCtx);
    }

]]></Source>
