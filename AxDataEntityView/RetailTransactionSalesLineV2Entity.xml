			<Method>
				<Name>insertEntityDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Insert entity data source.
    /// </summary>
    /// <param name = "_entityCtx">Entity runtime context.</param>
    /// <param name = "_dataSourceCtx">Data source runtime context.</param>
    /// <returns>true on success; otherwise false.</returns>
    public boolean insertEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        boolean ret;

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(RetailTransactionSalesLineV2Entity, RetailTransactionSalesTrans))
        {
            if (this.ItemId)
            {
                RetailTransactionSalesTrans retailTransactionSalesTrans = _dataSourceCtx.getBuffer();

                InventTable product =  InventTable::find(this.ItemId);

                if (!product)
                {
                    throw error(strFmt("@Retail:InvalidItemId", this.ItemId));
                }

                if (this.ItemColor || this.ItemSize || this.ItemStyle || this.ItemConfigId || this.ItemVersion)
                {
                    InventDimCombination inventDimCombination;
                    InventDim inventDim;
                    select firstonly inventDimCombination
                        where inventDimCombination.ItemId == this.ItemId
                            && inventDimCombination.DistinctProductVariant != 0
                    join inventDim
                        where inventDim.inventDimId == inventDimCombination.InventDimId
                            && inventDim.InventColorId == this.ItemColor
                            && inventDim.InventSizeId == this.ItemSize
                            && inventDim.InventStyleId == this.ItemStyle
                            && inventDim.configId == this.ItemConfigId
                            && inventDim.InventVersionId == this.ItemVersion;

                    if (!inventDimCombination)
                    {
                        throw error(strFmt("@Retail:InvalidVariantV2", this.ItemId, this.ItemColor, this.ItemSize, this.ItemStyle, this.ItemVersion, this.ItemConfigId));
                    }

                    retailTransactionSalesTrans.inventDimId = inventDim.inventDimId;
                }

                // Store field is mandatory on the table level, but is not exposed on the entity.
                retailTransactionSalesTrans.store = RetailStoreTable::findRecId(retailTransactionSalesTrans.channel).StoreNumber;
            }
        }

        ret = super(_entityCtx, _dataSourceCtx);

        return ret;
    }

]]></Source>
