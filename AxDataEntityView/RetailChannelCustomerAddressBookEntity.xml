    /// <summary>
    /// This is required for entity configuration validation. This provides the menuitem to navigate
    /// to when a dependency link is clicked on the config progress dialog.
    /// </summary>
    /// <param name = "_sourceEntityId">TableId of the entity which is being validated.</param>
    /// <returns>MenuitemName of the form where the dependency can be fixed.</returns>
    public MenuItemName getAssociatedFormRef(TableId _sourceEntityId)
    {
        switch (_sourceEntityId)
        {
            case tableNum(RetailStoreEntity):
                return menuItemDisplayStr(RetailStoreTable);
            case tableNum(RetailOnlineChannelEntity):
                return menuItemDisplayStr(RetailOnlineChannels);
            default:
                throw Global::error(strFmt("@Retail:NoMenuItemMapping",tableId2Name(_sourceEntityId)));
        }
    }

]]></Source>
