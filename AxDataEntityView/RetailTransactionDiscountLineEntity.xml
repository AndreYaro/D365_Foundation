				<Source><![CDATA[
    /// <summary>
    /// Insert entity data source.
    /// </summary>
    /// <param name = "_entityCtx">Data entity runtime context.</param>
    /// <param name = "_dataSourceCtx">Data source runtime context.</param>
    /// <returns>true if entity is inserted successfully; otherwise, false.</returns>
    public boolean insertEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        boolean ret;

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(RetailTransactionDiscountLineEntity, RetailTransactionDiscountTrans))
        {
            RetailTransactionDiscountTrans discountTrans = _dataSourceCtx.getBuffer();

            // Store field is mandatory on the table level, but is not exposed on the entity.
            discountTrans.StoreId = RetailStoreTable::findRecId(discountTrans.channel).StoreNumber;
        }
    
        ret = super(_entityCtx, _dataSourceCtx);
    
        return ret;
    }

]]></Source>
