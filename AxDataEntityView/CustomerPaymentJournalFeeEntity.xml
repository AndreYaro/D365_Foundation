			<Method>
				<Name>resolveSourceLedgerJournalTrans</Name>
				<Source><![CDATA[
    private LedgerJournalTrans resolveSourceLedgerJournalTrans()
    {
        LedgerJournalTrans sourceLedgerJournalTrans;

        select firstonly sourceLedgerJournalTrans
            where sourceLedgerJournalTrans.JournalNum == this.SourceJournalBatchNumber
                && sourceLedgerJournalTrans.LineNum == this.SourceJournalLineNumber;

        return sourceLedgerJournalTrans;
    }

]]></Source>
