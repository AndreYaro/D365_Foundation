			<Method>
				<Name>initializeEntityDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes the entity data sources.
    /// </summary>
    /// <param name = "_entityCtx">The entity context.</param>
    /// <param name = "_dataSourceCtx">The data source context.</param>
    public void initializeEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        super(_entityCtx, _dataSourceCtx);

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(RetailTransactionAuditableEntity, OrderStatus))
        {
            // This datasource is not directly updated by the entity, we rely on RetailTransactionOrderStatus::setOrderStatus
            // to change the data appropriatelly on the table, so we have to skip the validations here
            _dataSourceCtx.skipValidateWrite(true);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateEntityDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Generates an audit log of the changes and then updates the underlying data sources.
    /// </summary>
    /// <param name = "_entityCtx">The entity context.</param>
    /// <param name = "_dataSourceCtx">The data source context.</param>
    /// <returns>true if saved successfully; otherwise, false.</returns>
    public boolean updateEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        boolean ret;

        switch (_dataSourceCtx.name())
        {
            case dataEntityDataSourceStr(RetailTransactionAuditableEntity, OrderStatus):
                // Override the default logic to update the OrderStatus data source
                ret = this.insertOrUpdateOrderStatus(_dataSourceCtx);
                break;

            case dataEntityDataSourceStr(RetailTransactionAuditableEntity, RetailTransactionTable):
                // Audit the changes
                RetailTransactionAuditLogController::construct(_dataSourceCtx.getBuffer()).auditChanges();
                // And then continue to the super() call so that the datasource can be updated.

            default:
                ret = super(_entityCtx, _dataSourceCtx);
                break;
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>insertEntityDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Overrides the insert logic for the OrderStatus data source so that only "Status" can be changed through the entity.
    /// </summary>
    /// <param name = "_entityCtx">The entity context.</param>
    /// <param name = "_dataSourceCtx">The data source context.</param>
    /// <returns>true if saved successfully; otherwise, false.</returns>
    public boolean insertEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        boolean ret;

        switch (_dataSourceCtx.name())
        {
            case dataEntityDataSourceStr(RetailTransactionAuditableEntity, OrderStatus):
                // Override the default logic to update the OrderStatus data source
                ret = this.insertOrUpdateOrderStatus(_dataSourceCtx);
                break;

            default:
                ret = super(_entityCtx, _dataSourceCtx);
                break;
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>insertOrUpdateOrderStatus</Name>
				<Source><![CDATA[
    private boolean insertOrUpdateOrderStatus(DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        if (RetailTransactionAsyncOrdersHelper::isAsyncTransactionType(this.TransactionType))
        {
            // Only execute this logic for Asynchronous order

            RetailTransactionOrderStatus localOrderStatus;

            select firstonly forupdate localOrderStatus
            where localOrderStatus.RecId == _dataSourceCtx.getBuffer().RecId;

            localOrderStatus.Status = this.AsynchronousOrderStatus;
            localOrderStatus.ManuallyModified = NoYes::Yes;
            localOrderStatus.LastEditUser = curUserId();

            if (localOrderStatus)
            {
                localOrderStatus.update();
            }
            else
            {
                localOrderStatus.Channel = this.Channel;
                localOrderStatus.store = this.StoreNumber;
                localOrderStatus.terminal = this.RegisterNumber;
                localOrderStatus.transactionId = this.TransactionId;

                localOrderStatus.insert();
            }

            return true;
        }
        else
        {
            // For all other transaction types, this should be a no-op.

            return false;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateField</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if the fields being changed have valid values.
    /// </summary>
    /// <param name = "_fieldIdToCheck">The field being checked.</param>
    /// <returns>true if allowed; false, otherwise.</returns>
    public boolean validateField(FieldId _fieldIdToCheck)
    {
        boolean ret = super(_fieldIdToCheck);
            
        ret = ret && this.RetailTransactionAuditableValidationsMap::mappedValidateField(tableNum(RetailTransactionAuditableEntity), _fieldIdToCheck);

        return ret;
    }

]]></Source>
