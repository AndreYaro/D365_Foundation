			<Method>
				<Name>findByTransactionTable</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds the corresponding <c>RetailTransactionAuditableEntity</c> for the given <c>RetailTransactionTable</c> record.
    /// </summary>
    /// <param name = "_transactionTable">The transaction table.</param>
    /// <returns>A <c>RetailTransactionAuditableEntity</c> record.</returns>
    public static RetailTransactionAuditableEntity findByTransactionTable(RetailTransactionTable _transactionTable)
    {
        RetailTransactionAuditableEntity entity;

        select firstonly entity
            where entity.Channel        == _transactionTable.Channel &&
                  entity.StoreNumber    == _transactionTable.store &&
                  entity.RegisterNumber == _transactionTable.terminal &&
                  entity.TransactionId  == _transactionTable.transactionId;

        return entity;
    }

]]></Source>
