				<Name>mapEntityToDataSource</Name>
				<Source><![CDATA[
    public void mapEntityToDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        super(_entityCtx, _dataSourceCtx);

        CzCustAdvanceInvoiceTable custAdvanceInvoiceTable = CzCustAdvanceInvoiceTable::findByHeadId(this.HeadId);

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(CustAdvancedInvoiceEntity, CzCustAdvanceInvoiceTable))
        {
            CzCustAdvanceInvoiceTable custAdvancedInvoice = _dataSourceCtx.getBuffer();

            if (this.Currency == Ledger::accountingCurrency())
            {
                custAdvancedInvoice.ExchRate = 100;

                if (this.SecondaryExchangeRate)
                {
                    custAdvancedInvoice.ExchrateSecond = 100;
                }
            }

            if (_entityCtx.getDatabaseOperation() == DataEntityDatabaseOperation::Update)
            {
                if (!this.PostVAT)
                {
                    custAdvancedInvoice.PostVATDate = custAdvanceInvoiceTable.PostVATDate;
                }
            }

            if (_entityCtx.getDatabaseOperation() == DataEntityDatabaseOperation::Insert)
            {
                if (!this.PostVAT)
                {
                    custAdvancedInvoice.PostVATDate = dateNull();
                }
            }

            if (!this.HeadId)
            {
                custAdvancedInvoice.HeadId = newGuid();
            }
            else
            {
                if (custAdvanceInvoiceTable)
                {
                    _dataSourceCtx.setBuffer(this.updateEntityBuffer(custAdvanceInvoiceTable, custAdvancedInvoice));
                    _dataSourceCtx.setDatabaseOperation(DataEntityDatabaseOperation::Update);
                }
            }
        }

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(CustAdvancedInvoiceEntity, CzCustAdvanceInvoiceLine))
        {
            CzCustAdvanceInvoiceLine custAdvanceInvoiceLine;

            select firstonly LineNum from custAdvanceInvoiceLine
                order by LineNum desc
                where custAdvanceInvoiceLine.ParentRecId == custAdvanceInvoiceTable.Recid;

            CzCustAdvanceInvoiceLine advanceInvoiceLine = _dataSourceCtx.getBuffer();

            if (this.existLine(advanceInvoiceLine))
            {
                advanceInvoiceLine.LineNum = this.LineNum ? this.LineNum : custAdvanceInvoiceLine.LineNum + 1;
            }
            else
            {
                _dataSourceCtx.setDatabaseOperation(DataEntityDatabaseOperation::None);
                _dataSourceCtx.setDataSaved(true);
            }
        }
    }

]]></Source>
