    public boolean insertEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        if (dataEntityDataSourceStr(CustAdvancedInvoiceEntity, CzCustAdvanceInvoiceTable) == _dataSourceCtx.name())
        {
            LogisticsLocationRecId locationRecId = LogisticsPostalAddressBaseEntity::saveAddress(_entityCtx, dataEntityDataSourceStr(CustAdvancedInvoiceEntity, LogisticsPostalAddressBaseEntity));

            LogisticsPostalAddress logisticsPostalAddress = LogisticsPostalAddress::findByLocation(locationRecId);

            CzCustAdvanceInvoiceTable CustAdvanceInvoiceTable = _dataSourceCtx.getBuffer();
            CustAdvanceInvoiceTable.PostalAddress = logisticsPostalAddress.RecId;
        }

        return super(_entityCtx, _dataSourceCtx);
    }

