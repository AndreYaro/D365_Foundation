	<Name>LedgerJournalLineCDSEntity</Name>
	<SourceCode>
		<Declaration><![CDATA[
/// <summary>
/// Entity to handle integrations to CDS with Ledger Journal line records.
/// </summary>
/// <remarks>
/// This entity is optimized for single record in and out which is how CDS integrations work.
/// It will not perform well for mass data import.
/// </remarks>
public class LedgerJournalLineCDSEntity extends common
{
}
]]></Declaration>
		<Methods>
			<Method>
				<Name>IntegrationComputedKey</Name>
				<Source><![CDATA[
    /// <summary>
    /// Computed column for the integration computed key.
    /// </summary>
    /// <returns>A string representing the SQL code for the computed column.</returns>
    public static str IntegrationComputedKey()
    {
        TableName viewName = tableStr(LedgerJournalLineCDSEntity);
        str dataSourceName = identifierStr(LedgerJournalTable);
        str integrationKey = fieldStr(LedgerJournalTable, IntegrationKey);
        str journalBatchNumber = fieldStr(LedgerJournalTable, JournalNum);
        str comparisonField = SysComputedColumn::comparisonField(viewName, dataSourceName, integrationKey);

        return SysComputedColumn::if(
            SysComputedColumn::notEqualExpression(comparisonField, SysComputedColumn::comparisonLiteral('')),
            SysComputedColumn::returnField(viewName, dataSourceName, integrationKey),
            SysComputedColumn::returnField(viewName, dataSourceName, journalBatchNumber));
    }

]]></Source>
			</Method>
			<Method>
				<Name>mapEntityToDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Maps the entity to the backing datasources.
    /// </summary>
    /// <param name = "_entityCtx">The context of the entity.</param>
    /// <param name = "_dataSourceCtx">The context of the datasource being mapped.</param>
    public void mapEntityToDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        // When editing through the entity, always delay tax calculation to avoid
        // a O(n^2) tax calculation while importing multiple lines. The journal will
        // automatically calculate taxes later at posting time or when necessary.
        this.DelayTaxCalculation = NoYes::Yes;

        super(_entityCtx, _dataSourceCtx);

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(LedgerJournalLineCDSEntity, LedgerJournalTrans))
        {
            LedgerJournalTrans ledgerJournalTrans = _dataSourceCtx.getBuffer();

            if (!this.JournalBatchNumber)
            {
                LedgerJournalTable ledgerJournalTable = this.findLedgerJournalTable();

                ledgerJournalTrans.JournalNum = ledgerJournalTable.JournalNum;
            }
     
            if (!this.ReverseEntry)
            {
                this.ReverseDate = dateNull();
                ledgerJournalTrans.ReverseDate = dateNull();
            }

            if (!this.Document)
            {
                this.DocumentDate = dateNull();
                ledgerJournalTrans.DocumentDate = dateNull();
            }
        }

        if (_dataSourceCtx.name() == dataEntityDataSourceStr(LedgerJournalLineCDSEntity, LedgerJournalTable))
        {
            LedgerJournalTable ledgerJournalTable = _dataSourceCtx.getBuffer();
          
            if (ledgerJournalTable.RecId == 0)
            {
                ledgerJournalTable = this.findLedgerJournalTable();
                _dataSourceCtx.setBuffer(ledgerJournalTable);
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>findLedgerJournalTable</Name>
				<Source><![CDATA[
    private LedgerJournalTable findLedgerJournalTable()
    {
        LedgerJournalTable ledgerJournalTable;

        if (this.JournalBatchNumber)
        {
            ledgerJournalTable = ledgerJournalTable::find(this.JournalBatchNumber);
        }
        else
        {
            ledgerJournalTable = ledgerJournalTable::findByIntegrationKey(this.IntegrationComputedKey);
        }

        return ledgerJournalTable;
    }

]]></Source>
			</Method>
			<Method>
				<Name>isJournalValid</Name>
				<Source><![CDATA[
    private boolean isJournalValid()
    {
        return LedgerDailyJournalExcelTemplate::validateDailyJournal(this.JournalBatchNumber);
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    public boolean validateWrite()
    {
        boolean isValid = this.isJournalValid();

        if (isValid)
        {
            this.DefaultDimensionDisplayValue = LedgerJournalLineEntity::validateDefaultDimensionDisplayValueIsNonLedgerType(this.AccountType, this.DefaultDimensionDisplayValue);
            this.OffsetDefaultDimensionDisplayValue = LedgerJournalLineEntity::validateDefaultDimensionDisplayValueIsNonLedgerType(this.OffsetAccountType, this.OffsetDefaultDimensionDisplayValue, true);

            isValid = super();
        }

        return isValid;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateDelete</Name>
				<Source><![CDATA[
    public boolean validateDelete()
    {
        return this.isJournalValid() && super();
    }

]]></Source>
			</Method>
