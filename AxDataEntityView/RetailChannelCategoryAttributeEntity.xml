		<Methods>
			<Method>
				<Name>insertEntityDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    /// Insert data into table buffer that is saved to the database by the entity framework. Called for each datasource.
    /// </summary>
    /// <param name = "_entityCtx">Data entity runtime context.</param>
    /// <param name = "_dataSourceCtx">Data source runtime context.</param>
    /// <returns>true if the data was successfully set; otherwise, false.</returns>
    public boolean insertEntityDataSource(DataEntityRuntimeContext _entityCtx, DataEntityDataSourceRuntimeContext _dataSourceCtx)
    {
        switch (_dataSourceCtx.name())
        {
            case dataEntityDataSourceStr(RetailChannelCategoryAttributeEntity, RetailChannelCategoryAttribute):

                // skip in case of other database operations apart from insert.
                if (_dataSourceCtx.getDatabaseOperation() == DataEntityDatabaseOperation::Insert)
                {
                    RetailChannelCategoryAttribute channelCategoryAttribute = _dataSourceCtx.getBuffer();
                    EcoResCategoryAttributeGroupAttrView categoryAttributeGroupAttrView;

                    // Validation.
                    // If the attribute is not defined on any of the attribute groups assigned to the category,
                    // the insert will fail.
                    select firstonly categoryAttributeGroupAttrView
                    where categoryAttributeGroupAttrView.Category == channelCategoryAttribute.Category
                        && categoryAttributeGroupAttrView.Attribute == channelCategoryAttribute.Attribute;

                    if (!categoryAttributeGroupAttrView)
                    {
                        throw Global::error(strFmt("@Retail:AttributeNotAssociatedWithCategory", this.AttributeName, this.AttributeTypeName, this.ProductCategoryName));
                    }
                    else
                    {
                        // Set key.
                        EcoResAttribute attribute = EcoResAttribute::find(channelCategoryAttribute.Attribute);
                        channelCategoryAttribute.KeyName = RetailChannelCategoryAttribute::createKey(
                            attribute,
                            channelCategoryAttribute.HostChannel,
                            channelCategoryAttribute.Category
                        );

                        _dataSourceCtx.setBuffer(channelCategoryAttribute);
                    }
                }
                break;
        }

        boolean ret = super(_entityCtx, _dataSourceCtx);

        return ret;
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
