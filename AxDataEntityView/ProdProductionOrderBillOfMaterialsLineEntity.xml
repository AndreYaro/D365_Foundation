        switch (_dataSourceCtx.name())
        {
            case dataEntityDataSourceStr(ProdProductionOrderBillOfMaterialsLineEntity, InventDim):
                var prodBOMContext = _entityCtx.getRuntimeContextByName(dataEntityDataSourceStr(ProdProductionOrderBillOfMaterialsLineEntity, ProdBOM));
                ProdBOM prodBOM = prodBOMContext.getBuffer() as ProdBOM;

                ret = InventDim::find(prodBOM.InventDimId);
                break;

            default:
                ret = super(_entityCtx, _dataSourceCtx);
        }

        return ret;
    }

