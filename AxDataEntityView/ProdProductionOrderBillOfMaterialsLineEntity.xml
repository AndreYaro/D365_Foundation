				<Source><![CDATA[
    public boolean validateWrite()
    {
        ProdTable prodTable = ProdTable::find(this.ProductionOrderNumber);

        ProdStatus productionStatus = prodTable.ProdStatus;
        if (productionStatus != ProdStatus::Created)
