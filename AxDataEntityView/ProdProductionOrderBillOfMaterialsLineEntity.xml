				<Name>setDefaultDimensions</Name>
				<Source><![CDATA[
    /// <summary>
    /// Sets the default dimensions.
    /// </summary>
    /// <param name = "_prodRoute">The production route used for setting default dimensions.</param>
    private void setDefaultDimensions(ProdBOM _prodBOM)
    {
        if (!this.DefaultDimensionDisplayValue && _prodBOM.ProdId)
        {
            _prodBOM.DefaultDimension = _prodBOM.copyDimension(
                this.getExtensionProdTable().DefaultDimension);
        }
    }

]]></Source>
