			<Method>
				<Name>setQuantityFields</Name>
				<Source><![CDATA[
    private void setQuantityFields(ProdBOM _prodBOM)
    {
        if(this.isFieldSet(fieldNum(ProdProductionOrderBillOfMaterialsLineEntity, BOMLineQuantity)))
        {
            InventMovement::setMinMaxQty(_prodBOM);
            this.BOMLineQuantity = _prodBOM.BOMQty;
        }

        if(this.isFieldSet(fieldNum(ProdProductionOrderBillOfMaterialsLineEntity, EstimatedInventoryQuantity)))
        {
            this.EstimatedInventoryQuantity = _prodBOM.QtyInventCalc;
            InventMovement::bufferSetTransQty(_prodBOM);
        }

        if(this.isFieldSet(fieldNum(ProdProductionOrderBillOfMaterialsLineEntity, EstimatedBOMLineQuantity)))
        {
            this.EstimatedBOMLineQuantity = _prodBOM.QtyBOMCalc;
            InventMovement::setMinMaxQty(_prodBOM);
            InventMovement::bufferSetTransQtyUnit(_prodBOM);
        }

        InventMovement::bufferSetRemainQty(_prodBOM);
    }

]]></Source>
