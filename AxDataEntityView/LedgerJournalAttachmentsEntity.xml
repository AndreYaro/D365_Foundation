{
}
]]></Declaration>
		<Methods>
			<Method>
				<Name>postLoad</Name>
				<Source><![CDATA[
    /// <summary>
    /// Populates virtual fields.
    /// </summary>
    public void postLoad()
    {
        super();

        // Virtual fields are not auto-populated from the base entity, so an extra select is necessary.
        if (this.RecId && this.DocumentId != emptyGuid())
        {
            DocuRefEntity baseEntity;
            select firstonly baseEntity where baseEntity.DocumentId == this.DocumentId;
            this.FileContents = baseEntity.FileContents;
            this.fieldExternallySet(fieldNum(LedgerJournalAttachmentsEntity, FileContents), false);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    public boolean validateWrite()
    {
        boolean isValid = this.isJournalInEditState();

        if (!isValid)
        {
            checkFailed("@GeneralLedger:LedgerJournalAttachmentsEntityNotEditable");
        }

        return isValid;
    }

]]></Source>
			</Method>
			<Method>
				<Name>isJournalInEditState</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets a value indicating whether the journal is in an editable state.
    /// </summary>
    /// <returns>true if the journal is in an editable state; otherwise, false</returns>
    protected boolean isJournalInEditState()
    {
        LedgerJournalTable ledgerJournalTable = LedgerJournalTable::find(this.JournalBatchNumber, false);

        boolean isValid = ledgerJournalTable.Posted == NoYes::No;

        return isValid && LedgerJournalEntityBase::isWorkflowDocumentEditable(this.JournalBatchNumber);
    }

]]></Source>
			</Method>
		</Methods>
