			<Method>
				<Name>creditNote</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if Invoice amount is positive or negative.
    /// </summary>
    /// <returns>True, if amount is negative, othervise false.</returns>
    public boolean creditNote()
    {
        return this.Invoice4PaymAmount < 0;
    }

]]></Source>
