			<Method>
				<Name>initFromVendInvoiceJour</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes current <c>VendInvoice4PaymJour_RU</c> record with values from given <c>VendInvoiceJour</c>
    /// </summary>
    /// <param name="_vendInvoiceJour">
    /// <c>VendInvoiceJour</c> record to take data from
    /// </param>
    void initFromVendInvoiceJour(VendInvoiceJour _vendInvoiceJour)
    {
        this.Invoice4PaymDate       = _vendInvoiceJour.InvoiceDate;
        this.InvoiceAccount         = _vendInvoiceJour.InvoiceAccount;
        this.InternalInvoice4PaymId = _vendInvoiceJour.InternalInvoiceId;
        this.NumberSequenceGroupId  = _vendInvoiceJour.NumberSequenceGroup;
        this.OrderAccount           = _vendInvoiceJour.OrderAccount;
        this.DeliveryPostalAddress  = _vendInvoiceJour.deliveryPostalAddressRecId_RU();
        this.SumMarkup              = 0;
        this.PurchBalance           = 0;
        this.EndDisc                = 0;
        this.RoundOff               = 0;
        this.Invoice4PaymAmount     = _vendInvoiceJour.InvoiceAmount;
        this.CurrencyCode           = _vendInvoiceJour.CurrencyCode;
        this.Triangulation          = Currency::euroTriangulation(this.CurrencyCode, this.Invoice4PaymDate);
        this.ExchRate               = ExchangeRateHelper::exchRate(this.CurrencyCode, this.Invoice4PaymDate);
        this.ExchRateSecondary      = ExchangeRateHelper::exchRateSecond(this.CurrencyCode, this.Invoice4PaymDate);
        this.DefaultDimension       = _vendInvoiceJour.DefaultDimension;
        this.VendGroup              = _vendInvoiceJour.VendGroup;
        this.PaymTermId             = _vendInvoiceJour.Payment;
        this.PurchId                = _vendInvoiceJour.PurchId;
        this.LanguageId             = _vendInvoiceJour.LanguageId;

        if (!this.LanguageId)
        {
            this.LanguageId = VendTable::find(this.InvoiceAccount).languageId();
        }
    }

]]></Source>
