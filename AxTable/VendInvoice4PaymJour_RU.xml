			<Method>
				<Name>findRecId</Name>
				<Source><![CDATA[
    /// <summary>
    /// Searches <c>VendInvoice4PaymJour_RU</c> by given record id
    /// </summary>
    /// <param name="_recId">
    /// Record id to look for
    /// </param>
    /// <param name="_update">
    /// Determines whether record should be selected for update
    /// </param>
    /// <returns>
    /// Found <c>VendInvoice4PaymJour_RU</c> record
    /// </returns>
    public static VendInvoice4PaymJour_RU findRecId(RecId   _recId,
                                                    boolean _update = false)
    {
        VendInvoice4PaymJour_RU invoice4PaymJour;

        if (_recId)
        {
            invoice4PaymJour.selectForUpdate(_update);

            select firstonly invoice4PaymJour
                index hint recid
                where invoice4PaymJour.RecId == _recId;
        }

        return invoice4PaymJour;
    }

]]></Source>
			</Method>
			<Method>
				<Name>numberOfInvoice4Paym</Name>
				<Source><![CDATA[
    private static RecId numberOfInvoice4Paym(PurchTable _source)
    {
        VendInvoice4PaymJour_RU  invoice4PaymJour;

        select count (RecId) from invoice4PaymJour
            index hint PurchIdx
            where invoice4PaymJour.PurchId == _source.PurchId;

        return invoice4PaymJour.RecId;
    }

]]></Source>
