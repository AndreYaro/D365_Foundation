			<Method>
				<Name>setDocNum</Name>
				<Source><![CDATA[
    public void setDocNum(PurchTable _purchTable)
    {
        this.Invoice4PaymDocNum = strfmt('%1-%2',
                                         this.PurchId,
                                         VendInvoice4PaymJour_RU::numberOfInvoice4Paym(_purchTable) + 1);
    }

]]></Source>
