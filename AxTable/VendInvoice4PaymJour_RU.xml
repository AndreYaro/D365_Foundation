			<Method>
				<Name>initFromPurchTable</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes values from purhase table.
    /// </summary>
    /// <param name = "_purchTable"><c>PurchTable</c> cursor object.</param>
    public void initFromPurchTable(PurchTable _purchTable)
    {
        this.PurchId                 = _purchTable.PurchId;
        this.OrderAccount            = _purchTable.OrderAccount;
        this.InvoiceAccount          = _purchTable.InvoiceAccount;
        this.NumberSequenceGroupId   = _purchTable.NumberSequenceGroup;
        this.DeliveryName            = _purchTable.DeliveryName;
        this.DeliveryPostalAddress   = _purchTable.DeliveryPostalAddress;
        this.VendGroup               = _purchTable.VendGroup;
        this.LanguageId              = _purchTable.LanguageId;
        this.PaymTermId              = _purchTable.Payment;
        this.CashDiscCode            = _purchTable.CashDisc;

        this.SumMarkup               = 0;
        this.PurchBalance            = 0;
        this.EndDisc                 = 0;
        this.RoundOff                = 0;
        this.Invoice4PaymAmount      = 0;
        this.CurrencyCode            = _purchTable.CurrencyCode;

        this.Triangulation           = Currency::euroTriangulation(this.CurrencyCode, this.Invoice4PaymDate);
        this.ExchRate                = ExchangeRateHelper::exchRate(this.CurrencyCode, this.Invoice4PaymDate);
        this.ExchRateSecondary       = ExchangeRateHelper::exchRateSecond(this.CurrencyCode, this.Invoice4PaymDate);

        this.DefaultDimension        = _purchTable.DefaultDimension;
    }

]]></Source>
