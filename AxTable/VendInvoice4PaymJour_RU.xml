			<Method>
				<Name>printJournal</Name>
				<Source><![CDATA[
    public void printJournal(PurchFormLetter  _purchFormLetter = null,
                             RecordSortedList _journalList     = null)
    {
        Args            args = new Args();
        MenuFunction    invoice4PaymMenu;
        ;

        invoice4PaymMenu = new MenuFunction(menuitemoutputstr(PurchInvoice4Paym_RU), MenuItemType::Output);

        if (_purchFormLetter)
        {
            args.caller(_purchFormLetter);
        }
        if (_journalList)
        {
            args.object(_journalList);
        }
        args.record(this);

        invoice4PaymMenu.run(args);
    }

]]></Source>
