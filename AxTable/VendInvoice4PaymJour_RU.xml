				<Source><![CDATA[
    /// <summary>
    /// Gets amount in MST currency.
    /// </summary>
    /// <param name = "_amountCur">Amount in transaction currency.</param>
    /// <returns>MST amount.</returns>
    public AmountMST amountMST(AmountCur _amountCur)
    {
        return CurrencyExchangeHelper::mstAmount(_amountCur, this.CurrencyCode, this.Invoice4PaymDate, Currency::noYes2UnknownNoYes(this.Triangulation), this.ExchRate, this.ExchRateSecondary);
    }

]]></Source>
