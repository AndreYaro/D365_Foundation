		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a specific <c>HMIMClass</c> record with given parameters.
    /// </summary>
    /// <param name="_classCode">
    /// Class code.
    /// </param>
    /// <param name="_forupdate">
    /// Determines whether the record is selected for update.
    /// </param>
    /// <returns>
    /// The record that is searched for.
    /// </returns>
    public static HMIMClass find(HMIMClassCode _classCode,
                                 boolean       _forupdate = false)
    {
        HMIMClass hmimClass;

        if (_classCode)
        {
            hmimClass.selectForUpdate(_forupdate);

            select firstonly hmimClass
                where hmimClass.ClassCode == _classCode;
        }

        return hmimClass;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if a specific <c>HMIMClass</c> record exists.
    /// </summary>
    /// <param name="_classCode">
    /// Class code.
    /// </param>
    /// <returns>
    /// True if the record exists.
    /// </returns>
    public static boolean exist(HMIMClassCode _classCode)
    {
        return _classCode && HMIMClass::find(_classCode).RecId != 0;
    }

]]></Source>
			</Method>
