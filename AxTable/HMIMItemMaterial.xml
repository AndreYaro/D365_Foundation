				<Source><![CDATA[
    /// <summary>
    /// Finds a specific <c>HMIMItemMaterial</c> record with given parameters.
    /// </summary>
    /// <param name="_recId">
    /// Item material recid.
    /// </param>
    /// <param name="_forupdate">
    /// Determines whether the record is selected for update.
    /// </param>
    /// <returns>
    /// The record that is searched for.
    /// </returns>
    public static HMIMItemMaterial find(RecId _recId , boolean _forupdate = false)
    {
        HMIMItemMaterial itemMaterial;

        if(_forupdate)
        {
            itemMaterial.selectForUpdate(_forupdate);
        }

        select firstonly itemMaterial
            where itemMaterial.RecId == _recId;

        return itemMaterial;
    }

