			<Method>
				<Name>findShipPrintText</Name>
				<Source><![CDATA[
    /// <summary>
    /// Method used to return the shipping print txt for an item based on regulation code and language.
    /// </summary>
    /// <param name="_itemId">
    /// Item id.
    /// </param>
    /// <param name="_regCode">
    /// Hazardous Material Regulation code
    /// </param>
    /// <param name="_languageId">
    /// Language Id to identify the ItemShipTxt Translation
    /// </param>
    /// <returns>
    /// The ShippingPrintText or description of the product
    /// </returns>
    public static Description findShipPrintText(ItemId _itemId, HMIMRegCode _regCode, LanguageId _languageId)
    {
        HMIMItemMaterial itemMaterial = HMIMItemMaterial::findByItemRegCode(_itemId, _regCode);

        if (itemMaterial.RecId)
        {
            HMIMItemShipTxtTranslation itemShipTxtTranslation = HMIMItemShipTxtTranslation::find(itemMaterial.RecId, _languageId);
            
            return itemShipTxtTranslation.ShippingPrintText ? itemShipTxtTranslation.ShippingPrintText : itemMaterial.MaterialDescription;
        }
        else
        {
            return InventTable::find(_itemId).defaultProductName();
        }

    }

]]></Source>
