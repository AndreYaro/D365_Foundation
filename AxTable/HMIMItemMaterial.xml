			<Method>
				<Name>getItemShipPrintText</Name>
				<Source><![CDATA[
    /// <summary>
    /// Method used to return the shipping print txt of an item for a language.
    /// </summary>
    /// </param>
    /// <param name="_languageId">
    /// Language Id to identify the ItemShipTxt Translation
    /// </param>
    public Description255 getItemShipPrintText(LanguageId _languageId)
    {
        HMIMItemShipTxtTranslation itemShipTxtTranslation = HMIMItemShipTxtTranslation::find(this.RecId, _languageId);
            
        return itemShipTxtTranslation.ShippingPrintText ? itemShipTxtTranslation.ShippingPrintText : this.MaterialDescription;

    }

]]></Source>
