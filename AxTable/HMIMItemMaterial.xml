    /// <summary>
    /// Finds the specified record in the <c>HMIMItemMaterial</c> record table.
    /// </summary>
    /// <param name="_itemId">
    /// Item id.
    /// </param>
    /// <param name="_regCode">
    /// Hazardous Material Regulation code.
    /// </param>
    /// <param name="_recId">
    /// The record ID of <c>HMIMItemMaterial</c> table.
    /// </param>
    /// <param name="_forupdate">
    /// A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <returns>
    ///  A record in the <c>HMIMItemMaterial</c> table; otherwise, an empty record.
    /// </returns>
    public static HMIMItemMaterial findByRecIdItemRegCode(ItemId _itemId, HMIMRegCode _regCode, RecId _recId, boolean _forupdate = false)
    {
        HMIMItemMaterial itemMaterial;

        if (_forupdate)
        {
            itemMaterial.selectForUpdate(_forupdate);
        }

        select firstonly itemMaterial
            where itemMaterial.ItemId == _itemId
                && itemMaterial.RegCode == _regCode
                    && itemMaterial.RecId == _recId;

        return itemMaterial;
    }

