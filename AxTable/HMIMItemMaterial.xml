			<Method>
				<Name>getMaterialDescription</Name>
				<Source><![CDATA[
    /// <summary>
    /// Method used to return the material description for an item based on regulation code.
    /// </summary>
    /// <param name="_itemId">
    /// Item id.
    /// </param>
    /// <param name="_regCode">
    /// Hazardous Material Regulation code.
    /// </param>
    /// <returns>
    /// Material description for the product.
    /// </returns>
    public static str getMaterialDescription(ItemId _itemId, HMIMRegCode _regCode)
    {
        HMIMItemMaterial itemMaterial;
        container        conDescription = conNull();
 
        while select MaterialDescription from itemMaterial
            where itemMaterial.ItemId == _itemId
                && itemMaterial.RegCode == _regCode
        {
            conDescription += [itemMaterial.MaterialDescription];
        }
 
        return con2Str(conDescription);
    }

]]></Source>
			</Method>
			<Method>
				<Name>getItemShipPrintTextByRegCode</Name>
				<Source><![CDATA[
    /// <summary>
    /// Returns item shipping print text based on regulation code.
    /// </summary>
    /// <param name="_itemId">
    /// Item id.
    /// </param>
    /// <param name="_regCode">
    /// Hazardous Material Regulation code.
    /// </param>
    /// <param name="_languageId">
    /// Language Id to identify the item shipping print text translation.
    /// </param>
    /// <returns>
    /// Item shipping print text for the product.
    /// </returns>
    [Hookable(false)]
    internal static str getItemShipPrintTextByRegCode(ItemId _itemId, HMIMRegCode _regCode, LanguageId _languageId)
    {
        HMIMItemMaterial itemMaterial;
        container conDescription = conNull();
 
        while select MaterialDescription from itemMaterial
            where itemMaterial.ItemId == _itemId
                && itemMaterial.RegCode == _regCode
        {
            conDescription += [itemMaterial.getItemShipPrintText(_languageId)];
        }
 
        return con2Str(conDescription);
    }

]]></Source>
