			<Method>
				<Name>existRadioactiveByItem</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if a specific radioactive <c>HMIMItemMaterial</c> record exists.
    /// </summary>
    /// <param name="_itemId">
    /// ItemId to identify the radioactive record
    /// </param>
    /// <returns>
    /// true or false based on whether record exists
    /// </returns>
    public static boolean existRadioactiveByItem(ItemId _itemId)
    {
        HMIMItemMaterial itemMaterial;

        select firstOnly RecId from itemMaterial
            where itemMaterial.IsRadioactive    ==  NoYes::Yes
               && itemMaterial.ItemId           ==  _itemId;

        return itemMaterial.recid != 0;
    }

]]></Source>
