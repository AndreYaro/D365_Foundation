			<Method>
				<Name>validateItemMaterial</Name>
				<Source><![CDATA[
    /// <summary>
    /// Determines where the current record is present in the database
    /// </summary>
    /// <param name="_itemId">
    /// Item id.
    /// </param>
    /// <param name="_regCode">
    /// Hazardous Material Regulation code.
    /// </param>
    /// <param name="_recId">
    /// The record ID of <c>HMIMItemMaterial</c> table.
    /// </param>
    /// <returns>true or false based on validation</returns>
    public static boolean validateItemMaterial(ItemId _itemId, HMIMRegCode _regCode, RecId _recId)
    {
        boolean ret = true;

        HMIMItemMaterial itemMaterial = HMIMItemMaterial::findByRecIdItemRegCode(_itemId, _regCode, _recId);

        if (!itemMaterial)
        {
            ret = checkFailed (strFmt("@SCM:InvalidItemMaterial", _itemId, _regCode));
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    /// <summary>
    /// Determines where the current record is valid and ready to be written to database
    /// </summary>
    /// <returns>true or false based on validation</returns>
    public boolean validateWrite()
    {
        boolean ret = super();

        boolean divisionExists = HMIMMaterialDivisionUniqueInClassFeature::instance().isEnabled()
            ? HMIMMaterialDivision::exist(this.ClassCode, this.DivisionCode)
            : HMIMDivision::existForClassCode(this.DivisionCode, this.ClassCode);
    
        if (!this.ClassCode)
        {
            ret = checkFailed("@SCM:ClassCheckError");
        }
        else if (!this.DivisionCode)
        {
            ret = checkFailed("@SCM:DivisionCheckError");
        }
        else if (!divisionExists)
        {
            ret = checkFailed("@SCM:ClassDivisionAssociationCheck");
        }
        else if (HMIMItemMaterialBlockItemRegCodeDuplicatesFlight::instance().isEnabled())
        {
            HMIMItemMaterial existingItemMaterial = HMIMItemMaterial::findByItemRegCode(this.ItemId, this.RegCode);

            if (existingItemMaterial.RecId != 0
                && existingItemMaterial.RecId != this.RecId)
            {
                ret = checkFailed(strFmt("@SCM:HMIMItemMaterialValidateWriteDuplicateItemRegCodeError", this.ItemId, this.RegCode));
            }
        }
    
        return ret;
    }

]]></Source>
