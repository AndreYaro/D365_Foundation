				<Source><![CDATA[
    /// <summary>
    /// Inserts a record in the <c>HMIMItemMaterial</c> table, creates translation & ShippingPrintText
    /// </summary>
    public void insert()
    {
        super();        
        
        if (EcoResProductParameters::find().UpdateShippingPrintTextItem)
        {     
            HMIMMaterialTranslation::createOrUpdateTranslation(this.RecId, this.MaterialDescription);

            //Generate the shipping print text
            HMIMShippingPrintText::construct().generateShippingPrintTxtLanguage(this);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>update</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates the record in <c>HMIMItemMaterial</c> table.
    /// </summary>
    public void update()
    {
        if (EcoResProductParameters::find().UpdateShippingPrintTextItem)
        {
            //Generate the shipping print text
            HMIMShippingPrintText::construct().generateShippingPrintTxtLanguage(this);
        }
        
        if (this.MaterialDescription != this.orig().MaterialDescription)
        {
            info("@SCM:UpdateTranslationInfo");
        }

        super();
    }

