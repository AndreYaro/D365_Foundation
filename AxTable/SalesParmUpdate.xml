			<Method>
				<Name>isCreditCheckEligible</Name>
				<Source><![CDATA[
    boolean isCreditCheckEligible()
    {
        // <GEERU>
        #ISOCountryRegionCodes
        // </GEERU>
        return  this.DocumentStatus == DocumentStatus::Confirmation     ||
                this.DocumentStatus == DocumentStatus::PickingList      ||
                this.DocumentStatus == DocumentStatus::PackingSlip      ||
                this.DocumentStatus == DocumentStatus::Invoice          ||
                // <GEERU>
                (SysCountryRegionCode::isLegalEntityInCountryRegion([#isoRU]) ? (this.DocumentStatus == DocumentStatus::Facture_RU) : false) ||
                // </GEERU>
                this.DocumentStatus == DocumentStatus::ProjectInvoice   ||
                this.DocumentStatus == DocumentStatus::ProjectPackingSlip;
    }

]]></Source>
