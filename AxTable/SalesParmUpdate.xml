				<Name>isQuantityModified</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if the quantity specification has been modified compared to the current <c>SalesParmUpdate</c> record.
    /// </summary>
    /// <param name = "_otherSalesParmUpdate">
    /// The other <c>SalesParmUpdate</c> record.
    /// </param>
    /// <returns>
    /// true if the quantity specification for the given <c>SalesParmUpdate</c> is different from the current record; otherwise, false.
    /// </returns>
    public boolean isQuantityModified(SalesParmUpdate _otherSalesParmUpdate)
    {
        return (this.SpecQty          != _otherSalesParmUpdate.SpecQty
             || this.DocumentStatus   != _otherSalesParmUpdate.DocumentStatus
             || this.Proforma         != _otherSalesParmUpdate.Proforma
             || this.CreditRemaining  != _otherSalesParmUpdate.CreditRemaining);
    }

]]></Source>
