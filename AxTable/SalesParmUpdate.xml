			<Method>
				<Name>deleteWithoutUpdate</Name>
				<Source><![CDATA[
    static void  deleteWithoutUpdate(ParmId  parmId)
    {
        SalesParmUpdate  salesParmUpdate;
        // <GEEU>
        SalesParmTable   salesParmTable;
        #EECountryRegionCodes
        // </GEEU>

        ttsbegin;
        // <GEEU>
        if (SysCountryRegionCode::isLegalEntityInCountryRegion(#easternEuropeAllandRU))
        {
            while select OffSessionId_RU, OffReportType_RU, Ordering from salesParmTable
                where salesParmTable.ParmId == parmId
            {
                OfficialsServ_RU::deleteRelationTrans(salesParmTable.OffSessionId_RU, salesParmTable.OffReportType_RU);
                if (salesParmTable.Ordering == DocumentStatus::Facture_RU)
                {
                    OfficialsServ_RU::deleteRelationTrans(salesParmTable.OffSessionId_RU,
                                                          salesParmTable.OffReportType_RU == OffReportType_RU::SalesInvoice ?
                                                          OffReportType_RU::CustFacture                           :
                                                          OffReportType_RU::CustFactureCreditNote);
                }
            }
        }
        // </GEEU>

        delete_from salesParmUpdate
            index hint ParmIdIdx
            where salesParmUpdate.ParmId == parmId;

        ttscommit;
    }

]]></Source>
