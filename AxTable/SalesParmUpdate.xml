			<Method>
				<Name>validateItemTrackingDimensions</Name>
				<Source><![CDATA[
    internal boolean validateItemTrackingDimensions(WHSLoadId    _whsLoadId)
    {
        boolean ret = true;

        if (this.DocumentStatus == DocumentStatus::PackingSlip && !_whsLoadId)
        {
            SalesParmLine   salesParmLine;
            InventTable     inventTable;
            Set checkedItemIds;
            
            while select ItemId from salesParmLine 
                where salesParmLine.ParmId == this.ParmId
            join ItemId from inventTable
                where inventTable.itemId == salesParmLine.itemId
            {
                if (!checkedItemIds)
                {
                    checkedItemIds = new Set(Types::String);
                }

                if (checkedItemIds.in(inventTable.itemId))
                {
                    continue;
                }
                else
                {
                    if (inventTable.hasTrackingDimensions())
                    {
                        ret = false;
                        break;
                    }
                    else
                    {
                        checkedItemIds.add(inventTable.ItemId);
                    }
                }
            }
        }
    
        return ret;    
    }

]]></Source>
