			<Method>
				<Name>setFieldsFromWHSLoad</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates vehicle license plate and carrier code based on the WHSLoadTable information.
    /// SpecQty field will also be set to All.
    /// </summary>
    /// <param name="_loadId">
    /// Id of the WHSLoadTable that contains the information that will be used to update the current
    /// salesParmUpdate
    /// </param>
    /// <exception cref="Exception::Error">
    /// If a TMS carrier is found at the WHSLoadTable, but there isn t a sales carrier that uses the same vendor
    /// account of this TMS carrier, then, an exception will be thrown.
    /// </exception>
    public void setFieldsFromWHSLoad(WHSLoadId _loadId)
    {
        WHSLoadTable loadTable;
        TMSCarrier   tmsCarrier;

        if (_loadId)
        {
            this.SpecQty = SalesUpdate::All;

            loadTable = WHSLoadTable::find(_loadId);

            if (loadTable.CarNumber)
            {
                this.VehicleLicensePlate_BR = loadTable.CarNumber;
            }

            if (loadTable.CarState_BR)
            {
                this.VehicleLicensePlateState_BR = loadTable.CarState_BR;
            }

            if (loadTable.CarrierCode)
            {
                tmsCarrier = TMSCarrier::find(loadTable.carrierCode);

                if (!tmsCarrier.validateVendorExistsAtSalesCarrier_BR())
                {
                    throw error("@SYS21628");
                }

                this.BOLCarrierName = tmsCarrier.getSalesCarrierNameByVendor_BR();
            }
        }
    }

]]></Source>
