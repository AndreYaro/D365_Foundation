				<Source><![CDATA[
    private void clearShippingBillNumber_IN()
    {
        SalesParmTable  salesParmTableLoc;
        SalesParmLine   salesParmLineLoc;
        SalesParmLine_IN salesParmLineINLoc;

        // reset CustomsShippingBillNumberTable_IN field value for SalesParmTable and SalesParmLine when SalesParmUpdate.CustomsShippingBill_IN field is modified.
        ttsbegin;
        update_recordset salesParmTableLoc
            setting CustomsShippingBillNumberTable_IN = 0
            where salesParmTableLoc.ParmId == this.ParmId;

        update_recordset salesParmLineINLoc
            setting CustomsShippingBillNumberTable = 0
            where salesParmLineLoc.ParmId == this.ParmId;
        ttscommit;
    }

]]></Source>
