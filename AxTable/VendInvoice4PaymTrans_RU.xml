    /// <summary>
    /// Selects record by <c>RecId</c>.
    /// </summary>
    /// <param name = "_recId">Record Id.</param>
    /// <param name = "_update">Selection mode.</param>
    /// <returns></returns>
    public static VendInvoice4PaymTrans_RU findRecId(RecId   _recId,
                                                     boolean _update = false )
    {
        VendInvoice4PaymTrans_RU vendInvoice4PaymTrans;

        vendInvoice4PaymTrans.selectForUpdate(_update);

        if (_recId)
        {
            select firstonly vendInvoice4PaymTrans
                where vendInvoice4PaymTrans.RecId == _recId;
        }

        return vendInvoice4PaymTrans;
    }

