		<Methods>
			<Method>
				<Name>applyRate</Name>
				<Source><![CDATA[
    /// <summary>
    /// Apply the rate of an accessorial assignment to the current record.
    /// </summary>
    /// <param name="_accessorialAssignment">
    /// Accessorial assignment record that has a rate calculated.
    /// </param>
    void applyRate(TMSAccessorialAssignment _accessorialAssignment)
    {
        this.RateCur = _accessorialAssignment.Fee;
    }

]]></Source>
			</Method>
			<Method>
				<Name>modifiedField</Name>
				<Source><![CDATA[
    public void modifiedField(fieldId _fieldId)
    {
        TMSAccessorialAssignment    accessorialAssignment;

        super(_fieldId);

        if (_fieldId == fieldnum(TMSShipmentAccessorialTable, AccessorialMasterCode))
        {
            select firstOnly accessorialAssignment
                    where accessorialAssignment.CarrierCode              == this.CarrierCode &&
                          accessorialAssignment.CarrierServiceCode       == this.CarrierServiceCode &&
                          accessorialAssignment.AccessorialMasterCode    == this.AccessorialMasterCode;
            this.RefTableId = accessorialAssignment.TableId;
            this.RefRecId = accessorialAssignment.RecId;
            this.applyRate(accessorialAssignment);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if a specific <c>TMSShipmentAccessorialTable</c> record exists.
    /// </summary>
    /// <param name="_recId">
    /// Id of the record.
    /// </param>
    /// <returns>
    /// True if the record exists.
    /// </returns>
    public static boolean exist(RecId   _recId)
    {
        return (_recId) &&
               (select firstonly TMSShipmentAccessorialTable
                    where tmsShipmentAccessorialTable.RecId == _recId).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a specific <c>TMSShipmentAccessorialTable</c> record by id.
    /// </summary>
    /// <param name="_recId">
    /// Id of the record.
    /// </param>
    /// <param name="_forupdate">
    /// Determines whether the record is selected for update.
    /// </param>
    /// <returns>
    /// The record that is searched for.
    /// </returns>
    public static TMSShipmentAccessorialTable find(RecId   _recId,
                                                   boolean _forupdate = false)
    {
        TMSShipmentAccessorialTable    tmsShipmentAccessorialTable;

        if (_recId)
        {
            tmsShipmentAccessorialTable.selectForUpdate(_forupdate);

            select firstonly tmsShipmentAccessorialTable
                where tmsShipmentAccessorialTable.RecId == _recId;
        }

        return tmsShipmentAccessorialTable;
    }

]]></Source>
