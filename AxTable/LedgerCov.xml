			<Method>
				<Name>determineEnterpriseCurrencyAmountWithCurrentRate</Name>
				<Source><![CDATA[
    /// <summary>
    /// Determine enterprise currency amount with the current rate.
    /// </summary>
    /// <param name = "_currencyCode">The transaction currency code.</param>
    /// <param name = "_amountCur">The transaction amount.</param>
    /// <param name = "_companyCurrencyCode">The company currency code.</param>
    /// <param name = "_amountMST">The company amount.</param>
    /// <param name = "_enterpriseCurrencyCode">The enterprise currency code.</param>
    /// <returns>The enterprise currency amount.</returns>
    /// <remarks>The enterprise currency amounts are always calcualted amounts.  They are not revalued.  Therefore, the current exchange rates 
    /// to reflect the current value.  
    /// </remarks>
    public static AmountCur determineEnterpriseCurrencyAmountWithCurrentRate(
        CurrencyCode _currencyCode,
        AmountCur _amountCur,
        CurrencyCode _companyCurrencyCode,
        AmountMST _amountMST,
        CurrencyCode _enterpriseCurrencyCode)
    {
        AmountCur enterpriseCurrencyAmount;

        switch (_enterpriseCurrencyCode)
        {
            case _currencyCode:
                enterpriseCurrencyAmount = _amountCur;
                break;

            case _companyCurrencyCode:
                enterpriseCurrencyAmount = _amountMST;
                break;

            default:

                TransDate currentDate = DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone());
                CurrencyExchangeHelper currencyHelper = CurrencyExchangeHelper::newExchangeDate(Ledger::current(), currentDate);

                ExchangeRateTypeId enterpriseExchangeRateTypeId = SystemParameters::find().SystemExchangeRateType;
                currencyHelper.parmExchangeRateTypeRecId(enterpriseExchangeRateTypeId);

                enterpriseCurrencyAmount = currencyHelper.calculateAccountingToTransaction(_enterpriseCurrencyCode, _amountMST, true);
                break;

        }

        return enterpriseCurrencyAmount;
    }

]]></Source>
