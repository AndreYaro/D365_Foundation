				<Source><![CDATA[
    public static LedgerCov find(
        RefRecId _transRecId,
        recId _recId,
        boolean _forupdate = false,
        ConcurrencyModel _concurrencyModel = ConcurrencyModel::Auto)
    {
        LedgerCov ledgerCov;

        ledgerCov.selectForUpdate(_forupdate);

        if (_forupdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            ledgerCov.concurrencyModel(_concurrencyModel);
        }

        select firstonly ledgerCov
            where ledgerCov.TransRecId == _transRecId
                && ledgerCov.RecId == _recId;

        return ledgerCov;
    }

