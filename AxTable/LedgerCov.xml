			<Method>
				<Name>determineBankAccountCurrencyAmountFromTransactionDate</Name>
				<Source><![CDATA[
    /// <summary>
    /// Determine bank account currency amount.
    /// </summary>
    /// <param name = "_bankAccountMainAccount">The bank account and main account relationships.</param>
    /// <param name = "_currencyHelper">The currency helper.</param>
    /// <param name = "_mainAccount">The main account.</param>
    /// <param name = "_currencyCode">The transaction currency code.</param>
    /// <param name = "_amountCur">The transaction amount.</param>
    /// <param name = "_companyCurrencyCode">The company currency code.</param>
    /// <param name = "_amountMST">The company amount.</param>
    /// <returns>The bank account currency amount.</returns>
    public static AmountCur determineBankAccountCurrencyAmountFromTransactionDate(
        BankAccountMainAccount _bankAccountMainAccount,
        CurrencyExchangeHelper _currencyHelper,
        RefRecId _mainAccount,
        CurrencyCode _currencyCode,
        AmountCur _amountCur,
        CurrencyCode _companyCurrencyCode,
        AmountMST _amountMST)
    {
        AmountCur bankAccountCurrencyAmount;

        if (_bankAccountMainAccount.doesMainAccountHaveBankAccount(_mainAccount))
        {
            CurrencyCode bankCurrencyCode = _bankAccountMainAccount.findBankAccountCurrencyCodeForMainAccount(_mainAccount);
            switch (bankCurrencyCode)
            {
                case _currencyCode:
                    bankAccountCurrencyAmount = _amountCur;
                    break;
                case _companyCurrencyCode:
                    bankAccountCurrencyAmount = _amountMST;
                    break;
                default:
                    bankAccountCurrencyAmount = _currencyHelper.calculateAccountingToTransaction(bankCurrencyCode, _amountMST, true);
                    break;
            }
        }

        return bankAccountCurrencyAmount;
    }

]]></Source>
