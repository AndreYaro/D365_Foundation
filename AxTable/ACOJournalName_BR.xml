				<Name>find</Name>
				<Source><![CDATA[
    static ACOJournalName_BR find(JournalNameId _journalNameId, boolean _forUpdate = false)
    {
        ACOJournalName_BR journalName;

        journalName.selectForUpdate(_forUpdate);

        if (_journalNameId)
            select firstonly journalName
                where journalName.JournalNameId == _journalNameId;

        return journalName;
    }

]]></Source>
