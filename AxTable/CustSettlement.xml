				<Name>checkExist</Name>
				<Source><![CDATA[
    public static boolean checkExist(RecId _recId )
    {
        boolean ret = true;

        if (!CustSettlement::exist(_recId))
        {
            ret = checkFailed(strFmt(CustSettlement::txtNotExist(), _recId));
        }

        return ret;
    }

]]></Source>
