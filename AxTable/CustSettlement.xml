			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    public static boolean exist(RecId _recId )
    {
        boolean found;

        found = (select firstonly
                     RecId
                from
                     custSettlement
                 where
                     custSettlement.TransRecId == _recId).RecId != 0;

        return found;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static CustSettlement find(
        RecId _recId ,
        boolean _forupdate = false,
        ConcurrencyModel _concurrencyModel = ConcurrencyModel::Auto)
    {
        CustSettlement custSettlement;

        custSettlement.selectForUpdate(_forupdate);

        if (_forupdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            custSettlement.concurrencyModel(_concurrencyModel);
        }

        select firstonly
            custSettlement
            where
            custSettlement.RecId == _recId;

        return custSettlement;
    }

]]></Source>
