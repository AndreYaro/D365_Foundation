				<Name>displayReverseDate_RU</Name>
				<Source><![CDATA[
    public display TransDate displayReverseDate_RU()
    {
        TransDate reverseDate;
        CustSettlement custSettlementMain = this.data();

        if (this.ReversedRecId_RU)
        {
            select firstonly TransDate from custSettlementMain
                where custSettlementMain.RecId == this.ReversedRecId_RU;
            reverseDate = custSettlementMain.TransDate;
        }

        return reverseDate;
    }

]]></Source>
