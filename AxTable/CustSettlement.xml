			<Method>
				<Name>insert</Name>
				<Source><![CDATA[
    public void insert()
    {
        this.TransCompany = this.DataAreaId;
        this.EUSalesList = '';

        this.defaultClosedDateForReversed();
        this.defaultTransType();

        super();

        if (BrazilParameters::isEnabled())
        {
            CustTransOpen custTransOpen = CustTransOpen::findRefId(this.TransRecId);
            CustTransEPRemit_BR custTransEPRemit_BR = CustTransEPRemit_BR::findByRefRecId(custTransOpen.RecId, true);

            if (custTransEPRemit_BR)
            {
                custTransEPRemit_BR.CustSettleRefRecId = this.RecId;
                custTransEPRemit_BR.update();
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>defaultClosedDateForReversed</Name>
				<Source><![CDATA[
    private void defaultClosedDateForReversed()
    {
        if (this.CanBeReversed == NoYes::No && this.ClosedDate == dateNull())
        {
            this.ClosedDate = this.TransDate;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>defaultTransType</Name>
				<Source><![CDATA[
    private void defaultTransType()
    {
        if (this.TransType == LedgerTransType::None)
        {
            this.TransType = CustTrans::find(this.TransRecId).TransType;
        }
    }

]]></Source>
