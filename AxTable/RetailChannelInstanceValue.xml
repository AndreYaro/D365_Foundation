			<Method>
				<Name>findByChannel</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Find the <c>RetailChannelInstanceValue</c> table record by a channel Id
    /// </summary>
    /// <param name="_channel">
    ///     Channel id
    /// </param>
    /// <param name="_forupdate">
    ///     A Boolean value that indicates whether the record must be selected for update; optional.
    /// </param>
    /// <returns>
    ///     A <c>RetailChannelInstanceValue</c> table record, or an empty buffer.
    /// </returns>
    public static RetailChannelInstanceValue findByChannel(RefRecId _channel, boolean _forupdate = false)
    {
        RetailChannelInstanceValue retailChannelInstanceValue;

        if (_channel)
        {
            retailChannelInstanceValue.selectForUpdate(_forupdate);
            select firstonly retailChannelInstanceValue
                where retailChannelInstanceValue.Channel == _channel;
        }
        return retailChannelInstanceValue;
    }

]]></Source>
