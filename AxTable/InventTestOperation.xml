		<Methods>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Determines whether the specified record in the <c>InventTestOperation</c> table exists.
    /// </summary>
    /// <param name="_inventTestOperationId">
    ///    The test operation ID of the record to find.
    /// </param>
    /// <returns>
    ///    true if the specified record exists; otherwise, false.
    /// </returns>
    public static boolean exist(InventTestOperationId _inventTestOperationId)
    {
        return _inventTestOperationId &&
        (select     firstonly inventTestOperation
         where  inventTestOperation.OperationId == _inventTestOperationId).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Finds the specified record in the <c>InventTestOperation</c> table.
    /// </summary>
    /// <param name="_inventTestOperationId">
    ///    The test operation ID of the record to find.
    /// </param>
    /// <param name="_forUpdate">
    ///    A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <returns>
    ///    A record in the <c>InventTestOperation</c> table; otherwise, an empty record.
    /// </returns>
    public static InventTestOperation   find(InventTestOperationId _inventTestOperationId, boolean _forUpdate = false)
    {
        InventTestOperation     inventTestOperation;

        inventTestOperation.selectForUpdate(_forUpdate);

        if (_inventTestOperationId)
        {
            select  firstonly inventTestOperation
            where   inventTestOperation.OperationId == _inventTestOperationId;
        }

        return inventTestOperation;
    }

]]></Source>
