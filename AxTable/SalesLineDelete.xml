        this.ItemId         = _salesLine.ItemId;
        this.SalesCategory  = _salesLine.SalesCategory;
        this.Name           = _salesLine.Name;
        this.LineAmount     = _salesLine.LineAmount;
        this.InventTransId  = _salesLine.InventTransId;
        this.SalesType      = _salesLine.SalesType;
        this.ProjCategoryId = _salesLine.ProjCategoryId;
        this.ProjId         = _salesLine.ProjId;
        this.ActivityNumber = _salesLine.ActivityNumber;
        this.InventDimId    = _salesLine.InventDimId;

        this.SalesLine      = [_salesLine];
    }

]]></Source>
