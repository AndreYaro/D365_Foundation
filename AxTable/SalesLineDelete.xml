			<Method>
				<Name>createFromSalesLine</Name>
				<Source><![CDATA[
        static void createFromSalesLine(SalesLine _salesLine)
    {
        SalesLineDelete salesLineDelete;

        // <GEEPL>
        #ISOCountryRegionCodes
        // </GEEPL>

        ttsbegin;

        salesLineDelete.clear();
        salesLineDelete.initFromSalesLine(_salesLine);
        if (_salesLine.isCategoryBased())
        {
            salesLineDelete.initFromSalesLineForeignTradeCategory(SalesLineForeignTradeCategory::findSalesLine(_salesLine));
        }
        salesLineDelete.insert();

        // <GEEPL>
        if (SysCountryRegionCode::isLegalEntityInCountryRegion([#isoPL]))
        {
            TaxServiceTariff::insertTaxServiceTariffForNewParent(_salesLine.TableId, _salesLine.RecId, salesLineDelete.TableId, salesLineDelete.RecId);
        }
        // </GEEPL>

        Docu::copy(_salesLine, salesLineDelete);

        if (!SalesTableDelete::exist(_salesLine.SalesId))
            SalesTableDelete::createFromSalesTable(_salesLine.salesTable(), Voided::linesVoided);

        ttscommit;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
        static boolean  exist(SalesId   salesId,
                                 LineNum   lineNum = naReal())
    {
        if (!salesId)
            return false;

        if (lineNum == naReal())
        {
            return (select firstonly RecId from salesLineDelete
                        index hint SalesLineIdx
                        where salesLineDelete.SalesId     == salesId).RecId != 0;
        }

        return (select firstonly RecId from salesLineDelete
                    index hint SalesLineIdx
                    where salesLineDelete.SalesId     == salesId  &&
                          salesLineDelete.LineNum     == lineNum).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>shouldThrowExceptionOnZeroDelete</Name>
				<Source><![CDATA[
    /// <summary>
    /// Determines if concurrent deletes should throw exception.
    /// </summary>
    /// <returns>true if exception should be thrown; otherwise false.</returns>
    public boolean shouldThrowExceptionOnZeroDelete()
    {
        return super() || SalesEnforceConcurrentDeletesFlight::instance().isEnabled();
    }

]]></Source>
			</Method>
