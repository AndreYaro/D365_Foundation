		<Methods>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if a record of <c>RetailTransactionFiscalTransExtendedData</c> exists.
    /// </summary>
    /// <param name="_transactionId">The retail transaction id.</param>
    /// <param name="_storeId">The store id.</param>
    /// <param name="_terminalId">The terminal id.</param>
    /// <param name="_channel">The channel record id.</param>
    /// <param name="_fiscalTransLineNum">The fiscal transaction line number.</param>
    /// <param name="_fiscalTransRecordGuid">The fiscal transaction record guid.</param>
    /// <returns>
    /// True if <c>RetailTransactionFiscalTransExtendedData</c> exists; otherwise, false.
    /// </returns>
    internal static boolean exist(
        RetailTransactionId _transactionId,
        RetailStoreId _storeId,
        RetailTerminalId _terminalId,
        RefRecId _channel,
        LineNum _fiscalTransLineNum,
        SysGUID _fiscalTransRecordGuid)
    {
        RetailTransactionFiscalTransExtendedData fiscalTransExtData;

        if (_transactionId && _storeId && _terminalId && _channel && _fiscalTransRecordGuid)
        {
            select firstonly RecId from fiscalTransExtData
                    where fiscalTransExtData.transactionId == _transactionId
                       && fiscalTransExtData.store == _storeId
                       && fiscalTransExtData.terminal == _terminalId
                       && fiscalTransExtData.Channel == _channel
                       && fiscalTransExtData.FiscalTransLineNum == _fiscalTransLineNum
                       && fiscalTransExtData.FiscalTransRecordGuid == _fiscalTransRecordGuid;
        }

        return fiscalTransExtData.RecId != 0;
    }

]]></Source>
			</Method>
		</Methods>
