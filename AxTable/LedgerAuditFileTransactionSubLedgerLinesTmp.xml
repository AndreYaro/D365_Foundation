		<Methods>
			<Method>
				<Name>getTaxTransactions</Name>
				<Source><![CDATA[
    /// <summary>
    /// Returns a query of tax transactions linked to the current record.
    /// </summary>
    /// <returns>A query that has tax transactions as a first datasource.</returns>
    [ERTableName(tableStr(TaxTrans)), Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
    public Query getTaxTransactions()
    {
        var ret = new Query();
        var taxTransDs = ret.addDataSource(tableNum(TaxTrans));
        taxTransDs.addSelectionField(fieldNum(TaxTrans,TaxCode));
        taxTransDs.addSelectionField(fieldNum(TaxTrans,TaxAmount));
        taxTransDs.addSelectionField(fieldNum(TaxTrans,TaxValue));
        taxTransDs.addRange(fieldNum(TaxTrans,Voucher)).value(this.Voucher);
        taxTransDs.addRange(fieldNum(TaxTrans,TransDate)).value(queryValue(this.TransDate));
        return ret;
    }

]]></Source>
			</Method>
		</Methods>
