    /// The <c>RetailStoreId</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_terminalId">
    /// The <c>RetailTerminalId</c> value of the record to be retrieved.
    /// </param>
    /// <param name = "_forUpdate">
    /// A boolean value to determine whether the record should be retrieved for update or not; optional.
    /// </param>
    /// <returns>
    /// A <c>RetailFiscalDocumentModel2_BR</c> table buffer.
    /// </returns>
    [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
    public static RetailFiscalDocumentModel2_BR find(  RetailTransactionId _transactionId,
                                                RetailStoreId       _storeId,
                                                RetailTerminalId    _terminalId,
                                                boolean             _forUpdate = false)
    {
        RetailFiscalDocumentModel2_BR retailFiscalDocumentModel2;

        if (_transactionId)
        {
            retailFiscalDocumentModel2.selectForUpdate(_forUpdate);

            select firstonly retailFiscalDocumentModel2
                where retailFiscalDocumentModel2.StoreId       == _storeId
                &&    retailFiscalDocumentModel2.TerminalId    == _terminalId
                &&    retailFiscalDocumentModel2.TransactionId == _transactionId;
        }

        return retailFiscalDocumentModel2;
    }

