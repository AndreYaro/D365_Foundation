			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Retrieves a <c>RetailFiscalReceiptTaxTrans_BR</c> table record with the given values, if found.
    /// </summary>
    /// <param name="_lineNum">
    /// The <c>LineNum</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_taxType">
    /// The <c>TaxType_BR</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_transactionId">
    /// The <c>RetailTransactionId</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_storeId">
    /// The <c>RetailStoreId</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_terminalId">
    /// The <c>RetailTerminalId</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_forUpdate">
    /// A boolean value to determine whether the record should be retrieved for update or not; optional.
    /// </param>
    /// <returns>
    /// A <c>RetailFiscalReceiptTaxTrans_BR</c> table buffer.
    /// </returns>
    [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
    public static RetailFiscalReceiptTaxTrans_BR find(  LineNum          _lineNum,
                                                        TaxType_BR          _taxType,
                                                        RetailTransactionId _transactionId,
                                                        RetailStoreId       _storeId,
                                                        RetailTerminalId    _terminalId,
                                                        boolean             _forUpdate = false)
    {
        RetailFiscalReceiptTaxTrans_BR retailFiscalReceiptTaxTrans;

        if (_lineNum && _transactionId && _taxType)
        {
            retailFiscalReceiptTaxTrans.selectForUpdate(_forUpdate);

            select firstonly retailFiscalReceiptTaxTrans
                where retailFiscalReceiptTaxTrans.StoreId       == _storeId
                &&    retailFiscalReceiptTaxTrans.TerminalId    == _terminalId
                &&    retailFiscalReceiptTaxTrans.TransactionId == _transactionId
                &&    retailFiscalReceiptTaxTrans.LineNum       == _lineNum
                &&    retailFiscalReceiptTaxTrans.TaxType       == _taxType;
        }

        return retailFiscalReceiptTaxTrans;
    }

]]></Source>
