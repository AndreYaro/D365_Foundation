			<Method>
				<Name>insertPost_BR</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates electronic payment
    /// </summary>
    /// <remarks>
    /// Updated with settlement reference
    /// </remarks>
    public void insertPost_BR()
    {
        VendTransEPRemit_BR vendTransEPRemit_BR;
        VendTransOpen       vendTransOpen;

        if (!this.RecId)
        {
            return;
        }

        vendTransOpen = VendTransOpen::findRefId(this.TransRecId);
        vendTransEPRemit_BR = VendTransEPRemit_BR::find(vendTransOpen.AccountNum, vendTransOpen.RecId, true);
        if (vendTransEPRemit_BR)
        {
            vendTransEPRemit_BR.VendSettleRefRecId = this.RecId;
            vendTransEPRemit_BR.update();
        }
    }

]]></Source>
