			<Method>
				<Name>displayReverseDate_RU</Name>
				<Source><![CDATA[
    display TransDate displayReverseDate_RU()
    {
        TransDate      reverseDate;
        VendSettlement vendSettlementMain = this.data();

        if (this.ReversedRecId_RU)
        {
            select firstonly TransDate from vendSettlementMain
                where vendSettlementMain.RecId == this.ReversedRecId_RU;

            reverseDate = vendSettlementMain.TransDate;
        }

        return reverseDate;
    }

]]></Source>
