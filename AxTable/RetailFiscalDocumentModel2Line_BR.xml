				<Source><![CDATA[
    /// <summary>
    /// Retrieves a <c>RetailFiscalDocumentModel2Line_BR</c> table record with the given values, if found.
    /// </summary>
    /// <param name="_lineNum">
    /// The <c>LineNum</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_transactionId">
    /// The <c>RetailTransactionId</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_storeId">
    /// The <c>RetailStoreId</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_terminalId">
    /// The <c>RetailTerminalId</c> value of the record to be retrieved.
    /// </param>
    /// <param name="_forUpdate">
    /// A boolean value to determine whether the record should be retrieved for update or not; optional.
    /// </param>
    /// <returns>
    /// A <c>RetailFiscalDocumentModel2Line_BR</c> table buffer.
    /// </returns>
    [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
    public static RetailFiscalDocumentModel2Line_BR find(  LineNum  _lineNum,
                                                RetailTransactionId _transactionId,
                                                RetailStoreId       _storeId,
                                                RetailTerminalId    _terminalId,
                                                boolean             _forUpdate = false)
    {
        RetailFiscalDocumentModel2Line_BR retailFiscalDocumentModel2Line;

        if (_lineNum && _transactionId)
        {
            retailFiscalDocumentModel2Line.selectForUpdate(_forUpdate);

            select firstonly retailFiscalDocumentModel2Line
                where retailFiscalDocumentModel2Line.StoreId       == _storeId
                &&    retailFiscalDocumentModel2Line.TerminalId    == _terminalId
                &&    retailFiscalDocumentModel2Line.TransactionId == _transactionId
                &&    retailFiscalDocumentModel2Line.LineNum       == _lineNum;
        }

        return retailFiscalDocumentModel2Line;
    }

]]></Source>
