				<Source><![CDATA[
    /// <summary>
    /// Finds the specified asset rule record in the <c>AssetRuleQualifierOptionLanguageLocal</c> table.
    /// </summary>
    /// <param name="_assetRuleQualifierOptionLocal">
    /// A record ID of the record in the <c>AssetRuleQualifierOptionLocal</c> table.
    /// </param>
    /// <returns>
    /// true if record exist in the <c>AssetRuleQualifierOptionLanguageLocal</c> table; otherwise, false.
    /// </returns>
    public static boolean existsQualifierOptionLanguageLocal(RefRecId _assetRuleQualifierOptionLocal)
    {
        AssetRuleQualifierOptionLanguageLocal assetRuleQualifierOptionLanguageLocal;

        select firstonly RecId from assetRuleQualifierOptionLanguageLocal
            where assetRuleQualifierOptionLanguageLocal.AssetRuleQualifierOptionLocal == _assetRuleQualifierOptionLocal;

        return assetRuleQualifierOptionLanguageLocal.RecId != 0;
    }

