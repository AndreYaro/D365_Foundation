    /// The record ID of the record to find.
    /// </param>
    /// <param name="_forupdate">
    /// A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <returns>
    /// A record in the <c>PCRuntimeCache</c> table; otherwise, an empty record.
    /// </returns>
    public static PCRuntimeCache find(RefRecId _recId, boolean _forupdate = false)
    {
        PCRuntimeCache record;

        if (!_recId)
        {
            return record;
        }
        record.selectForUpdate(_forupdate);
        select firstonly record where record.RecId == _recId;
        return record;
    }

