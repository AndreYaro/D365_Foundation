		<Methods>
			<Method>
				<Name>addCoveringDateRanges</Name>
				<Source><![CDATA[
    /// <summary>
    /// Stores all the date intevals that are covering the validtity of the catalog based on assortment.
    /// </summary>
    /// <param name="_catalog">
    /// Catalog record.
    /// </param>
    /// <remarks>
    /// Make sure that the actual products in the catalog is calculated before calling this.
    /// </remarks>
    public static void addCoveringDateRanges(Catalog _catalog)
    {
        RetailTmpAssortedProductDateRange tmpRetailAssortedProductDateRange;
        RetailTmpCatalogProductDateRange tmpRetailCatalogProductDateRange;
        date catalogValidFrom, catalogValidTo;

        catalogValidFrom = _catalog.ValidFrom;
        catalogValidTo = _catalog.ValidTo;

        insert_recordset tmpRetailCatalogProductDateRange (Catalog, Product, ValidFrom, ValidTo, OMOperatingUnitId)
        select Catalog, Product, catalogValidFrom, catalogValidTo, OmOperatingUnitId
        from tmpRetailAssortedProductDateRange
        group by Catalog, Product, OmOperatingUnitId
        where tmpRetailAssortedProductDateRange.Catalog == _catalog.RecId &&
              tmpRetailAssortedProductDateRange.ValidFrom <= _catalog.ValidFrom &&
              tmpRetailAssortedProductDateRange.ValidTo >= _catalog.ValidTo;
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
