				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if a specific <c>TMSRouteOverride</c> record exists.
    /// </summary>
    /// <param name="_recId">
    /// Id of the record.
    /// </param>
    /// <returns>
    /// True if the record exists.
    /// </returns>
    public static boolean exist(RecId   _recId)
    {
        return (_recId) &&
               (select firstonly TMSRouteOverride
                    where tmsRouteOverride.RecId == _recId).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a specific <c>TMSRouteOverride</c> record by id.
    /// </summary>
    /// <param name="_recId">
    /// Id of the record.
    /// </param>
    /// <param name="_forupdate">
    /// Determines whether the record is selected for update.
    /// </param>
    /// <returns>
    /// The record that is searched for.
    /// </returns>
    public static TMSRouteOverride find(RecId   _recId,
                                         boolean _forupdate = false)
    {
        TMSRouteOverride    tmsRouteOverride;

        if (_recId)
        {
            tmsRouteOverride.selectForUpdate(_forupdate);

            select firstonly tmsRouteOverride
                where tmsRouteOverride.RecId == _recId;
        }

        return tmsRouteOverride;
    }

]]></Source>
