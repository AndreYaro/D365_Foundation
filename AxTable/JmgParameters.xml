				<Name>getAbsenceJobId</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Retrieve the absence or overtime/flex job id based on whether the worker is flex enabled or not.
    /// </summary>
    /// <param name="_worker">
    ///     Worker id
    /// </param>
    /// <returns>
    ///     Absence or overtime/flex job id.
    /// </returns>
    public JmgJobIDAbs getAbsenceJobId(JmgWorkerRecId _worker)
    {
        return JmgEmployee::find(_worker).isFlexProfile() ?
                this.OverTimeAndFlexLimitJobID : this.AbsenceJobID;
    }

]]></Source>
