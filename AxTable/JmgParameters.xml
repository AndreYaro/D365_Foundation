			<Method>
				<Name>validateField</Name>
				<Source><![CDATA[
    public boolean validateField(FieldId _fieldIdToCheck)
    {
        boolean ret = super(_fieldIdToCheck);
    
        switch (_fieldIdToCheck)
        {
            case fieldNum(JmgParameters, AbsenceJobID):
                if (this.AbsenceJobID)
                {
                    ret = this.validateOverTimeAndAbsence(this.AbsenceJobID, false) && ret;
                }
                break;
        }
    
        return ret;
    }

]]></Source>
