				<Name>initValue</Name>
				<Source><![CDATA[
    void initValue()
    {
        super();

        // General tab - System
        this.SignOutString                  = strRem("@SYS38411",' ');
        this.UsePINcode                     = NoYes::Yes;
        this.UseBadgeId                     = NoYes::No;
        this.SecondPresentation             = JmgSecondPresentationEnum::Centitime;
        this.KeepAssistance                 = NoYes::No;
        this.RestartOnSignIn                = NoYes::No;
        this.JobTableSynchronizationMode    = JmgJobTableSynchronizationMode::Online;
        this.RegistrationErrorMode          = JmgRegistrationErrorMode::Prohibit;
        this.GetRegistrationTimeFrom        = JmgGetRegistrationTimeFrom::AOSServer;

        // General tab - Clock-out
        this.MaxMinutes                     = 1200;
        this.AutoSignout                    = NoYes::Yes;

        // General tab - Synchronization
        this.SignUpdateClock                = 10;       // Every 10 seconds
        this.SignUpdateBoard                = 60 * 2;   // Every 2 minuts refresh
        this.SignUpdateWrkCtr               = 60 * 5;   // Every 5 minuts refresh
        this.SignWaitForJob                 = 30;       // Every 30 seconds refresh

        // General tab - Absence
        this.TransferAbsenceToHRM           = NoYes::No;

        // Project tab - Project
        this.ProjJournalNameId              = '';
        this.CostUseStandardCostProj        = NoYes::Yes;
        this.ProjStatusUpdate               = JmgProjStatusUpdate::Status;

        // Project tab - Dimension
        this.ProjDimensionFrom              = JmgDimensionFrom::Job;

        // Indirect activities - IPC Recording
        this.JournalName                    = '';
        this.CostUseStandardCostIPC         = NoYes::No;

        // Indirect activities - Dimension
        this.IpcJobDimensionFrom            = JmgDimensionFrom::Employee;
        this.IpcAbsenceDimensionFrom        = JmgDimensionFrom::Employee;
        this.IpcBreakDimensionFrom          = JmgDimensionFrom::Employee;

        // Cost price - Direct payroll costs.
        this.CostBuildNormTime              = JmgCostSlizeNoYesEnum::Yes;
        this.CostBuildOverTime              = JmgCostSlizeNoYesEnum::Slize;
        this.CostBuildBreak                 = JmgCostSlizeNoYesEnum::Slize;
        this.CostBuildFlexAdd               = JmgCostSlizeNoYesEnum::Yes;
        this.CostBuildPayAdd                = JmgCostSlizeNoYesEnum::Yes;
        this.CostBuildPayAddManual          = JmgCostSlizeNoYesEnum::No;

        // Cost price - Indirect payroll costs
        this.CostBuildFlexSub               = JmgCostSlizeNoEnum::No;
        this.CostBuildAbsence               = JmgCostSlizeNoEnum::No;

        // Cost price - Cost price factor
        this.CostBuildFactor                = 1;

        // Payroll - Payroll
        this.PayExportType                  = JmgPayEventsExportType::Standard;
        this.ErrorLevelOnCancelation        = JmgErrorTolerance::Error;

        // Display options - Default values
        this.DefaultReportAsFinish          = JmgFeedbackStatus::InProgress;

        // Display options - Electronic time-card
        this.TimecardInputMode              = JmgTimecardInputMode::Timestamps;
        this.TimecardShowStartDate          = NoYes::Yes;
        this.TimecardShowStopDate           = NoYes::Yes;

        // Number sequences (none to set)

        // Terminals
        this.TerminalInsertMode             = JmgTerminalInsertMode::StartStop;
    }

]]></Source>
