			<Method>
				<Name>validateOverTimeAndAbsence</Name>
				<Source><![CDATA[
    private boolean validateOverTimeAndAbsence(JmgJobId _jmgJobId, boolean _isOverTimeAndFlex)
    {
        boolean ret = true;
        HRMAbsenceCode hrmAbsenceCode = HRMAbsenceCode::findByJobId(_jmgJobId);

        if (!_isOverTimeAndFlex && hrmAbsenceCode.isFlexEnabled())
        {
            ret = checkFailed(strFmt("@SCM:CannotAddAbsenceCodeGroupSetUp", _jmgJobId, hrmAbsenceCode.hrmAbsenceCodeGroupId));
        }

        return ret;
    }

]]></Source>
