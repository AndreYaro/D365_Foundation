			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Retrieves the <c>JmgParameters</c> record from the database.
    /// </summary>
    /// <param name="_forUpdate">
    /// true if the returned record must be selected for update; otherwise, false.
    /// </param>
    /// <returns>
    /// A <c>JmgParameters</c> record.
    /// </returns>
    static JmgParameters find(boolean _forUpdate = false)
    {
        JmgParameters jmgParameters;
        // Race conditions can occur during upgrade, when the parameter record is created.
        try
        {
            jmgParameters.selectForUpdate(_forUpdate);

            select firstonly jmgParameters
                index Key
                where jmgParameters.Key == 0;

            if (!jmgParameters && !jmgParameters.isTmp())
            {
                Company::createParameter(jmgParameters);
            }
        }
        catch (Exception::UpdateConflict)
        {
            retry;
        }
        catch (Exception::DuplicateKeyException)
        {
            retry;
        }

        return jmgParameters;
    }

]]></Source>
