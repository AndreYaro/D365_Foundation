			<Method>
				<Name>checkSFCSetupOK</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks whether the parameters are set up correctly.
    /// </summary>
    /// <param name="onErrorThrow">
    /// true if an error must be thrown when setup is not ok; otherwise, false.
    /// </param>
    /// <returns>
    /// true if the parameters are set up ok; otherwise, false.
    /// </returns>
    /// <exception cref="M:Exception::Error">
    /// The parameters are not setup correctly.
    /// </exception>
    static boolean checkSFCSetupOK(boolean onErrorThrow=true)
    {
        boolean ret = true;
        str     errTxt1, errTxt2;
        if ((!(select firstonly RecId from jmgParameters).RecId))
        {
            ret = false;
        }
        if ((!(select firstonly RecId from jmgIpcActivity where jmgIpcActivity.SignInOut == JmgSignInOut::SignIn).RecId))
        {
            errTxt1 = "@SYS39168";
            ret = false;
        }
        if ((!(select firstonly RecId from jmgIpcActivity where jmgIpcActivity.SignInOut == JmgSignInOut::SignOut).RecId))
        {
            errTxt2 = "@SYS39169";
            ret = false;
        }

        if (! ret)
        {
            error("@SYS19406");
            if (errTxt1)
                error(errTxt1);
            if (errTxt2)
                error(errTxt2);
            if (onErrorThrow)
                throw error("@SYS39012");
        }
        return ret;
    }

]]></Source>
