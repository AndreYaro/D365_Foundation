		<Methods>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static boolean exist(RHRMViolationCodeId    _rhrmViolationCodeId)
    {
        return _rhrmViolationCodeId && (select RecId from rhrmViolationCode
                                            where rhrmViolationCode.ViolationCodeId == _rhrmViolationCodeId).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static RHRMViolationCode find(RHRMViolationCodeId  _rhrmViolationCodeId,
                                         boolean              _forUpdate = false)
    {
        RHRMViolationCode rhrmViolationCode;
        ;

        if (_rhrmViolationCodeId)
        {
            rhrmViolationCode.selectForUpdate(_forUpdate);

            select firstonly rhrmViolationCode
                where rhrmViolationCode.ViolationCodeId == _rhrmViolationCodeId;
        }
        return rhrmViolationCode;
    }

]]></Source>
