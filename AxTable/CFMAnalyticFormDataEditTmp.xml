    /// <summary>
    /// Lookup form for select Paym Spec
    /// </summary>
    /// <param name = "_formControl"></param>
    /// <param name = "_dataEditTmp"></param>
    public static void lookupPaymSpec(FormControl _formControl, CFMAnalyticFormDataEditTmp _dataEditTmp)
    {
        SysTableLookup  sysTableLookup;
        Query query = new Query();
        QueryBuildDataSource qbdsPaymSpec;

        switch (_dataEditTmp.AccountType)
        {
            case CFMAccountType::Cust:
                sysTableLookup = SysTableLookup::newParameters(tableNum(CustPaymModeSpec), _formControl);
                query.allowCrossCompany(true);
                query.addCompanyRange(_dataEditTmp.PaymentDataAreaId);
                qbdsPaymSpec = query.addDataSource(tableNum(CustPaymModeSpec));
                if (_dataEditTmp.PaymModeNew)
                {
                    qbdsPaymSpec.addRange(fieldNum(CustPaymModeSpec, PaymMode)).value(_dataEditTmp.PaymModeNew);
                }
                else
                {
                    qbdsPaymSpec.addRange(fieldNum(CustPaymModeSpec, PaymMode)).value(SysQuery::valueEmptyString());
                }

                sysTableLookup.addLookupfield(fieldNum(CustPaymModeSpec, Specification));
                sysTableLookup.addLookupfield(fieldNum(CustPaymModeSpec, Name));
                break;

            case CFMAccountType::Vend:
                sysTableLookup = SysTableLookup::newParameters(tableNum(VendPaymModeSpec), _formControl);
                query.allowCrossCompany(true);
                query.addCompanyRange(_dataEditTmp.PaymentDataAreaId);
                qbdsPaymSpec = query.addDataSource(tableNum(VendPaymModeSpec));
                if (_dataEditTmp.PaymModeNew)
                {
                    qbdsPaymSpec.addRange(fieldNum(VendPaymModeSpec, PaymMode)).value(_dataEditTmp.PaymModeNew);
                }
                else
                {
                    qbdsPaymSpec.addRange(fieldNum(VendPaymModeSpec, PaymMode)).value(SysQuery::valueEmptyString());
                }

                sysTableLookup.addLookupfield(fieldNum(VendPaymModeSpec, Specification));
                sysTableLookup.addLookupfield(fieldNum(VendPaymModeSpec, Name));
                break;
        }

        if (sysTableLookup)
        {
            sysTableLookup.parmQuery(query);
            sysTableLookup.performFormLookup();
        }
    }

