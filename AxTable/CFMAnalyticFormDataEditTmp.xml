			<Method>
				<Name>initFromPaymMode</Name>
				<Source><![CDATA[
    /// <summary>
    /// Init record from link CustVendPaymModeTable
    /// </summary>
    public void initFromPaymMode()
    {
        CustVendPaymModeTable paymModeTable = this.custVendPaymMode();

        this.PaymentAccountNumNew = '';

        if (paymModeTable.RecId)
        {
            if (paymModeTable.AccountType == LedgerJournalACType::Bank || paymModeTable.AccountType == LedgerJournalACType::RCash)
            {
                this.PaymentAccountNumNew = paymModeTable.parmPaymAccount();

                if (paymModeTable.AccountType == LedgerJournalACType::Bank)
                {
                    this.PaymentAccountTypeNew = CFMPaymentAccountType::Bank;
                }
                else
                {
                    this.PaymentAccountTypeNew = CFMPaymentAccountType::Cash_RU;
                }
            }
        }
    }

]]></Source>
