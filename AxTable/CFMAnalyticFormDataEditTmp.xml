			<Method>
				<Name>custVendPaymMode</Name>
				<Source><![CDATA[
    /// <summary>
    /// return link CustVendPaymModeTable record
    /// </summary>
    /// <returns> return CustVendPaymModeTable record </returns>
    public CustVendPaymModeTable custVendPaymMode()
    {
        CustVendPaymModeTable paymModeTable;

        switch (this.AccountType)
        {
            case CFMAccountType::Cust:
                paymModeTable = CustPaymModeTable::findByCompany(this.PaymentDataAreaId, this.PaymModeNew);
                break;

            case CFMAccountType::Vend:
                paymModeTable = VendPaymModeTable::findByCompany(this.PaymentDataAreaId, this.PaymModeNew);
                break;
        }

        return paymModeTable;
    }

]]></Source>
