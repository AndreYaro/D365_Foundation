			<Method>
				<Name>validatePaymAccount</Name>
				<Source><![CDATA[
    /// <summary>
    /// Vsalidate correct value for PaymAccount
    /// </summary>
    /// <param name = "_currencyCode"></param>
    /// <param name = "_paymentAccountNum"></param>
    /// <returns></returns>
    public boolean validatePaymAccount(CurrencyCode _currencyCode, CFMPaymentAccountNum _paymentAccountNum = this.PaymentAccountNumNew)
    {
        boolean ret = true;

        switch (this.PaymentAccountTypeNew)
        {
            case CFMPaymentAccountType::Bank:
                BankAccountTable bankTable = BankAccountTable::findByCompany(this.PaymentDataAreaId, _paymentAccountNum);
                ret = bankTable.CurrencyCode == _currencyCode || bankTable.BankMultiCurrency;
                break;
        }

        return ret;
    }

]]></Source>
