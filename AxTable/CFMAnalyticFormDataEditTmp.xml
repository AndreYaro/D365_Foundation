			<Method>
				<Name>validatePaymMode</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validate paym Mode value
    /// </summary>
    /// <param name = "_currencyCode"></param>
    /// <returns></returns>
    public boolean validatePaymMode(CurrencyCode _currencyCode)
    {
        CustVendPaymModeTable paymModeTable = this.custVendPaymMode();
        boolean ret = true;

        if (paymModeTable)
        {
            switch (paymModeTable.AccountType)
            {
                case LedgerJournalACType::Bank:
                    if (paymModeTable.parmPaymAccount())
                    {
                        ret = this.validatePaymAccount(_currencyCode, paymModeTable.parmPaymAccount());
                    }
                    else
                    {
                        ret = true;
                    }
                    break;
            }
        }
        return ret;
    }

]]></Source>
