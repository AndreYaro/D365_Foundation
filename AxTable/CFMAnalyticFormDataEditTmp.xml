			<Method>
				<Name>lookupPaymentAccountNum</Name>
				<Source><![CDATA[
    /// <summary>
    /// Lookup form for select Payment Account num
    /// </summary>
    /// <param name = "_formControl"></param>
    /// <param name = "_dataEditTmp"></param>
    /// <param name = "_currencyCode"></param>
    public static void lookupPaymentAccountNum(FormControl _formControl, CFMAnalyticFormDataEditTmp _dataEditTmp, CurrencyCode _currencyCode)
    {
        SysTableLookup  sysTableLookup;
        Query query = new Query();

        if (_dataEditTmp.PaymentAccountTypeNew == CFMPaymentAccountType::Bank)
        {
            sysTableLookup = SysTableLookup::newParameters(tableNum(BankAccountTable), _formControl);
            query.allowCrossCompany(true);
            query.addCompanyRange(_dataEditTmp.PaymentDataAreaId);
            QueryBuildDataSource qbdsBankAccountTable = query.addDataSource(tableNum(BankAccountTable));

            str currencyFilterValue = strFmt('((%1.%2 == "%4") || (%1.%3 == %5))',
                                             qbdsBankAccountTable.name(),
                                             fieldStr(BankAccountTable, CurrencyCode),
                                             fieldStr(BankAccountTable, BankMultiCurrency),
                                             _currencyCode,
                                             any2int(NoYes::Yes));

            qbdsBankAccountTable.addRange(fieldNum(BankAccountTable, CurrencyCode)).value(currencyFilterValue);

            sysTableLookup.addLookupfield(fieldNum(BankAccountTable, AccountId));
            sysTableLookup.addLookupfield(fieldNum(BankAccountTable, Name));
        }
        else
        {
            sysTableLookup = SysTableLookup::newParameters(tableNum(RCashTable), _formControl);
            query.allowCrossCompany(true);
            query.addCompanyRange(_dataEditTmp.PaymentDataAreaId);
            QueryBuildDataSource qbdsCashTable = query.addDataSource(tableNum(RCashTable));
            qbdsCashTable.addRange(fieldNum(RCashTable, CurrencyCode)).value(_currencyCode);

            sysTableLookup.addLookupfield(fieldNum(RCashTable, AccountNum));
            sysTableLookup.addLookupfield(fieldNum(RCashTable, Name));
        }

        if (sysTableLookup)
        {
            sysTableLookup.parmQuery(query);
            sysTableLookup.performFormLookup();
        }
    }

]]></Source>
