			<Method>
				<Name>modifiedField</Name>
				<Source><![CDATA[
    public void modifiedField(FieldId _fieldId)
    {
        super(_fieldId);

        switch (_fieldId)
        {
            case fieldNum(CFMAnalyticFormDataEditTmp, PaymentAccountTypeNew):
                this.PaymentAccountNumNew = '';
                this.PaymModeNew = '';
                this.PaymSpecNew = '';
                break;

            case fieldNum(CFMAnalyticFormDataEditTmp, PaymModeNew):
                this.initFromPaymMode();
                this.PaymSpecNew = '';
                break;
        }
    }

]]></Source>
