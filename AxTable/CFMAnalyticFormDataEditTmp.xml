				<Name>fieldIdChanges</Name>
				<Source><![CDATA[
    /// <summary>
    /// Create container with field Ids which contain new values
    /// </summary>
    /// <returns>
    /// Container with field Ids
    /// </returns>
    /// <exception cref="Exception::Error">
    /// Throws exception if field doesn't have appropriate type
    /// </exception>
    public container fieldIdChanges()
    {
        container conChanges;
        Map fieldsMap = CFMAnalyticFormDataEditTmp::fieldIdNewValues();
        MapEnumerator mapEnum = fieldsMap.getEnumerator();

        while (mapEnum.moveNext())
        {
            FieldId fieldIdNew = mapEnum.currentKey();
            FieldId fieldIdOld = mapEnum.currentValue();
            DictField dictField = SysDictField::findFieldById(this.TableId, fieldIdNew);
            str strNewValue;
            date dateNewValue;
            boolean newValueIsNull;

            switch (dictField.baseType())
            {
                case Types::String:
                    strNewValue = this.(fieldIdNew);
                    newValueIsNull = strNewValue == '';
                    break;

                case Types::Date:
                    dateNewValue = this.(fieldIdNew);
                    newValueIsNull = dateNewValue == dateNull();
                    break;

                case Types::Enum:
                    newValueIsNull = false;
                    break;

                default:
                    throw error(error::missingParameter(null));
            }

            if (this.(fieldIdNew) != this.(fieldIdOld) && !newValueIsNull)
            {
                conChanges += fieldIdNew;
            }
        }

        return conChanges;
    }

]]></Source>
