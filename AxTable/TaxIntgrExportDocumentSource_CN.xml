		<Methods>
			<Method>
				<Name>getExportDocTransactionCurrencyAmount</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets the export document transaction currency amount.
    /// </summary>
    /// <returns>
    /// The export document transaction currency amount.
    /// </returns>
    public AmountCur getExportDocTransactionCurrencyAmount()
    {
        return TaxIntgrUtility_CN::roundAmount(this.TransactionCurrencyAmount);
    }

]]></Source>
			</Method>
			<Method>
				<Name>getExportDocumenrSourceKeys</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets the key fields for meger comparison.
    /// </summary>
    /// <returns>
    /// The container with the key fields.
    /// </returns>
    public container getExportDocumenrSourceKeys()
    {
        return [this.TaxCode,
                this.CurrencyCode,
                this.InvoiceAccount,
                this.TaxRegistration,
                this.CustBankAccountId,
                sign(this.TransactionCurrencyAmount)];
    }

]]></Source>
			</Method>
			<Method>
				<Name>isOverAmountLimit</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks whether the export document source is over amount limit.
    /// </summary>
    /// <returns>
    /// True if it is over amount limit; otherwsie, false.
    /// </returns>
    public boolean isOverAmountLimit()
    {
        return TaxIntgrUtility_CN::isOverAmountLimit(this.TaxCode, this.getExportDocTransactionCurrencyAmount());
    }

]]></Source>
			</Method>
			<Method>
				<Name>isWithDocumentExported</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks whether the export document source is with exported document.
    /// </summary>
    /// <returns>
    /// True if the export document source is with exported document; otherwise, false.
    /// </returns>
    public boolean isWithDocumentExported()
    {
        boolean                             result;
        TaxIntgrExportDocument_CN           exportDocument;
        TaxIntgrExportDocumentOperation_CN  operation;

        select RecId from exportDocument
        exists join operation
            where   operation.TaxIntgrExportDocument_CN         == exportDocument.RecId
                &&  operation.TaxIntgrExportDocumentSource_CN   == this.RecId
                &&  exportDocument.IsExported                   == true;

        if (exportDocument.RecId)
        {
            result = true;
        }

        return result;
    }

]]></Source>
			</Method>
			<Method>
				<Name>isWithExportDocumentOperation</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks whether the export document source is with the specified operation type.
    /// </summary>
    /// <param name="_operationType">
    /// The operation type.
    /// </param>
    /// <returns>
    /// True if the export document source is with exported document; otherwise, false.
    /// </returns>
    public boolean isWithExportDocumentOperation(TaxIntgrExportDocumentOperationType_CN _operationType)
    {
        boolean                             result;
        TaxIntgrExportDocumentOperation_CN  operation;

        select firstOnly RecId from operation
            where   operation.TaxIntgrExportDocumentSource_CN   == this.RecId
                &&  operation.OperationType                     == _operationType;

        if (operation.RecId)
        {
            result = true;
        }

        return result;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds the export document source.
    /// </summary>
    /// <param name="_recId">
    /// The export document source record id.
    /// </param>
    /// <returns>
    /// The export document source.
    /// </returns>
    public static TaxIntgrExportDocumentSource_CN find(RefRecId _recId)
    {
        TaxIntgrExportDocumentSource_CN exportDocumentSource;

        select firstOnly exportDocumentSource
            where exportDocumentSource.RecId == _recId;

        return exportDocumentSource;
    }

]]></Source>
			</Method>
		</Methods>
