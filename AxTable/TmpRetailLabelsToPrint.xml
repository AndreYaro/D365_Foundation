		<Methods>
			<Method>
				<Name>getStoreFromWarehouse</Name>
				<Source><![CDATA[
    /// <summary>
    /// Retrieve the store number from specific warehouse.
    /// </summary>
    /// <param name="_inventLocationId">
    /// InventLocation Id.
    /// </param>
    /// <param name="_dataAreaId">
    /// Data area id.
    /// </param>
    /// <returns>
    /// Return store number if store and warehouse has mapping relationship.
    /// </returns>
    public static RetailStoreId getStoreFromWarehouse(InventLocationId _inventLocationId, DataAreaId _dataAreaId)
    {
        RetailStoreTable        storeTable;

        select count(RecId) from storeTable
           where storeTable.InventLocation == _inventLocationId &&
                 storeTable.inventLocationDataAreaId == _dataAreaId;

        //If store and warehoue has mapping relationship, get default store number from warehouse
        if (storeTable.RecId >= 1)
        {
            select storeNumber from storeTable
                where storeTable.inventLocation == _inventLocationId &&
                      storeTable.inventLocationDataAreaId == _dataAreaId;
            return storeTable.StoreNumber;
        }

        return '';
    }

]]></Source>
			</Method>
		</Methods>
