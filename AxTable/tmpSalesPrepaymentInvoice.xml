				<Name>initPrepaymentInvoice</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes a temporary prepayment invoice table.
    /// </summary>
    /// <param name = "_custPrepaymentInvoiceTable">
    /// The prepayment invoice table used to initialize the temporary table.
    /// </param>
    /// <param name = "_custPrepaymentCustInvoice">
    /// The prepayment application transaction used to initialize the temporary table.
    /// </param>
    public void initPrepaymentInvoice(CustPrepaymentInvoiceTable _custPrepaymentInvoiceTable,CustPrepaymentCustInvoice _custPrepaymentCustInvoice)
    {
        this.clear();
        this.CurrencyCode = _custPrepaymentInvoiceTable.CurrencyCode;
        this.AdvanceAmount = _custPrepaymentInvoiceTable.RemainAmountCur;
        this.AppliedAmount = _custPrepaymentCustInvoice?_custPrepaymentCustInvoice.AmountCur:0;
        this.AdvanceRemaining = this.AdvanceAmount - this.AppliedAmount;
        this.InvoiceAccount = _custPrepaymentInvoiceTable.InvoiceAccount;
        this.InvoiceAmount = _custPrepaymentInvoiceTable.InvoiceAmount;
        this.InvoiceRecId = _custPrepaymentInvoiceTable.RecId;
        this.InvoiceDate = _custPrepaymentInvoiceTable.InvoiceDate;
        this.InvoiceNum = _custPrepaymentInvoiceTable.InvoiceId;
        this.SalesId = _custPrepaymentInvoiceTable.SalesId;

        this.insert();
    }

]]></Source>
