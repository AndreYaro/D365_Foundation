			<Method>
				<Name>assessableValueV2</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets the assessable value with <c>PurchLine</c> buffer.
    /// </summary>
    /// <param name = "_purchLine">
    /// Buffer of the related <c>PurchLine</c>.
    /// </param>
    internal void assessableValueV2(PurchLine _purchLine)
    {
        CustomsImportAssessableValue_IN customsImportAssessableValue;
        MarkupValue                     value;
        CustomsAmount_IN                landingCharges;

        if (PurchTable::find(_purchLine.PurchId).purchTable_W().CustomsImportOrder_IN == NoYes::Yes)
        {
            customsImportAssessableValue = CustomsImportAssessableValue_IN::findInventTransId(_purchLine.InventTransId);
            if (customsImportAssessableValue.RecId)
            {
                ttsbegin;
                customsImportAssessableValue.selectForUpdate(true);
                customsImportAssessableValue.Amount = _purchLine.LineAmount;
                customsImportAssessableValue.Value  = CustomsImportAssessableValue_IN::getMarkupValueByBuffer(_purchLine);
                customsImportAssessableValue.update();
                ttscommit;
                this.AssessableValueTransactionCurrency = CustomsImportAssessableValue_IN::findInventTransId(_purchLine.InventTransId).invoiceAssessableValue();
            }
            else
            {
                value                   = CustomsImportAssessableValue_IN::getMarkupValueByBuffer(_purchLine);
                landingCharges          = (_purchLine.LineAmount + value) * VendParameters::find().CustomsLandingChargesPct_IN/100;
                this.AssessableValueTransactionCurrency = _purchLine.LineAmount + value + landingCharges;
            }
        }
        else
        {
            this.AssessableValueTransactionCurrency = _purchLine.getMiscChargesAmount_IN();
        }

        this.AssessableValueAccountingCurrency = TaxAssessableValueHandler_IN::calcAssessableValueAccountingCurrency(this.AssessableValueTransactionCurrency, _purchLine.CurrencyCode, _purchLine.purchTable().AccountingDate);
        _purchLine.packPurchLine_IN(this);
    }

]]></Source>
			</Method>
			<Method>
				<Name>convertCurrencyCode</Name>
				<Source><![CDATA[
    /// <summary>
    /// This method is used to get the value in the converted currency.
    /// </summary>
    /// <param name="_currencyCode">
    /// A currency code for conversion.
    /// </param>
    public void convertCurrencyCode(CurrencyCode _currencyCode)
    {
        CustomsImportAssessableValue_IN customsImportAssessableValue;
        MarkupValue                     value;
        CustomsAmount_IN                landingCharges;
        PurchLine                       purchLine = this.purchLine();

        this.MaximumRetailPrice = ExchangeRateHelper::curPrice2CurPrice(this.MaximumRetailPrice, PurchLine.CurrencyCode, _currencyCode);

        if (TaxParameters::checkParameterForPosting_IN(TaxType_IN::Customs) && PurchTable::find(PurchLine.PurchId).purchTable_W().CustomsImportOrder_IN == NoYes::Yes)
        {
            customsImportAssessableValue = CustomsImportAssessableValue_IN::findInventTransId(PurchLine.InventTransId);
            if (customsImportAssessableValue.RecId != 0)
            {
                ttsbegin;
                customsImportAssessableValue.selectForUpdate(true);
                customsImportAssessableValue.Amount = PurchLine.LineAmount;
                customsImportAssessableValue.Value  = CustomsImportAssessableValue_IN::getMarkupValue(PurchLine.TableId, PurchLine.RecId, PurchLine.InventTransId);
                customsImportAssessableValue.update();
                ttscommit;
                this.AssessableValueTransactionCurrency = CustomsImportAssessableValue_IN::findInventTransId(PurchLine.InventTransId).invoiceAssessableValue();
            }
            else
            {
                value                   = CustomsImportAssessableValue_IN::getMarkupValue(PurchLine.TableId, PurchLine.RecId, PurchLine.InventTransId);
                landingCharges          = (PurchLine.LineAmount + value) * VendParameters::find().CustomsLandingChargesPct_IN/100;
                this.AssessableValueTransactionCurrency = PurchLine.LineAmount + value + landingCharges;
                this.AssessableValueAccountingCurrency = TaxAssessableValueHandler_IN::calcAssessableValueAccountingCurrency(this.AssessableValueTransactionCurrency, PurchLine.CurrencyCode, PurchLine.purchTable().AccountingDate);
            }
        }

        PurchLine.packPurchLine_IN(this);
    }

]]></Source>
			</Method>
			<Method>
				<Name>update</Name>
				<Source><![CDATA[
    public void update()
    {
        // Keep consistent with PurchLine update.
        // Don't update PurchLine_IN if PurchLine would not be updated due to quality order.
        // e.g. When there is open quality order, the quantity is not allowed to update in PurchLine,
        // then the derived assessable value in PurchLine_IN should not be updated as well.
        if (!this.purchLine().allowUpdateWithQualityOrder())
        {
            throw error("@SYS113930");
        }
        super();
    }

]]></Source>
