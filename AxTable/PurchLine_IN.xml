			<Method>
				<Name>modifiedField</Name>
				<Source><![CDATA[
    /// <summary>
    /// Performs specific actions when a field is modified.
    /// </summary>
    /// <param name="_fieldId">
    /// The field ID of the field that has been modified.
    /// </param>
    public void modifiedField(FieldId _fieldId)
    {
        PurchLine purchLine = this.purchLine();
        TransDate transDate = purchLine.purchTable().AccountingDate;

        super(_fieldId);

        switch (_fieldId)
        {
            case fieldNum(PurchLine_IN, AssessableValueTransactionCurrency):
                if (TaxParameters::checkTaxParameters_IN())
                {
                    this.AssessableValueAccountingCurrency = TaxAssessableValueHandler_IN::calcAssessableValueAccountingCurrency(this.AssessableValueTransactionCurrency, this.PurchLine().CurrencyCode, transDate);
                }
                break;
            case fieldNum(PurchLine_IN, AssessableValueAccountingCurrency):
                if (TaxParameters::checkTaxParameters_IN())
                {
                    this.AssessableValueTransactionCurrency = TaxAssessableValueHandler_IN::calcAssessableValue(this.AssessableValueAccountingCurrency, this.PurchLine().CurrencyCode, transDate);
                }
                break;
            case fieldNum(PurchLine_IN, TDSGroup):
                TransTaxInformation transTaxInformation;
                transTaxInformation = TransTaxInformationHelper::findOrCreateTransTaxInformation(tableNum(PurchLine), this.PurchLine, true);
                if (transTaxInformation.TaxWithholdSoftwareDeclReceived == NoYes::Yes)
                {
                    ttsbegin;
                    transTaxInformation.TaxWithholdSoftwareDeclReceived = NoYes::No;
                    transTaxInformation.update();
                    ttscommit;
                }
                break;
        }
    }

]]></Source>
