	<SourceCode>
		<Declaration><![CDATA[
public class InventTestOperationTimeSheet extends common
{
}
]]></Declaration>
		<Methods>
			<Method>
				<Name>calcEmplCost</Name>
				<Source><![CDATA[
    /// <summary>
    /// Calculates the estimated cost of the hours that are used in the operation.
    /// </summary>
    /// <returns>
    /// The estimated cost amount.
    /// </returns>
    public CostAmount calcEmplCost()
    {
        return InventParameters::find().TestHourlyRate * this.OperationHours;
    }

]]></Source>
			</Method>
			<Method>
				<Name>checkAndFixInvoicedSalesId</Name>
				<Source><![CDATA[
    /// <summary>
    /// Sets the value of the <c>InvoicedSalesId</c> field on the current buffer to the empty string if the
    /// value of the <c>OperationBilled</c> enumeration type is equal to the <c>NoYes::No</c> enumeration
    /// value.
    /// </summary>
    private void checkAndFixInvoicedSalesId()
    {
        if (this.InvoicedSalesId && !this.OperationBilled)
        {
            this.InvoicedSalesId    = '';
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initValue</Name>
				<Source><![CDATA[
    public void initValue()
    {
        this.OperationDate  = DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone());
        this.Worker         = DirPersonUserEx::findUserWorkerReference();
        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>insert</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Overrides the standard insert method and inserts the cost amount.
    /// </summary>
    public void insert()
    {
        ttsbegin;

        this.checkAndFixInvoicedSalesId();
        this.CostAmount = this.calcEmplCost();

        super();

        ttscommit;
    }

]]></Source>
			</Method>
			<Method>
				<Name>update</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Updates the cost amount.
    /// </summary>
    /// <remarks>
    ///    This method overrides the standard <c>update</c> method.
    /// </remarks>
    public void update()
    {
        ttsbegin;

        this.checkAndFixInvoicedSalesId();
        this.CostAmount = this.calcEmplCost();

        super();

        ttscommit;
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
