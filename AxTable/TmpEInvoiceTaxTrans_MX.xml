]]></Declaration>
		<Methods>
			<Method>
				<Name>taxAmountCalculated</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets unrounded tax amount calculated form adjusted tax base amount.
    /// </summary>
    /// <returns>Unrounded tax amount calculated form adjusted tax base amount</returns>
    public Amount taxAmountCalculated()
    {
        return CurrencyExchangeHelper::amount(this.taxBaseAmountRegulated()) * this.TaxValue / 100;
    }

]]></Source>
			</Method>
			<Method>
				<Name>taxBaseAmountRegulated</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets adjusted tax base amount.
    /// </summary>
    /// <returns>Adjusted tax base amount.</returns>
    public Amount taxBaseAmountRegulated()
    {
        return this.SourceBaseAmountCur + this.Adjustment + this.AdjustmentTotal;
    }

]]></Source>
			</Method>
		</Methods>
