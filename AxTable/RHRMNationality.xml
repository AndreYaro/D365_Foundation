		<Methods>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static boolean exist(RHRMNationalityId _rhrmNationalityId)
    {
        return _rhrmNationalityId && (select RecId from rhrmNationality
                                          where rhrmNationality.NationalityId == _rhrmNationalityId).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static RHRMNationality find(RHRMNationalityId _rhrmNationalityId,
                                       boolean           _forUpdate = false)
    {
        RHRMNationality rhrmNationality;
        ;

        if (_rhrmNationalityId)
        {
            rhrmNationality.selectForUpdate(_forUpdate);

            select firstonly rhrmNationality
                where rhrmNationality.NationalityId == _rhrmNationalityId;
        }
        return rhrmNationality;
    }

]]></Source>
