		<Methods>
			<Method>
				<Name>refreshDocumentIdAndStatus</Name>
				<Source><![CDATA[
    /// <summary>
    /// Refresh the reduction entry document id and status for specified document entry.
    /// </summary>
    public void refreshDocumentIdAndStatus()
    {
        AssetDocumentEntryTableView_JP          assetDocumentEntryTableView;
        AssetDocumentReductionEntryTable_JP     assetDocumentReductionEntry;

        select DocumentId from assetDocumentEntryTableView
            where assetDocumentEntryTableView.BookId == this.AssetBookMergeId &&
                assetDocumentEntryTableView.AssetId == this.AssetId;
        this.DocumentId = assetDocumentEntryTableView.DocumentId;
        select Status from assetDocumentReductionEntry
            where assetDocumentReductionEntry.AssetDocumentEntry_JP == assetDocumentEntryTableView.RecId;
        this.DocumentStatus = assetDocumentReductionEntry.Status;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    public boolean validateWrite()
    {
        boolean ret;

        ret = super();

        if (ret && this.AssetId)
        {
            ret = this.AssetId && this.AssetBookMergeId;
            if (!ret)
            {
                error(strFmt("@FixedAssetsReductionEntry:MassUpdateLineWithoutAssetBookMergeId", this.AssetId));
            }
        }
        return ret;
    }

]]></Source>
			</Method>
		</Methods>
