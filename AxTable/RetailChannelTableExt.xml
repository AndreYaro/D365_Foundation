				<Name>find</Name>
				<Source><![CDATA[
    public static RetailChannelTableExt find(RetailChannelRecId _channel, boolean _forUpdate = false)
    {
        RetailChannelTableExt retailChannelTableExt;
        retailChannelTableExt.selectForUpdate(_forUpdate);

        select firstonly retailChannelTableExt
            where retailChannelTableExt.Channel == _channel;

        return retailChannelTableExt;
    }

]]></Source>
