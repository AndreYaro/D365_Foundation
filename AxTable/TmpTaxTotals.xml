		<Methods>
			<Method>
				<Name>addByTaxWorkTrans</Name>
				<Source><![CDATA[
    /// <summary>
    /// Add the field value by <c>TmpTaxWorkTrans</c>.
    /// </summary>
    /// <param name = "_taxWorkTrans">
    /// The buffer of <c>TmpTaxWorkTrans</c>.
    /// </param>
    /// <param name = "signReverse">
    /// The sign for reverse.
    /// </param>
    [Replaceable(true)]
    void addByTaxWorkTrans(TmpTaxWorkTrans _taxWorkTrans, Counter signReverse)
    {
        this.TaxAmountCur                  += _taxWorkTrans.SourceTaxAmountCur * signReverse;
        if (!_taxWorkTrans.TaxAutogenerated)
                this.TaxAmountCurRegulated += _taxWorkTrans.SourceRegulateAmountCur * signReverse;
        if (!_taxWorkTrans.TaxAutogenerated)
                this.TaxAmountCurCorrect   += _taxWorkTrans.SourceRegulateAmountCur * signReverse;
        else
                this.TaxAmountCurCorrect   += _taxWorkTrans.SourceTaxAmountCur * signReverse;

    }

]]></Source>
			</Method>
			<Method>
				<Name>addByTaxTrans</Name>
				<Source><![CDATA[
    /// <summary>
    /// Add the field value by <c>TaxTrans</c>.
    /// </summary>
    /// <param name = "_taxTrans">
    /// The buffer of <c>TaxTrans</c>.
    /// </param>
    [Replaceable(true)]
    void addByTaxTrans(TaxTrans _taxTrans)
    {
        this.TaxAmountCur               += _taxTrans.SourceTaxAmountCur;
        this.TaxAmountCurRegulated      += _taxTrans.SourceRegulateAmountCur;
        if (!_taxTrans.TaxAutogenerated)

                this.TaxAmountCurCorrect     += _taxTrans.SourceRegulateAmountCur;
        else
                this.TaxAmountCurCorrect     += _taxTrans.SourceTaxAmountCur;
    }

]]></Source>
			</Method>
		</Methods>
