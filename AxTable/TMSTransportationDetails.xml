		<Methods>
			<Method>
				<Name>displayTotalRate</Name>
				<Source><![CDATA[
    /// <summary>
    /// Calculates total rate for transportation.
    /// </summary>
    /// <returns>
    /// The total rate for transportation.
    /// </returns>
    display TMSRate displayTotalRate()
    {
        return this.Qty * this.UnitPrice;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if a specific <c>TMSTransportationDetails</c> record exists.
    /// </summary>
    /// <param name="_recId">
    /// Id of the record.
    /// </param>
    /// <returns>
    /// True if the record exists.
    /// </returns>
    public static boolean exist(RecId   _recId)
    {
        return (_recId) &&
               (select firstonly TMSTransportationDetails
                    where tmsTransportationDetails.RecId == _recId).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a specific <c>TMSTransportationDetails</c> record by id.
    /// </summary>
    /// <param name="_recId">
    /// Id of the record.
    /// </param>
    /// <param name="_forupdate">
    /// Determines whether the record is selected for update.
    /// </param>
    /// <returns>
    /// The record that is searched for.
    /// </returns>
    public static TMSTransportationDetails find(RecId   _recId,
                                                boolean _forupdate = false)
    {
        TMSTransportationDetails    tmsTransportationDetails;

        if (_recId)
        {
            tmsTransportationDetails.selectForUpdate(_forupdate);

            select firstonly tmsTransportationDetails
                where tmsTransportationDetails.RecId == _recId;
        }

        return tmsTransportationDetails;
    }

]]></Source>
