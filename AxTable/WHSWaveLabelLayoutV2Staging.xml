    /// <summary>
    /// When exporting this DMF data entity, the <c>WorkQuery</c> field value will be packaged into a separate file.
    /// Queries are sometimes malformed due to truncation if translated directly into a string.
    /// </summary>
    /// <returns>A container used to separate the field into a file.</returns>
    public static container getFieldsToBeConvertedToFile()
    {
        // Entity fieldname, Staging table field name, StagingFieldName + FileSuffix
        return [[fieldstr(WHSWaveLabelLayoutV2Entity,  WorkQuery),
                 fieldstr(WHSWaveLabelLayoutV2Staging, WorkQuery),
                 fieldstr(WHSWaveLabelLayoutV2Staging, WorkQueryFileName)]];
    }

]]></Source>
