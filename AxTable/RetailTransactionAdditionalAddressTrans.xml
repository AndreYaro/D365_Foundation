			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a specified record in the <c>RetailTransactionAdditionalAddressTrans</c> table.
    /// </summary>
    /// <param name="_transactionId">
    /// The ID of a transaction.
    /// </param>
    /// <param name="_channelRecId">
    /// The ID of a channel.
    /// </param>
    /// <param name="_storeId">
    /// The ID of a store.
    /// </param>
    /// <param name="_terminalId">
    /// The ID of a POS terminal.
    /// </param>
    /// <param name="_addressType">
    /// The address type.
    /// </param>
    /// <param name="_forUpdate">
    /// A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <returns>
    /// A record in the <c>RetailTransactionAdditionalAddressTrans</c> table; otherwise, an empty record.
    /// </returns>
    [Hookable(false)]
    public static RetailTransactionAdditionalAddressTrans find(RetailTransactionId _transactionId,
                                                RetailChannelRecId _channelRecId,
                                                RetailStoreId _storeId,
                                                RetailTerminalId _terminalId,
                                                RetailAdditionalAddressType _addressType,
                                                boolean _forUpdate = false)
    {
        RetailTransactionAdditionalAddressTrans transactionAddressTrans;

        if (_transactionId && _channelRecId && _addressType)
        {
            transactionAddressTrans.selectForUpdate(_forUpdate);

            select firstonly transactionAddressTrans
                where transactionAddressTrans.Store         == _storeId
                &&    transactionAddressTrans.Channel       == _channelRecId
                &&    transactionAddressTrans.terminal      == _terminalId
                &&    transactionAddressTrans.TransactionId == _transactionId
                &&    transactionAddressTrans.AddressType   == _addressType;
        }

        return transactionAddressTrans;
    }

]]></Source>
