		<Methods>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes the list of properties for the entity, specified by reference table ID and field ID.
    /// </summary>
    /// <param name="_refTableId">
    /// The reference table ID.
    /// </param>
    /// <param name="_refRecId">
    /// The reference record ID.
    /// </param>
    public static void init(RefTableId _refTableId, RefRecId _refRecId)
    {
        EInvoicePropertyTypeTable propertyTypeTable;
        EInvoicePropertyTable propertyTable, propertyTableExisting;
        EInvoicePropertyValue emptyValue = '';
        EInvoicePropertyTypeApplicabilitySetup applicabilitySetup;

        ttsBegin;

        insert_recordset propertyTable(RefTableId, RefRecId, Type, Value)
        select _refTableId, _refRecId, RecId, emptyValue
            from propertyTypeTable
        exists join applicabilitySetup
            where applicabilitySetup.Type           == propertyTypeTable.RecId
               && applicabilitySetup.RefTableId     == _refTableId
        notExists join propertyTableExisting
            where propertyTableExisting.RefTableId  == _refTableId
                && propertyTableExisting.RefRecId   == _refRecId
                && propertyTableExisting.Type       == propertyTypeTable.RecId;

        ttsCommit;
    }

]]></Source>
			</Method>
			<Method>
				<Name>initFromRecord</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes the list of properties for the entity, passed as a record.
    /// </summary>
    /// <param name="_record">
    /// The entity to create the properties for.
    /// </param>
    public static void initFromRecord(Common _record)
    {
        if (_record)
        {
            EInvoicePropertyTable::init(_record.TableId, _record.RecId);
        }
    }

]]></Source>
			</Method>
