        return _input.parmDimensionValueName(InventProductDimensionConfig::getInstance());
    }

]]></Source>
			</Method>
			<Method>
				<Name>isSegmentAllowedForNomenclatureType</Name>
				<Source><![CDATA[
    public static boolean isSegmentAllowedForNomenclatureType(TableId _nomenclatureType)
    {
        return (_nomenclatureType == tableNum(EcoResProductVariantNomenclature));
    }

]]></Source>
			</Method>
			<Method>
				<Name>getSegmentSampleValue</Name>
				<Source><![CDATA[
    public EcoResNomenclatureOutput getSegmentSampleValue(EcoResNomenclatureInput _input)
    {
        EcoResNomenclatureOutput ret = "@SYS40133";

        EcoResProductMasterConfiguration config;
        config = this.getFirstOrNull(config);
   
        if (config)
        {
            LanguageId sysLanguage  = SystemParameters::getSystemLanguageId();
            Name configName         = EcoResProductMasterDimValueTranslation::findByProductMasterDimValLanguage(config.RecId, sysLanguage).Name;

            if (configName)
            {
                _input.parmDimensionValueName(InventProductDimensionConfig::getInstance(), configName);
                ret = this.getSegmentValue(_input);
            }                       
        }
                       
        return ret;
    }

]]></Source>
