			<Method>
				<Name>initFromCustInvoiceTrans</Name>
				<Source><![CDATA[
        void  initFromCustInvoiceTrans(CustInvoiceTrans _custInvoiceTrans)
    {
        this.CurrencyCode     = _custInvoiceTrans.CurrencyCode;
        this.InvoiceId        = _custInvoiceTrans.InvoiceId;
        this.SalesCategory    = _custInvoiceTrans.SalesCategory;
        this.InvoiceDate      = _custInvoiceTrans.InvoiceDate;
        this.ItemId           = _custInvoiceTrans.ItemId;
        this.DefaultDimension = this.copyDimension(_custInvoiceTrans.DefaultDimension);
    }

]]></Source>
