		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static RPayJournalName find(RPayJournalNameId _journalNameId, boolean _forUpdate = false)
    {
        RPayJournalName payJournalName;

        payJournalName.selectForUpdate(_forUpdate);

        if (_journalNameId)
        {
            select firstonly payJournalName
                where payJournalName.JournalNameId == _journalNameId;
        }

        return payJournalName;
    }

]]></Source>
