		<Methods>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static boolean exist(RHRMEducationDegreeId    _educationDegreeId)
    {
        return _educationDegreeId && (select RecId from rhrmEducationDegree
                                              where rhrmEducationDegree.EducationDegreeId == _educationDegreeId).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static RHRMEducationDegree find(RHRMEducationDegreeId  _educationDegreeId,
                                           boolean                _forUpdate = false)
    {
        RHRMEducationDegree rhrmEducationDegree;
        ;

        if (_educationDegreeId)
        {
            rhrmEducationDegree.selectForUpdate(_forUpdate);

            select firstonly rhrmEducationDegree
                where rhrmEducationDegree.EducationDegreeId == _educationDegreeId;
        }
        return rhrmEducationDegree;
    }

]]></Source>
