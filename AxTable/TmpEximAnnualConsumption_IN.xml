			</Method>
			<Method>
				<Name>modifiedField</Name>
				<Source><![CDATA[
    /// <summary>
    /// Modify field method of temp exim annual consumption.
    /// </summary>
    /// <param name = "_fieldId">Field id.</param>
    public void modifiedField(fieldId _fieldId)
    {
        super(_fieldId);

        switch (_fieldId)
        {
            case fieldnum(TmpEximAnnualConsumption_IN, Qty),
                 fieldnum(TmpEximAnnualConsumption_IN, Price):
                this.AssessableValue = (this.Qty * this.Price);
                break;

            case fieldnum(TmpEximAnnualConsumption_IN, AssessableValue):
                this.Price = 0;
                break;

            case fieldnum(TmpEximAnnualConsumption_IN, ItemId):
                this.Unit = InventTableModule::find(this.ItemId, ModuleInventPurchSales::Invent).UnitId;
                break;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateField</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validate field.
    /// </summary>
    /// <param name = "_fieldIdToCheck">Field id.</param>
    /// <returns>True if field is validate, otherwise false.</returns>
    public boolean validateField(fieldId _fieldIdToCheck)
    {
        boolean     ret;
        InventTable inventTable;

        ret = super(_fieldIdToCheck);

        if (ret)
        {
            switch (_fieldIdToCheck)
            {
                case fieldnum(TmpEximAnnualConsumption_IN, ItemId):
                    if (this.ItemId)
                    {
                        inventTable = InventTable::find(this.ItemId);
                        if (inventTable.RecId
                            && (InventItemSetupSupplyType::find(inventTable.ItemId, inventTable.DataAreaId).DefaultOrderType != ReqPOType::Production))
                        {
                            ret = checkFailed(strfmt("@GLS61467", this.ItemId));
                        }
                    }
                    break;
            }
        }
        return ret;
    }

]]></Source>
