		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
        public static WHSTmpLoadDemand find(ItemId              _itemId,
                                        InventDimId         _inventDimId,
                                        WHSTmpLoadDemand    _whsTmpLoadDemand,
                                        boolean             _forUpdate = false)
    {
        if (_itemId && _inventDimId)
        {
            _whsTmpLoadDemand.selectForUpdate(_forUpdate);

            select firstonly _whsTmpLoadDemand
                where _whsTmpLoadDemand.ItemId           == _itemId          &&
                      _whsTmpLoadDemand.InventDimId      == _inventDimId;
        }

        return _whsTmpLoadDemand;
    }

]]></Source>
