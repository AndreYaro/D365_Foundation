			<Method>
				<Name>findRecId</Name>
				<Source><![CDATA[
    /// <summary>
    /// Selecrs CustInvoice4PaymJour record.
    /// </summary>
    /// <param name = "_recId">Record Id.</param>
    /// <param name = "_update">Selection mode.</param>
    /// <returns><c>CustInvoice4PaymJour_RU</c> object.</returns>
    public static CustInvoice4PaymJour_RU findRecId(recId   _recId,
                                                    boolean _update = false)
    {
        CustInvoice4PaymJour_RU custInvoice4PaymJour;


        custInvoice4PaymJour.selectForUpdate(_update);

        select firstonly custInvoice4PaymJour
            index hint RecId
            where custInvoice4PaymJour.RecId == _recId;

        return custInvoice4PaymJour;
    }

]]></Source>
			</Method>
			<Method>
				<Name>numberOfInvoice4Paym</Name>
				<Source><![CDATA[
    private static RecId numberOfInvoice4Paym(SalesTable _source)
    {
        CustInvoice4PaymJour_RU custInvoice4PaymJour;

        select count (RecId) from custInvoice4PaymJour
            index hint SalesIdx
            where custInvoice4PaymJour.SalesId == _source.SalesId;

        return custInvoice4PaymJour.RecId;
    }

]]></Source>
