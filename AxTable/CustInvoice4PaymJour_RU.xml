			<Method>
				<Name>setInvoicePostalAddress</Name>
				<Source><![CDATA[
    /// <summary>
    /// Sets the invoice postal address.
    /// </summary>
    public void setInvoicePostalAddress()
    {
        LogisticsPostalAddress  logisticsPostalAddress;
        CustTable               custTable = CustTable::find(this.OrderAccount);

        if (custTable.InvoiceAddress == InvoiceOrderAccount::InvoiceAccount)
        {
            custTable = CustTable::find(this.InvoiceAccount);
        }

        logisticsPostalAddress = DirParty::findPostalAddressByRole(custTable.Party, LogisticsLocationRoleType::Delivery);

        if (!logisticsPostalAddress)
        {
            logisticsPostalAddress = DirParty::findPostalAddressByRole(custTable.Party, LogisticsLocationRoleType::Invoice);
        }

        //If the Invoice address does not exist, find the Primary address finally.
        if (!logisticsPostalAddress)
        {
            logisticsPostalAddress = DirParty::primaryPostalAddress(custTable.Party);
        }

        this.DeliveryPostalAddress = logisticsPostalAddress.RecId;

        if (logisticsPostalAddress.Location)
        {
            this.DeliveryName          = LogisticsLocation::find(logisticsPostalAddress.Location).Description;
        }
    }

]]></Source>
