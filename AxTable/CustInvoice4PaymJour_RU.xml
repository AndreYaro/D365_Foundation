			<Method>
				<Name>initFromCustInvoiceTable</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes values from CustInvoiceTable.
    /// </summary>
    /// <param name = "_custInvoiceTable"><c>CustInvoiceTable</c> object.</param>
    public void initFromCustInvoiceTable(CustInvoiceTable  _custInvoiceTable)
    {
        CustPaymModeTable            custPaymMode = CustPaymModeTable::find(_custInvoiceTable.PaymMode);

        this.OrderAccount            = _custInvoiceTable.OrderAccount;
        this.InvoiceAccount          = _custInvoiceTable.InvoiceAccount;
        this.DeliveryName            = _custInvoiceTable.Name;

        this.CustGroup               = _custInvoiceTable.CustGroup;
        this.LanguageId              = _custInvoiceTable.LanguageId;
        this.Payment                 = _custInvoiceTable.Payment;
        this.CashDiscCode            = _custInvoiceTable.CashDiscCode;

        this.SalesBalance            = 0;
        this.EndDisc                 = 0;
        this.RoundOff                = 0;
        this.Invoice4PaymAmount      = 0;
        this.CurrencyCode            = _custInvoiceTable.CurrencyCode;

        this.Triangulation           = _custInvoiceTable.euroTriangulation(this.Invoice4PaymDate);
        this.ExchRate                = _custInvoiceTable.getExchRate(this.Invoice4PaymDate);
        this.ExchRateSecondary       = _custInvoiceTable.getExchRateSecondary(this.Invoice4PaymDate);

        this.DefaultDimension        = _custInvoiceTable.DefaultDimension;
        this.FixedDueDate            = _custInvoiceTable.DueDate;
        this.CurrencyCode            = _custInvoiceTable.CurrencyCode;

        this.CompanyBankAccountId    = custPaymMode.AccountType == LedgerJournalACType::Bank ?
                                       custPaymMode.parmPaymAccount() : '';
        this.CompanyBankAccountId    = this.CompanyBankAccountId ? this.CompanyBankAccountId : CompanyInfo::find().Bank;

        this.setInvoicePostalAddress();
    }

]]></Source>
