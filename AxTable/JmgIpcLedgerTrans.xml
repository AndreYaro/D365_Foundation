    /// The name of the indirect activity or absence that is associated with this <c>JmgIpcLedgerTrans</c> record.
    /// </returns>
    display JmgIpcActivityName ipcActivityName()
    {
        JmgStampTrans jmgStampTrans;

        select RecId from jmgStampTrans where jmgStampTrans.RecId == this.StampTransRecId;

        if (jmgStampTrans)
        {
            return jmgStampTrans.ipcActivityDescription();
        }

        return '';
    }

]]></Source>
