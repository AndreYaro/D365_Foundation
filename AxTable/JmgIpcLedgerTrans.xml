		<Methods>
			<Method>
				<Name>ipcActivity</Name>
				<Source><![CDATA[
    /// <summary>
    /// Retrieves the indirect activity or absence code that is associated with this <c>JmgIpcLedgerTrans</c> record.
    /// </summary>
    /// <returns>
    /// The indirect activity or absence code that is associated with this <c>JmgIpcLedgerTrans</c> record.
    /// </returns>
    public display JmgIpcActivityId ipcActivity()
    {
        JmgStampTrans jmgStampTrans;

        select RecId from jmgStampTrans where jmgStampTrans.RecId == this.StampTransRecId;

        if (jmgStampTrans)
        {
            return jmgStampTrans.ipcActivity();
        }

        return '';
    }

]]></Source>
			</Method>
