			<Method>
				<Name>createLine</Name>
				<Source><![CDATA[
    public static void createLine(TmpSalesBookVATProcessLogTrans_RU _salesBookVATProcessLogTrans,
                                  FactureJour_RU                    _factureJour,
                                  TransDate                         _offsetTransDate,
                                  TransDate                         _settlementDate,
                                  Name                              _accountName,
                                  CountryGTD_RU                     _countryGTD,
                                  SalesPurchBookTransType_RU        _transType,
                                  recId                             _transRecId,
                                  recId                             _offsetRecId,
                                  RefTableId                        _refTableId,
                                  CompanyId                         _transCompany,
                                  CompanyId                         _offsetCompany,
                                  recId                             _settlementRecId,
                                  recId                             _reverseSettlementRecId = 0,
                                  TransDate                         _reverseDate            = _settlementDate)
    {
        _salesBookVATProcessLogTrans.clear();

        _salesBookVATProcessLogTrans.FactureId          = _factureJour.FactureId;
        _salesBookVATProcessLogTrans.FactureExternalId  = _factureJour.FactureExternalId;
        _salesBookVATProcessLogTrans.FactureDate        = _factureJour.FactureDate;
        _salesBookVATProcessLogTrans.PaymentDate        = _offsetTransDate;
        _salesBookVATProcessLogTrans.AccountNum         = _factureJour.CustVendInvoiceAccount;
        _salesBookVATProcessLogTrans.AccountName        = _accountName;
        _salesBookVATProcessLogTrans.CountryGTD         = _countryGTD;
        _salesBookVATProcessLogTrans.InvoiceRecIdRef    = _transRecId;
        _salesBookVATProcessLogTrans.PaymentRecIdRef    = _offsetRecId;
        _salesBookVATProcessLogTrans.RefTableId         = _refTableId;
        _salesBookVATProcessLogTrans.InvoiceCompany     = _transCompany;
        _salesBookVATProcessLogTrans.PaymentCompany     = _offsetCompany;
        _salesBookVATProcessLogTrans.SettlementDate     = _settlementDate;
        _salesBookVATProcessLogTrans.SettlementRecId    = _settlementRecId;

        _salesBookVATProcessLogTrans.SalesBookTransDate = max(_settlementDate, max(_factureJour.FactureReportDate, _offsetTransDate));
        _salesBookVATProcessLogTrans.TransType          = _transType;

        if (_reverseDate != _settlementDate)
        {
            _salesBookVATProcessLogTrans.Reversed               = NoYes::Yes;
            _salesBookVATProcessLogTrans.ReverseDate            = _reverseDate;
            _salesBookVATProcessLogTrans.ReverseSettlementRecId = _reverseSettlementRecId;
        }

        _salesBookVATProcessLogTrans.ConfirmationDate = _factureJour.ConfirmationDate;
        _salesBookVATProcessLogTrans.ExpirationDate   = _factureJour.expirationDate();
        _salesBookVATProcessLogTrans.PackageDate      = _factureJour.PackageDate;
        _salesBookVATProcessLogTrans.ExportDate       = _factureJour.ExportDate;
        _salesBookVATProcessLogTrans.ProcessingType   = SalesBookVATProcessLogTrans_RU::factureType2type(_factureJour.ProcessingType);

        _salesBookVATProcessLogTrans.insert();
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateLine</Name>
				<Source><![CDATA[
    public static void updateLine(TmpSalesBookVATProcessLogTrans_RU _salesBookVATProcessLogTrans,
                                  BookStructVATAmounts_RU           _factureAmounts,
                                  BookStructVATAmounts_RU           _settledAmounts,
                                  BookStructVATAmounts_RU           _usedAmounts,
                                  TransDate                         _fromDate)
    {
        if (_settledAmounts.amountInclVAT() - _usedAmounts.amountInclVAT())
        {
            _salesBookVATProcessLogTrans.FactureAmounts = _factureAmounts.pack();
            _salesBookVATProcessLogTrans.SettledAmounts = _settledAmounts.pack();
            _salesBookVATProcessLogTrans.UsedAmounts    = _usedAmounts.pack();
            _salesBookVATProcessLogTrans.update();
        }
        else
        {
            _salesBookVATProcessLogTrans.delete();
        }
    }

]]></Source>
