			<Method>
				<Name>displayTransType</Name>
				<Source><![CDATA[
    public display SalesPurchBookTransType_RU displayTransType()
    {
        FactureJour_RU             factureJour;
        SalesPurchBookTransType_RU ret;

        if (this.TransType != SalesPurchBookTransType_RU::VATRestoring)
        {
            return this.TransType;
        }

        factureJour = FactureJour_RU::find(this.FactureId, this.factureModule());

        switch (factureJour.FactureType)
        {
            case FactureType_RU::Invoice :
                ret = SalesPurchBookTransType_RU::Invoice;
                break;
            case FactureType_RU::TaxCorrection :
                ret = SalesPurchBookTransType_RU::TaxCorrection;
                break;
            case FactureType_RU::Prepayment :
                ret = SalesPurchBookTransType_RU::PrepaymentStorno;
                break;
            case FactureType_RU::AmoutDifference :
                ret = SalesPurchBookTransType_RU::SumDiff;
                break;
            case FactureType_RU::AdvanceReport :
                ret = SalesPurchBookTransType_RU::AdvanceReport;
                break;
            case FactureType_RU::ExchAdj :
                ret = SalesPurchBookTransType_RU::ExchAdj;
                break;
            case FactureType_RU::AdvanceAdjustment :
                ret = SalesPurchBookTransType_RU::AdvanceAdjustment;
                break;
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>factureDate_External</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    public display FactureDate_External_RU factureDate_External()
    {
        FactureJour_RU factureJour;

        switch (this.TransType)
        {
            case SalesPurchBookTransType_RU::TaxCorrection :
                select firstonly FactureDate_External from factureJour
                    where  factureJour.FactureId == this.FactureId                       &&
                          (factureJour.Module    == FactureModule_RU::TaxCorrectionSales ||
                           factureJour.Module    == FactureModule_RU::TaxCorrectionSales_Vend);
                break;

            default:
                select firstonly FactureDate_External from factureJour
                    where factureJour.FactureId == this.FactureId &&
                          factureJour.Module    == this.factureModule();
        }

        return factureJour.FactureDate_External;
    }

]]></Source>
			</Method>
			<Method>
				<Name>factureModule</Name>
				<Source><![CDATA[
    public FactureModule_RU factureModule()
    {
        FactureModule_RU ret;

        switch (this.TransType)
        {
            case SalesPurchBookTransType_RU::CreditNote        :
            case SalesPurchBookTransType_RU::Invoice           :
            case SalesPurchBookTransType_RU::SumDiff           :
            case SalesPurchBookTransType_RU::ExchAdj           :
            case SalesPurchBookTransType_RU::AdvanceAdjustment :
            case SalesPurchBookTransType_RU::Prepayment        :
                ret = FactureModule_RU::Cust;
                break;

            case SalesPurchBookTransType_RU::TaxCorrection     :
                ret = FactureJour_RU::findTaxCorrection(this.FactureId, FactureBook_RU::Sales).Module;
                break;

            case SalesPurchBookTransType_RU::PrepaymentStorno  :
            case SalesPurchBookTransType_RU::VATRestoring      :
                ret = FactureModule_RU::Vend;
                break;
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>factureType</Name>
				<Source><![CDATA[
    public FactureType_RU factureType()
    {
        FactureType_RU  ret = FactureType_RU::All;

        switch (this.TransType)
        {
            case SalesPurchBookTransType_RU::CreditNote        :
            case SalesPurchBookTransType_RU::Invoice           :
                ret = FactureType_RU::Invoice;
                break;
            case SalesPurchBookTransType_RU::TaxCorrection     :
                ret = FactureType_RU::TaxCorrection;
                break;
            case SalesPurchBookTransType_RU::PrepaymentStorno  :
                ret = FactureType_RU::Prepayment;
                break;
            case SalesPurchBookTransType_RU::SumDiff           :
                ret = FactureType_RU::AmoutDifference;
                break;
            case SalesPurchBookTransType_RU::ExchAdj           :
                ret = FactureType_RU::ExchAdj;
                break;
            case SalesPurchBookTransType_RU::AdvanceAdjustment :
                ret = FactureType_RU::AdvanceAdjustment;
                break;
        }

        return ret;
    }

]]></Source>
