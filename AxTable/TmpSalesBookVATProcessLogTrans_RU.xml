				<Name>initFromSBookVATProcessLogTrans</Name>
				<Source><![CDATA[
    public void initFromSBookVATProcessLogTrans(SalesBookVATProcessLogTrans_RU _salesBookVATProcessLogTrans)
    {
        BookStructVATAmounts_RU usedAmounts = new BookStructVATAmounts_RU();

        this.FactureExternalId      = _salesBookVATProcessLogTrans.FactureExternalId;
        this.FactureDate            = _salesBookVATProcessLogTrans.FactureDate;
        this.PaymentDate            = _salesBookVATProcessLogTrans.PaymentDate;
        this.AccountName            = _salesBookVATProcessLogTrans.AccountName;
        this.CountryGTD             = _salesBookVATProcessLogTrans.CountryGTD;

        this.UsedAmounts            = usedAmounts.pack();
        this.FactureAmounts         = _salesBookVATProcessLogTrans.FacturedAmounts;

        this.AccountNum             = _salesBookVATProcessLogTrans.AccountNum;
        this.FactureId              = _salesBookVATProcessLogTrans.FactureId;
        this.TransType              = _salesBookVATProcessLogTrans.TransType;
        this.InvoiceRecIdRef        = _salesBookVATProcessLogTrans.InvoiceRecIdRef;
        this.PaymentRecIdRef        = _salesBookVATProcessLogTrans.PaymentRecIdRef;
        this.RefTableId             = _salesBookVATProcessLogTrans.RefTableId;
        this.SettlementDate         = _salesBookVATProcessLogTrans.SettlementDate;
        this.SettlementRecId        = _salesBookVATProcessLogTrans.SettlementRecId;
        this.SalesBookTransDate     = max(_salesBookVATProcessLogTrans.FactureDate,
                                          max(_salesBookVATProcessLogTrans.SettlementDate, _salesBookVATProcessLogTrans.PaymentDate));
        this.Reversed               = _salesBookVATProcessLogTrans.ReverseDate ? NoYes::Yes : NoYes::No;
        this.ReverseDate            = _salesBookVATProcessLogTrans.ReverseDate;
        this.ReverseSettlementRecId = _salesBookVATProcessLogTrans.ReverseSettlementRecId;
        this.InvoiceCompany         = _salesBookVATProcessLogTrans.InvoiceCompany;
        this.PaymentCompany         = _salesBookVATProcessLogTrans.PaymentCompany;
        this.ConfirmationDate       = _salesBookVATProcessLogTrans.ConfirmationDate;
        this.ExpirationDate         = _salesBookVATProcessLogTrans.ExpirationDate;
        this.ExportDate             = _salesBookVATProcessLogTrans.ExportDate;
        this.PackageDate            = _salesBookVATProcessLogTrans.PackageDate;
        this.ProcessingType         = _salesBookVATProcessLogTrans.ProcessingType;
    }

]]></Source>
