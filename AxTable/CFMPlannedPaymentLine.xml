		<Methods>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    public static boolean exist(RefRecId _refRecId)
    {
        CFMPlannedPaymentLine record;

        if (_refRecId)
        {
            select firstonly RecId from record
                where record.RecId == _refRecId;
        }

        return record.RecId;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static CFMPlannedPaymentLine find(RefRecId _refRecId, boolean _forUpdate = false)
    {
        CFMPlannedPaymentLine record;

        if (_refRecId)
        {
            record.selectForUpdate(_forUpdate);

            select firstonly record
                where record.RecId == _refRecId;
        }

        return record;
    }

]]></Source>
