		<Methods>
			<Method>
				<Name>insertInventtransToOhcsQueue</Name>
				<Source><![CDATA[
    internal static void insertInventtransToOhcsQueue(InventTrans _inventTrans, boolean _negativeQty = false)
    {
        InventInventoryDataServiceOnhandChangeScheduleQueue ohcsQueue;

        ttsbegin;

        InventDim inventDim = InventDim::find(_inventTrans.InventDimId);

        ohcsQueue.ItemId            = _inventTrans.ItemId;
        ohcsQueue.InventDimId       = _inventTrans.InventDimId;
        ohcsQueue.Company           = _inventTrans.DataAreaId;
        ohcsQueue.StatusIssue       = _inventTrans.StatusIssue;
        ohcsQueue.StatusReceipt     = _inventTrans.StatusReceipt;
        ohcsQueue.Qty               = _negativeQty ? -_inventTrans.Qty : _inventTrans.Qty;
        ohcsQueue.DateExpected      = _inventTrans.DateExpected;
        ohcsQueue.InventTransOrigin = _inventTrans.InventTransOrigin;

        ohcsQueue.InventSiteId      = inventDim.InventSiteId;
        ohcsQueue.InventLocationId  = inventDim.InventLocationId;

        ohcsQueue.insert();

        ttscommit;
    }

]]></Source>
			</Method>
