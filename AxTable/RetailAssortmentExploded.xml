			<Method>
				<Name>findByRecId</Name>
				<Source><![CDATA[
    /// <summary>
    /// Retrieves the record associated with the specified ID.
    /// </summary>
    /// <param name="_recId">
    /// The record ID.
    /// </param>
    /// <param name="_forUpdate">
    /// A value indicating whether the record will support update.
    /// </param>
    /// <returns>
    /// The record for the specified ID.
    /// </returns>
    public static RetailAssortmentExploded findByRecId(RefRecId _recId, boolean _forUpdate = false)
    {
        RetailAssortmentExploded exploded;

        if (_recId)
        {
            if (_forUpdate)
            {
                exploded.selectForUpdate(_forUpdate);
            }

            select firstonly exploded
                where exploded.RecId == _recId;
        }

        return exploded;
    }

]]></Source>
