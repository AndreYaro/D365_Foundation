			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a record using the specified parameters.
    /// </summary>
    /// <param name="_assortmentId">
    /// The assortment ID number.
    /// </param>
    /// <param name="_omOperatingUnitId">
    /// The operating unit ID number.
    /// </param>
    /// <param name="_dataAreaId">
    /// The data area ID number.
    /// </param>
    /// <param name="_itemId">
    /// The item ID number.
    /// </param>
    /// <param name="_inventDimId">
    /// The inventory dimension ID number.
    /// </param>
    /// <param name="_forUpdate">
    /// A value indicating whether the record will support update.
    /// </param>
    /// <returns>
    /// The assortment record.
    /// </returns>
    public static RetailAssortmentExploded find(
        RetailAssortmentRefRecId _assortmentId,
        OMOperatingUnitRefRecId _omOperatingUnitId,
        DataAreaId _dataAreaId,
        ItemId _itemId,
        InventDimId _inventDimId,
        boolean _forUpdate = false)
    {
        RetailAssortmentExploded    exploded;

        if (_forUpdate)
        {
            exploded.selectForUpdate(_forUpdate);
        }

        select firstonly exploded
            where
                exploded.AssortmentRecId == _assortmentId &&
                exploded.omOperatingUnitId == _omOperatingUnitId &&
                exploded.ChannelDataAreaId == _dataAreaId &&
                exploded.ItemId == _itemId &&
                exploded.InventDimId == _inventDimId;

        return exploded;
    }

]]></Source>
