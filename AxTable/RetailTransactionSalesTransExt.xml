			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a specified record in the <c>RetailTransactionSalesTransExt</c> table.
    /// </summary>
    /// <param name="_transactionId">
    /// The ID of a transaction.
    /// </param>
    /// <param name="_storeId">
    /// The ID of a store.
    /// </param>
    /// <param name="_terminalId">
    /// The ID of a POS terminal.
    /// </param>
    /// <param name="_lineNum">
    /// The line number of a transaction.
    /// </param>
    /// <param name="_channelRecId">
    /// The ID of a channel; optional.
    /// </param>
    /// <param name="_forUpdate">
    /// A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <returns>
    /// A record in the <c>RetailTransactionSalesTransExt</c> table; otherwise, an empty record.
    /// </returns>
    [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
    public static RetailTransactionSalesTransExt find(RetailTransactionId _transactionId,
                                                RetailStoreId _storeId,
                                                RetailTerminalId _terminalId,
                                                LineNum _lineNum,
                                                RetailChannelRecId _channelRecId = 0,
                                                boolean _forUpdate = false)
    {
        RetailTransactionSalesTransExt transactionSalesTransExt;

        if (_transactionId && _storeId && _terminalId && _lineNum)
        {
            transactionSalesTransExt.selectForUpdate(_forUpdate);

            select firstonly transactionSalesTransExt
                index hint transactionLineIdx
                where (transactionSalesTransExt.Store == _storeId || (_channelRecId != 0 && transactionSalesTransExt.Channel == _channelRecId))
                && transactionSalesTransExt.Terminal == _terminalId
                && transactionSalesTransExt.TransactionId == _transactionId
                && transactionSalesTransExt.LineNum == _lineNum;
        }

        return transactionSalesTransExt;
    }

]]></Source>
			</Method>
