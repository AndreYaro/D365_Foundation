		<Methods>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static boolean exist(RHRMEvaluationCodeId    _evaluationCodeId)
    {
        return _evaluationCodeId && (select RecId from rhrmEvaluationCode
                                             where rhrmEvaluationCode.EvaluationCodeId == _evaluationCodeId).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static RHRMEvaluationCode find(RHRMEvaluationCodeId  _evaluationCodeId,
                                          boolean               _forUpdate = false)
    {
        RHRMEvaluationCode rhrmEvaluationCode;
        ;

        if (_evaluationCodeId)
        {
            rhrmEvaluationCode.selectForUpdate(_forUpdate);

            select firstonly rhrmEvaluationCode
                where rhrmEvaluationCode.EvaluationCodeId == _evaluationCodeId;
        }
        return rhrmEvaluationCode;
    }

]]></Source>
