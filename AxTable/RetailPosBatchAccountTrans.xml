		<Methods>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Indicates whether the specified record in the <c>RetailPosBatchAccountTrans</c> table exists.
    /// </summary>
    /// <param name="_storeId">
    /// The current value of the store Id.
    /// </param>
    /// <param name="_terminalId">
    /// The current value of the terminal Id.
    /// </param>
    /// <param name="_batchId">
    /// The current value of the batch Id.
    /// </param>
    /// <param name="_accountNum">
    /// The current value of the account number.
    /// </param>
    /// <returns>
    /// true if the specified record exists; otherwise, false.
    /// </returns>
    public static boolean exist(RetailStoreId      _storeId,
                                RetailTerminalId   _terminalId,
                                RetailPosBatchID   _batchId,
                                AccountNum      _accountNum)
    {
        return _storeId && _terminalId && _batchId && _accountNum && (select firstonly RecId from retailPosBatchAccountTrans
                                                                        where retailPosBatchAccountTrans.StoreId       == _storeId     &&
                                                                              retailPosBatchAccountTrans.TerminalId    == _terminalId  &&
                                                                              retailPosBatchAccountTrans.BatchID       == _batchId     &&
                                                                              retailPosBatchAccountTrans.AccountNum    == _accountNum).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds the specified record in the <c>RetailPosBatchAccountTrans</c> table.
    /// </summary>
    /// <param name="_storeId">
    /// The current value of the store ID.
    /// </param>
    /// <param name="_terminalId">
    /// The current value of the terminal ID.
    /// </param>
    /// <param name="_batchId">
    /// The current value of the batch ID.
    /// </param>
    /// <param name="_accountNum">
    /// The current value of the account number.
    /// </param>
    /// <param name="_forUpdate">
    /// A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <returns>
    /// A record in the <c>RetailPosBatchAccountTrans</c> table; otherwise, an empty record.
    /// </returns>
    public static RetailPosBatchAccountTrans find(RetailStoreId       _storeId,
                                               RetailTerminalId    _terminalId,
                                               RetailPosBatchID    _batchId,
                                               AccountNum       _accountNum,
                                              boolean          _forUpdate = false)
    {
        RetailPosBatchAccountTrans     rboPosBatchAccountTrans;
        ;

        rboPosBatchAccountTrans.selectForUpdate(_forUpdate);

        select firstonly rboPosBatchAccountTrans
            where rboPosBatchAccountTrans.StoreId       == _storeId      &&
                  rboPosBatchAccountTrans.TerminalId    == _terminalId   &&
                  rboPosBatchAccountTrans.BatchID       == _batchId      &&
                  rboPosBatchAccountTrans.AccountNum    == _accountNum;

        return rboPosBatchAccountTrans;
    }

]]></Source>
