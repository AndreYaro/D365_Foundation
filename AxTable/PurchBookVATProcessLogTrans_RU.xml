			<Method>
				<Name>findFromSettlement</Name>
				<Source><![CDATA[
    public static RecordSortedList findFromSettlement(FactureId_RU               _factureId,
                                                             recId                      _transRecId,
                                                             recId                      _offsetRecId,
                                                             CompanyId                  _transCompany,
                                                             CompanyId                  _offsetCompany,
                                                             recId                      _settlementRecId,
                                                             TransDate                  _settlementDate,
                                                             boolean                    _forUpdate          = false,
                                                             boolean                    _useSettlementInfo  = true,
                                                             PurchBookProcessingType_RU _processingType     = PurchBookProcessingType_RU::Standard,
                                                             TransDate                  _correctedPeriod    = dateNull(),
                                                             FactureId_RU               _correctedFactureId = '')

    {
        PurchBookVATProcessLogTrans_RU  processLogTrans;
        RecordSortedList                processLogTransList;

        processLogTrans.selectForUpdate(_forUpdate);

        processLogTransList = new RecordSortedList(tableNum(PurchBookVATProcessLogTrans_RU));
        processLogTransList.sortOrder(fieldNum(PurchBookVATProcessLogTrans_RU, RecId));

        if (_useSettlementInfo)
        {
            select firstonly processLogTrans index Settlement
                where processLogTrans.SettlementRecId    == _settlementRecId &&
                      processLogTrans.FactureId          == _factureId       &&
                      processLogTrans.InvoiceRecIdRef    == _transRecId      &&
                      processLogTrans.PaymentRecIdRef    == _offsetRecId     &&
                      processLogTrans.InvoiceCompany     == _transCompany    &&
                      processLogTrans.PaymentCompany     == _offsetCompany   &&
                      processLogTrans.SettlementDate     == _settlementDate  &&
                      processLogTrans.ProcessingType     == _processingType  &&
                      processLogTrans.CorrectedPeriod    == _correctedPeriod &&
                      processLogTrans.CorrectedFactureId == _correctedFactureId;

            if (processLogTrans)
            {
                processLogTransList.ins(processLogTrans);
            }
        }
        else
        {
            while select processLogTrans index Settlement
                where processLogTrans.FactureId          == _factureId       &&
                      processLogTrans.InvoiceRecIdRef    == _transRecId      &&
                      processLogTrans.InvoiceCompany     == _transCompany    &&
                      processLogTrans.ProcessingType     == _processingType  &&
                      processLogTrans.CorrectedPeriod    == _correctedPeriod &&
                      processLogTrans.CorrectedFactureId == _correctedFactureId
            {
                processLogTransList.ins(processLogTrans);
            }
        }

        return processLogTransList;
    }

]]></Source>
