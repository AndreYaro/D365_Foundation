			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static PurchBookVATProcessLogTrans_RU find(recId _recId, boolean _forUpdate = false)
    {
        PurchBookVATProcessLogTrans_RU  logTrans;

        logTrans.selectForUpdate(_forUpdate);

        if (_recId)
        {
            select firstonly logTrans
                where logTrans.RecId == _recId;
        }
        return logTrans;
    }

]]></Source>
