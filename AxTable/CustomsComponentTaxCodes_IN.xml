    public static CustomsComponentTaxCodes_IN find(
        RefRecId            _recId,
        boolean             _forupdate          = false,
        ConcurrencyModel    _concurrencyModel   = ConcurrencyModel::Auto)
    {
        CustomsComponentTaxCodes_IN customsComponentTaxCodes;

        customsComponentTaxCodes.selectForUpdate(_forupdate);
        if (_forupdate  && _concurrencyModel != ConcurrencyModel::Auto)
        {
            customsComponentTaxCodes.concurrencyModel(_concurrencyModel);
