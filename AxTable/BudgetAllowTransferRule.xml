    /// </returns>
    public static BudgetAllowTransferRule find(
        RecId              _recId,
        boolean            _forupdate = false,
        ConcurrencyModel   _concurrencyModel = ConcurrencyModel::Auto)
    {
        BudgetAllowTransferRule budgetAllowTransferRule;

        budgetAllowTransferRule.selectForUpdate(_forupdate);

        if (_forupdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            budgetAllowTransferRule.concurrencyModel(_concurrencyModel);
        }

        if (_recId != 0)
        {
            select firstonly budgetAllowTransferRule
                where budgetAllowTransferRule.RecId == _recId;
        }

        return budgetAllowTransferRule;
    }

