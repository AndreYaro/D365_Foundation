		<Methods>
			<Method>
				<Name>publishChannel</Name>
				<Source><![CDATA[
    /// <summary>
    /// Populates publishing data for the specified channel in the <c>RetailPubRetailOnlineChannelLanguages</c> table.
    /// </summary>
    /// <param name="_channelId">
    /// Id of the channel to publish
    /// </param>
    public static void publishChannel(RefRecId _channelId)
    {
        RetailOnlineChannelLanguages channelLanguage;
        RetailPubRetailOnlineChannelLanguages pubChannelLanguage, foundPubChannelLanguage;

        ttsBegin;

        // Delete
        delete_from pubChannelLanguage
            where pubChannelLanguage.OnlineChannel == _channelId
        notExists join channelLanguage
            where channelLanguage.RecId == pubChannelLanguage.OriginId;

        // Update
        update_recordSet pubChannelLanguage
        setting
            OriginRecVersion = channelLanguage.recVersion,
            IsDefault = channelLanguage.IsDefault,
            LanguageId = channelLanguage.LanguageId
            where pubChannelLanguage.OnlineChannel == _channelId
        join channelLanguage
            where channelLanguage.RecId == pubChannelLanguage.OriginId
               && pubChannelLanguage.OriginRecVersion != channelLanguage.recVersion;

        // Insert
        insert_recordset pubChannelLanguage (OriginId, OriginRecVersion, OnlineChannel, IsDefault, LanguageId)
        select RecId, RecVersion, OnlineChannel, IsDefault, LanguageId from channelLanguage
            where channelLanguage.OnlineChannel == _channelId
        notExists join foundPubChannelLanguage
            where foundPubChannelLanguage.OriginId == channelLanguage.RecId;

        ttsCommit;
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
