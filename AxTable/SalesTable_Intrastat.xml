			<Method>
				<Name>findBySalesId</Name>
				<Source><![CDATA[
    /// <summary>
    /// Selects record in <c>SalesTable_Intrastat</c> assotiated with specific sales Id.
    /// </summary>
    /// <param name="_salesId">
    /// Identifiaction of the sales.
    /// </param>
    /// <param name="_forUpdate">
    /// Defines that return buffer marked as for update. Optional. By default false.
    /// </param>
    /// <returns>
    /// Buffer of <c>SalesTable_Intrastat</c>.
    /// </returns>
    public static SalesTable_Intrastat findBySalesId(
        SalesId _salesId,
        boolean _forUpdate = false)
    {
        SalesTable salesTable;
        SalesTable_Intrastat salesTable_Intrastat;

        salesTable_Intrastat.selectForUpdate(_forUpdate);

        if (_salesId != '')
        {
            select firstonly * from salesTable_Intrastat
                exists join salesTable
                    where salesTable.SalesId == _salesId
                        && salesTable.RecId == salesTable_Intrastat.SalesTable;
        }

        return salesTable_Intrastat;
    }

]]></Source>
