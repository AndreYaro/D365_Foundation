				<Name>exist</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static boolean exist(RHRMMilitaryGroupId _rhrmMilitaryGroupId)
    {
        return _rhrmMilitaryGroupId && (select RecId from rhrmMilitaryGroup
                                            where rhrmMilitaryGroup.MilitaryGroupId == _rhrmMilitaryGroupId).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
        [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
        public static RHRMMilitaryGroup find(RHRMMilitaryGroupId _rhrmMilitaryGroupId,
                                         boolean             _forUpdate = false)
    {
        RHRMMilitaryGroup rhrmMilitaryGroup;
        ;

        if (_rhrmMilitaryGroupId)
        {
            rhrmMilitaryGroup.selectForUpdate(_forUpdate);

            select firstonly rhrmMilitaryGroup
                where rhrmMilitaryGroup.MilitaryGroupId == _rhrmMilitaryGroupId;
        }
        return rhrmMilitaryGroup;
    }

]]></Source>
