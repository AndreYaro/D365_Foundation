			<Method>
				<Name>lastManualLineNum</Name>
				<Source><![CDATA[
    /// <summary>
    /// Calculates the maximum line number for the transactions.
    /// </summary>
    /// <param name="_statementId">
    /// A statement number.
    /// </param>
    /// <returns>
    /// The maximum line number.
    /// </returns>

    static LineNum  lastManualLineNum(RetailStatementId _statementId)
    {
        return (select maxof(LineNum) from retailTransactionBankedTenderTrans
                    index hint transactionLineIdx
                    where retailTransactionBankedTenderTrans.StatementId == _statementId &&
                          retailTransactionBankedTenderTrans.StatusType == RetailBankedStatusTypeBase::Manual).LineNum;
    }

]]></Source>
