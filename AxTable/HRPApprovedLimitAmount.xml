			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds the specified record in the <c>HRPApprovedLimitAmount</c> table.
    /// </summary>
    /// <param name="_approvedSigningLimitAmount">
    /// The reference record ID that indicates the approved limit amount.
    /// </param>
    /// <param name="_forUpdate">
    /// A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <returns>
    /// A record in the <c>HRPApprovedLimitAmount</c> table.
    /// </returns>
    public static HRPApprovedLimitAmount find(RefRecId _approvedSigningLimitAmount, boolean _forUpdate = false)
    {
        HRPApprovedLimitAmount approvedSigningLimitAmount;

        if (_approvedSigningLimitAmount)
        {
            approvedSigningLimitAmount.selectForUpdate(_forUpdate);
            select firstonly approvedSigningLimitAmount
                where approvedSigningLimitAmount.RecId == _approvedSigningLimitAmount;
        }
        return approvedSigningLimitAmount;
    }

