		<Methods>
			<Method>
				<Name>initFromTaxWorkTrans</Name>
				<Source><![CDATA[
        void initFromTaxWorkTrans(TmpTaxWorkTrans _tmpTaxWorkTrans)
    {
        this.TransDate                  = _tmpTaxWorkTrans.TransDate;
        this.TaxCode                    = _tmpTaxWorkTrans.TaxCode;
        this.TaxBaseAmount              = _tmpTaxWorkTrans.TaxBaseAmount;
        this.SourceBaseAmountCur        = _tmpTaxWorkTrans.SourceBaseAmountCur;
        this.TaxBaseQty                 = _tmpTaxWorkTrans.TaxBaseQty;
        this.TaxAmount                  = _tmpTaxWorkTrans.TaxAmount;
        this.SourceTaxAmountCur         = _tmpTaxWorkTrans.SourceTaxAmountCur;
        this.SourceCurrencyCode         = _tmpTaxWorkTrans.SourceCurrencyCode;
        this.TaxDirection               = _tmpTaxWorkTrans.TaxDirection;
        this.ExemptTax                  = _tmpTaxWorkTrans.ExemptTax;
        this.TaxAutoGenerated           = _tmpTaxWorkTrans.TaxAutogenerated;
        this.SourceRegulateAmountCur    = _tmpTaxWorkTrans.SourceRegulateAmountCur;
        this.euroTriangulation          = _tmpTaxWorkTrans.euroTriangulation;
        this.TaxValue                   = _tmpTaxWorkTrans.showTaxValue();
        this.ExemptCode                 = TaxGroupData::find(_tmpTaxWorkTrans.TaxGroup, _tmpTaxWorkTrans.TaxCode).TaxExemptCode;
        this.PrintCode                  = TaxTable::find(_tmpTaxWorkTrans.TaxCode).PrintCode;
        this.TaxGroup                   = _tmpTaxWorkTrans.TaxGroup;
        this.TaxPrintDetail             = TaxGroupHeading::find(_tmpTaxWorkTrans.TaxGroup).TaxPrintDetail;
        this.InventTransId              = _tmpTaxWorkTrans.InventTransId;
        this.OperationLedgerDimension   = _tmpTaxWorkTrans.OperationLedgerDimension;
    }

]]></Source>
			</Method>
			<Method>
				<Name>taxAmount</Name>
				<Source><![CDATA[
        static Amount taxAmount(anytype _Query)
    {
        TaxJournalTrans  taxJournalTrans;
        QueryRun         taxQuery;
        AmountCur        taxAmountCur;
        AmountMST        taxAmount;

        CurrencyCode     currencyCode;
        boolean          companyCurrency;

        companyCurrency = false;

        taxQuery = new QueryRun(_Query);
        while (taxQuery.next())
        {
            taxJournalTrans = taxQuery.get(tablenum(TaxJournalTrans));

            if (!taxJournalTrans.TaxAutoGenerated)
            {
                taxAmountCur += taxJournalTrans.SourceRegulateAmountCur;
            }
            else
            {
                taxAmountCur += taxJournalTrans.SourceTaxAmountCur;
            }
            taxAmount += taxJournalTrans.TaxAmount;

            if (! currencyCode)
                currencyCode = taxJournalTrans.SourceCurrencyCode;
            if (currencyCode != taxJournalTrans.SourceCurrencyCode)
                companyCurrency = true;
        }
        if (companyCurrency)
            return taxAmount;
        else
            return taxAmountCur;
    }

]]></Source>
			</Method>
		</Methods>
