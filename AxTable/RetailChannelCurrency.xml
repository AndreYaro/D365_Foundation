				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a record in the table <c>RetailChannelCurrencyTable</c>.
    /// </summary>
    /// <param name="_channelRecId">
    /// RecId of the channel (retail store, online store...).
    /// </param>
    /// <param name="_currency">
    /// Code of the currency.
    /// </param>
    /// <param name="_forUpdate">
    /// A boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <returns>
    /// A record in the <c>RetailChannelCurrencyTable</c> table; otherwise an empty record.
    /// </returns>
    public static RetailChannelCurrency find(RefRecId _channelRecId, CurrencyCode _currency, boolean _forUpdate = false)
    {
        RetailChannelCurrency retailChannelCurrency;

        if (_channelRecId && _currency)
        {
            retailChannelCurrency.selectForUpdate(_forUpdate);

            select firstonly retailChannelCurrency
                where retailChannelCurrency.Channel == _channelRecId
                && retailChannelCurrency.CurrencyCode == _currency;
        }

        return retailChannelCurrency;
    }

]]></Source>
