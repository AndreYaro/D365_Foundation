				<Name>findByAssetRule</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Finds an active record in the <c>AssetRuleThreshold</c> table.
    /// </summary>
    /// <param name="_assetRule">
    ///    A record ID of the record in the <c>AsssetRule</c> table.
    /// </param>
    /// <param name="_validFrom">
    ///    A valid start date.
    /// </param>
    /// <param name="_validto">
    ///    A valid end date.
    /// </param>
    /// <returns>
    ///    An active record in the <c>AssetRuleThreshold</c> table; otherwise, an empty record.
    /// </returns>
    public static AssetRuleThreshold findByAssetRule(recId  _assetRule,
                                                     date   _validFrom  = DateTimeUtil::date(DateTimeUtil::utcNow()),
                                                     date   _validto    = DateTimeUtil::date(DateTimeUtil::utcNow()))
    {
        AssetRuleThreshold assetRuleThreshold;

        select validTimeState(_validFrom, _validTo) * from  assetRuleThreshold
            where assetRuleThreshold.AssetRule == _assetRule;

        return assetRuleThreshold;
    }

]]></Source>
