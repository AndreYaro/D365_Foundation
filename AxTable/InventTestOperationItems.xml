		<Methods>
			<Method>
				<Name>calcItemCost</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Calculates the estimated cost of the items that are used in the operation.
    /// </summary>
    /// <returns>
    ///    The cost amount.
    /// </returns>
    AmountMST   calcItemCost()
    {
        InventNonConformanceTable   inventNonConformanceTable = InventNonConformanceTable::find(this.InventNonConformanceID);
        InventTable                 inventTable = InventTable::find(this.ItemId);
        InventPriceMap              inventPriceMap;

        inventPriceMap = inventTable.inventCostPriceMap(inventNonConformanceTable.InventDimId, inventNonConformanceTable.inventDim(),inventTable.inventTableModuleInvent());

        return PriceDisc::price2Amount(inventPriceMap.price(),
                                       inventPriceMap.priceUnit(),
                                       0,
                                       this.InventQty,
                                       this.orig().InventQty,
                                       inventPriceMap.markup(),
                                       0,
                                       '',
                                       this.CostAmount,
                                       true);
    }

]]></Source>
			</Method>
			<Method>
				<Name>insert</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Inserts the cost amount.
    /// </summary>
    /// <remarks>
    ///    This method overrides the standard <c>insert</c> method.
    /// </remarks>
    public void insert()
    {
        ttsbegin;

        this.CostAmount = this.calcItemCost();

        super();

        ttscommit;
    }

]]></Source>
			</Method>
			<Method>
				<Name>update</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Updates the cost amount.
    /// </summary>
    public void update()
    {
        ttsbegin;

        this.CostAmount = this.calcItemCost();

        super();

        ttscommit;
    }

]]></Source>
			</Method>
