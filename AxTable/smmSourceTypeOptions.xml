		<Methods>
			<Method>
				<Name>createFromAllowFreeFormText</Name>
				<Source><![CDATA[
        static smmSourceTypeOptions createFromAllowFreeFormText(SmmSourceTypeId _sourceTypeId, smmSourceName _sourceName)
    {
        smmSourceTypeOptions sourceTypeOptions;

        if (_sourceTypeId && _sourceName)
        {
            sourceTypeOptions.clear();
            sourceTypeOptions.TypeId = _sourceTypeId;
            sourceTypeOptions.Name   = _sourceName;
            sourceTypeOptions.insert();
        }

        return sourceTypeOptions;
    }

]]></Source>
			</Method>
			<Method>
				<Name>shouldThrowExceptionOnZeroDelete</Name>
				<Source><![CDATA[
    /// <summary>
    /// Determines if concurrent deletes should throw exception.
    /// </summary>
    /// <returns>true if exception should be thrown; otherwise false.</returns>
    public boolean shouldThrowExceptionOnZeroDelete()
    {
        return super() || SalesEnforceConcurrentDeletesFlight::instance().isEnabled();
    }

]]></Source>
