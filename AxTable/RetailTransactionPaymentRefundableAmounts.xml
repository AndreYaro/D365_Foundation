				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a specified record in the <c>RetailTransactionPaymentTrans</c> table.
    /// </summary>
    /// <param name="_transactionId">
    /// The transaction number of the record to find.
    /// </param>
    /// <param name="_storeId">
    /// The store number of the record to find.
    /// </param>
    /// <param name="_terminalId">
    /// The terminal number of the record to find.
    /// </param>
    /// <param name="_lineNum">
    /// The line number of the record to find.
    /// </param>
    /// <param name="_forUpdate">
    /// A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <returns>
    /// A record in the <c>RetailTransactionPaymentTrans</c> table; otherwise, an empty record.
    /// </returns>
    internal static RetailTransactionPaymentRefundableAmounts find(RetailTransactionId _transactionId,RetailStoreId _storeId,RetailTerminalId _terminalId, LineNum _lineNum, boolean _forUpdate = false)
    {
        RetailTransactionPaymentRefundableAmounts  retailTransactionPaymentRefundableAmounts;

        if (_transactionId && _storeId && _terminalId && _lineNum)
        {
            retailTransactionPaymentRefundableAmounts.selectForUpdate(_forUpdate);

            select firstonly retailTransactionPaymentRefundableAmounts
                index hint transactionLineIdx
                where retailTransactionPaymentRefundableAmounts.Store         == _storeId
                &&    retailTransactionPaymentRefundableAmounts.Terminal      == _terminalId
                &&    retailTransactionPaymentRefundableAmounts.TransactionId == _transactionId
                && retailTransactionPaymentRefundableAmounts.LineNum == _lineNum;
        }

        return retailTransactionPaymentRefundableAmounts;
    }

]]></Source>
