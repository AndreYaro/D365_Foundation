				<Source><![CDATA[
public void insert()
    {
        System.String   scheduleName;

        scheduleName = this.Name;

        if (scheduleName.Contains('\\')
            || scheduleName.Contains('\/')
            || scheduleName.Contains(':')
            || scheduleName.Contains('*')
            || scheduleName.Contains('?')
            || scheduleName.Contains('<')
            || scheduleName.Contains('|')
            || scheduleName.Contains('|'))
        {
            // Schedule name has invalid charater such as \\ \/ : ? * < > |
            throw error("@SYS4002735");
        }
        else
        {
            super();
        }
    }
]]></Source>
