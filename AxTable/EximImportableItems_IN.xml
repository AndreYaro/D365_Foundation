    /// </returns>
    [Microsoft.Dynamics.BusinessPlatform.SharedTypes.InternalUseOnlyAttribute]
    public static EximImportableItems_IN find(
        RefRecId            _recId,
        boolean             _forUpdate        = false,
        ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        EximImportableItems_IN eximImportableItems;

        eximImportableItems.selectForUpdate(_forUpdate);
        if (_forUpdate  && _concurrencyModel != ConcurrencyModel::Auto)
        {
            eximImportableItems.concurrencyModel(_concurrencyModel);
        }

        select firstonly eximImportableItems
            where eximImportableItems.RecId == _recId;

        return eximImportableItems;
    }

