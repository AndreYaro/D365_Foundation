			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    public boolean validateWrite()
    {
        boolean ret;
        boolean isCustomerMandatory = (!this.CustAccount &&
                                     this.isCustomerRequired());

        ret = super();

        if (ret)
        {
            ret = this.checkDate();

            if (isCustomerMandatory)
            {
                ret = checkFailed(strfmt("@SYS116347", fieldid2pname(tablenum(ProjGrant), fieldnum(ProjGrant, CustAccount))));
            }
        }

        ret = ret && ProjWorkerValidation::isWorkerInCurrentCompany(this.WorkerContact);

        return ret;
    }

]]></Source>
