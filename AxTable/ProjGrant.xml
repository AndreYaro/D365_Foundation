			<Method>
				<Name>isCustomerRequired</Name>
				<Source><![CDATA[
    /// <summary>
    /// Indicates whether the customer is required based on the current grant status.
    /// </summary>
    /// <returns>
    /// true if the customer is required; otherwise, false.
    /// </returns>
    public boolean isCustomerRequired()
    {
        boolean chk = (this.GrantStatus == ProjGrantStatus::Awarded ||
                                this.GrantStatus == ProjGrantStatus::Active);
        return chk;
    }

]]></Source>
