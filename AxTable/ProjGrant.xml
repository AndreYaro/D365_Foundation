			<Method>
				<Name>validateField</Name>
				<Source><![CDATA[
    public boolean validateField(fieldId _fieldIdToCheck)
    {
        boolean ret;
        ProjFundingSource   projFundingSource;
        ProjGrant           projGrantLocal;

        ret = super(_fieldIdToCheck);

        switch (_fieldIdToCheck)
        {
            case fieldnum(ProjGrant, MatchingPercentage) :
                if (this.MatchingPercentage < 0 || this.MatchingPercentage > 100)
                {
                    ret = checkFailed("@SYS67342");
                }
                break;

            case fieldnum(ProjGrant, GrantParentId) :
                if ((this.GrantParentId) &&
                (this.GrantParentId == this.GrantId || !this.validateGrant(this.GrantParentId)))
                {
                    ret = checkFailed(strfmt("@SYS112224",
                        this.GrantParentId,
                        fieldpname(ProjGrant,GrantParentId),
                        tablepname(ProjGrant)));
                }
                if (ret && this.GrantParentId)
                {
                    select RecId from projGrantLocal
                        where (projGrantLocal.GrantId == this.GrantParentId && projGrantLocal.GrantParentId != '') ||
                         (projGrantLocal.GrantParentId == this.GrantId);

                    if (projGrantLocal.RecId)
                    {
                        ret = checkFailed("@SYS308902");
                    }
                }
                break;
            case fieldnum(ProjGrant, GrantId) :
                if ((this.GrantStatus == ProjGrantStatus::Awarded) ||
                (this.GrantStatus == ProjGrantStatus::Active)   ||
                (this.GrantStatus == ProjGrantStatus::Closed)   ||
                (this.GrantStatus == ProjGrantStatus::Rejected))
                {
                    ret = checkFailed(strfmt("@SYS306730",this.GrantStatus));
                }

            case fieldnum(ProjGrant, GrantStatus) :
                select firstonly RecId from projFundingSource
                            where projFundingSource.ProjGrant == this.RecId;
                if (this.RecId && projFundingSource.RecId &&
                        (this.GrantStatus == ProjGrantStatus::Entered ||
                            this.GrantStatus == ProjGrantStatus::Submitted ||
                            this.GrantStatus == ProjGrantStatus::Approved))
                {
                    ret = checkFailed("@SYS306620");
                }
                break;

            case fieldnum(ProjGrant, WorkerContact) :
                ret = ret && ProjWorkerValidation::isWorkerInCurrentCompany(this.WorkerContact);
                break;
        }
        return ret;
    }

]]></Source>
