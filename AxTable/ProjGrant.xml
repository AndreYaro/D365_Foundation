			<Method>
				<Name>validateGrant</Name>
				<Source><![CDATA[
    public boolean validateGrant(ProjGrantId _grantId)
    {
        boolean isValidParentGrant = true;
        ProjGrant projGrantLocal = ProjGrant::findByGrantId(_grantId);

        if (!projGrantLocal)
        {
            isValidParentGrant = false;
        }

        if ((projGrantLocal.GrantStatus != ProjGrantStatus::Active) && (projGrantLocal.GrantStatus != ProjGrantStatus::Awarded))
        {
            isValidParentGrant = false;
        }

        return isValidParentGrant;
    }

]]></Source>
