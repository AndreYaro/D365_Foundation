				<Name>checkDate</Name>
				<Source><![CDATA[
    private boolean checkDate()
    {
        boolean ret = true;

        if ((this.ActualExpirationDate && this.ActualStartDate >= this.ActualExpirationDate) ||
            (this.RenewalActionDate && this.ActualStartDate >= this.RenewalActionDate))
        {
            ret = checkFailed("@SYS191128");
        }

        return ret;
    }

]]></Source>
