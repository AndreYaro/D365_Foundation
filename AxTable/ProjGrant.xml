			<Method>
				<Name>modifiedField</Name>
				<Source><![CDATA[
    public void modifiedField(fieldId _fieldId)
    {
        DialogButton    dialogBox;
        super(_fieldId);

        switch (_fieldId)
        {
            case fieldnum(ProjGrant, ProjGrantMatching) :
                this.recalculateMatchingAmount(_fieldId);
                break;

            case fieldnum(ProjGrant, AwardedAmount) :
            case fieldnum(ProjGrant, MatchingPercentage) :
            case fieldnum(ProjGrant, MatchingAmount) :
                if (this.RecId)
                {
                    if (this.AwardedAmount > 0 && (this.MatchingAmount > 0 || this.MatchingPercentage > 0))
                    {
                        if (_fieldId == fieldnum(ProjGrant, MatchingPercentage))
                        {
                            dialogBox = Box::yesNo("@SYS327960", DialogButton::No, "@SYS866");
                        }
                        else
                        {
                            dialogBox = Box::yesNo("@SYS305608", DialogButton::No, "@SYS866");
                        }
                    }
                    else
                    {
                        dialogBox = DialogButton::Yes;
                    }
                }

                if (!this.RecId || dialogBox == DialogButton::Yes)
                {
                    this.recalculateMatchingAmount(_fieldId);
                }
                break;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>recalculateMatchingAmount</Name>
				<Source><![CDATA[
    /// <summary>
    /// Recalculates the matching amount or matching percentage field values that are based on the provided
    /// field ID.
    /// </summary>
    /// <param name="_modifiedfieldId">
    /// The field ID that has been modified.
    /// </param>
    public void recalculateMatchingAmount(fieldId _modifiedfieldId)
    {
        switch (_modifiedfieldId)
        {
            case fieldnum(ProjGrant, AwardedAmount) :
            case fieldnum(ProjGrant, MatchingPercentage) :
            case fieldnum(ProjGrant, ProjGrantMatching) :
                this.MatchingAmount = (this.AwardedAmount * this.MatchingPercentage) / 100;
                break;

            case fieldnum(ProjGrant, MatchingAmount) :
                if (this.AwardedAmount)
                {
                    this.MatchingPercentage = (this.MatchingAmount / this.AwardedAmount) * 100;
                    if (!this.validateField(fieldnum(ProjGrant, MatchingPercentage)))
                    {
                        this.MatchingPercentage = this.orig().MatchingPercentage;
                        this.MatchingAmount     = this.orig().MatchingAmount;
                    }
                }
                break;
        }
    }

]]></Source>
