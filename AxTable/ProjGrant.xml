			<Method>
				<Name>validateDelete</Name>
				<Source><![CDATA[
    public boolean validateDelete()
    {
        ProjGrant projGrant;
        boolean ret;

        ret = super();

        if (this.GrantId)
        {
            select firstonly GrantParentId from projGrant where
            projGrant.GrantParentId == this.GrantId;

            if (projGrant.GrantParentId)
            {
                ret = checkFailed("@SYS191047");
            }
        }

        if (this.GrantStatus == ProjGrantStatus::Awarded)
        {
            ret = checkFailed("@SYS191046");
        }

        return ret;
    }

]]></Source>
