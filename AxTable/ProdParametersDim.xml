			<Method>
				<Name>projPostExPostingType</Name>
				<Source><![CDATA[
    /// <summary>
    /// Specifies whether posts should be made excl. the transaction type.
    /// </summary>
    /// <param name="_common">
    /// A <c>ProdJournalTable</c> table buffer or a <c>ProdTable</c> table buffer that is used to determine
    /// the production ID.
    /// </param>
    /// <returns>
    /// A <c>YesNo</c> enumeration value that indicates whether posts should be made excl. the transaction
    /// type.
    /// </returns>
    /// <exception cref="M:exception::error">
    /// The table buffer specified by the <paramref name="_common" /> parameter is not of the
    /// <c>ProdJournalTable</c> type or the <c>ProdTable</c> type.
    /// </exception>
    public static NoYes projPostExPostingType(Common _common)
    {
        ProdJournalTable    prodJournalTable;
        ProdJournalBOM      prodJournalBOM;
        ProdJournalRoute    prodJournalRoute;
        ProdTable           prodTable;
        NoYes               ret;

        //Post excl. transaction type is not applicable for project
        switch (_common.TableId)
        {
            case tablenum(ProdJournalTable):
                prodJournalTable = _common;
                ret = ProdParametersDim::find(ProdTable::find(prodJournalTable.ProdId).InventDimId).PostExPostingType;
                switch (prodJournalTable.JournalType)
                {
                    case ProdJournalType::Picklist:
                        select firstonly ProdId from prodJournalBOM
                            join prodTable
                            where prodJournalBOM.JournalId == prodJournalTable.JournalId &&
                                prodTable.ProdId == prodJournalBOM.ProdId &&
                                prodTable.ProjId;
                        if (prodJournalBOM.ProdId)
                            ret = NoYes::No;
                        break;
                    case ProdJournalType::JobCard:
                    case ProdJournalType::RouteCard:
                        select firstonly ProdId from prodJournalRoute
                            join prodTable
                            where prodJournalRoute.JournalId == prodJournalTable.JournalId &&
                                prodTable.ProdId == prodJournalRoute.ProdId &&
                                prodTable.ProjId;
                        if (prodJournalRoute.ProdId)
                            ret = NoYes::No;
                        break;
                }
                break;
            case tablenum(ProdTable):
                prodTable = _common;
                ret = ProdParametersDim::find(prodTable.InventDimId).PostExPostingType;
                if (prodTable.ProjId)
                    ret = NoYes::No;
                break;
            default:
                throw error("@SYS18626");
        }

        return ret;
    }

]]></Source>
