				<Name>validateField</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Validates the specified field
    /// </summary>
    /// <param name="_p1">
    ///    The field ID of the field to validate.
    /// </param>
    /// <returns>
    ///    true if the value in the specified field is valid; otherwise, false.
    /// </returns>
    public boolean validateField(FieldId _fieldId)
    {
        boolean ret = super(_fieldId);

        switch (_fieldId)
        {
            case fieldNum(ProdParametersDim, LimitCapProdPO):
            case fieldNum(ProdParametersDim, MatLimited):
                ReqParameters reqParameters = ReqParameters::find();
                
                if (!this.isMatLimitedFieldValidWithPlanningParameters(reqParameters)
                    || !this.isLimitCapProdPOFieldValidWithPlanningParameters(reqParameters))
                {
                    ret = checkFailed("@SCM:ReqParametersFormWarning");
                }
                break;
        }

        return ret;
    }

]]></Source>
