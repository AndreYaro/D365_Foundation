			<Method>
				<Name>validateInventDim</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Validates that the InventDim had no fields sent or that only the InventSiteId has been set.
    /// </summary>
    /// <returns>
    ///     true if the InventDim had no fields sent or that only the InventSiteId has been set; otherwise false.
    /// </returns>
    private boolean validateInventDim()
    {
        boolean ret = true;

        InventDim inventDim = InventDim::find(this.InventDimId);
            
        InventDim inventDimOnlySite = inventDim.data();
        inventDimOnlySite.clearNotSiteDim();

        if (!InventDim::isInventDimEqual(inventDim, inventDimOnlySite))
        {
            ret = checkFailed(strFmt("@SYS78966", fieldPName(ProdParametersDim, InventDimId)));
        }

        return ret;
    }

]]></Source>
