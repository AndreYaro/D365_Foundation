			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks the values of the <c>PhyValueUseCalcPrice</c> and <c>SpecifyCalc</c> fields.
    /// </summary>
    /// <returns>
    /// A Boolean value that indicates whether the values of the <c>PhyValueUseCalcPrice</c> and
    /// <c>SpecifyCalc</c> fields are correct.
    /// </returns>
    /// <remarks>
    /// This method returns false if the <c>PhyValueUseCalcPrice</c> field is true and the
    /// <c>SpecifyCalc</c> field is false.
    /// </remarks>
    public boolean validateWrite()
    {
        boolean ret;

        if (!this.RecId)
        {
            ret = ret && this.validateInventDim();
        }

        if (this.PhyValueUseCalcPrice && ! this.SpecifyCalc)
        {
            return checkFailed(strfmt("@SYS80269",fieldpname(ProdParametersDim,PhyValueUseCalcPrice),fieldpname(ProdParametersDim,SpecifyCalc)));
        }

        ret = super();

        return ret;
    }

]]></Source>
