			<Method>
				<Name>initValue</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes values from the company wide record, if such a record exists, or with default values.
    /// </summary>
    public void initValue()
    {
        ProdParametersDim parameter;

        super();

        //The ProdParametersDim::find method is not used here because it would result in an indefinite loop
        select firstonly parameter where parameter.InventDimId == InventDim::inventDimIdBlank();
        if (parameter)
        {
            //If any non-site specific records exists, then initialize from that
            buf2Buf(parameter,this);
        }
        else
        {
            //Set default values
            if (!this.InventDimId)
            {
                this.InventDimId                = InventDim::inventDimIdBlank();
            }

            this.MaxJobDays                     = 100;
            this.SpecifyCalc                    = NoYes::Yes;
            this.bomAutoConsumpStUp             = BOMAutoConsump::FlushingPrincip;
            this.RouteAutoConsumpStUp           = RouteAutoConsump::RouteDependent;
            this.bomAutoConsumpRepFin           = BOMAutoConsump::Never;
            this.RouteAutoConsumpRepFin         = RouteAutoConsump::Never;
            this.SchedMethod                    = ProdSchedMethod::OperationScheduling;
            this.LimitCapProdPO                 = NoYes::No;
            this.LimitCapProdProject            = NoYes::No;
        }
    }

]]></Source>
