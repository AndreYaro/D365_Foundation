			<Method>
				<Name>insert</Name>
				<Source><![CDATA[
    /// <summary>
    /// Inserts a record into the <c>ProdParametersDim</c> table.
    /// </summary>
    /// <remarks>
    /// When a record that has a non-default value for the <c>InventDimId</c> field is inserted, records
    /// are also inserted into the <c>WrkCtrParametersDim</c> table and the <c>ProdStatusParameters</c>
    /// table depending on the same <c>InventDimId</c> value.
    /// </remarks>
    public void insert()
    {
        ttsbegin;

        super();

        //Also insert WrkCtrParameters and ProdStatusParameters by inventDimId
        if (this.InventDimId != InventDim::inventDimIdBlank())
        {
            WrkCtrParametersDim::copyFromDefault(this.InventDimId);
            ProdStatusParameters::copyFromDefault(this.InventDimId);
        }
        else
        {
            WrkCtrParametersDim::findDefault();
            ProdStatusParameters::checkExistCreate();
        }

        ttscommit;
    }

]]></Source>
