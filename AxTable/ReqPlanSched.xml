			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    boolean validateWrite()
    {
        boolean ret;

        ReqPlan reqPlan;

        if (!this)
        {
            reqPlan = ReqPlan::findReqPlanId(this.ReqPlanIdSched,true);
            if (reqPlan && ReqPlan.PlanType != ReqPlanType::SchedPlan)
            {
                return checkFailed(strFmt("@SYS25859", this.ReqPlanIdSched, reqPlan.PlanType));
            }
        }

        ret = super();

        return ret;
    }

]]></Source>
