				<Name>checkTimeFenceLimit</Name>
				<Source><![CDATA[
    internal static boolean checkTimeFenceLimit(int _timeFence)    
    {
        const int TimeFenceLimit = 5000;

        if (_timeFence > TimeFenceLimit)
        {
            return checkFailed(strFmt("@SCM:TimeFenceExceedsLimitError", _timeFence, TimeFenceLimit));
        }

        return true;
    }

