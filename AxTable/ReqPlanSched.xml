			<Method>
				<Name>validateDelete</Name>
				<Source><![CDATA[
    public boolean validateDelete()
    {
        boolean                             result = super();
        ReqParameters                       reqParameters = ReqParameters::find();
        ReqIntercompanyPlanningGroupMember  intercompanyPlanningGroupMember;
        ReqIntercompanyPlanningGroup        intercompanyPlanningGroup;
        ReqIntercompanyMasterPlanMapping    intercompanyPlanMapping;

        if (reqParameters.CurrentReqPlanIdSchedStatic == this.ReqPlanIdSched)
        {
            result = checkFailed(strFmt("@SYS341107", this.ReqPlanIdSched));
        }
        if (reqParameters.CurrentReqPlanIdSchedDynamic == this.ReqPlanIdSched)
        {
            result = checkFailed(strFmt("@SYS341108", this.ReqPlanIdSched));
        }

        while select VendorPlanId, VendorDataAreaId from intercompanyPlanMapping
        where intercompanyPlanMapping.CustomerPlanId == this.ReqPlanIdSched
            && intercompanyPlanMapping.CustomerDataAreaId == curext()
        {
            result = checkFailed(strFmt("@SYS341109", this.ReqPlanIdSched, intercompanyPlanMapping.VendorPlanId, intercompanyPlanMapping.VendorDataAreaId));
        }

        while select crossCompany RecId from intercompanyPlanningGroupMember
        where intercompanyPlanningGroupMember.IntercompanyPlanId == this.ReqPlanIdSched
            && intercompanyPlanningGroupMember.IntercompanyPlanDataAreaId == curext()
        join Name from intercompanyPlanningGroup
        where intercompanyPlanningGroupMember.PlanningGroup == intercompanyPlanningGroup.RecId
        {
            result = checkFailed(strFmt("@SYS341110", this.ReqPlanIdSched, intercompanyPlanningGroup.Name));
        }

        return result;
    }

]]></Source>
