			<Method>
				<Name>validateFieldValue</Name>
				<Source><![CDATA[
    public boolean validateFieldValue(FieldName _fieldName, int _arrayIndex = 1)
    {        
        boolean ret;

        ret = super(_fieldName, _arrayIndex);

        switch (_fieldName)
        {
            case fieldStr(ReqPlanSched, IssueMargin):
                if (this.IssueMargin < 0)
                {
                    return checkFailed("@SYS53966");
                }

                break;
            case fieldStr(ReqPlanSched, ReceiptMargin):
                if (this.ReceiptMargin < 0)
                {
                    return checkFailed("@SYS53966");
                }

                break;
            case fieldStr(ReqPlanSched, OrderingMargin):
                if (this.OrderingMargin < 0)
                {
                    return checkFailed("@SYS53966");
                }

                break;

            case fieldStr(ReqPlanSched, TimeFenceAction),
                 fieldStr(ReqPlanSched, TimeFenceFutures),
                 fieldStr(ReqPlanSched, TimeFenceExplosion),
                 fieldStr(ReqPlanSched, TimeFenceCapacity),
                 fieldStr(ReqPlanSched, TimeFenceCov),
                 fieldStr(ReqPlanSched, TimeFenceLocking),
                 fieldStr(ReqPlanSched, TimeFenceMasterPlan),
                 fieldStr(ReqPlanSched, TimeFenceAuthorization),
                 fieldStr(ReqPlanSched, TimeFenceMaxNegativeDays):

                if (this.(fieldName2Id(this.TableId, _fieldName)) < 0)
                {
                    return checkFailed("@SYS53966");
                }

                if (ReqCheckFieldsTimeFenceLimitFlight::instance().isEnabled())
                {
                    ret = ret && ReqPlanSched::checkTimeFenceLimit(this.(fieldName2Id(this.TableId, _fieldName)));
                }

                break;    
        
            case fieldStr(ReqPlanSched, TimeFenceBackRequisition),
                 fieldStr(ReqPlanSched, TimeFenceFiniteCap),
                 fieldStr(ReqPlanSched, TimeFenceBottleneckCap):

                if (ReqCheckFieldsTimeFenceLimitFlight::instance().isEnabled())
                {
                    ret = ret && ReqPlanSched::checkTimeFenceLimit(this.(fieldName2Id(this.TableId, _fieldName)));
                }

                break;
        }

        return ret;
    }

]]></Source>
