			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    static ReqPlanSched find(ReqPlanIdSched _reqPlanSched, boolean _forUpdate = false)
    {
        ReqPlanSched reqPlanSched;

        reqPlanSched.selectForUpdate(_forUpdate);

        if (_reqPlanSched)
            select firstonly reqPlanSched
                index hint PlanIdx
                where reqPlanSched.ReqPlanIdSched == _reqPlanSched;
        return reqPlanSched;
    }

