				<Name>defaultDynamicId</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds and retrieves the default dynamic master plan ID.
    /// </summary>
    /// <returns>
    /// A string that contains the master plan ID.
    /// </returns>
    static ReqPlanIdSched defaultDynamicId()
    {
        ReqPlanId       reqPlanId;
        ReqParameters   reqParameters = ReqParameters::find();

        if (reqParameters.RecId)
        {
            reqPlanId = reqParameters.CurrentReqPlanIdSchedDynamic;
            if (! reqPlanId)
            {
                reqPlanId = reqParameters.CurrentReqPlanIdSchedStatic;
                if (! reqPlanId)
                {
                    reqPlanId = ReqPlan::createDefaultReqPlanSched();
                }
            }
        }

        return reqPlanId;
    }

]]></Source>
