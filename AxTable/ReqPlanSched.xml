				<Source><![CDATA[
    static boolean exist(ReqPlanIdSched reqPlanSched)
    {
        return reqPlanSched && (select firstonly reqPlanSched
                index hint PlanIdx
                where reqPlanSched.ReqPlanIdSched == reqPlanSched
                ).RecId != 0;
    }

