		<Methods>
			<Method>
				<Name>initCommercialRegisterFromTaxRegistration</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initilises CZ registration fields CommercialRegister* from registration ID's of party.
    /// </summary>
    /// <param name = "_dirPartyMap">The cursor of <c>DirPartyMap</c> type</param>
    /// <param name = "_date">Actual date of registration number</param>
    public void initCommercialRegisterFromTaxRegistration(DirPartyMap _dirPartyMap,
                                                          date        _date  = DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone()))
    {
        TaxRegistration taxRegistration = TaxRegistration::getPrimaryAddressTaxRegistration(_dirPartyMap, TaxRegistrationTypesList::CommercialRegisterCZ, _date);

        this.CompanyCommercialRegisterInsetNumber = taxRegistration.RegistrationNumber;
        this.CompanyCommercialRegister = taxRegistration.Issuer;
        this.CompanyCommercialRegisterSection = taxRegistration.Section;
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
