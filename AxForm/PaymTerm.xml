			<Control>
				<Name>Administration_DefaultPaymTerm_PSN</Name>
				<Type>CheckBox</Type>
				<Methods>
					<Method>
						<Name>modified</Name>
						<Source><![CDATA[
        /// <summary>
        /// Determines whether any other default payment already exists. if exists then ask the confirmation message from the user.
        /// </summary>
        /// <returns>
        /// True if default exists and user select yes or no default payment exists; otherwise, false.
        /// </returns>
        public boolean modified()
        {
            boolean isModified = true;
            PaymTerm paymTermLocal;
            DialogButton dialogButton;

            if (this.checked())
            {
                paymTermLocal = PaymTerm::getDefaultPaymTerm();

                if (paymTermLocal.RecId)
                {
                    dialogButton = Box::yesNo(strFmt("@SPS1766",paymTermLocal.PaymTermId, payment.PaymTermId), DialogButton::No, "@SYS6998");

                    if (dialogButton == DialogButton::No)
                    {
                        isModified = false;
                    }
                    else
                    {
                        PaymTerm::clearDefaultPaymTerm();
                    }
                }
            }
            else
            {
                dialogButton = Box::yesNo(strFmt("@SPS2342", payment.PaymTermId), DialogButton::No, "@SYS6998");

                if (dialogButton == DialogButton::No)
                {
                    isModified = false;
                }
            }

            isModified = isModified && super();

            isDefaultPaymentChange = isModified;

            return isModified;
        }

]]></Source>
