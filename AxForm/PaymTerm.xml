				<Source><![CDATA[
    /// <summary>
    /// Sets visible property for field <c>CutoffDay</c> according to the caller or menu item.
    /// </summary>
    private void updateDueDateUpdateControls()
    {
        payment_ds.object(fieldNum(PaymTerm, CutOffDay)).enabled(payment.PaymMethod == NetCurrent::CutOffDate);

        if (this.args().caller() is FormRun)
        {
            switch (this.args().caller().name())
            {
                case formStr(CustTable):
                case formStr(SalesTable):
                case formStr(CustFreeInvoice):
                    element.updateCustDueDateUpdateControl();
                    break;

                case formStr(VendTable):
                case formStr(PurchTable):
                case formStr(VendEditInvoice):
                    element.updateVendDueDateUpdateControl();
                    break;
            }
        }
        else if (this.args().menuItemType() == MenuItemType::Display)
        {
            if (this.args().menuItemName() == menuItemDisplayStr(CustPaymTerm))
            {
                element.updateCustDueDateUpdateControl();
            }
            else if (this.args().menuItemName() == menuItemDisplayStr(PaymTerm))
            {
                element.updateVendDueDateUpdateControl();
            }
        }
    }

]]></Source>
