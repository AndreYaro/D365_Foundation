				<Name>updateCustDueDateUpdateControl</Name>
				<Source><![CDATA[
    /// <summary>
    /// Sets visible property for field <c>CutoffDay_JP</c> according to sales parameter.
    /// </summary>
    private void updateCustDueDateUpdateControl()
    {
        payment_ds.object(fieldNum(PaymTerm, CustomerUpdateDueDate)).visible(true);

