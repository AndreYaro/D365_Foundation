        int active()
        {
            int ok = super();

            element.enableFields();

            if (PublicSectorUtils::isFrenchRegulatoryEnabled())
            {
                // since paymTerm table is entire table cache, so need to refresh the data if it has been change on server.
                if (isDefaultPaymentChange)
                {
                    payment_ds.executeQuery();
                    isDefaultPaymentChange = false;
                }
            }

            return ok;
        }

