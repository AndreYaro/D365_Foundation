			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        AccessRight accessRight;

        super();
        cashAccount.parmPostingType(LedgerPostingType::Liquidity);

        accessRight = FormSecurity::getMenuItemAccessRight(this);
        other.enabled(accessRight >= AccessRight::Edit);
        other.visible(element.args().parmEnumType() == enumNum(CustVendACType) && element.args().parmEnum() == CustVendACType::Cust);

        TabPage_PSN.visible(PublicSectorUtils::isFrenchRegulatoryEnabled());
    }

]]></Source>
