			<Method>
				<Name>enableFields</Name>
				<Source><![CDATA[
    /// <summary>
    ///Enables or disables window fields as changes are made to the PaymTerm window.
    /// </summary>
    void enableFields()
    {
        boolean isJapanRegion = SysCountryRegionCode::isLegalEntityInCountryRegion([#ISOJP]);

        if (payment.PaymMethod == NetCurrent::CutOffDate)
        {
            payment_ds.object(fieldNum(PaymTerm, CutOffDay)).mandatory(true);
            payment_ds.object(fieldNum(PaymTerm, AdditionalMonths)).mandatory(true);
        }
        else
        {
            payment_ds.object(fieldNum(PaymTerm, CutOffDay)).mandatory(false);
            payment_ds.object(fieldNum(PaymTerm, AdditionalMonths)).mandatory(false);
        }

        if (!isJapanRegion)
        {
            payment_ds.object(fieldNum(PaymTerm, NumOfDays)).enabled(payment.PaymMethod != NetCurrent::CutOffDate);
            payment_ds.object(fieldNum(PaymTerm, NumOfMonths)).enabled(payment.PaymMethod != NetCurrent::CutOffDate);
        }

        payment_ds.object(fieldNum(PaymTerm, CutOffDay)).enabled(payment.PaymMethod == NetCurrent::CutOffDate);

        if (isJapanRegion)
        {
            payment_ds.object(fieldNum(PaymTerm, AdditionalMonths)).enabled(false);
        }
        else
        {
            payment_ds.object(fieldNum(PaymTerm, AdditionalMonths)).enabled(payment.PaymMethod == NetCurrent::CutOffDate);
        }

        payment_ds.object(fieldnum(PaymTerm, Cash)).enabled(payment.PaymMethod == NetCurrent::COD);
        payment_ds.object(fieldnum(PaymTerm, CashLedgerDimension)).enabled(payment.Cash == NoYes::Yes);
        payment_ds.object(fieldnum(PaymTerm, PostOffsettingAR)).enabled(payment.PaymMethod == NetCurrent::COD);

        payment_ds.object(fieldNum(PaymTerm, VendorUpdateDueDate)).visible(false);
        payment_ds.object(fieldNum(PaymTerm, CustomerUpdateDueDate)).visible(false);
        element.updateDueDateUpdateControls();

        // If Payment Method is COD disable the Credit Card Payment Type field
        payment_ds.object(fieldNum(PaymTerm, CreditCardPaymentType)).enabled(payment.PaymMethod != NetCurrent::COD);

        payment_ds.object(fieldNum(PaymTerm, CreditCardCreditCheck)).enabled(payment.CreditCardPaymentType == CreditCardPaymentType::CreditCard);

        if (EmplParameters_RU::enableAdvanceHolders())
        {
            payment_ds.object(fieldnum(PaymTerm, UseEmplAccount_RU)).enabled(payment.PaymMethod == NetCurrent::COD);

            if (payment.UseEmplAccount_RU && payment.PaymMethod != NetCurrent::COD)
            {
                payment.UseEmplAccount_RU = NoYes::No;
            }
        }
    }

]]></Source>
