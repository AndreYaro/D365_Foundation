            public void lookup(FormControl _formControl, str _filterStr)
            {
                SysTableLookup sysTableLookup = SysTableLookup::newParameters(tableNum(RetailPOSTheme), _formControl, true);
                Query query = new Query();
                QueryBuildDataSource qbds;

                sysTableLookup.addLookupfield(fieldNum(RetailPOSTheme, Name));

                qbds = query.addDataSource(tableNum(RetailPOSTheme));

                qbds.addRange(fieldNum(RetailPOSTheme, DeviceType)).value(queryValue(RetailVisualProfile.DeviceType));

                sysTableLookup.parmQuery(query);

                sysTableLookup.performFormLookup();
            }

]]></Source>
