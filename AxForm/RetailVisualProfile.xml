			<Method>
				<Name>enableScreenHeightWidth</Name>
				<Source><![CDATA[
    public void enableScreenHeightWidth()
    {
        if (RetailVisualProfile.FullScreenMode)
        {
            Screen_ScreenHeight.enabled(false);
            RetailVisualProfile.ScreenHeight = 0;
            Screen_ScreenWidth.enabled(false);
            RetailVisualProfile.ScreenWidth = 0;
        }
        else
        {
            Screen_ScreenHeight.enabled(true);
            Screen_ScreenWidth.enabled(true);
        }
    }

]]></Source>
