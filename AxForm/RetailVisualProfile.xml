			<Method>
				<Name>setupDeviceType</Name>
				<Source><![CDATA[
    public void setupDeviceType()
    {
        boolean enable = false;
        RetailDeviceTypes retailDeviceType;

        retailDeviceType = RetailDeviceTypes::findRecId(RetailVisualProfile.DeviceType);

        if (retailDeviceType && retailDeviceType.DesignerForm == RetailDesignerFormEnum::RetailTillLayoutDesigner)
        {
            enable = true;
        }

        Screen_FullScreenMode.visible(enable);
        Screen_ScreenHeight.visible(enable);
        Screen_ScreenWidth.visible(enable);
        Screen_designAllowedOnPos.visible(enable);
        Screen_hideCursor.visible(enable);
        TabPage_Background.visible(!enable);
        Screen_Pallet.visible(!enable);
        Screen_HeaderPalette.visible(!enable);
    }

]]></Source>
			</Method>
			<Method>
				<Name>showImage</Name>
				<Source><![CDATA[
    public void showImage(FormWindowCOntrol _imageControl, int _imageId)
    {
        Image           itemImage;
        RetailImages    retailImages;

        retailImages.reread();

        try
        {
            retailImages = RetailImages::find(_imageId);
            if (retailImages.picture)
            {
                itemImage = new Image();
                itemImage.setData(retailImages.picture);
                _imageControl.image(itemImage);
                _imageControl.visible(true);
            }
            else
            {
                _imageControl.visible(false);
                _imageControl.imageResource(0);
                _imageControl.widthValue(32);
                _imageControl.heightValue(32);
            }
        }
        catch (EXCEPTION::Warning)
        {
            error(StrFmt("@SYS19312", retailImages));
        }
    }

]]></Source>
			</Method>
