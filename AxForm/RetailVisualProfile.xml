						<Source><![CDATA[
        public int active()
        {
            int ret;

            ret = super();

            // Show the background image (both landscape and portrait)
            element.showImage(LoginBackgroundImage, RetailVisualProfile.PictureId);
            element.showImage(LoginBackgroundImagePortrait, RetailVisualProfile.PictureIdPortrait);
            element.showImage(LoginBackgroundImageCompact, RetailVisualProfile.PictureIdCompact);
            element.showImage(LoginBackgroundImageCompactPortrait, RetailVisualProfile.PictureIdCompactPortrait);

            // Show the background image.
            element.showImage(BackgroundImage, RetailVisualProfile.BackgroundPictureId);

            // Show the logo image.
            element.showImage(LogoImage, RetailVisualProfile.PictureIdLogo);

            element.enableScreenHeightWidth();
            element.setupDeviceType();

            return ret;
        }

]]></Source>
