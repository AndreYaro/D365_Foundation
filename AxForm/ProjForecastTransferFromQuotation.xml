			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        ProjQuotationTransTypeFilter    filterProjTransType;
        Common                          common;
        ProjParameters                  projParameters  = ProjParameters::find();

        super();

        salesQuotationLine.setTmp();
        selectionChanged = true;

        imageListAppl = new ImageListAppl_Sales();

        if (element.args())
        {
            if (element.args().parmEnum())
            {
                filterProjTransType = element.args().parmEnum();
            }
            else
            {
                filterProjTransType = ProjQuotationTransTypeFilter::All;
            }

            if (element.args().parm() && filterProjTransType == ProjQuotationTransTypeFilter::Item)
            {
                itemReq = true;
            }

            if (element.args().record())
            {
                common = element.args().record();
            }
            else
            {
                if (element.args().caller())
                {
                    parentFrmRun = element.args().caller();

                    if (parentFrmRun.args() && parentFrmRun.args().record())
                    {
                        common = parentFrmRun.args().record();
                    }
                }
            }
        }

        if (common.RecId)
        {
            switch (common.TableId)
            {
                case (tableNum(ProjTable))          :
                    projTable           = common;
                    if (itemReq)
                    {
                        ctrlProjTransType.selection(enum2int(filterProjTransType));
                        ctrlProjTransType.visible(false);
                        element.initFromProjTransType(filterProjTransType, true);
                        element.design().caption("@SYS342070");
                        supplementalInstruction.text("@SYS342071");
                        forcastModelId.visible(false);
                    }
                    else
                    {
                        if (filterProjTransType == ProjQuotationTransTypeFilter::All)
                        {
                            ctrlProjTransType.enabled(true);
                            element.initFromProjTable();
                        }
                        else
                        {
                            element.initFromProjTransType(filterProjTransType);
                        }
                        element.design().caption("@SYS84639");
                    }
                    break;

                case (tableNum(ProjForecastEmpl))   :
                    projForecastEmpl    = element.args().record();
                    projTable           = ProjTable::find(projForecastEmpl.ProjId);
                    ctrlProjTransType.selection(enum2int(filterProjTransType));
                    element.initFromProjTransType(filterProjTransType);
                    element.design().caption("@SYS84640");
                    break;

                case (tableNum(ProjForecastCost))   :
                    projForecastCost    = element.args().record();
                    projTable           = ProjTable::find(projForecastCost.ProjId);
                    ctrlProjTransType.selection(enum2int(filterProjTransType));
                    element.initFromProjTransType(filterProjTransType);
                    element.design().caption("@SYS106236");
                    break;

                case (tableNum(ProjForecastOnAcc))   :
                    projForecastOnAcc   = element.args().record();
                    projTable           = ProjTable::find(projForecastOnAcc.ProjId);
                    ctrlProjTransType.selection(enum2int(filterProjTransType));
                    element.initFromProjTransType(filterProjTransType);
                    element.design().caption("@SYS84642");
                    break;

                case (tableNum(ProjForecastRevenue))   :
                    projForecastRevenue = element.args().record();
                    projTable           = ProjTable::find(projForecastRevenue.ProjId);
                    ctrlProjTransType.selection(enum2int(filterProjTransType));
                    element.initFromProjTransType(filterProjTransType);
                    element.design().caption("@SYS84643");
                    break;

                case (tableNum(ForecastSales))   :
                    forecastSales       = element.args().record();
                    projTable           = ProjTable::find(forecastSales.ProjId);
                    ctrlProjTransType.selection(enum2int(filterProjTransType));
                    element.initFromProjTransType(filterProjTransType,true);
                    element.design().caption("@SYS84644");
                    break;

                case (tableNum(SalesLine))   :
                    if (itemReq)
                    {
                        salesLine = element.args().record();
                        projTable = ProjTable::find(salesLine.ProjId);

                        ctrlProjTransType.selection(enum2int(filterProjTransType));
                        ctrlProjTransType.visible(false);
                        element.initFromProjTransType(filterProjTransType, true);
                        element.design().caption("@SYS342070");
                        supplementalInstruction.text("@SYS342071");
                        forcastModelId.visible(false);
                    }
                    break;
            }
        }
        else
        {
            throw error(strFmt("@SYS84645", element.name()));
        }

        forcastModelId.text(projParameters.ModelId);
    }

]]></Source>
