			<Method>
				<Name>initFromProjTable</Name>
				<Source><![CDATA[
    void initFromProjTable()
    {
        SalesQuotationTable     locSalesQuotationTable;
        SalesQuotationLine      locSalesQuotationLine;
        boolean                 recordsInserted     = false;

        while select locSalesQuotationLine
            order by QuotationId, ProjTransType
            where locSalesQuotationLine.Transferred2Forecast == NoYes::No
                exists join locSalesQuotationTable
                    where locSalesQuotationTable.QuotationId          == locSalesQuotationLine.QuotationId    &&
                          locSalesQuotationTable.ProjIdRef             == projTable.ProjId                  &&
                          locSalesQuotationTable.QuotationStatus       == SalesQuotationStatus::Confirmed   &&
                          locSalesQuotationTable.TransferredToForecast == NoYes::No
        {
            salesQuotationLine.data(locSalesQuotationLine.data());
            salesQuotationLine.doInsert();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initFromProjTransType</Name>
				<Source><![CDATA[
    public void initFromProjTransType(ProjQuotationTransTypeFilter   _projTransType, boolean _itemReq = false)
    {
        SalesQuotationTable     locSalesQuotationTable;
        SalesQuotationLine      locSalesQuotationLine;
        boolean                 recordsInserted     = false;
        QuotationProjTransType  filterProjTransType;

        locSalesQuotationLine.recordLevelSecurity(true);

        if (_projTransType == ProjQuotationTransTypeFilter::Item && _itemReq)
        {
           filterProjTransType = QuotationProjTransType::Item;

            while select locSalesQuotationLine
                order by QuotationId, ProjTransType
                where locSalesQuotationLine.ProjTransType           == filterProjTransType    &&
                      locSalesQuotationLine.Transferred2ItemReq     == NoYes::No
                exists join locSalesQuotationTable
                    where locSalesQuotationTable.QuotationId           == locSalesQuotationLine.QuotationId    &&
                          locSalesQuotationTable.ProjIdRef             == projTable.ProjId                  &&
                          locSalesQuotationTable.QuotationStatus       == SalesQuotationStatus::Confirmed   &&
                          locSalesQuotationTable.TransferredToItemReq  == NoYes::No
            {
                salesQuotationLine.data(locSalesQuotationLine.data());
                salesQuotationLine.doInsert();
            }
        }
        else
        {
            filterProjTransType = this.findQuotationProjTransType(_projTransType);

            while select locSalesQuotationLine
                order by QuotationId, ProjTransType
                where locSalesQuotationLine.ProjTransType        == filterProjTransType   &&
                      locSalesQuotationLine.Transferred2Forecast == NoYes::No
                exists join locSalesQuotationTable
                    where locSalesQuotationTable.QuotationId           == locSalesQuotationLine.QuotationId    &&
                          locSalesQuotationTable.ProjIdRef             == projTable.ProjId                  &&
                          locSalesQuotationTable.QuotationStatus       == SalesQuotationStatus::Confirmed   &&
                          locSalesQuotationTable.TransferredToForecast == NoYes::No
            {
                salesQuotationLine.data(locSalesQuotationLine.data());
                salesQuotationLine.doInsert();
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>findQuotationProjTransType</Name>
				<Source><![CDATA[
    public QuotationProjTransType findQuotationProjTransType(ProjQuotationTransTypeFilter _projTransType)
    {
        QuotationProjTransType filterProjTransType;

        switch (_projTransType)
        {
            case  ProjQuotationTransTypeFilter::Item:
                filterProjTransType = QuotationProjTransType::Item;
                break;
            case  ProjQuotationTransTypeFilter::Hour:
                filterProjTransType = QuotationProjTransType::Hour;
                break;
            case  ProjQuotationTransTypeFilter::Fee:
                filterProjTransType = QuotationProjTransType::Fee;
                break;
            case  ProjQuotationTransTypeFilter::Expense:
                filterProjTransType = QuotationProjTransType::Expense;
                break;
            default:
                filterProjTransType = QuotationProjTransType::None;
        }

        return filterProjTransType;
    }

]]></Source>
