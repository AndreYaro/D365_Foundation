			<Method>
				<Name>prepareTransfer</Name>
				<Source><![CDATA[
    void prepareTransfer()
    {
        SalesQuotationLine  localSalesQuotationLine;
        RecordSortedList    recordSortedList;

        recordSortedList = new RecordSortedList(tableNum(SalesQuotationLine));
        recordSortedList.sortOrder(fieldNum(SalesQuotationLine, RecId));

        for (localSalesQuotationLine = salesQuotationLine_ds.getFirst(true); localSalesQuotationLine; localSalesQuotationLine = salesQuotationLine_ds.getNext())
        {
            recordSortedList.ins(localSalesQuotationLine);
        }

        while select salesQuotationLine
        {
            if (!recordSortedList.find(salesQuotationLine))
            {
                salesQuotationLine.doDelete();
            }
        }
    }

]]></Source>
