			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    boolean canClose()
    {
        ProjForecastTransferFromQuotation   projForecastTransferFromQuotation = new ProjForecastTransferFromQuotation();
        boolean                             canClose;

        canClose = super();

        if (element.closedOk() && canClose)
        {
            element.prepareTransfer();

            projForecastTransferFromQuotation.initParameters(salesQuotationLine,
                                                             any2Enum(ctrlProjTransType.selection()),
                                                             itemReq);
            projForecastTransferFromQuotation.transfer();

            if (element.args().caller())
            {
                callerObject = element.args().caller();
                switch (callerObject.name())
                {
                    case formStr(ProjForecastEmpl) :
                        callerObject.doReFreshDatasource();
                        break;

                    case formStr(ProjForecastCost) :
                        callerObject.doReFreshDatasource();
                        break;

                    case formStr(ProjForecastRevenue) :
                        callerObject.doReFreshDatasource();
                        break;

                    case formStr(ForecastSales) :
                        callerObject.doReFreshDatasource();
                        break;

                    case formStr(ProjSalesItemReq) :
                        callerObject.doReFreshDatasource();
                        break;
                }
            }
        }

        return canClose;
    }

]]></Source>
