				<Methods>
					<Method>
						<Name>lookup</Name>
						<Source><![CDATA[
        public void lookup()
        {
            SysTableLookup          sysTableLookup;
            Query                   query;
            QueryBuildDataSource    qbd;

            sysTableLookup = SysTableLookup::newParameters(tablenum(JmgRegistrationSetup),this);

            query = new Query();
            qbd = query.addDataSource(tablenum(JmgRegistrationSetup));
            qbd.addRange(fieldNum(JmgRegistrationSetup, EnableDevice)).value(queryValue(NoYes::No));

            sysTableLookup.addLookupfield(fieldnum(JmgRegistrationSetup,SetupId));
            sysTableLookup.addLookupfield(fieldnum(JmgRegistrationSetup,SignInStartPage));

            sysTableLookup.parmQuery(query);
            sysTableLookup.performFormLookup();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>OKButton</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            super();

            JmgRegistrationSetup clientJmgRegistrationSetup = JmgRegistrationSetup::find(RegistrationSetupId.valueStr());

            if (clientJmgRegistrationSetup.RecId != 0)
            {
                registrationForm.parmJmgRegistrationUserSetup(clientJmgRegistrationSetup);
            }
            else
            {
                throw error(strfmt("@SYS26332", "@SYS26677"));
            }
        }

]]></Source>
