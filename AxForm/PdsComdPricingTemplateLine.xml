				<Source><![CDATA[
    public void init()
    {
        if (!this.args() || !this.args().caller())
        {
            throw error(strfmt("@SYS22862", element.name()));
        }

        switch (this.args().dataset())
        {
            case tablenum(PdsComdPricingTemplate):
                pdsComdPricingTemplate = this.args().record();
                break;

            default:
                throw error(strfmt("@PDS421", element.name(),
                    tablestr(PdsComdPricingTemplate)));
        }

        super();

        element.updateDesign(InventDimFormDesignUpdate::Init);
    }

]]></Source>
