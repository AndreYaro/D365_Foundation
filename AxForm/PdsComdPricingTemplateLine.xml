			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    public void updateDesign(InventDimFormDesignUpdate _mode)
    {
        InventDimParm       inventDimParmShow;
        InventDimParm       inventDimParmEnabled;

        pdsComdPricingTemplateLine_ds.enableField(
            fieldnum(PdsComdPricingTemplateLine, MarginItemRelation));
        pdsComdPricingTemplateLine_ds.enableField(
            fieldnum(PdsComdPricingTemplateLine, MarginAccountRelation));

        if (pdsComdPricingTemplateLine.MarginItemCode == PriceDiscProductCodeType::All)
        {
            pdsComdPricingTemplateLine_ds.enableField(
                fieldnum(PdsComdPricingTemplateLine, MarginItemRelation), false);
            pdsComdPricingTemplateLine.MarginItemRelation = "";
        }

        if (pdsComdPricingTemplateLine.MarginAccountCode == PriceDiscPartyCodeType::All)
        {
            pdsComdPricingTemplateLine_ds.enableField(
                fieldnum(PdsComdPricingTemplateLine, MarginAccountRelation),
                false);
            pdsComdPricingTemplateLine.MarginAccountRelation = "";
        }

        switch (_mode)
        {
            case InventDimFormDesignUpdate::Init:
                this.inventDimSetupObject();

                InventDimParmEnabled.clear();

                inventDimParmEnabled.InventBatchIdFlag = true;
                inventDimParmEnabled.WMSLocationIdFlag = true;
                inventDimParmEnabled.InventSerialIdFlag = true;
                inventDimFormSetup.parmDimParmLockedRightClick(inventDimParmEnabled);

                inventDimParmShow.initProductDimensionsAllGroups();
                inventDimParmShow.InventSiteIdFlag = true;

                inventDimParmShow.InventBatchIdFlag  = false;
                inventDimParmShow.WMSLocationIdFlag  = false;
                inventDimParmShow.InventSerialIdFlag = false;

                inventDimFormSetup.parmDimParmVisibleGrid(inventDimParmShow);
                //fall through
            case InventDimFormDesignUpdate::Active:

            case InventDimFormDesignUpdate::FieldChange:
                if (pdsComdPricingTemplateLine.MarginItemCode == PriceDiscProductCodeType::Table)
                {
                    inventDimFormSetup.formActiveSetup(InventDimGroupSetup::newItemId(pdsComdPricingTemplateLine.MarginItemRelation));
                }
                else
                {
                    inventDimFormSetup.dimFieldsActive(inventDimParmEnabled);
                }
                inventDimFormSetup.formSetControls(true);
                break;

            default:
                throw error(strfmt("@SYS54195", funcname()));
        }
    }

]]></Source>
