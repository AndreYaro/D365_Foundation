					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            CustPackingSlipVersion  custPackingSlipVersion;
            CustPackingSlipJour     custPackingSlipJour;

            super();

            if (element.args().record() && element.args().record().TableId == tableNum(custPackingSlipVersion))
            {
                custPackingSlipVersion = element.args().record();
                custPackingSlipJour = CustPackingSlipJour::findRecId(custPackingSlipVersion.CustPackingSlipJour);

                this.query().validTimeStateAsOfDateTime(custPackingSlipVersion.VersionDateTime);

                this.query().dataSourceTable(tableNum(SalesShippingStat)).clearDynalinks();
                this.query().dataSourceTable(tableNum(SalesShippingStat)).clearRanges();
                this.query().dataSourceTable(tableNum(SalesShippingStat)).addRange(fieldnum(SalesShippingStat, PackingSlipId)).value(custPackingSlipJour.PackingSlipId);
                this.query().dataSourceTable(tableNum(SalesShippingStat)).addRange(fieldnum(SalesShippingStat, DeliveryDate)).value(queryValue(custPackingSlipJour.DeliveryDate));
                this.query().dataSourceTable(tableNum(SalesShippingStat)).addRange(fieldnum(SalesShippingStat, SalesId)).value(custPackingSlipJour.SalesId);
            }
        }

]]></Source>
					</Method>
