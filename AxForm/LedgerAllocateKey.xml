						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            DialogField dialogFromKey;
            Dialog dialog = new Dialog("@SYS12898");

            dialogFromKey = dialog.addField(extendedTypeStr(LedgerAllocateKeyId), "@SYS3387");

            super();

            if (!dialog.run())
            {
                return;
            }

            LedgerAllocateTrans::copyKey(ledgerAllocateKey.Key, dialogFromKey.value());

            ledgerAllocateKey_ds.active();
        }

]]></Source>
