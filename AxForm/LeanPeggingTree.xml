[Form]
public class LeanPeggingTree extends FormRun
{
    LeanPeggingTreeFormHelper   formHelper;
    FormButtonManager           formButtonManager;

    QueryBuildRange             qbrKanban;
    QueryBuildRange             qbrInventDim;

    List                        collectedItemsWithNoChildren;

}
]]></Source>
			</Method>
			<Method>
				<Name>expandSubTree</Name>
				<Source><![CDATA[
    private void expandSubTree(int _idx)
    {
        int child;

        // Expand the current node
        treePegging.expand(_idx, FormTreeExpand::Expand);

        // Explode the children recursively
        child = treePegging.getChild(_idx);
        while (child)
        {
            this.expandSubTree(child);
            child = treePegging.getNextSibling(child);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        List list = new List(Types::Record);
        xArgs args = this.args();

        // check input parameters
        if (!args ||
            !args.record())
        {
            throw error(Error::missingFormActiveBuffer(formStr(LeanPeggingTree)));
        }

        super();

        // initialize the helper class
        list.addEnd(args.record());
        formHelper = LeanPeggingTreeFormHelper::newStandard(this, treePegging, list);

        formHelper.init();

        collectedItemsWithNoChildren = new List(Types::Class);
        this.expandSubTree(treePegging.getRoot());

        //since items without children are 'artificially' expanded, they must be updated in the tree
        //in order to remove the expand/collapse button
        ListEnumerator listEnumerator = collectedItemsWithNoChildren.getEnumerator();
        while (listEnumerator.moveNext())
        {
            FormTreeItem item = listEnumerator.current();
            item.children(0);
            treePegging.setItem(item);
        }
        collectedItemsWithNoChildren = null;

        // initialize the button manager
        this.initializeButtonManager();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initializeButtonManager</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes the rules-to-buttons mapping for the form button manager.
    /// </summary>
    private void initializeButtonManager()
    {
        formButtonManager = FormButtonManager::construct();
        formButtonManager.parmRuleImplementer(formHelper);
        formButtonManager.setDatasources(reqPeggingTreeNodeDetails, relatedKanban, inventDim);

        // register action pane buttons
        formButtonManager.registerButtonRule(ActionGroup, btnKanban, methodStr(LeanPeggingTreeFormHelper, ruleCheckIsKanbanAvailable));
        formButtonManager.registerButtonRule(ActionGroup, btnKanbanRule, methodStr(LeanPeggingTreeFormHelper, ruleCheckIsKanbanAvailable));
        formButtonManager.registerButtonRule(ActionGroup, btnPlanPeggingTree, methodStr(LeanPeggingTreeFormHelper, ruleCheckCanPlanSubTree));
    }

]]></Source>
			</Method>
			<Method>
				<Name>refreshDependentDatasources</Name>
				<Source><![CDATA[
    public void refreshDependentDatasources()
    {
        qbrKanban.value(SysQuery::value(reqPeggingTreeNodeDetails.Kanban));
        relatedKanban_ds.executeQuery();
        kanbanJob_ds.executeQuery();

        qbrInventDim.value(SysQuery::value(reqPeggingTreeNodeDetails.InventDimId));
        inventDim_ds.executeQuery();

        reqPeggingTreeNodeDetails_ds.refresh();
    }

]]></Source>
