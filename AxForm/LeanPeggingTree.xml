				<Methods>
					<Method>
						<Name>expanding</Name>
						<Source><![CDATA[
        public boolean expanding(int _idx, FormTreeExpand _action, anytype _data)
        {
            boolean ret;

            formHelper.onTreeNodeExpanding(_idx, _action, _data);

            ret = super(_idx, _action, _data);

            if (collectedItemsWithNoChildren)
            {
                FormTreeItem item = treePegging.getItem(_idx);
                if (!item.children())
                {
                    collectedItemsWithNoChildren.addEnd(item);
                }
            }

            return ret;
        }

]]></Source>
