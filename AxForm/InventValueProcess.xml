					<Method>
						<Name>getAverageUnitCost</Name>
						<Source><![CDATA[
        /// <summary>
        /// Get Average unit cost
        /// </summary>
        display CostAmount getAverageUnitCost(InventValueReportTmpLine _inventValueReportTmpLine)
        {
            real result = 0;
            
            if (inventValueExecutionHistory.CalcAvgUnitCost
                && (inventValueExecutionHistory.DetailLevel == TotalTrans::Total
                    || element.forceShowAverageUnitCostOnTransDetailLevel()))
            {
                InventQty inventQty = this.getInventoryQtyTotal(_inventValueReportTmpLine);
                CostAmount costAmount = this.getInventoryAmountTotal(_inventValueReportTmpLine);
                result = (inventQty == 0 ? 0 : costAmount / inventQty);
            }

            return result;
        }

]]></Source>
					</Method>
					<Method>
						<Name>getInventoryQtyTotal</Name>
						<Source><![CDATA[
        /// <summary>
        /// Get Inventory Quantity Total
        /// </summary>
        display InventQty getInventoryQtyTotal(InventValueReportTmpLine _inventValueReportTmpLine)
        {
            return _inventValueReportTmpLine.InventoryFinancialQty + _inventValueReportTmpLine.InventoryPhysicalPostedQty
                + (inventValueExecutionHistory.IncludeNotPostedValue ? _inventValueReportTmpLine.InventoryPhysicalNonPostedQty : 0);
        }

]]></Source>
