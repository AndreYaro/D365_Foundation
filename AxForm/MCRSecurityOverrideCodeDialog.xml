			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if the entered security code is matching the valid one when the dialog is closed.
    /// </summary>
    public void close()
    {
        if (!element.closedOk())
        {
            // Indicates return payment should not be overridden.
            element.args().parm('');
        }
        else
        {
            element.args().parm(SecurityOverrideCode.text());
        }
        super();
    }

]]></Source>
