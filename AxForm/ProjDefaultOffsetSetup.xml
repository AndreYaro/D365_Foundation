				<Methods>
					<Method>
						<Name>lookup</Name>
						<Source><![CDATA[
        public void lookup()
        {
            switch (projDefaultOffsetSetup.ProjOffsetType)
            {
                case ProjOffsetType::Bank:
                    BankAccountTable::lookupBankAccount(this);
                    break;

                case ProjOffsetType::Cust:
                    CustTable::lookupCustomer(this);
                    break;

                case ProjOffsetType::Ledger:
                    super();
                    break;

                case ProjOffsetType::Vend:
                    VendTable::lookupVendor(this);
                    break;

                default:
                    super();
                    break;
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>checkUseCustomLookup</Name>
						<Source><![CDATA[
        public boolean checkUseCustomLookup(int _accountTypeEnumValue, int _secondaryAccountTypeEnumValue)
        {
            boolean ret = false;

            switch (_accountTypeEnumValue)
            {
                case ProjOffsetType::Bank:
                case ProjOffsetType::Cust:
                case ProjOffsetType::Vend:
                    ret = true;
                    break;
                case ProjOffsetType::Ledger:
                default:
                    ret = false;
            }

            return ret;
        }

]]></Source>
