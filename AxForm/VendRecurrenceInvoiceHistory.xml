}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        Name vendTableName;
        VendTable vendTable;
        VendInvoiceTemplate vendInvoiceTemplate;

        switch (element.args().dataset())
        {
            case tableNum(VendRecurrenceInvoice) :
                vendRecurrenceInvoice = element.args().record();
                vendTable = VendTable::find(vendRecurrenceInvoice.VendTable);
                vendTableName = DirPartyTable::findRec(vendTable.Party).Name;
                vendInvoiceTemplate = VendInvoiceTemplate::find(vendRecurrenceInvoice.VendInvoiceTemplate);
                break;

            default:
                throw error("@SYS18626");
        }

        super();
    }

]]></Source>
