						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            vendorRecurrenceInvoice_ds.cacheAddMethod(tableMethodStr(VendorRecurrenceInvoice, displayVendRecurrenceInvoiceAmount));
            vendorRecurrenceInvoice_ds.cacheAddMethod(tableMethodStr(VendorRecurrenceInvoice, displayVendRecurrenceInvoiceTax));
            vendorRecurrenceInvoice_ds.cacheAddMethod(tableMethodStr(VendorRecurrenceInvoice, displayVendRecurrenceMiscChargesAmount));
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            vendorRecurrenceInvoice_ds.query().dataSourceName(vendorRecurrenceInvoice_ds.name()).addRange(
                fieldNum(VendorRecurrenceInvoice, VendRecurrenceInvoice)).value(SysQuery::value(vendRecurrenceInvoice.RecId));

            super();

            if (!vendorRecurrenceInvoice)
            {
                VendEditInvoice.enabled(false);
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>displayRecurrenceInvoiceStatus</Name>
						<Source><![CDATA[
        public display str displayRecurrenceInvoiceStatus(VendorRecurrenceInvoice _vendRecurrenceInvoice)
        {
            str recurrenceInvoiceStatus = "@SYS75939";
            VendInvoiceInfoTable vendInvoiceInfoTableLocal = VendInvoiceInfoTable::findRecId(_vendRecurrenceInvoice.VendInvoiceInfoTable);

            if (vendInvoiceInfoTableLocal.RecId && vendInvoiceInfoTableLocal.ParmJobStatus == ParmJobStatus::Executed)
            {
                recurrenceInvoiceStatus = "@SYS22361";
            }

            return recurrenceInvoiceStatus;
        }

]]></Source>
