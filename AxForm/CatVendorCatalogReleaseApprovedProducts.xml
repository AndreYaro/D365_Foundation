	<SourceCode>
		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class CatVendorCatalogReleaseApprovedProducts extends FormRun
{
    Set         companiesSet;
    boolean     releaseApprovedProducts;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        CatVendorCatalog    vendorCatalog;
        super();
        if (! element.args().caller())
        {
            throw error("@SYS96590");
        }
        companiesSet = new Set(Types::String);
        releaseApprovedProducts = this.isReleaseApprovedProducts();
        vendorCatalog = element.args().record();
        CatVendorReleaseCatalog::updateCatalog(vendorCatalog); //Update the release catalog with newly created or updated catalog details.
    }

]]></Source>
			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        CatVendorCatalogProductReleaseManager catVendorCatalogProductReleaseManager;
        super();

        if (companiesSet.elements() > 0 && releaseApprovedProducts)
        {
            catVendorCatalogProductReleaseManager = new CatVendorCatalogProductReleaseManager();
            catVendorCatalogProductReleaseManager.releaseApprovedProductsToCompanies(catVendorCatalog.RecId,companiesSet);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>isReleaseApprovedProducts</Name>
				<Source><![CDATA[
    private boolean isReleaseApprovedProducts()
    {
        Object      myCaller;
        FormRun     callerForm;
        boolean     releaseProducts = false;

        if (element.args())
        {
            myCaller = element.args().caller();
            if (myCaller && myCaller is FormRun)
            {
                callerForm = myCaller;
                if (callerForm.name() == formstr(CatVendorCatalogListPage)
                    || callerForm.name() == formstr(CatVendorCatalogMaintenanceRequest))
                {
                    releaseProducts = true;
                }
            }
        }
        return releaseProducts;
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        str formCaption = releaseApprovedProducts? "@SYS320644" : "@SYS328932";

        this.design().caption(formCaption);
        chkBoxRelease.visible(releaseApprovedProducts);
        catVendorReleaseCatalog_Release.visible(!releaseApprovedProducts);

        super();
    }

]]></Source>
