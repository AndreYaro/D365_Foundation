						<Name>delete</Name>
						<Source><![CDATA[
        public void delete()
        {
            RTax25ProfitId      nextProfitId;
            RTax25ProfitTable   profitTable;

            if (rTax25ProfitTable.RecId)
            {
                profitTable = rTax25ProfitTable_ds.cursor();

                rTax25ProfitTable_ds.next();
                nextProfitId = profitTable.ProfitId;
                rTax25ProfitTable_ds.prev();

                if (! nextProfitId)
                {
                    rTax25ProfitTable_ds.prev();
                    nextProfitId = profitTable.ProfitId;
                    rTax25ProfitTable_ds.next();
                }

                super();

                rTax25ProfitTable_ds.research();
                rTax25ProfitTable_ds.positionToRecordByValue(fieldnum(RTax25ProfitTable, ProfitId), nextProfitId);
            }
            else
            {
                super();
            }
        }

]]></Source>
