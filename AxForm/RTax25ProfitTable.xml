					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int             ret;
            boolean         existChild = rTax25ProfitTable.existChild();

            ret = super();

            buttonRTax25LedgerParameter.enabled(! existChild);

            rTax25ProfitTable_ds.object(fieldnum(RTax25ProfitTable, ProfitType)).allowEdit((rTax25ProfitTable.ParentProfitRecId ? false : true));

            listBuilt = false;

            if (tabIncludeInTotals.isExpanded())
            {
                element.buildLists(true);
            }

            if (treeView && profitTree)
            {
                treeProfit.select(profitTree.lookupProfitId(rTax25ProfitTable.ProfitId));
            }

            element.enableButtons();

            return ret;
        }

]]></Source>
