			<Method>
				<Name>removeSelected</Name>
				<Source><![CDATA[
    void removeSelected()
    {
        FormListItem            formListItem;
        int                     idx = selRegisters.getNextItem(FormListNext::Selected);

        ttsbegin;

        while (idx != -1)
        {
            formListItem            = selRegisters.getItem(idx);

            if (! formListItem)
            {
                ttscommit;
                return;
            }

            RTax25ProfitTotalsSetup::deleteRegisterProfitSetup(formListItem.data(),rTax25ProfitTable.RecId);
            idx = selRegisters.getNextItem(FormListNext::Selected, idx);
        }

        ttscommit;

        element.buildLists(true);
    }

]]></Source>
