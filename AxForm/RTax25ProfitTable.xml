			<Method>
				<Name>setTreeView</Name>
				<Source><![CDATA[
    public void setTreeView(boolean _treeView, boolean forceChangeView)
    {
        if (forceChangeView || treeView != _treeView)
        {
            rTax25ProfitTable_DS.allowCreate(! _treeView);
            rTax25ProfitTable_DS.allowDelete(! _treeView);

            tabTree.visible(_treeView);
            tabOverview.visible(!_treeView);

            if (_treeView)
            {
                profitTree.buildTree();
                treeProfit.setFocus();

                const int TabTreeConst = 1;
                tabProfitTable.tab(TabTreeConst);
            }
            else
            {
                const int TabOverviewConst = 2;
                tabProfitTable.tab(TabOverviewConst);
            }

            profitTree.parmTreeVisible(_treeView);
            ButtonTreeGrid.text((_treeView) ? "@SYS95914" : "@SYS89309");
            treeView = _treeView;
        }
    }

]]></Source>
