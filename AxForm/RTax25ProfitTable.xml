			<Method>
				<Name>buildLists</Name>
				<Source><![CDATA[
    void buildLists(boolean _rebuild = false)
    {
        FormListItem            formListItem;
        container               selected;
        int                     idx;
        RTax25ProfitTotalsSetup setup;
        RTax25RegisterTable     registers;

        if (listBuilt && !_rebuild)
        {
            return;
        }

        selRegisters.deleteAll();
        allRegisters.deleteAll();

        if (rTax25ProfitTable.existChild())
        {
            selRegisters.enabled(false);
            allRegisters.enabled(false);
        }
        else
        {
            selRegisters.enabled(true);
            allRegisters.enabled(true);

            while select RegisterId from setup
                where setup.rTax25ProfitTable == rTax25ProfitTable.RecId
            {
                selected += setup.RegisterId;
                formListItem = new FormListItem(setup.RegisterId,0,setup.RegisterId);
                idx = selRegisters.addItem(formListItem);
                formListItem = new FormListItem(RTax25RegisterTable::findRegisterId(setup.RegisterId).RegisterName);
                formListItem.subItem(1);
                formListItem.idx(idx);
                selRegisters.setItem(formListItem);
            }

            while select registers
            {
                if (confind(selected, registers.RegisterId))
                {
                    continue;
                }

                formListItem = new FormListItem(registers.RegisterId,0,registers.RegisterId);
                idx = allRegisters.addItem(formListItem);
                formListItem = new FormListItem(registers.RegisterName);
                formListItem.subItem(1);
                formListItem.idx(idx);
                allRegisters.setItem(formListItem);
            }
        }

        listBuilt = true;
    }

]]></Source>
