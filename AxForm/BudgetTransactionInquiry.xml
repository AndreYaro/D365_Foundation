			<Method>
				<Name>hideNegativeDRCRController_CN</Name>
				<Source><![CDATA[
    /// <summary>
    /// Hide the DR and CR fields in form <c>BudgetTransactionInquiry</c>
    /// </summary>
    private void hideNegativeDRCRController_CN()
    {
        BudgetTransactionLine_AmountCurCredit.visible(false);
        BudgetTransactionLine_AmountCredit.visible(false);
        BudgetTransactionLine_AmountCurDebit.visible(false);
        BudgetTransactionLine_AmountDebit.visible(false);
    }

]]></Source>
