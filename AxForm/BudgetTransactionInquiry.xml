			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        #SysSystemDefinedButtons

        FormCommandButtonControl showFiltersButton;

        boolean executeQuery = false;

        if (element.args().parmObject() &&
            element.args().parmObject() is BudgetTransactionInquiryRequest)
        {
            transactionInquiryRequest = element.args().parmObject();
            executeQuery = true;
        }
        else
        {
            // Construct a request from the passed record.
            // If the record is null a default request to show all transactions is constructed.
            common = element.args().record();
            transactionInquiryRequest = BudgetTransactionInquiryRequest::construct(common);

            if (common)
            {
                executeQuery = true;
            }
        }

        enableShowDRsCRs = LedgerShowDebCRedController_CN::isShowDRsCRsFeatureEnabled();

        super();

        // Link the combination name temp table data source to the budget transaction line data source.
        DimensionCombinationNameTmp_Q.dataSourceTable(tableNum(DimensionCombinationNameTmp))
            .addLink(fieldNum(BudgetTransactionLine, LedgerDimension),
                fieldNum(DimensionCombinationNameTmp, DimensionAttributeValueCombination));

        // Inform the DimensionCombinationPreviewPart of the location of the dimension foreign key field.
        DimensionCombinationNameTmp_Name.userDataItem(
            fieldNum(DimensionCombinationNameTmp, DimensionAttributeValueCombination));

        // Hide Filter pane button since parameter fasttab filters are unbound and cannot be displayed in Filter pane
        showFiltersButton = this.control(this.controlId(#SystemDefinedShowFiltersButton)) as FormCommandButtonControl;
        showFiltersButton.visible(false);

        if (executeQuery)
        {
            dimensionCombinationNameTmp.linkPhysicalTableInstance(
                transactionInquiryRequest.populateDimensionCombinationNameTmp());

            BudgetTransactionLine_DS.executeQuery();
        }

        // <GCN>
        if (!enableShowDRsCRs)
        {
            this.hideNegativeDRCRController_CN();
        }
        // </GCN>

        IncludeSubModels.checked(transactionInquiryRequest.parmIncludeSubModels());
        BudgetModelId.text(transactionInquiryRequest.parmBudgetModelId());
        BudgetModelId.modified();

        BudgetTransferSourceNumber.text(transactionInquiryRequest.parmTransferSourceNumber());
        AllDraftCompleted.selection(transactionInquiryRequest.parmTransactionStatus());
        BudgetTransactionType.selection(transactionInquiryRequest.parmTransactionType());
        StartDate.dateValue(transactionInquiryRequest.parmStartDate());
        EndDate.dateValue(transactionInquiryRequest.parmEndDate());

        this.hideFields();
    }

]]></Source>
