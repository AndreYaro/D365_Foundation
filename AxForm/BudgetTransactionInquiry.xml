						<Source><![CDATA[
        /// <summary>
        /// Adds the edit and display methods to the form cache.
        /// </summary>
        public void init()
        {
            super();

            // Add the request ranges for the line to the query.
            transactionInquiryRequest.addLineRangesToQuery(this.query());

            // caching the call to editTransactionCurrencyAmount() in BudgetTransactionLine table
            budgetTransactionLine_ds.cacheAddMethod(tableMethodStr(BudgetTransactionLine, editTransactionCurrencyAmount));

            // caching the call to displayAccountingAmount() in BudgetTransactionLine table
            budgetTransactionLine_ds.cacheAddMethod(tableMethodStr(BudgetTransactionLine, displayAccountingAmount));

            // <GCN>
            if (enableShowDRsCRs)
            {
                showDRsCRsController = LedgerShowDebCredController_CN::construct(budgetTransactionLine.TableId);
                showDRsCRsController.addAmountControl(budgetTransactionLine_TransactionCurrencyAmount);
                showDRsCRsController.addAmountControl(budgetTransactionLine_AccountingAmount);
                showDRsCRsController.addAmountCreditControl(budgetTransactionLine_AmountCurCredit);
                showDRsCRsController.addAmountCreditControl(budgetTransactionLine_AmountCredit);
                showDRsCRsController.addAmountDebitControl(budgetTransactionLine_AmountCurDebit);
                showDRsCRsController.addAmountDebitControl(budgetTransactionLine_AmountDebit);

                showDRsCRsController.initControls();
            }
            // </GCN>
        }

