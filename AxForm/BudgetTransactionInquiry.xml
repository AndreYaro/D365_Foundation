    {
        budgetTransactionHeader_BudgetTransactionCode.visible(transactionInquiryRequest.isTransactionCodeVisible());
        budgetTransactionHeader_TransferSourceNumber.visible(transactionInquiryRequest.isTransferSourceNumberVisible());
    }

]]></Source>
