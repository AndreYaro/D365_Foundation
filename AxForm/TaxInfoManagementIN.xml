				<Methods>
					<Method>
						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            boolean             ret;
            TaxInformation_IN   taxInfo, taxInfoLoc;

            TaxInfo = TaxInformation_IN_ds.cursor();

            if (this.value())
            {
                taxInfoLoc = TaxInformation_IN::findDefaultByLocation(TaxInformation_IN.RegistrationLocation);

                // If Primary already exist
                if (taxInfoLoc && taxinfoloc.recid != taxInfo.RecId)
                {
                    if (Box::yesNo("@GLS221443", DialogButton::No) == DialogButton::Yes) //This will unmark the current primary tax information. Do you want to continue ?
                    {
                        ttsBegin;
                        taxInfoLoc.selectForUpdate(true);
                        taxInfoLoc.IsPrimary = NoYes::No;
                        taxInfo.IsPrimary    = NoYes::Yes;
                        taxInfoLoc.update();
                        ttsCommit;

                        TaxInformation_IN_ds.setRecord(taxInfoLoc);
                    }
                }
                if (!taxInfoLoc)
                {
                    ret = super();
                }
            }
            else
            {
                ret = false;
            }
            return ret;
        }

]]></Source>
