				<Name>updateControlsForIndia</Name>
				<Source><![CDATA[
    private void updateControlsForIndia()
    {
        #ISOCountryRegionCodes

        if (SysCountryRegionCode::isLegalEntityInCountryRegion([#isoIN]))
        {
            // Note: CountryRegionCodes in Form fields does not work on a global table datasource, thus need set visible directly on UI
            TaxID.visible(false);
            GSTIN1.visible(true);
            Excise1.visible(true);
            Customs2.visible(true);
            ServiceTax3.visible(true);
            SalesTax4.visible(true);
            VAT1.visible(true);
            WithholdingTax1.visible(true);
        }
    }

]]></Source>
