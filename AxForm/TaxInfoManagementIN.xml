				<Source><![CDATA[
[Form]
    public class TaxInfoManagementIN extends FormRun
    {
        CompanyInfo                             companyInfo;
        LogisticsLocation                       logisticsLocation;
        DirPartyLocation                        dirPartyLocation;
        LogisticsEntityPostalAddressFormHandler postalAddressForm;
}
]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        Object  object;
        FormRun formRun;

        object = element.args().caller();
        if (object is FormRun && element.args().record().TableId != tableNum(LogisticsPostalAddress))
        {
            formRun = object;
            formRun.dataSource(tableStr(TaxInformation_IN)).research();
        }
        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        Object              object;
        FormRun             formRun;
        TaxInformation_IN   taxInformation;
        LogisticsPostalAddress logisticsPostalAddress;

        object = element.args().caller();
        
        if (!object)
        {
            // Form was called incorrectly.
            throw error("@SYS22996");
        }
        else if (object is FormRun)
        {
            formRun = object;
        }

        companyInfo = CompanyInfo::find();

        if (formRun)
        {
            if (element.args().record().TableId == tableNum(LogisticsPostalAddress))
            {
                // For Bank account table.
                postalAddressForm = formRun.getAddressController();
                logisticsPostalAddress = element.args().record() as LogisticsPostalAddress;
                logisticsLocation = LogisticsLocation::find(logisticsPostalAddress.Location);
            }
            else
            {
                postalAddressForm = formRun.args().parmObject();
                logisticsLocation = formRun.dataSource(tableStr(LogisticsLocation)).cursor();
                dirPartyLocation = formRun.dataSource(tableStr(DirpartyLocation)).cursor();
            }
        }
        else
        {
            if (!logisticsLocation && element.args().record().TableId == tableNum(TaxInformation_IN))
            {
                taxInformation    = element.args().record();
                logisticsLocation = LogisticsLocation::find(taxInformation.RegistrationLocation);
            }
        }

        super();

        if (object is TransTaxInformationUIController)
        {
            element.design().viewEditMode(ViewEditMode::View);
            taxInformation_IN_DS.allowCreate(false);
            taxInformation_IN_DS.allowDelete(false);
            taxInformation_IN_DS.allowEdit(false);
        }

        element.updateControlsForIndia();
        element.updateControls();

        boolean isPartyAddress = DirPartyTableHelper_IN::isCustomerLocation(logisticsLocation.RecId) || DirPartyTableHelper_IN::isVendorLocation(logisticsLocation.RecId);

        GST_GSTTransNumSeqGroup_IN.visible(!isPartyAddress);
    }

]]></Source>
			</Method>
