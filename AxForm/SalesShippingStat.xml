					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        /// <summary>
        /// Initializes the sales shipping specification datasource.
        /// </summary>
        /// <remarks>
        /// A range is added removing the successfull invoice posted shipping specifications.
        /// </remarks>
        public void init()
        {
            QueryBuildDataSource    qbds;
            QueryBuildRange         qbr;

            super();

            if (!element.args().caller())
            {
                qbds  = this.query().dataSourceTable(tablenum(SalesShippingStat));
                qbr = qbds.addRange(fieldnum(SalesShippingStat,InvoiceId));
                qbr.value(strfmt('(%1 != %3) || (%2 != %3)',
                          fieldstr(SalesShippingStat, InvoiceId),
                          fieldstr(SalesShippingStat, PackingSlipId),
                          SysQuery::valueEmptyString()));
            }
        }

]]></Source>
					</Method>
				</Methods>
				<Fields />
			</DataSource>
