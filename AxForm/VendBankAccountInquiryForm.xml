				<Name>isDateRangeValid</Name>
				<Source><![CDATA[
    private boolean isDateRangeValid(date _startDate, date _endDate)
    {
        return _startDate == dateNull()
            || _endDate == dateNull()
            || _endDate >= _startDate;
    }

]]></Source>
