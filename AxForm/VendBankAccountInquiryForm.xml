                QueryBuildRange vendTableQueryDateRange = SysQuery::findOrCreateRange(vendTableQueryDataSource, fieldnum(VendTable, AccountNum));

                vendTableQueryDateRange.value(queryValue(_vendBankAccountNum));
                VendTable_ds.executeQuery();

                this.setControlVisibility(false);
