				<Name>applyDateFilter</Name>
				<Source><![CDATA[
    private void applyDateFilter(date _startDate, date _endDate)
    {
        if (this.isDateRangeValid(_startDate, _endDate))
        {
            this.refreshDateDataset(_startDate, _endDate);
        }
        else
        {
            Error("@SYS84349");
        }
    }

]]></Source>
