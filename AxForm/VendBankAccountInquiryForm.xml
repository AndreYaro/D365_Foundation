						<Source><![CDATA[
        private void setControlVisibility(boolean _isVisible)
        {
            if (_isVisible)
            {
                MaskedBankAccountNumber.visible(_isVisible);
                MaskedBankAccountRegistrationNumber.visible(_isVisible);
                MaskedBankIBAN.visible(_isVisible);
                VendBankAccount_SWIFTNo.visible(_isVisible);
            }
            else if (VendTable.Currency == 'USD')
            {
                MaskedBankIBAN.visible(_isVisible);
                VendBankAccount_SWIFTNo.visible(_isVisible);
                MaskedBankAccountNumber.visible(!_isVisible);
                MaskedBankAccountRegistrationNumber.visible(!_isVisible);
            }
            else
            {
                MaskedBankIBAN.visible(!_isVisible);
                VendBankAccount_SWIFTNo.visible(!_isVisible);
                MaskedBankAccountNumber.visible(_isVisible);
                MaskedBankAccountRegistrationNumber.visible(_isVisible);
            }
        }

