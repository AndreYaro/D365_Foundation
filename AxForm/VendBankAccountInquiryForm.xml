					<Method>
						<Name>showOrHideReviewed</Name>
						<Source><![CDATA[
        private void showOrHideReviewed()
        {
            QueryBuildDataSource vendBankQueryDataSource = VendBankAccount_ds.queryBuildDataSource();

            if (reviewedShowing)
            {
                vendBankQueryDataSource.addRange(fieldnum(VendBankAccount, Reviewed)).value(queryValue(NoYes::No));
                reviewedShowing = false;
                MarkAsReviewedButtonControl.multiSelect(1);
            }
            else
            {
                vendBankQueryDataSource.clearRange(fieldnum(VendBankAccount, Reviewed));
                reviewedShowing = true;
                MarkAsReviewedButtonControl.multiSelect(2);
            }

            VendBankAccount_ds.executeQuery();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>MarkAsReviewedButtonControl</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        [Hookable(false)]
        public void clicked()
        {
            super();
            VendBankAccount vendBankAccountTable = element.dataSource().cursor();

            if (!reviewedShowing)
            {
                MultiSelectionHelper multiSelection = MultiSelectionHelper::construct();
                multiSelection.parmDatasource(VendBankAccount_ds);

                VendBankAccount localVendBankAccount = multiSelection.getFirst();
                VendBankAccount vendBankAccountUpdate;

                while (localVendBankAccount)
                {
                    ttsbegin;
                    select firstonly forupdate vendBankAccountUpdate
                        where vendBankAccountUpdate.RecId == localVendBankAccount.RecId;
                    vendBankAccountUpdate.Reviewed = NoYes::Yes;
                    vendBankAccountUpdate.update();
                    ttscommit;

                    localVendBankAccount = multiSelection.getNext();
                }
            }
            else if (vendBankAccountTable.Reviewed == NoYes::No)
            {
                vendBankAccountTable.Reviewed = NoYes::Yes;
            }
            else
            {
                vendBankAccountTable.Reviewed = NoYes::No;
            }

            element.doRefresh();
        }

]]></Source>
					</Method>
