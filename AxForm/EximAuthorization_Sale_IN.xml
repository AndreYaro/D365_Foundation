				<Source><![CDATA[
    [Form]
    public class EximAuthorization_Sale_IN extends FormRun
    {
        EximUpd_Sale_IN                         eximUpdSale;
        TmpEximImportableItems_IN               importableItems;
        AmountMST                               importAssessableValue;
        TaxAccountType_IN                       taxAccountType;
        EximAuthorizationType_IN                schemeType;
        boolean                                 isPostTax;
}
]]></Source>
			</Method>
			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    public boolean canClose()
    {
        boolean ret;

        ret = super();

        if (ret && this.closedOk() && !this.closedCancel())
        {
            ret = element.validatePostingParameters();

            if (ret)
            {
                if (Box::yesNo("@GLS61716", DialogButton::No) == DialogButton::Yes)
                {
                    element.setValues();
                    eximUpdSale.parmPostingStatus(true);
                }
                else
                {
                    ret = false;
                    element.close();
                }
            }
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        EximAuthorizationSchemesTable_IN authorizationSchemeTable;
        TransDate                        importExpiryDate;
        Object                           caller;

        super();

        caller = element.args().caller();

        if (!element.args()
            || !caller
            || !(caller is EximUpd_Sale_IN))
        {
            throw error(strFmt("@SYS22862", element.name()));
        }

        eximUpdSale = caller;

        if (!element.args().record()
            || element.args().dataset() != tableNum(EximAuthorizationSchemesTable_IN)
            || element.args().parmEnumType() != enumNum(EximAuthorizationType_IN))
        {
            throw error("@SYS22539");
        }

        authorizationSchemeTable = element.args().record();
        schemeType               = element.args().parmEnum();

        isPostTax = true;

        if (!EximParameters_IN::checkParameterForPosting(schemeType))
        {
            taxInfo.visible(false);
            isPostTax = false;
        }

        element.updateDataSource(false);
        authNum.text(authorizationSchemeTable.LicenseNum);
        issueDate.dateValue(authorizationSchemeTable.IssueDate);

        [importExpiryDate, importAssessableValue] = eximUpdSale.importValues();

        impExpiryDate.dateValue(importExpiryDate);

        impAssessableVal.realValue(importAssessableValue);

        if (schemeType == EximAuthorizationType_IN::AA)
        {
            taxAccountType = TaxAccountType_IN::EximAAReceivableAccount;
        }
        else
        {
            taxAccountType = TaxAccountType_IN::EximDFIAReceivableAccount;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>setValues</Name>
				<Source><![CDATA[
    public void setValues()
    {
        eximUpdSale.parmLicenseNumber(authNum.valueStr());
        eximUpdSale.parmIssueDate(issueDate.dateValue());
        eximUpdSale.parmImportAssessableValue(impAssessableVal.realValue());
        eximUpdSale.parmImportExpiryDate(impExpiryDate.dateValue());
        eximUpdSale.parmSaleValue(saleValue.realValue());
        eximUpdSale.parmSaleTxt(saleText.valueStr());
        eximUpdSale.parmTaxAccountGroup(taxLedgerAccountGroup.valueStr());
        eximUpdSale.parmIECNumber(taxRegistrationNumber.valueStr());
        eximUpdSale.parmTransDate(transDate.dateValue());
        eximUpdSale.parmTmpEximTaxDetails(tmpEximTaxDetails);
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDataSource</Name>
				<Source><![CDATA[
    public void updateDataSource(boolean _status)
    {
        tmpEximTaxDetails_ds.allowCreate(_status);
        tmpEximTaxDetails_ds.allowDelete(_status);
    }

]]></Source>
			</Method>
			<Method>
				<Name>validatePostingParameters</Name>
				<Source><![CDATA[
    boolean validatePostingParameters()
    {
        boolean isParmValid = true;
        ;

        if (!impAssessableVal.realValue())
        {
            isParmValid = checkFailed(strFmt("@SYS116347", "@GLS61532"));
        }

        if (isPostTax)
        {
            if (!taxLedgerAccountGroup.valueStr())
            {
                isParmValid = checkFailed(strFmt("@SYS116347", "@GLS5094"));
            }

            if (!taxRegistrationNumber.valueStr())
            {
                isParmValid = checkFailed(strFmt("@SYS116347", "@GLS5100"));
            }

            if (!transDate.dateValue())
            {
                isParmValid = checkFailed(strFmt("@SYS116347", "@SYS67"));
            }

            if (!tmpEximTaxDetails.RecId)
            {
                isParmValid = checkFailed("@SYS78876");
            }
        }

        return isParmValid;
    }

]]></Source>
