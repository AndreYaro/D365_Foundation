								<Name>modified</Name>
								<Source><![CDATA[
            public void modified()
            {
                TaxRegistrationNumbers_IN     taxRegistrationNumberTableLoc;
                TaxLedgerAccountGroup_IN      taxLedgerAccountGroupLoc;
                TaxLedgerAccountSetup_IN      taxLedgerAccountSetupLoc;

                super();

                taxRegistrationNumberTableLoc = TaxRegistrationNumbers_IN::findByNaturalKey(TaxRegistrationType_IN::Company,
                                                                                                TaxType_IN::Customs,
                                                                                                taxRegistrationNumber.text());
                taxLedgerAccountGroupLoc = TaxLedgerAccountGroup_IN::findByTaxAccountGroup(taxLedgerAccountGroup.valueStr());
                taxLedgerAccountSetupLoc = TaxLedgerAccountSetup_IN::findByTaxAccountGroup(taxLedgerAccountGroupLoc.RecId,
                                                                                           TaxAccountCode_IN::Table,
                                                                                           taxRegistrationNumberTableLoc.RecId);
                if (!taxLedgerAccountSetupLoc)
                {
                        taxLedgerAccountSetupLoc = TaxLedgerAccountSetup_IN::findByTaxAccountGroup(taxLedgerAccountGroupLoc.RecId,
                                                                                                   TaxAccountCode_IN::All);
                }

                tmpEximTaxDetails.LedgerDimension = TaxLedgerAccounts_IN::getAccountNum(taxLedgerAccountSetupLoc.RecId,
                                                                                        taxAccountType,
                                                                                        tmpEximTaxDetails.TaxComponentTable);
                tmpEximTaxDetails.OffsetLedgerDimension = LedgerDimensionFacade::serviceCreateLedgerDimension(EximParameters_IN::authorizationType2BenefitLedgerDimension(schemeType));
                tmpEximTaxDetails_ds.rereadReferenceDataSources();
            }

]]></Source>
