        void clicked()
        {
            Args         args;
            FormRun      formRun;
            MenuFunction menuFunction;
            MenuItemName menuItemName;
            ;

            super();

            menuItemName = menuitemDisplayStr(EximTmpImportableItems_IN);

            if (hasMenuItemAccess(menuItemName, MenuItemType::Display))
            {
                args = new Args(menuItemName);
                args.caller(eximUpdSale);

                menuFunction  = new MenuFunction(menuItemName, MenuItemType::Display);

                if (menuFunction)
                {
                    formRun = menuFunction.create(args);
                    if (formRun)
                    {
                        formRun.init();
                        formRun.run();
                        formRun.wait();
                    }
                }
            }
        }

]]></Source>
