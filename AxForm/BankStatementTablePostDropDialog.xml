        void clicked()
        {
            BankStmtISOAccountStatement statement;

            super();

            if (element.args().caller() != null)
            {
                statement = element.args().record();
                if (statement)
                {
                    BankStatementPost::postBankStatement(statement.RecId);
                }
            }

            element.closeOk();
        }

