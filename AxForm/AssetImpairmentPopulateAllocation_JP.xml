				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        cguSharedAssets         = this.args().record();
        AssetImpairmentCGUSharedAssets_JP_ds.query().addQueryFilter(AssetImpairmentCGUSharedAssets_JP_ds.query().dataSourceTable(AssetImpairmentCGUSharedAssets_JP.TableId), fieldStr(AssetImpairmentCGUSharedAssets_JP, AssetId)).value((SysQuery::valueNot(cguSharedAssets.AssetId)));
    }

]]></Source>
