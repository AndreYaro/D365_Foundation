						<Source><![CDATA[
        display AssetImpairmentProportionPercent_JP displayPropotionAfter(
            AssetImpairmentCGUSharedAssetsAlloc_JP  _cgu)
        {
            AssetImpairmentCGUSharedAssetsAlloc_JP      cguSharedAssetsAllocation;
            AssetImpairmentProportionPercent_JP         ret;

            select Percent from cguSharedAssetsAllocation
                where   cguSharedAssetsAllocation.CGU           == _cgu.CGU
                &&      cguSharedAssetsAllocation.SharedAsset   == cguSharedAssets.RecId;

            if (cguSharedAssetsAllocation)
            {
                ret = cguSharedAssetsAllocation.Percent;
            }

            return ret;
        }

