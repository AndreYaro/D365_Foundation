			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        FormDataSource                      formDataSource = FormDataUtil::getFormDataSource(AssetImpairmentCGUSharedAssets_JP);
        AssetImpairmentCGUSharedAssets_JP   assetImpairmentCGUSharedAssetsLocal;
        AssetImpairmentCGUSharedAssetsAlloc_JP      cguSharedAssetsAllocation;
        AssetImpairmentCGUSharedAssetsAlloc_JP      cguSharedAssetsAllocationAfter;
        super();

        for (assetImpairmentCGUSharedAssetsLocal = getFirstSelection(formDataSource);
            assetImpairmentCGUSharedAssetsLocal;
            assetImpairmentCGUSharedAssetsLocal = formDataSource.getNext() as AssetImpairmentCGUSharedAssets_JP)
        {
            update_recordSet cguSharedAssetsAllocation
                setting Percent = cguSharedAssetsAllocationAfter.Percent
                    where   cguSharedAssetsAllocation.SharedAsset == assetImpairmentCGUSharedAssetsLocal.RecId
                    join cguSharedAssetsAllocationAfter
                        where   cguSharedAssetsAllocationAfter.SharedAsset   == cguSharedAssets.RecId
                        &&      cguSharedAssetsAllocationAfter.CGU           == cguSharedAssetsAllocation.CGU;
        }
    }

]]></Source>
