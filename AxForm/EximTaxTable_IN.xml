			<Method>
				<Name>displayTaxPct</Name>
				<Source><![CDATA[
    public void displayTaxPct()
    {
        EximIncentiveSchemeData_IN callerBuffer;

        callerBuffer = element.args().record();

        if (callerBuffer.TableId == tableNum(EximIncentiveSchemeData_IN)
            && callerBuffer.RecId)
        {
            eximTaxTable_ds.object(fieldNum(EximTaxTable_IN, TaxPct)).visible(callerBuffer.IncentiveScheme == EximAuthorizationType_IN::EPCG ? true : false);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        Object callerForm;

        super();

        if (!element.args()
            || !element.args().caller()
            || element.args().dataset() != tableNum(EximIncentiveSchemeData_IN))
        {
            throw error("@SYS22539");
        }

        element.displayTaxPct();

        callerForm = element.args().caller();

        if (callerForm.name() == formStr(EximIncentiveSchemeGroup_IN))
        {
            if (callerForm.parmEnable())
            {
                eximTaxTable_ds.allowCreate(true);
                eximTaxTable_ds.allowDelete(true);
                eximTaxTable_ds.allowEdit(true);
            }
            else
            {
                eximTaxTable_ds.allowCreate(false);
                eximTaxTable_ds.allowDelete(false);
                eximTaxTable_ds.allowEdit(false);
            }
        }
    }

]]></Source>
