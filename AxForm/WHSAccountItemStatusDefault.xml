            qbds = whsAccountItemStatusDefault_q.dataSourceTable(tableNum(WHSAccountItemStatusDefault));
            qbrAccountNum = qbds.addRange(fieldNum(WHSAccountItemStatusDefault, AccountNum));
            qbrModule = qbds.addRange(fieldNum(WHSAccountItemStatusDefault, ModuleInventCustVend));

            switch (this.args().dataset())
            {
                case tableNum(CustTable):
                    custTable = this.args().record();
                    qbrAccountNum.value(custTable.AccountNum);
                    qbrModule.value(SysQuery::value(ModuleInventCustVend::Cust));
