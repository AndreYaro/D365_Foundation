                case tableNum(VendTable):
                    vendTable = this.args().record();
                    qbrAccountNum.value(vendTable.AccountNum);
                    qbrModule.value(SysQuery::value(ModuleInventCustVend::Vend));
                    break;
            }
        }
    }

