			<Method>
				<Name>initLinksForLGAgreementDS</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Add links between BankLGFacilityAgreementLine_DS and the five BankLGAmountCalculation DS.
    /// </summary>
    private void initLinksForLGAgreementDS()
    {
        QueryBuildDataSource        qbds;

        qbds = bankLGFacilityAgreementLine_ds.query().dataSourceName(cashMarginCalc_ds.name());
        qbds.clearLinks();
        qbds.addLink(fieldNum(BankLGFacilityAgreementLine, CashMarginCalc), fieldNum(BankLGAmountCalculation, RecId));

        qbds = bankLGFacilityAgreementLine_ds.query().dataSourceName(extensionCommissionCalc_ds.name());
        qbds.clearLinks();
        qbds.addLink(fieldNum(BankLGFacilityAgreementLine, ExtensionCommissionCalc), fieldNum(BankLGAmountCalculation, RecId));

        qbds = bankLGFacilityAgreementLine_ds.query().dataSourceName(issuanceCommissionCalc_ds.name());
        qbds.clearLinks();
        qbds.addLink(fieldNum(BankLGFacilityAgreementLine, IssuanceCommissionCalc), fieldNum(BankLGAmountCalculation, RecId));

        qbds = bankLGFacilityAgreementLine_ds.query().dataSourceName(increaseValueCommissionCalc_ds.name());
        qbds.clearLinks();
        qbds.addLink(fieldNum(BankLGFacilityAgreementLine, IncreaseValueCommissionCalc), fieldNum(BankLGAmountCalculation, RecId));

        qbds = bankLGFacilityAgreementLine_ds.query().dataSourceName(decreaseValueCommissionCalc_ds.name());
        qbds.clearLinks();
        qbds.addLink(fieldNum(BankLGFacilityAgreementLine, DecreaseValueCommissionCalc), fieldNum(BankLGAmountCalculation, RecId));
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateBankLGAmountCalcDS</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Update the visibility of fields in data source based on the calculation method.
    /// </summary>
    /// <param name="_bankLGAmountCalcDS">
    ///     The BankLGAmountCalculation DS which is to be updated.
    /// </param>
    public void updateBankLGAmountCalcDS(FormDataSource _bankLGAmountCalcDS)
    {
        BankLGAmountCalculation     curBankLGAmountCalculation;

        if (_bankLGAmountCalcDS
            && _bankLGAmountCalcDS.table() == tableNum(BankLGAmountCalculation))
        {
            curBankLGAmountCalculation = _bankLGAmountCalcDS.cursor();

            _bankLGAmountCalcDS.formRun().lockWindowUpdate(true);

            switch (curBankLGAmountCalculation.CalcMethod)
            {
                case BankLGAmountCalcType::FixedValue:
                    _bankLGAmountCalcDS.object(fieldNum(BankLGAmountCalculation, Amount)).visible(true);
                    _bankLGAmountCalcDS.object(fieldNum(BankLGAmountCalculation, Percentage)).visible(false);
                    break;
                case BankLGAmountCalcType::Percentage:
                    _bankLGAmountCalcDS.object(fieldNum(BankLGAmountCalculation, Amount)).visible(false);
                    _bankLGAmountCalcDS.object(fieldNum(BankLGAmountCalculation, Percentage)).visible(true);
                    break;
            }

            _bankLGAmountCalcDS.formRun().lockWindowUpdate(false);
        }
    }

]]></Source>
