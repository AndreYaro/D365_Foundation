					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            if (isLedgerSettlementsEnabled)
            {
                Query query = generalJournalAccountEntrySim_IT_ds.query();
                QueryBuildDataSource ds = query.dataSourceTable(tablenum(LedgerTransSettlement));

                if (ds == null)
                {
                    QueryBuildDataSource lineDataSource = query.dataSourceTable(tablenum(GeneralJournalAccountEntrySim_IT));
                    ds = lineDataSource.addDataSource(tablenum(LedgerTransSettlement));
                    ds.joinMode(JoinMode::NoExistsJoin);
                    ds.fetchMode(QueryFetchMode::One2One);
                    ds.addLink(fieldnum(GeneralJournalAccountEntrySim_IT, RecId), fieldnum(LedgerTransSettlement, TransRecId));
                }

                if (isShowUnsettledTransFeatureEnabled)
                {
                    boolean showUnsettledTransDateConfigured = showUnsettledTransProvider.parmLastConfiguredDate() != dateNull();
                    if (showUnsettledTransDateConfigured)
                    {
                        ds.addRange(fieldNum(LedgerTransSettlement, SettleDate)).value(SysQuery::range(dateNull(), showUnsettledTransProvider.parmLastConfiguredDate(), true));
                    }
                    else
                    {
                        // If empty date is configured, it's equivalent as reset to default view, show all transactions
                        ds.clearRange(fieldNum(LedgerTransSettlement, SettleDate));
                    }

                    ds.enabled(showUnsettledTransProvider.isDateConfigured() && showUnsettledTransDateConfigured);
                }
                else
                {
                    ds.enabled(ShowOpen.value());
                }

                element.updateFormLabel();
            }

            super();

        }

