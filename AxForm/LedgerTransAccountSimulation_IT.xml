[Form]
public class LedgerTransAccountSimulation_IT extends FormRun
{
    private boolean isLedgerSettlementsEnabled;
    private boolean isShowUnsettledTransFeatureEnabled;
    private LedgerAccount accountToDisplay;

    private LedgerTransShowUnsettledTransByDateProvider showUnsettledTransProvider;

    #ISOCountryRegionCodes

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        DimensionAttributeValue dimAttributeValue;
        DimensionAttributeValueCombination dimAttrValueCombo;
        GeneralJournalAccountEntrySim_IT generalJournalAccountEntryArg;
        MainAccount mainAccountArg;

        Object caller = this.args().caller();
        tableId callingTable = this.args().dataset();
        boolean useMainAccount;
        boolean useDimAttrValueCombo;
        DimensionHierarchy dimensionHierarchy;

        super();

        isLedgerSettlementsEnabled = LedgerTransSettlement::isLedgerSettlementsEnabled();

        if (isLedgerSettlementsEnabled)
        {
            ShowUnsettledTransactions.visible(true);

            showUnsettledTransProvider = LedgerTransShowUnsettledTransByDateProvider::construct();
            showUnsettledTransProvider.parmDefaultDate(DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone()));
        }

        if (!LedgerParameters::find().AcknowledgementDate_IT)
        {
            ledgerTrans_AcknowledgementDate.visible(false);
        }

        Query query = GeneralJournalAccountEntrySim_IT_ds.query();

        // sort by accounting date because it is the preferred user sort
        // additionally sort by journal and voucher to provide a consistent order within duplicate dates
        QueryBuildDataSource ds = query.dataSourceTable(tableNum(GeneralJournalEntrySim_IT));
        ds.addSortField(fieldNum(GeneralJournalEntrySim_IT, AccountingDate));
        ds.addSortField(fieldNum(GeneralJournalEntrySim_IT, JournalNumber));
        ds.addSortField(fieldNum(GeneralJournalEntrySim_IT, SubledgerVoucher));

        // explicitly select the LedgerEntryJournal FK because it is
        // not implicitly included and the active join requires it
        ds.addSelectionField(fieldNum(GeneralJournalEntrySim_IT, LedgerEntryJournal));

        GeneralJournalEntrySim_IT::addSecureRangeForLedger(ds);

        switch (callingTable)
        {
            case tablenum(GeneralJournalAccountEntry):
                generalJournalAccountEntryArg = element.args().record() as GeneralJournalAccountEntrySim_IT;

                mainAccountArg = LedgerDimensionFacade::getMainAccountFromLedgerDimension(generalJournalAccountEntryArg.LedgerDimension);
                Debug::assert(mainAccountArg != null);

                accountToDisplay = mainAccountArg.MainAccountId;

                useMainAccount = true;

                if (element.args().parm() == "@SYS54253" + int642str(generalJournalAccountEntryArg.LedgerDimension))
                {
                    // special case for adding a date range
                    Struct dateRange = element.args().object();

                    TransDate startDate = dateRange.value('startDate');
                    TransDate endDate = dateRange.value('endDate');

                    ds = query.dataSourceTable(tablenum(GeneralJournalEntrySim_IT));
                    QueryBuildRange range = SysQuery::findOrCreateRange(ds, fieldnum(GeneralJournalEntrySim_IT, AccountingDate));
                    range.value(SysQuery::range(startDate, endDate));
                }
                break;

            case tablenum(MainAccount):
                mainAccountArg = element.args().record() as MainAccount;
                Debug::assert(mainAccountArg != null);

                accountToDisplay = mainAccountArg.MainAccountId;

                useMainAccount = true;
                break;

            case tablenum(DimensionAttributeValue):
                dimAttributeValue = element.args().record() as DimensionAttributeValue;

                accountToDisplay = dimAttributeValue.getValue();

                ds = query.dataSourceTable(tablenum(GeneralJournalAccountEntrySim_IT));

                ds = ds.addDataSource(tablenum(DimensionAttributeValueGroupCombination));
                ds.joinMode(JoinMode::ExistsJoin);
                ds.fetchMode(QueryFetchMode::One2One);
                ds.addLink(
                    fieldNum(GeneralJournalAccountEntrySim_IT, LedgerDimension),
                    fieldnum(DimensionAttributeValueGroupCombination, DimensionAttributeValueCombination));

                ds = ds.addDataSource(tablenum(DimensionAttributeValueGroup));
                ds.joinMode(JoinMode::ExistsJoin);
                ds.fetchMode(QueryFetchMode::One2One);
                ds.addLink(fieldnum(DimensionAttributeValueGroupCombination, DimensionAttributeValueGroup), fieldnum(DimensionAttributeValueGroup, RecId));

                ds = ds.addDataSource(tablenum(DimensionAttributeLevelValue));
                ds.joinMode(JoinMode::ExistsJoin);
                ds.fetchMode(QueryFetchMode::One2One);
                ds.addLink(fieldnum(DimensionAttributeValueGroup, RecId), fieldnum(DimensionAttributeLevelValue, DimensionAttributeValueGroup));

                // This restricts to only those matching the dimension value being passed in.
                QueryBuildRange range = ds.addRange(fieldnum(DimensionAttributeLevelValue, DimensionAttributeValue));
                range.value(queryValue(dimAttributeValue.RecId));
                break;

            case tablenum(DimensionAttributeValueCombination):
                dimAttrValueCombo = element.args().record() as DimensionAttributeValueCombination;

                accountToDisplay = dimAttrValueCombo.DisplayValue;

                useDimAttrValueCombo = true;
                break;
        }

        if (this.args().parmEnumType() == enumNum(LedgerTransAccountDisplayType))
        {
            LedgerTrialBalanceListPageBalanceParms balanceParameters;
            LedgerITrialBalanceBalanceParameterProvider balanceParameterProvider = caller as LedgerITrialBalanceBalanceParameterProvider;

            if (balanceParameterProvider)
            {
                balanceParameters = balanceParameterProvider.getBalanceParameters();
            }
            else
            {
                // passing the Trial Balance balance parameters on Args.parmObject is supported because there is no other way to get them in some cases.
                balanceParameters = this.args().parmObject() as LedgerTrialBalanceListPageBalanceParms;
            }

            if (balanceParameters != null)
            {
                LedgerTransAccountDisplayType displayType = any2enum(this.args().parmEnum());

                balanceParameters.updateQueryForDisplayType(query, displayType);
                dimensionHierarchy = balanceParameters.parmDimensionSetHierarchy();

                if (isShowUnsettledTransFeatureEnabled && isLedgerSettlementsEnabled)
                {
                    showUnsettledTransProvider.parmDefaultDate(balanceParameters.parmEndDate());
                }
            }
        }


        ds = query.dataSourceTable(tablenum(GeneralJournalAccountEntrySim_IT));

        if (useMainAccount)
        {
            GeneralJournalAccountEntrySim_IT::addSecureRangeForMainAccount(ds, mainAccountArg.RecId);
        }

        if (useDimAttrValueCombo)
        {
            // this has to be cleared in case the record on the args is from a different hierarchy
            ds.clearDynalinks();
            this.updateQueryForLedgerDimension(query, dimAttrValueCombo, dimensionHierarchy);
        }

        this.updateFormLabel();

        identification.setFocus();
    }

]]></Source>
