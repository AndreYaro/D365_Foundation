			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        super();

        // Hide the tip, if it is already configured
        if (this.isAppInsightsConfigured())
        {
            AppInsightsTip.visible(false);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>isAppInsightsConfigured</Name>
				<Source><![CDATA[
    private boolean isAppInsightsConfigured()
    {
        // Configured via "Montoring and Telemetry" as connection string
        if (WHSApplicationInsightsConnectionStringProviderFlight::instance().isEnabled() &&
            SysIntParameters::find().CaptureWarehouseEvents &&
            SysApplicationInsightsConnectionstringProvider::connectionString())
        {
            return true;
        }

        // Configured via "Operational Insights" or "Montoring and Telemetry" as instrumentation key
        if (WHSOperationInsightsKeyProvider::instrumentationKey())
        {
            return true;
        }

        return false;
    }

]]></Source>
