					<Method>
						<Name>selectionChanged</Name>
						<Source><![CDATA[
        public void selectionChanged()
        {
            EcoResTextValue enumerationValue;
            EcoResEnumerationAttributeTypeValue enumerationAttributeTypeValue;
            EcoResAttributeGroupAttribute attributeGroupAttribute;
            int currentPosition;

            super();

            // Hide attribute value controls
            element.hideAttributeValueControls();

            // Find or create EcoResValue
            if (EcoResValue.RecId == 0)
            {
                currentPosition = RetailChanneAttributeGroupAttrView_ds.getPosition();

                select firstOnly attributeGroupAttribute
                    where attributeGroupAttribute.Attribute == RetailChanneAttributeGroupAttrView.Attribute
                       && attributeGroupAttribute.AttributeGroup == RetailChanneAttributeGroupAttrView.FirstAttributeGroup;
                EcoResAttributeValue::findOrCreateDefault(channelInstanceValue.RecId, RetailChanneAttributeGroupAttrView.Attribute, attributeGroupAttribute);
                RetailChanneAttributeGroupAttrView_ds.executeQuery();
                RetailChanneAttributeGroupAttrView_ds.setPosition(currentPosition);
            }
                        
            // Populate enumration attribute value if needed
            if (AttributeDataTypeDiscriminator::checkAttributeDataTypeText(EcoResAttributeType.DataType) && EcoResAttributeType.IsEnumeration)
            {
                // Populate enumeration combox box
                attributeEnumerationComboBox.clear();
                while select TextValue from enumerationValue
                    exists join enumerationAttributeTypeValue
                    where enumerationAttributeTypeValue.Value == enumerationValue.RecId
                       && enumerationAttributeTypeValue.AttributeType == EcoResAttributeType.RecId
                {
                    attributeEnumerationComboBox.add(enumerationValue.TextValue);
                }
                attributeEnumerationComboBox.selectText(EcoResValue_EcoResTextValue.TextValue);
            }

            // Show attribute value controls
            element.showAttributeValueControls();
        }

