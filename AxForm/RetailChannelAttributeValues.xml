			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        RetailChannelAttributeGroup channelAttributeGroup;
        RetailOnlineChannelTable onlineChannel;
        QueryBuildDataSource qbds;

        super();

        if (element.args() &&
            element.args().record() is RetailChannelAttributeGroup)
        {
            // Find the current channel
            channelAttributeGroup = element.args().record();
            channelId = channelAttributeGroup.Channel;
            attributeGroupId = channelAttributeGroup.AttributeGroup;

            // Add ranges to RetailChanneAttributeGroupAttrView
            qbds = RetailChanneAttributeGroupAttrView_ds.query().dataSourceTable(tableNum(RetailChanneAttributeGroupAttrView));
            qbds.addRange(fieldNum(RetailChanneAttributeGroupAttrView, ChannelAttributeModifier)).value(int2str(RetailChannelAttributeModifier::Channel));
            qbds.addRange(fieldNum(RetailChanneAttributeGroupAttrView, Channel)).value(int642str(channelId));
            qbds.addRange(fieldNum(RetailChanneAttributeGroupAttrView, FirstAttributeGroup)).value(int642str(attributeGroupId));

            // Create RetailChannelInstanceValue if not exists yet
            channelInstanceValue = RetailChannelInstanceValue::findByChannel(channelId);
            if (channelInstanceValue.RecId == 0)
            {
                channelInstanceValue.Channel = channelId;
                channelInstanceValue.insert();
            }

            // Add ranges to EcoResAttributeValue
            qbds = EcoResAttributeValue_ds.query().dataSourceTable(tableNum(EcoResAttributeValue));
            qbds.addRange(fieldNum(EcoResAttributeValue, InstanceValue)).value(int642str(channelInstanceValue.RecId));
        }
        else
        {
            this.close();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>showAttributeValueControls</Name>
				<Source><![CDATA[
    public void showAttributeValueControls()
    {
        if (EcoResValue)
        {
            switch (EcoResValue.getInstanceRelationType())
            {
                case tableStr(EcoResTextValue):
                    if (EcoResAttributeType.IsEnumeration == NoYes::Yes)
                    {
                        attributeEnumerationComboBox.visible(true);
                    }
                    else
                    {
                        attributeValueStringEdit.visible(true);
                        TranslateButton.enabled(true);
                    }
                    break;

                case tableStr(EcoResFloatValue):
                    attributeIntegerDecimalGroup.visible(true);
                    attributeValueRealEdit.visible(true);
                    if (EcoResAttributeTypeUnitOfMeasure::findByAttributeType(EcoResAttributeType).RecId)
                    {
                        attributeFloatUnitOfMeasure.visible(true);
                    }
                    break;

                case tableStr(EcoResIntValue):
                    attributeIntegerDecimalGroup.visible(true);
                    attributeValueIntEdit.visible(true);
                    if (EcoResAttributeTypeUnitOfMeasure::findByAttributeType(EcoResAttributeType).RecId)
                    {
                        attributeIntUnitOfMeasure.visible(true);
                    }
                    break;

                case tableStr(EcoResCurrencyValue):
                    attributeCurrencyGroup.visible(true);
                    attributeCurrencyValueRealEdit.visible(true);
                    attributeCurrencyCode.visible(true);
                    break;

                case tableStr(EcoResDateTimeValue):
                    attributeValueUtcDateTimeEdit.visible(true);
                    break;

                case tableStr(EcoResBooleanValue):
                    attributeValueBooleanEdit.visible(true);

                default:
                    // Unsupported
            }
        }
    }

]]></Source>
