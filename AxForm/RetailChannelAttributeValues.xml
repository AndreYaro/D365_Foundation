			<Method>
				<Name>hideAttributeValueControls</Name>
				<Source><![CDATA[
    public void hideAttributeValueControls()
    {
        attributeEnumerationComboBox.visible(false);
        attributeIntegerDecimalGroup.visible(false);
        attributeValueRealEdit.visible(false);
        attributeFloatUnitOfMeasure.visible(false);
        attributeValueIntEdit.visible(false);
        attributeIntUnitOfMeasure.visible(false);
        attributeCurrencyGroup.visible(false);
        attributeCurrencyValueRealEdit.visible(false);
        attributeCurrencyCode.visible(false);
        attributeValueStringEdit.visible(false);
        attributeValueUtcDateTimeEdit.visible(false);
        attributeValueBooleanEdit.visible(false);
        TranslateButton.enabled(false);
    }

]]></Source>
