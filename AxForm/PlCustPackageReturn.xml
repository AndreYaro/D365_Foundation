						<Source><![CDATA[
        public int active()
        {
            boolean depositExcempt;
            int     ret = super();

            element.updateDesign();

            depositExcempt = CustTable::find(inventPackageReturn.CustAccount).PackageDepositExcempt_PL;
            returnDepositAmountGrid.visible(! depositExcempt);
            currencyCodeGrid.visible       (! depositExcempt);

            return ret;
        }

]]></Source>
