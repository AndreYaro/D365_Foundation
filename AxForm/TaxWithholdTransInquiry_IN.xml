		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
    [Form]
    public class TaxWithholdTransInquiry_IN extends FormRun
    {
        NoYes               fromMenu;
        QueryBuildRange     dateCriteria;

        Query               query;
        QueryRun            queryRun;
        [FormObservable]
        AmountCur           taxWithholdAmountTotal;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (element.args().caller())
        {
            fromMenu    = NoYes::No;
        }
        else
        {
            fromMenu    = NoYes::Yes;
        }

        if (fromMenu)
        {
            query   = new Query();

            query.addDataSource(tablenum(TaxWithholdTrans_IN));
            query.dataSourceNo(1).addRange(fieldnum(TaxWithholdTrans_IN, Voucher));
            query.dataSourceNo(1).addRange(fieldnum(TaxWithholdTrans_IN, TransDate));
            query.dataSourceNo(1).addRange(fieldnum(TaxWithholdTrans_IN, TaxWithholdCode));
            query.dataSourceNo(1).addRange(fieldnum(TaxWithholdTrans_IN, TaxWithholdGroup));
            query.dataSourceNo(1).addRange(fieldnum(TaxWithholdTrans_IN, VendAccount));

            element.promptVoucher();
        }

        #ISOCountryRegionCodes
        if (SysCountryRegionCode::isLegalEntityInCountryRegion([#isoIN]))
        {
            TaxWithholdTrans_IN_RateOfDeduction.noOfDecimals(5);
            WithholdingTax_TaxWithholdValue.noOfDecimals(5);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>promptVoucher</Name>
				<Source><![CDATA[
    public void promptVoucher()
    {
        queryRun = new QueryRun(query);

        if (! queryRun.prompt())
        {
            if (fromMenu)
            {
                element.close();
            }
        }
        taxWithholdTrans_IN_ds.query(queryRun.query());
    }

]]></Source>
			</Method>
