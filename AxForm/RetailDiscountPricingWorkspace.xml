			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Initialize configuration for pricing and discount workspace.
    /// </summary>
    public void init()
    {
        RetailWorkspaceConfiguration retailWorkspaceConfiguration;

        changeEvent = SysFilterChangeEvent::construct();

        retailWorkspaceConfiguration = retailWorkspaceConfiguration::findOrCreateForCurrentUser(RetailWorkspaceConfigurationType::PricingDiscountWorkspace, true);

        super();

        if (RetailPricingFeatureToggle::isRetailPricingPropertyFeatureEnabled())
        {
            AffiliationDiscountsTabControl.visible(false);
            CatalogDiscountsTabControl.visible(false);
            ChannelPriceGroupsLink.visible(false);
            CatalogPriceGroupsLink.visible(false);
            LoyaltyPriceGroupsLink.visible(false);
            AffiliationPriceGroupsLink.visible(false);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmFilter</Name>
				<Source><![CDATA[
    /// <summary>
    /// Add filters to FilterProvider framework to filter tiles and form parts based on values
    /// </summary>
    /// <returns></returns>
    public SysIFilter parmFilter()
    {
        OMInternalOrganization omInternalOrganization;
        EcoResCategoryHierarchy categoryHierarchy;
        OMHierarchyRelationship omHierarchyRelationship;
        SysIFilterValue omHierarchyRelationshipFilterValue;
        EcoResCategory category;

        // Create a filter of OMInternalOrganization based on RetailWorkspaceConfigurationDataSource.OMInternalOrganization.
        SysIFilter filter = SysFilterFactory::createEmptyFilter();

        RetailWorkspaceConfiguration configuration = RetailWorkspaceConfiguration::find(RetailWorkspaceConfigurationType::PricingDiscountWorkspace);

        categoryHierarchy = EcoResCategoryHierarchy::find(configuration.CategoryHierarchy);

        category = EcoResCategory::find(configuration.Category);

        // Here we need to add 4 Filters
        // Filter 1. OMHierarchyRelationship.RecId for selected channel of Retail Workspace configuration filters.
        // Following 3 filters help to identify all parent category including self.
        // Filter 2. EcoResCategoryHierarchy RecId from Selected category of Retail Workspace configuration filters from EcoResCategory.
        // Filter 3. less than equal of NestedSetLeft from Selected category of Retail Workspace configuration filters from EcoResCategory.
        // Filter 4. greater than equal of NestedSetRight from Selected category of Retail Workspace configuration filters from EcoResCategory.

        // If RetailWorkspaceConfigurationDataSource.omHierarchyRelationship == 0, it means the user has not selected any node from the organization hierarchy filter
        // and hence no filter will be applied on the destination datasource. (i.e. filter operator will be set to LessThan and the custom filter strategy in
        // RetailWorkspaceOrgHierarchyFilterStrategy class will disable the datasource that are joined to retrieve data based on org hierarchy filter)
        // Otherwise, (i.e. if filter is not empty) the destination datasource will be filter by the specified filter value and records that match the org hierarchy filter will
        // be retrieved.

        omHierarchyRelationshipFilterValue = SysFilterValueFactory::createFilterValueForInteger(configuration.OMHierarchyRelationship, extendedTypeNum(RecId));

        SysFilterOperator omHierarchyRelationshipFilterOperator = configuration.OMHierarchyRelationship == 0 ? SysFilterOperator::LessThan : SysFilterOperator::Equal;

        filter.addItem(omHierarchyRelationshipFilterValue, omHierarchyRelationshipFilterOperator, SysFilterValueResolutionMethod::None);

        //Create filter for Categoryhierarchy
        SysIFilterValue categoryHierarchyFilter = SysFilterValueFactory::createFilterValueForInteger(configuration.CategoryHierarchy, extendedTypeNum(EcoResCategoryHierarchyId));

        // If Category Hierarchy is empty then use greater than operator so that custom filter can disable join with EcoResCategory.
        SysFilterOperator categoryHierarchyFilterOperator = configuration.CategoryHierarchy == 0 ? SysFilterOperator::GreaterEqual : SysFilterOperator::Equal;

        filter.addItem(categoryHierarchyFilter, categoryHierarchyFilterOperator, SysFilterValueResolutionMethod::None);

        if (configuration.Category != 0)
        {
            //Create filter for NestedSetLeft and look for NestedLeft node greater than equal to current node to search for child category
            SysIFilterValue categoryNestedLeftFilter = SysFilterValueFactory::createFilterValueForInteger(category.NestedSetLeft, extendedTypeNum(EcoResCategoryNestedSetLeft));
            filter.addItem(categoryNestedLeftFilter, SysFilterOperator::GreaterEqual, SysFilterValueResolutionMethod::None);

            //Create filter for NestedSetRight and look for NestedRight node less than equal to current node to search for child category
            SysIFilterValue categoryNestedRightFilter = SysFilterValueFactory::createFilterValueForInteger(category.NestedSetRight, extendedTypeNum(EcoResCategoryNestedSetRight));
            filter.addItem(categoryNestedRightFilter, SysFilterOperator::LessEqual, SysFilterValueResolutionMethod::None);
        }

        return filter;
    }

]]></Source>
