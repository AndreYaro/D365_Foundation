public class MCRCatalogFreeProduct extends FormRun
{
    Catalog catalog;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (element.args().record()
            && element.args().record().TableId == tableNum(Catalog))
        {
            catalog = element.args().record();
        }
        else
        {
            error::missingFormActiveBuffer(element.form().name());
        }

        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>task</Name>
				<Source><![CDATA[
    public int task(int _taskId)
    {
        #Task
        int ret;

        if (_taskId == #taskNew)
        {
            Object                  formRun;
            Args                    args;
            MenuFunction            menuFunction;
            RetailAddItemsParameter parameters;

            args = new Args();
            args.caller(element);
            args.record(catalog);

            parameters = RetailAddItemsParameter::construct();
            parameters.paramCatalogId(catalog.RecId);
            parameters.mcrParamAddToCatalogFreeProducts(true);

            args.parmObject(parameters);

            menuFunction = new MenuFunction(menuitemDisplayStr(RetailAddChannelItems), MenuItemType::Display);
            formRun = menuFunction.create(args);
            ret = formRun.run();
        }
        else
        {
            ret = super(_taskId);
        }

        return ret;
    }

]]></Source>
			</Method>
