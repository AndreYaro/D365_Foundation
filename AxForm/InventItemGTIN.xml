					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        void write()
        {
            ttsbegin;

            super();

            if (inventItemGTIN.InventDimId != inventDim.InventDimId)
            {
                inventDim.data(InventDim::find(inventItemGTIN.InventDimId));
                inventDim_ds.setCurrent();
            }

            ttscommit;
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
                int active()
        {
            int ret;
            ret = super();
            element.updateDesign(InventDimFormDesignUpdate::Active);

            inventDim_DS.active();

            return ret;
        }

]]></Source>
