						<Source><![CDATA[
        /// <summary>
        /// Event handler of init.
        /// </summary>
        public void init()
        {
            super();

            // Only show Processed discounts as active when the feature is enabled.
            if (RetailPricingFeatureToggle::isRetailDiscountPerformanceImprovementFeatureEnabled())
            {
                RetailPeriodicDiscount_DS.query().dataSourceTable(tableNum(RetailPeriodicDiscount)).addRange(fieldNum(RetailPeriodicDiscount, ProcessingStatus)).value(SysQuery::value(RetailPricingDiscountProcessingStatus::Processed));
            }
        }

]]></Source>
