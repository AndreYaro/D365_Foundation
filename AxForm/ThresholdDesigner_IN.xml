						<Source><![CDATA[
        /// <summary>
        ///     Write method of threshold section.
        /// </summary>
        public void write()
        {
            if (editmode == ThresholdTreeEditMode_IN::Creation)
            {
                if (thresholdDefDesignerCtrl.addThresholdSection(treeParentItemIdxOfNewItem, ThresholdSections_IN, thresholdDefDesignerCtrl.parmThresholdItemIdx()))
                {
                    editmode = ThresholdTreeEditMode_IN::Edition;
                }
            }
            else
            {
                thresholdDefDesignerCtrl.updateThresholdSectionNode(ThresholdSections_IN);
                editmode = ThresholdTreeEditMode_IN::Edition;
            }
        }

]]></Source>
