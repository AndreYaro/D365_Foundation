				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Initialize method of form.
    /// </summary>
    public void init()
    {
        Object                      callerForm;
        Common                      common;

        if (!element.args().caller() || !element.args().dataset() || element.args().dataset() != tableNum(Threshold_IN))
        {
            //Select a threshold record to design the structure for.
            throw error("@SYS4003315");
        }
        thresholdCallerRecord       = element.args().record();

        if (!thresholdCallerRecord.RecId)
        {
            throw error("@SYS4003315");
        }

        callerForm = element.args().caller();
        if (formHasMethod(callerForm, identifierStr(getSourceReferenceBuffer)))
        {
            common                  = callerForm.getSourceReferenceBuffer();
            SourceReferenceTableId  = common.TableId;
            SourceReferenceRecId    = common.RecId;
        }

        super();
        //Initialize splitter

        editmode                    = ThresholdTreeEditMode_IN::Edition;
        thresholdDefDesignerCtrl    = ThresholdDefDesignerCtrlFactory_IN::newThresholdDefDesignerCtrl(SourceReferenceTableId);
        thresholdDefDesignerCtrl.initialize(element, thresholdCallerRecord, Tree, ThresholdSections_IN_ds, SourceReferenceTableId, SourceReferenceRecId);
    }

]]></Source>
