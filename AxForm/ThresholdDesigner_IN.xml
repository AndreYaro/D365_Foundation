					<Method>
						<Name>create</Name>
						<Source><![CDATA[
        public void create(boolean _append = false)
        {
            boolean ret = true;
            ThresholdSections_IN newThresholdSection;

            if (editmode == ThresholdTreeEditMode_IN::Creation && !ThresholdSections_IN.RecId)
            {
                if (thresholdDefDesignerCtrl.addThresholdSection(treeParentItemIdxOfNewItem, ThresholdSections_IN, thresholdDefDesignerCtrl.parmThresholdItemIdx()))
                {
                    editmode = ThresholdTreeEditMode_IN::Edition;
                    ret = true;
                }
                else
                {
                    ret = false;
                }
            }

            if (ret)
            {
                treeParentItemIdxOfNewItem = Tree.getSelection();
                thresholdDefDesignerCtrl.initThresholdSectionNodeFromParent(treeParentItemIdxOfNewItem, newThresholdSection);
                //Create treeNode to show new added node
                if (thresholdDefDesignerCtrl.newThresholdTreeNode(treeParentItemIdxOfNewItem, newThresholdSection))
                {
                    super();
                    ThresholdSections_IN.data(newThresholdSection);
                    editmode = ThresholdTreeEditMode_IN::Creation;
                }
            }
        }

]]></Source>
