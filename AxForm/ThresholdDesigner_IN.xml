						<Source><![CDATA[
        /// <summary>
        ///     SelectionChanged method for tree control.
        /// </summary>
        /// <param name="_OldItem">
        ///     Indicates old item.
        /// </param>
        /// <param name="_NewItem">
        ///     Indicates new item.
        /// </param>
        /// <param name="_how">
        ///     Indicates the way of selectionChanged.
        /// </param>
        public void selectionChanged(FormTreeItem _OldItem, FormTreeItem _NewItem, FormTreeSelect _how)
        {
            super(_OldItem, _NewItem, _how);
            if (editmode == ThresholdTreeEditMode_IN::Edition)
            {
                thresholdDefDesignerCtrl.updateSelectThresholdSectionRecId(_NewItem);
                ThresholdSections_IN_ds.executeQuery();
            }
            else
            {// Save current thresholdSection into DB if selection change

                if (_NewItem.idx() != thresholdDefDesignerCtrl.parmNewThresholdItemIdx())
                {
                    if (!ThresholdSections_IN.RecId && ThresholdSections_IN.checkOverlapBetweenChildandParent() && ThresholdSections_IN.checkOverlapThresholdSection())
                    {
                        if (thresholdDefDesignerCtrl.addThresholdSection(treeParentItemIdxOfNewItem, ThresholdSections_IN, thresholdDefDesignerCtrl.parmThresholdItemIdx()))
                        {
                            editmode = ThresholdTreeEditMode_IN::Edition;
                            thresholdDefDesignerCtrl.updateSelectThresholdSectionRecId(_NewItem);
                            ThresholdSections_IN_ds.executeQuery();
                        }
                    }
                }
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>selectionChanging</Name>
						<Source><![CDATA[
        /// <summary>
        ///     SelectionChanging method for tree control.
        /// </summary>
        /// <param name="_OldItem">
        ///     Indicates old item.
        /// </param>
        /// <param name="_NewItem">
        ///     Indicates new item.
        /// </param>
        /// <param name="_how">
        ///     Indicates the way of selectionChanging.
        /// </param>
        /// <returns>
        ///     False, if changing validate pass, else, false.
        /// </returns>
        public boolean selectionChanging(FormTreeItem _OldItem, FormTreeItem _NewItem, FormTreeSelect _how)
        {
            boolean ret;

            ret = super(_OldItem, _NewItem, _how);

            // if in the process of create node, need check let user to change selection
            if (_NewItem.idx() != thresholdDefDesignerCtrl.parmNewThresholdItemIdx())
            {
                if (!ret && !ThresholdSections_IN.RecId && editmode == ThresholdTreeEditMode_IN::Creation)
                {
                    if (!(ThresholdSections_IN.checkOverlapBetweenChildandParent() && ThresholdSections_IN.checkOverlapThresholdSection()))
                    {
                        ret = true;
                    }
                }
            }

            if (editmode == ThresholdTreeEditMode_IN::Edition && ThresholdSections_IN.RecId)
            {
                if (!ThresholdSections_IN.orig().equal(ThresholdSections_IN))
                {
                    ret = !thresholdDefDesignerCtrl.updateThresholdSectionNode(ThresholdSections_IN);
                }
            }

            return ret;
        }

]]></Source>
