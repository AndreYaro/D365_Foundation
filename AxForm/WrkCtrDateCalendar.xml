        public void executeQuery()
        {
            WorkCalendarDate            workCalendarDateTmp;

            // Populate temporary table buffer based on the resource group resource
            workCalendarDateTmp.setTmp();
            wrkCtrTable.addDefinedCalendarDatesToTemporaryBuffer(workCalendarDateTmp,fromDate.dateValue(), toDate.dateValue());

            // Connect the form data source to the temporary data
            workCalendarDate1.setTmp();
            workCalendarDate1.setTmpData(workCalendarDateTmp);

            super();
        }

]]></Source>
