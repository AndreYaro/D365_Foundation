            queryBuildDataSource.clearSortIndex();
            queryBuildDataSource.addSortField(fieldnum(WorkCalendarDate,TransDate));

            workCalendarDate1_ds.cacheAddMethod(tablemethodstr(WorkCalendarDate, dateDayName));
            workCalendarDate1_ds.cacheAddMethod(tablemethodstr(WorkCalendarDate, week));
            workCalendarDate1_ds.cacheAddMethod(tablemethodstr(WorkCalendarDate, monthName));
        }

