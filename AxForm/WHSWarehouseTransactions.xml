				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {       
        if (!this.shouldInitForm())
        {
            throw error(Error::missingRecord(element.name()));
        }

        callerForm = element.args().caller();

        super();

        element.updateDesign(InventDimFormDesignUpdate::Init);

        WHSInventTransAndWarehouseTransactionView_TransactionDateTime.visible(isWHSWarehouseTransactionsTransDateTimeFlight);
        WHSInventTransAndWarehouseTransactionView_InventoryTransactionMechanism.visible(isWHSWarehouseTransactionsTransDateTimeFlight);
    }

]]></Source>
