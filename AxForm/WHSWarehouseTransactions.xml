[Form]
public class WHSWarehouseTransactions extends FormRun
{
    private FormRun callerForm;
    private InventDimCtrl_Frm_WarehouseTransactions inventDimFormSetup;

    // Set up variables for implementing the pack() and unpack() methods.
    private WHSInventTransFilter whsWarehouseTransactionFilterVal;
    private boolean skipTransactionFilter;
    private static boolean isWHSWarehouseTransactionsButtonFlightEnabled = WHSWarehouseTransactionsButtonFlight::instance().isEnabled();
    private static boolean isWHSWarehouseTransactionsTransDateTimeFlight = WHSWarehouseTransactionsTransDateTimeFlight::instance().isEnabled();

}
