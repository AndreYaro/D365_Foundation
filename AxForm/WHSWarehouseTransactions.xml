			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    private void updateDesign(InventDimFormDesignUpdate _mode)
    {
        InventDimParm   inventDimParm       = InventDimParm::allInventDim();
        InventDimParm   inventDimParmEnable = inventDimParm.data();

        switch (_mode)
        {
            case InventDimFormDesignUpdate::Init:
                if (! inventDimFormSetup)
                {
                    inventDimFormSetup  = InventDimCtrl_Frm_WarehouseTransactions::construct();
                    InventDimAxFormAdapter adapter = InventDimAxFormAdapter::newFromForm(element);
                    inventDimFormSetup.parmCallingElement(adapter);

                    Set validInventDimDSset = new Set(Types::String);
                    validInventDimDSset.add(WHSInventTransAndWarehouseTransactionView_ds.name());
                    inventDimFormSetup.parmValidDataSources(validInventDimDSset);

                    InventDimControlIdCollection inventDimControlIdCollection = InventDimControlIdCollection::construct();
                    inventDimControlIdCollection.add(inventoryDimensionsGrid.id(), true);
                    inventDimFormSetup.parmControlIdCollectionEnumerator(inventDimControlIdCollection.getEnumerator());

                    inventDimFormSetup.init();
                }
                    
                inventDimFormSetup.parmSkipOnHandLookUp(true);
                inventDimFormSetup.parmDimParmEnabled(inventDimParmEnable);
                inventDimFormSetup.parmDimParmVisible(inventDimParm);
                inventDimFormSetup.parmDontShowDisabled(false);
                inventDimFormSetup.formSetControls(true);

                break;
        }
    }

]]></Source>
