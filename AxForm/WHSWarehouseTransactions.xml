					<Field>
						<DataField>ReferenceId</DataField>
						<Methods>
							<Method>
								<Name>jumpRef</Name>
								<Source><![CDATA[
            public void jumpRef()
            {
                WHSInventTransAndWarehouseTransactionView cursor = WHSInventTransAndWarehouseTransactionView_ds.cursor();

                if (cursor.ReferenceId)
                {
                    InventTransOrigin inventTransOrigin;

                    if (cursor.InventTransId)
                    {
                        inventTransOrigin = InventTransOrigin::findByInventTransId(cursor.InventTransId);
                    }
                    else
                    {
                        inventTransOrigin.ReferenceCategory = cursor.InventoryReferenceCategory;
                        inventTransOrigin.ReferenceId = cursor.ReferenceId;
                    }

                    InventTransFormHelper::jumpRefReferenceId(inventTransOrigin);
                }
            }

]]></Source>
