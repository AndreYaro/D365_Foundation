						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            WHSInventTransFilter whsInventTransFilterEnum;
            boolean ret;
            WHSInventTransFilter originInventTransFilter = whsWarehouseTransactionFilterVal;

            ret = super();

            whsWarehouseTransactionFilterVal = str2enum(whsInventTransFilterEnum, whsWarehouseTransactionFilter.valueStr());

            if (skipTransactionFilter && originInventTransFilter != whsWarehouseTransactionFilterVal)
            {
                skipTransactionFilter = false;
            }

            WHSInventTransAndWarehouseTransactionView_ds.executeQuery();
            
            return ret;
        }

]]></Source>
