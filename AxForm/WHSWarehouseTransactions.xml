					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            Query inventTransAndWarehouseTransactionQuery = this.query();
            QueryBuildDataSource qbdsWarehouseTransaction = inventTransAndWarehouseTransactionQuery.dataSourceTable(tableNum(WHSInventTransAndWarehouseTransactionView));

            switch (element.args().dataset())
            {
                case tableNum(InventTable):
                    InventTable inventTable = element.args().record();

                    qbdsWarehouseTransaction.addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, ItemId)).value(queryValue(inventTable.ItemId));
                    break;
                case tableNum(InventSum):
                    InventSum inventSum = element.args().record();
                    
                    qbdsWarehouseTransaction.addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, ItemId)).value(queryValue(inventSum.ItemId));
                    this.filterTransactionsByNonEmptyInventoryDimensionsFromCallerForm(qbdsWarehouseTransaction);
                    break;
                case tableNum(WHSWorkInventTrans):
                    WHSWorkInventTrans workInventTrans = element.args().record();

                    qbdsWarehouseTransaction
                        .addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, ItemId))
                        .value(queryValue(workInventTrans.ItemId));
                    qbdsWarehouseTransaction
                        .addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, InventoryTransactionOriginId))
                        .value(queryValue(Global::guid2StrWithoutBraces(workInventTrans.InventoryTransactionOriginId)));
                    break;
                case tableNum(WHSInventTransAndWarehouseTransactionView):
                    WHSInventTransAndWarehouseTransactionView inventTransAndWarehouseTransaction = element.args().record();
                    
                    qbdsWarehouseTransaction.addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, ItemId)).value(queryValue(inventTransAndWarehouseTransaction.ItemId));
                    this.filterTransactionsByNonEmptyInventoryDimensionsFromCallerForm(qbdsWarehouseTransaction);
                    break;
                case tableNum(WHSInventoryTransactionView):
                    WHSInventoryTransactionView inventoryTransaction = element.args().record();                    

                    qbdsWarehouseTransaction
                        .addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, ItemId))
                        .value(queryValue(inventoryTransaction.ItemId));
                    qbdsWarehouseTransaction
                        .addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, InventoryTransactionOriginId))
                        .value(queryValue(Global::guid2StrWithoutBraces(inventoryTransaction.InventoryTransactionOriginId)));
                    break;
                case tableNum(InventBatch):       
                    if (isWHSWarehouseTransactionsButtonFlightEnabled)
                    {
                        InventBatch inventBatch = element.args().record();
                        qbdsWarehouseTransaction.addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, ItemId)).value(queryValue(inventBatch.ItemId));
                    }                    
                    break;
                case tableNum(InventSerial):
                    if (isWHSWarehouseTransactionsButtonFlightEnabled)
                    {
                        InventSerial inventSerial = element.args().record();
                        qbdsWarehouseTransaction.addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, ItemId)).value(queryValue(inventSerial.ItemId));
                    }
                    break;
                case tableNum(WMSLocation):
                    if (isWHSWarehouseTransactionsButtonFlightEnabled)
                    {
                        WMSLocation wmsLocation = element.args().record();
                        qbdsWarehouseTransaction.addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, WMSLocationId)).value(queryValue(wmsLocation.WMSLocationId));
                    }
                    break;
            }

            if (element.args().lookupTable() == tableNum(WHSInventTransAndWarehouseTransactionView)
                && element.args().lookupField() == fieldNum(WHSInventTransAndWarehouseTransactionView, InventoryTransactionOriginId)
                && element.args().lookupValue())
            {
                str inventoryTransactionOriginId = element.args().lookupValue();
                qbdsWarehouseTransaction
                    .addRange(fieldNum(WHSInventTransAndWarehouseTransactionView, InventoryTransactionOriginId))
                    .value(queryValue(inventoryTransactionOriginId));
            }

            if (isWHSWarehouseTransactionsTransDateTimeFlight)
            {
                qbdsWarehouseTransaction.addSortField(fieldNum(WHSInventTransAndWarehouseTransactionView, TransDateTime)); // To make sure warehouse inventory transaction is sorted correct when request happens on different AOS'es.
                qbdsWarehouseTransaction.addSortField(fieldNum(WHSInventTransAndWarehouseTransactionView, RecId)); // To make sure warehouse inventory transaction is sorted correct on a specific AOS.
            }
            else
            {
                qbdsWarehouseTransaction.addSortField(fieldNum(WHSInventTransAndWarehouseTransactionView, TransDate));
            }

            if (!skipTransactionFilter)
            {
                QueryFilter queryFilter;

                switch (whsWarehouseTransactionFilterVal)
                {
                    case WHSInventTransFilter::DisplayAll:
                        if (inventTransAndWarehouseTransactionQuery.findQueryFilter(qbdsWarehouseTransaction, fieldStr(WHSInventTransAndWarehouseTransactionView, InventoryReferenceCategory)))
                        {
                            inventTransAndWarehouseTransactionQuery.clearQueryFilters(
                                qbdsWarehouseTransaction,
                                fieldStr(WHSInventTransAndWarehouseTransactionView, InventoryReferenceCategory));
                        }
                        break;

                    case WHSInventTransFilter::OmitWork:
                        queryFilter = SysQuery::findOrCreateQueryFilter(
                            inventTransAndWarehouseTransactionQuery,
                            qbdsWarehouseTransaction,
                            fieldStr(WHSInventTransAndWarehouseTransactionView, InventoryReferenceCategory));
                        queryFilter.value(SysQuery::valueNot(InventTransType::WHSWork));
                        break;

                    case WHSInventTransFilter::WorkOnly:
                        queryFilter = SysQuery::findOrCreateQueryFilter(
                            inventTransAndWarehouseTransactionQuery,
                            qbdsWarehouseTransaction,
                            fieldStr(WHSInventTransAndWarehouseTransactionView, InventoryReferenceCategory));
                        queryFilter.value(queryValue(InventTransType::WHSWork));
                        break;
                }
            }

            super();
        }

]]></Source>
