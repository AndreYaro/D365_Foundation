                Args args = Args::createFromArgs(element.args());
                args.lookupTable(tableNum(WHSInventTransAndWarehouseTransactionView));
                args.lookupField(fieldNum(WHSInventTransAndWarehouseTransactionView, InventoryTransactionOriginId));
                args.lookupValue(cursor.InventoryTransactionOriginId);

                new MenuFunction(menuitemDisplayStr(WHSWarehouseTransactions), MenuItemType::Display).run(args);
            }

]]></Source>
