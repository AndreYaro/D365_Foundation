			<Method>
				<Name>enableDisableButtons</Name>
				<Source><![CDATA[
        void enableDisableButtons()
    {
        boolean               approvedSchemeStatus;
        boolean               licenseSchemeStatus;
        boolean               mergedRedeemedStatus;
        boolean               mergedStatus;
        boolean               createdStatus;
        boolean               schemeStatus;
        EximEPCGSchemeLine_IN epcgSchemeLine;

        element.enableDisableSubMenuItems(false);

        if (eximEPCGSchemeTable.RecId)
        {
            approvedSchemeStatus = eximEPCGSchemeTable.Status == EximEPCGSchemeStatus_IN::Approved;

            schemeStatus = eximEPCGSchemeTable.Status     == EximEPCGSchemeStatus_IN::TechUpgrade
                            || eximEPCGSchemeTable.Status  == EximEPCGSchemeStatus_IN::Approved
                            || eximEPCGSchemeTable.Status  == EximEPCGSchemeStatus_IN::Redeemed
                            || eximEPCGSchemeTable.Status  == EximEPCGSchemeStatus_IN::Merged;

            licenseSchemeStatus = eximEPCGSchemeLine.LicenseStatus == EximEPCGLicenseStatus_IN::Original
                                    || eximEPCGSchemeLine.LicenseStatus  == EximEPCGLicenseStatus_IN::Domestic;

            mergedRedeemedStatus = eximEPCGSchemeTable.Status != EximEPCGSchemeStatus_IN::Redeemed
                                   && eximEPCGSchemeTable.Status != EximEPCGSchemeStatus_IN::Merged;

            mergedStatus = eximEPCGSchemeTable.Status == EximEPCGSchemeStatus_IN::Merged;

            createdStatus = eximEPCGSchemeTable.Status == EximEPCGSchemeStatus_IN::Created;

            select firstonly RecId
                from epcgSchemeLine
                    where epcgSchemeLine.SchemeTableRecId == eximEPCGSchemeTable.RecId
                       && epcgSchemeLine.LicenseStatus == EximEPCGLicenseStatus_IN::Extended;

            eximEPCGSchemeTable_ds.allowDelete(!approvedSchemeStatus);
            //If license has redeemed, approved, TechUpgrade or Merged then the Delete button will be disabled.
            eximEPCGSchemeTable_ds.allowDelete(!schemeStatus);
            eximEPCGSchemeLine_ds.allowCreate(false);
            eximEPCGSchemeLine_ds.allowDelete(false);
            eximEPCGSchemeTable_ds.object(fieldnum(EximEPCGSchemeTable_IN, Closed)).allowEdit(eximEPCGSchemeTable.Status != EximEPCGSchemeStatus_IN::TechUpgrade
                                                    && eximEPCGSchemeTable.Status != EximEPCGSchemeStatus_IN::Redeemed);

            element.enableDisableSubMenuItems(true);

            grid_Closed.enabled(!mergedStatus);

            importableItems.enabled(!mergedStatus);

            functionsOverview.enabled(mergedRedeemedStatus);

            approve.enabled(createdStatus && eximEPCGSchemeTable.Basis == EximEPCGSchemeBasis_IN::Primary);
            techUpgrade.enabled(createdStatus && eximEPCGSchemeTable.Basis == EximEPCGSchemeBasis_IN::TechnologyUpgrade);
            redeemed.enabled(approvedSchemeStatus);
            extension.enabled(approvedSchemeStatus && (licenseSchemeStatus || eximEPCGSchemeLine.LicenseStatus  == EximEPCGLicenseStatus_IN::Extended));
            revalidation.enabled(approvedSchemeStatus
                                 && eximEPCGSchemeLine.LicenseStatus == EximEPCGLicenseStatus_IN::Original
                                 && !eximEPCGSchemeTable.checkImportOrder()
                                 && !epcgSchemeLine.RecId);
            buttonMerge.enabled(eximEPCGSchemeTable_ds.getFirst(true)
                                && eximEPCGSchemeTable_ds.getNext()
                                && EximEPCGSchemeTable_IN::enableMergeButton(eximEPCGSchemeTable_ds));
            inquiries.enabled(true);

            functionLines.enabled(mergedRedeemedStatus);

            approved.enabled((schemeStatus)
                              && eximEPCGSchemeTable.Basis   != EximEPCGSchemeBasis_IN::TechnologyUpgrade
                              && (licenseSchemeStatus));
            inquiryExtension.enabled(eximEPCGSchemeLine.LicenseStatus == EximEPCGLicenseStatus_IN::Extended);
            technologyUpgrade.enabled(eximEPCGSchemeTable.Basis == EximEPCGSchemeBasis_IN::TechnologyUpgrade
                                      && (schemeStatus)
                                      && (licenseSchemeStatus));
            merge.enabled(mergedStatus || eximEPCGSchemeLine.LicenseStatus == EximEPCGLicenseStatus_IN::Merged);
            lineInquiries.enabled(true);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>enableDisableSubMenuItems</Name>
				<Source><![CDATA[
        void enableDisableSubMenuItems(boolean _enable)
    {
        importableItems.enabled(_enable);
        approve.enabled(_enable);
        techUpgrade.enabled(_enable);
        redeemed.enabled(_enable);
        buttonMerge.enabled(_enable);
        exportObligation.enabled(_enable);
        importDetails.enabled(_enable);
        extension.enabled(_enable);
        revalidation.enabled(_enable);
        approved.enabled(_enable);
        inquiryExtension.enabled(_enable);
        technologyUpgrade.enabled(_enable);
        merge.enabled(_enable);
    }

]]></Source>
