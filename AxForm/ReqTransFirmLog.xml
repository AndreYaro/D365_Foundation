				<Name>ReqTransFirmLog</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            QueryBuildDataSource qbds = this.query().dataSourceName(this.name());
            SysQuery::findOrCreateRange(qbds, fieldnum(ReqTransFirmLog,ReqPlanId)).value(planFilter.text());

            super();
        }

]]></Source>
