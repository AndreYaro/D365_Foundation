			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        ReqParameters::checkPlanningProcessesAreEnabled();

        super();

        ReqPlanId reqPlanId;

        if (element.args().record())
        {
            var reqPo = element.args().record() as ReqPO;

            if (reqPo)
            {
                var planVersion = ReqPlanVersion::find(reqPo.PlanVersion);
                reqPlanId = planVersion.ReqPlanId;
            }
        }

        if (!reqPlanId)
        {
            reqPlanId = ReqTransPlanIdFilter::getPlanIdFromArgsSysFilter(element.args());
        }

        if (reqPlanId)
        {
            planFilter.text(reqPlanId);
        }
        else
        {
            planFilter.text(ReqPlanSched::defaultDynamicId());
        }
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
