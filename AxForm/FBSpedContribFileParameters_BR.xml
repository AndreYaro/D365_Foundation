				<Name>jumpRefFiscalEstablishment</Name>
				<Source><![CDATA[
    /// <summary>
    /// Calls the fiscal establishment form
    /// </summary>
    /// <param name="_fiscalEstablishment">
    /// A <c>FiscalEstablishment_BR</c> table record
    /// </param>
    public void jumpRefFiscalEstablishment(FiscalEstablishment_BR _fiscalEstablishment)
    {
        Args args = new Args();

        MenuFunction menuItem = new MenuFunction(menuitemDisplayStr(FiscalEstablishment_BR), MenuItemType::Display);

        FiscalEstablishment_BR fiscalEstablishment = FiscalOrganizationFEstablishment.fiscalEstablishment();

        if (_fiscalEstablishment)
        {
            changeCompany(_fiscalEstablishment.dataAreaId)
            {
                menuItem.run(args);
            }
        }
        else
        {
            menuItem.run(args);
        }
    }

]]></Source>
