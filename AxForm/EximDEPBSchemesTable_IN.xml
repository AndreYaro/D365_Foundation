			<Method>
				<Name>enableAttachButton</Name>
				<Source><![CDATA[
        public void enableAttachButton()
    {
        eximDEPBSchemesTable_ds.allowDelete(false);
        functions.enabled(false);
        details.enabled(false);
        eximDEPBSchemesTable_Transferable.allowEdit(false);
        exportOrderDetails.enabled(false);
        approve.enabled(false);
        reExport.enabled(false);
        purchase.enabled(false);
        sale.enabled(false);
        reject.enabled(false);
        if (eximDEPBSchemesTable && !eximDEPBSchemesTable.Closed)
        {
            switch (eximDEPBSchemesTable.Status)
            {
                case EximAuthorizationStatus_IN::Created :
                    eximDEPBSchemesTable_ds.object(fieldNum(EximDEPBSchemesTable_IN, Basis)).enabled(true);
                    eximDEPBSchemesTable_ds.object(fieldNum(EximDEPBSchemesTable_IN, EximPorts)).enabled(true);
                    functions.enabled(true);
                    eximDEPBSchemesTable_ds.allowDelete(true);
                    reject.enabled(true);
                    switch (eximDEPBSchemesTable.Basis)
                    {
                        case EximDEPBBasis_IN::ExportOrder :
                            exportOrderDetails.enabled(true);
                            break;
                        case EximDEPBBasis_IN::Purchase :
                            purchase.enabled(true);
                            break;
                        case EximDEPBBasis_IN::ReExport :
                            reExport.enabled(true);
                            break;
                    }
                    break;
                case EximAuthorizationStatus_IN::Applied :
                    functions.enabled(true);
                    switch (eximDEPBSchemesTable.Basis)
                    {
                        case EximDEPBBasis_IN::ExportOrder :
                            exportOrderDetails.enabled(true);
                            approve.enabled(true);
                            reject.enabled(true);
                            break;
                    }
                    eximDEPBSchemesTable_ds.object(fieldNum(EximDEPBSchemesTable_IN, Basis)).enabled(false);
                    eximDEPBSchemesTable_ds.object(fieldNum(EximDEPBSchemesTable_IN, EximPorts)).enabled(false);
                    break;
                case EximAuthorizationStatus_IN::Approved :
                    functions.enabled(true);
                    details.enabled(true);
                    reject.enabled(false);
                    eximDEPBSchemesTable_Transferable.allowEdit(true);
                    sale.enabled(eximDEPBSchemesTable.Transferable);
                    eximDEPBSchemesTable_ds.object(fieldNum(EximDEPBSchemesTable_IN, Basis)).enabled(false);
                    eximDEPBSchemesTable_ds.object(fieldNum(EximDEPBSchemesTable_IN, EximPorts)).enabled(false);
                    break;
            }
        }
        else
        {
            if (eximDEPBSchemesTable.Status == EximAuthorizationStatus_IN::Approved)
            {
                details.enabled(true);
            }
            else
            {
                eximDEPBSchemesTable_ds.object(fieldNum(EximDEPBSchemesTable_IN, Basis)).enabled(true);
                eximDEPBSchemesTable_ds.object(fieldNum(EximDEPBSchemesTable_IN, EximPorts)).enabled(true);
            }
        }
    }

]]></Source>
