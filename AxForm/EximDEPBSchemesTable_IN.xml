			<Method>
				<Name>enableLineInquiriesButton</Name>
				<Source><![CDATA[
        public void enableLineInquiriesButton()
    {
        revision.enabled(false);
        exportDetails.enabled(false);
        approved.enabled(false);
        extension.enabled(false);
        inqPurchase.enabled(false);
        inqSale.enabled(false);
        inqReExport.enabled(false);
        tax.enabled(true);
        revision.enabled(!eximDEPBSchemesTable.Closed && eximDEPBSchemesTable.Status == EximAuthorizationStatus_IN::Approved);

        if (eximDEPBSchemesTable.Status == EximAuthorizationStatus_IN::Approved)
        {
            switch (eximDEPBSchemesTable.Basis)
            {
                case EximDEPBBasis_IN::ExportOrder :
                    switch (eximDEPBSchemesLine.LicenseStatus)
                    {
                        case EximLicenseStatus_IN::Original :
                            exportDetails.enabled(true);
                            approved.enabled(true);
                            break;
                        case EximLicenseStatus_IN::Extended :
                            extension.enabled(true);
                            break;
                        case EximLicenseStatus_IN::Sold :
                            inqSale.enabled(true);
                            break;
                    }
                    break;

                case EximDEPBBasis_IN::Purchase :
                    switch (eximDEPBSchemesLine.LicenseStatus)
                    {
                        case EximLicenseStatus_IN::Original :
                            inqPurchase.enabled(true);
                            break;
                        case EximLicenseStatus_IN::Extended :
                            extension.enabled(true);
                            break;
                        case EximLicenseStatus_IN::Sold :
                            inqSale.enabled(true);
                            break;
                    }
                    break;

                case EximDEPBBasis_IN::ReExport :
                    switch (eximDEPBSchemesLine.LicenseStatus)
                    {
                        case EximLicenseStatus_IN::Original :
                            inqReExport.enabled(true);
                            break;
                        case EximLicenseStatus_IN::Extended :
                            extension.enabled(true);
                            break;
                        case EximLicenseStatus_IN::Sold :
                            inqSale.enabled(true);
                            break;
                    }
                    break;
            }
        }
    }

]]></Source>
