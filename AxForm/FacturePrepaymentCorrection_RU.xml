			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class FacturePrepaymentCorrection_RU extends FormRun
{
    ModuleCustVend module;

}
]]></Source>
			</Method>
			<Method>
				<Name>enableButtonsPaym</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes buttons availible from payments transactions.
    /// </summary>
    public void enableButtonsPaym()
    {
        boolean existsNonFactureTrans = correctionPaym.existsNonFactureTrans(correctionTax);

        CreateFacture.enabled(existsNonFactureTrans);
        FactureTaxCorrectionSales_RU.enabled(false);
        FactureTaxCorrectionPurch_RU.enabled(false);
    }

]]></Source>
			</Method>
			<Method>
				<Name>enableButtonsTax</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes buttons availible from tax transactions.
    /// </summary>
    public void enableButtonsTax()
    {
        boolean enable = correctionTax.FactureExternalId != '';

        FactureTaxCorrectionSales_RU.enabled(enable);
        FactureTaxCorrectionPurch_RU.enabled(enable);
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        Args args = this.args();

        super();

        module = args.parmEnum();
        this.initTmpTables();
        this.visibleButtons();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initArgs</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initializes <c>Args</c> object for menu items.
    /// </summary>
    /// <returns>
    /// <c>Args</c> object.
    /// </returns>
    public Args initArgs()
    {
        Args args = new Args();
        LedgerJournalTrans ledgerJournalTrans = LedgerJournalTrans::findRecId(correctionTax.LedgerJournalTransTax, false);

        args.record(ledgerJournalTrans);

        return args;
    }

]]></Source>
			</Method>
			<Method>
				<Name>initTmpTables</Name>
				<Source><![CDATA[
    private void initTmpTables()
    {
        delete_from correctionPaym;
        delete_from correctionTax;

        correctionPaym.initCorrectionPaym(module);
        correctionTax.initCorrectionTax(module, correctionPaym);

        if (!showAll.value())
        {
            correctionPaym.clearPaymentsWithFacture(correctionTax);
        }

    }

]]></Source>
			</Method>
			<Method>
				<Name>refreshData</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates data in temporary tables.
    /// </summary>
    public void refreshData()
    {
        this.initTmpTables();
        correctionPaym_ds.research(true);
        correctionTax_ds.research(true);

        this.visibleButtons();
    }

]]></Source>
			</Method>
			<Method>
				<Name>visibleButtons</Name>
				<Source><![CDATA[
    public void visibleButtons()
    {
        boolean visbile = module == ModuleCustVend::Cust;

        if (correctionTax)
        {
            visbile = correctionTax.FactureModule == FactureModule_RU::TaxCorrectionSales;
        }

        FactureTaxCorrectionSales_RU.visible(visbile);
        FactureTaxCorrectionPurch_RU.visible(!visbile);
    }

]]></Source>
			</Method>
