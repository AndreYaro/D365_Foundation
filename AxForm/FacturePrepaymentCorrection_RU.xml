						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            super();

            FacturePrepaymentCorrectionCreate_RU facturePrepaymentCorrectionCreate = FacturePrepaymentCorrectionCreate_RU::construct();
            facturePrepaymentCorrectionCreate.parmModule(module);
            facturePrepaymentCorrectionCreate.parmCorrectionPaym(correctionPaym);
            facturePrepaymentCorrectionCreate.parmCorrectionTax(correctionTax);
            facturePrepaymentCorrectionCreate.runOperation();

            correctionTax.updateTaxFactureFields();
            correctionTax_ds.research(true);
            element.enableButtonsPaym();
        }

]]></Source>
