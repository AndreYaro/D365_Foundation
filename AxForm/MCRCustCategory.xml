				<Source><![CDATA[
    /// <summary>
    /// Initializes the current hierarchy class variables.
    /// </summary>
    public void init()
    {
        if (element.args() && element.args().record())
        {
            if (element.args().record().TableId != tableNum(MCRCustCategoryHierarchy))
            {
                throw error(strfmt("@MCR10663", element.name()));
            }
            else
            {
                currentCategoryHierarchy = element.args().record();
            }
        }
        else
        {
            throw error(strfmt("@MCR10663", element.name()));
        }
        super();
    }

]]></Source>
