			<Method>
				<Name>closedOk</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates that the form can be closed and the value entered by the user
    /// in the override check out field is returned.
    /// </summary>
    /// <returns>
    /// The value entered in the override user field.
    /// </returns>
    public boolean closedOk()
    {
        boolean             ret;
        MCRHoldCodeTrans    mcrHoldCodeTrans;
        if (element.args() && element.args().record())
        {
            mcrHoldCodeTrans = element.args().record();
        }
        ret = super();
        if (mcrHoldCodeTrans)
        {
            if (mcrHoldCodeTrans.MCRCheckedOutTo != OverrideUser.valueStr())
            {
                mcrHoldCodeTrans.MCRCheckedOutTo = OverrideUser.valueStr();
            }
        }

        return ret;
    }

]]></Source>
