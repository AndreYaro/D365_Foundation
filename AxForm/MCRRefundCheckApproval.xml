			<Method>
				<Name>declineRefund</Name>
				<Source><![CDATA[
    void declineRefund(boolean _decline = true)
    {
        MCRRCSettlementTable    mcrRCSettlementTable_local;
        MCRCustPaymTable        mcrCustPaymTable_local;
        MCRRCAuthTable mcrRCAuthTable;

        if (_decline)
        {
            // NOTE:  if only 1 row is selected in the grid, the row is not marked
            if (!MCRRCSettlementTable_ds.anyMarked())
            {
                MCRRCSettlementTable_ds.mark(1);
            }
            mcrRCSettlementTable_local = mcrrcSettlementTable_ds.getFirst(1);

            ttsBegin;
            // must perform an actual select -- else, runtime error occurs
            //  during update
            mcrRCSettlementTable_local = MCRRCSettlementTable::findByRecID(mcrRCSettlementTable_local.RecId, true);

            if (mcrRCSettlementTable_local)
            {
                // Write mcrRCAuthTable record
                mcrrcAuthTable.mcrrcAuthAmount = mcrRCSettlementTable_local.mcrSettlementAmount;
                mcrRCAuthTable.MCRRCAuthStatus = MCRRCAuthStatus::Declined;
                mcrrcAuthTable.SalesId = mcrRCSettlementTable_local.SalesId;
                mcrrcAuthTable.LineNum = MCRRCAuthTable::nextLineNum(mcrRCSettlementTable_local.SalesId);
                mcrrcAuthTable.mcrCustPaymTableRecID = mcrRCSettlementTable_local.mcrCustPaymTableRecID;
                mcrRCAuthTable.MCRRCAuthBy = curuserid();
                mcrRCAuthTable.MCRRCAuthDateTime = DateTimeUtil::utcNow();
                mcrrcAuthTable.mcrrcSettlementTableRecID = mcrRCSettlementTable_local.RecId;

                if (mcrRCAuthTable.validateWrite())
                {
                    mcrRCAuthTable.insert();
                }
                else
                {
                    throw error("@SYS104318");
                }

                // associate the auth table to the settlement table
                mcrRCSettlementTable_local.mcrrcAuthTableRecID = mcrrcAuthTable.RecId;

                // update settlement status
                mcrRCSettlementTable_local.mcrrcSettlementStatus = MCRRCSettlementStatus::Declined;

                // make the settlement record point to the most current auth table record.
                mcrRCSettlementTable_local.mcrrcAuthTableRecID = mcrrcAuthTable.RecId;

                if (mcrRCSettlementTable_local.validateWrite())
                {
                    mcrRCSettlementTable_local.update();
                }
                else
                {
                    throw error("@SYS104318");
                }

                // update payment status
                mcrCustPaymTable_local = MCRCustPaymTable::findByRecID(mcrRCSettlementTable_local.mcrCustPaymTableRecID, true);

                // Write event - manual decline of refund check
                MCRCustPaymEvent::insertPaymEvent(MCRPaymentEventType::ManualDecline, mcrCustPaymTable_local);

                if (mcrCustPaymTable_local)
                {
                    mcrCustPaymTable_local.Status = MCRCustPaymStatus::Declined;
                    if (mcrCustPaymTable_local.validateWrite())
                    {
                        mcrCustPaymTable_local.update();
                    }
                    else
                    {
                        throw error("@SYS104318");
                    }
                }
            }

            ttsCommit;

            MCRRCSettlementTable_ds.executeQuery();
            DeclinedMCRRCSettlementTable_ds.executeQuery();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>promptDeclineDialog</Name>
				<Source><![CDATA[
    boolean promptDeclineDialog()
    {
        DialogButton    button;
        button = box::yesNo("@MCR23547" + '\n'+'\n'
                           + "@MCR20942", DialogButton::No, "@MCR23548");

        if (button == DialogButton::Yes)
        {
            return true;
        }
        else
        {
            return false;
        }
    }

]]></Source>
