						<Source><![CDATA[
        /// <summary>
        ///     Adds a range to only display records related to sales orders.
        /// </summary>
        public void init()
        {
            QueryBuildDataSource source;
            QueryBuildRange range;

            super();

            source = this.query().dataSourceName(MCRCustPaymTable_ds.name());

            range = source.addRange(fieldnum(MCRCustPaymTable, RefTableId));
            range.value(int2str(tableNum(SalesTable)));
            //  Allow the free text invoice related checks to be picked up on
            // the refunds workbench.
            range.value(int2str(tableNum(CustInvoiceTable)));
            range.value(strfmt('((%1 == %2) || (%1 == %3))', fieldStr(MCRCustPaymTable, RefTableID), int2str(tableNum(SalesTable)), int2str(tableNum(CustInvoiceTable))));
        }

]]></Source>
