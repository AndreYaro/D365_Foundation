					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Restricts the query to find settlement records
        ///     with a status of pending approval.
        /// </summary>
        public void init()
        {
            QueryBuildDataSource source;
            QueryBuildRange range;

            super();

            // get the MCRRCSettlementTable query datasource
            source = this.queryBuildDataSource();

            // limit the query to settlement records of status 'hold'
            range = source.addRange(fieldnum(MCRRCSettlementTable, MCRRCSettlementStatus));
            range.value(queryValue(MCRRCSettlementStatus::PendingApproval));
        }

]]></Source>
