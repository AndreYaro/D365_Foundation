				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Approves all selected refunds.
        /// </summary>
        void clicked()
        {
            MCRRCSettlementTable    mcrRCSettlementTable_local;
            MCRCustPaymTable        mcrCustPaymTable_local;
            MCRRCAuthTable          mcrRCAuthTable;
            MCRCustPaymTotals       mcrCustPaymTotals;
            SalesTable              salesTable;

            // Prompt user if an check on hold is on the same order
            MCRCustPaymTable MCRCustPaymTable_Check;
            DialogButton    promptResponse;

            // NOTE:  if only 1 row is selected in the grid, the row is not marked
            if (!MCRRCSettlementTable_ds.anyMarked())
            {
                MCRRCSettlementTable_ds.mark(1);
            }
            mcrRCSettlementTable_local = mcrrcSettlementTable_ds.getFirst(1);

            // Loop through the selected records
            while (mcrRCSettlementTable_local)
            {
                // must perform an actual select -- else, runtime error occurs
                //  during update
                mcrRCSettlementTable_local = MCRRCSettlementTable::findByRecID(mcrRCSettlementTable_local.RecId, true);

                // Moved find up so variable could be used in MCRCustPaymTable_Check select
                mcrCustPaymTable_local = MCRCustPaymTable::findByRecID(mcrRCSettlementTable_local.mcrCustPaymTableRecID, true);

                // Require approval if a check is on hold on the same order
                //          Regardless of the amount
                select firstonly MCRCustPaymTable_Check
                    where mcrCustPaymTable_Check.RefTableId == mcrCustPaymTable_local.RefTableId
                        && mcrCustPaymTable_Check.RefRecId == mcrCustPaymTable_local.RefRecId
                        && mcrCustPaymTable_Check.CustPaymType == MCRCustPaymType::Check
                        && mcrCustPaymTable_Check.Status == MCRCustPaymStatus::Posted
                        && mcrCustPaymTable_Check.CheckAuthorized == NoYes::No;

                if (MCRCustPaymTable_Check)
                {
                    promptResponse = Box::yesNo("@MCR35422", DialogButton::No, strFmt("@MCR35423", mcrRCSettlementTable_local.SalesId));

                    MCRCustPaymTable_Check.clear();

                    if (promptResponse == DialogButton::No)
                    {
                        // Get next on the appropriate data soruce
                        mcrRCSettlementTable_local = mcrrcSettlementTable_ds.getNext();
                        continue;
                    }
                }

                if (mcrRCSettlementTable_local)
                {
                    ttsBegin;

                    // Write mcrRCAuthTable record
                    mcrrcAuthTable.mcrrcAuthAmount = mcrRCSettlementTable_local.mcrSettlementAmount;
                    mcrRCAuthTable.MCRRCAuthStatus = MCRRCAuthStatus::Approved;
                    mcrrcAuthTable.SalesId = mcrRCSettlementTable_local.SalesId;
                    mcrrcAuthTable.LineNum = MCRRCAuthTable::nextLineNum(mcrRCSettlementTable_local.SalesId);
                    mcrrcAuthTable.mcrCustPaymTableRecID = mcrRCSettlementTable_local.mcrCustPaymTableRecID;
                    mcrRCAuthTable.MCRRCAuthBy = curuserid();
                    mcrRCAuthTable.MCRRCAuthDateTime = DateTimeUtil::utcNow();
                    mcrrcAuthTable.mcrrcSettlementTableRecID = mcrRCSettlementTable_local.RecId;

                    if (mcrRCAuthTable.validateWrite())
                    {
                        mcrRCAuthTable.insert();
                    }
                    else
                    {
                        throw error("@SYS104318");
                    }

                    // associate the auth table to the settlement table
                    mcrRCSettlementTable_local.mcrrcAuthTableRecID = mcrrcAuthTable.RecId;

                    // update settlement status
                    mcrRCSettlementTable_local.mcrrcSettlementStatus = MCRRCSettlementStatus::Approved;

                    // make the settlement record point to the most current auth table record.
                    mcrRCSettlementTable_local.mcrrcAuthTableRecID = mcrrcAuthTable.RecId;

                    if (mcrRCSettlementTable_local.validateWrite())
                    {
                        mcrRCSettlementTable_local.update();
                    }
                    else
                    {
                        throw error("@SYS104318");
                    }

                    ttsCommit;

                    // Write event - manual approval of refund check
                    MCRCustPaymEvent::insertPaymEvent(MCRPaymentEventType::RefundApprove, mcrCustPaymTable_local);

                    // update payment status
                    if (mcrCustPaymTable_local)
                    {
                        ttsBegin;
                        mcrCustPaymTable_local.Status = MCRCustPaymStatus::Authorized;
                        if (mcrCustPaymTable_local.validateWrite())
                        {
                            mcrCustPaymTable_local.update();
                        }
                        else
                        {
                            throw error("@SYS104318");
                        }

                        // Clear the stopped flag on the sales order
                        if (mcrCustPaymTable_local.RefTableId == tableNum(SalesTable))
                        {
                            salesTable = mcrCustPaymTable_local.salesTable(true);
                            salesTable.mcrOrderStopped = NoYes::No;
                            if (salesTable.validateWrite())
                            {
                                salesTable.update();
                            }
                            else
                            {
                                throw error("@SYS104318");
                            }
                        }
                        ttsCommit;
                        mcrCustPaymTotals = MCRCustPaymTotals::construct(mcrCustPaymTable_local.RefTableId, mcrCustPaymTable_local.RefRecId);
                        mcrCustPaymTotals.processPayments(MCRProcessPaymAction::PostOnlyPrepays, false, true, mcrCustPaymTable_local.RecId);
                    }
                }

                // Get next on the appropriate data soruce
                mcrRCSettlementTable_local = mcrrcSettlementTable_ds.getNext();
            }

            // refresh the grid
            MCRRCSettlementTable_ds.executeQuery();
            ApprovedMCRRCSettlementTable_ds.executeQuery();
        }

]]></Source>
