			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        QueryBuildDataSource tsHeader;
        QueryBuildDataSource tsLine;
        TSTimesheetLine argsTsLine;
        Common lookupRecord;
        ProjParameters projParameters = ProjParameters::find();

        super();

        lookupRecord = element.args().record();
        if (lookupRecord.RecId == 0)
        {
            lookupRecord = element.args().lookupRecord();
        }

        if (lookupRecord.RecId != 0)
        {
            tsHeader = tsTimesheetTable_ds.query().dataSourceTable(tableNum(TSTimesheetTable));
            tsLine = tsTimesheetLine_ds.query().dataSourceTable(tableNum(TSTimesheetLine));
            tsLine.addRange(fieldNum(TSTimesheetLine, ApprovalStatus)).value(queryValue(TSAppStatus::Pending));

            // Initialize query
            if (lookupRecord.TableId == tableNum(TSTimesheetTable))
            {
                tsHeader.addRange(fieldNum(TSTimesheetTable, RecId)).value(queryValue(lookupRecord.RecId));
            }
            else if (lookupRecord.TableId == tableNum(TSTimesheetLine))
            {
                argsTsLine = lookupRecord as TSTimesheetLine;

                tsHeader.addRange(fieldNum(TSTimesheetTable, TimesheetNbr)).value(queryValue(argsTsLine.TimesheetNbr));

                // clear out the lookupRecord (for Line Item Approval, this will throw an error since the root DS is TSTimesheetTable)
                element.args().lookupRecord(null);
            }
            else
            {
                throw error("@SYS22996");
            }

            element.setDays();

            tsTimesheetLine_ProjectDataAreaId.visible(projParameters.EnableIntercompanyTimesheet);
            tsTimesheetApprovalSummary_ProjectDataAreaId.visible(projParameters.EnableIntercompanyTimesheet);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>setDays</Name>
				<Source><![CDATA[
    public void setDays()
    {
        int i, dayNum = 1;
        str dayField;
        FormRealControl hoursField;
        DictEnum weekEnum = new DictEnum(enumNum(WeekDays));
        WeekDays weekDay;
        TSTimesheetTable localTimesheet;

        localTimesheet = element.args().record() as TSTimesheetTable;
        weekDay = ProjPeriodTable::find(localTimesheet.ProjPeriodId).WeekStart;

        if (weekDay == WeekDays::None)
        {
            weekDay = WeekDays::Monday;
        }

        dayNum = enum2int(weekDay);
        str dayControlNames = strReplace(formcontrolstr(TSTimesheetApproval, TSTimesheetLineWeek_Hours_1),'1','%1');

        for (i = 1; i <= 7; i++)
        {
            dayField = strFmt(dayControlNames, i);
            hoursField = element.design().controlName(dayField);

            hoursField.label(weekEnum.index2Label(dayNum));

            dayNum++;

            if (dayNum > 7)
            {
                dayNum = 1;
            }
        }
    }

]]></Source>
			</Method>
