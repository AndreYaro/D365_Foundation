			<DataSource>
				<Name>TSTimesheetLine</Name>
				<Methods>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;
            WorkflowWorkItemTable workItemTable;

            ret = super();

            select firstonly RecId from  workItemTable where workItemTable.RefTableId == tableNum(TSTimesheetLine) &&
                                                             workItemTable.RefRecId == tsTimesheetLine.RecId;

            if (workItemTable.RecId != 0)
            {
                grid.highlightActive(true);
                element.design().workflowDatasource(tsTimesheetLine_ds.id());
                element.updateWorkflowControls();
            }

            return ret;
        }

]]></Source>
