        /// <summary>
        /// Calculates ADR points total.
        /// </summary>
        [SysClientCacheDataMethodAttribute(true)]
        public display Amount ADRPoints(InventSum _inventSum)
        {
            Amount  calcPoints;

            calcPoints = _inventSum.AvailOrdered * HMIMItemMaterial::findByItemRegCode(_inventSum.ItemId, WHSParameters::find().HMIMStockListLimitsRegCode).Multiplier;

            return calcPoints;
        }

