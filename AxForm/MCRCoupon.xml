			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates form design based on selected options.
    /// </summary>
    public void updateDesign()
    {
        //Voided coupons cannot be modified or deleted.
        if (MCRCoupon.MCRCouponVoided == NoYes::Yes)
        {
            MCRCoupon_ds.allowEdit(false);
            MCRCoupon_ds.allowDelete(false);
            EnableCoupon.enabled(false);
            CouponVoid.enabled(false);
            MCRCouponDiscount_ds.allowDelete(false);
            MCRCouponDiscount_ds.allowEdit(false);
            MCRCouponDiscount_ds.allowCreate(false);
            MCRCouponCustomer.enabled(false);
            MCRCouponCatalog.enabled(false);
            MCRCouponItem.enabled(false);
        }
        else
        {
            // Enabled coupons cannot be modified or deleted, except voided.
            EnableCoupon.enabled(!MCRCoupon.MCREnabled && !MCRCoupon.MCRCouponParentId);
            MCRCoupon_ds.allowEdit(!MCRCoupon.MCREnabled);
            MCRCoupon_ds.allowDelete(!MCRCoupon.MCREnabled);
            CouponVoid.enabled(MCRCoupon.MCREnabled);
            // No enabled coupons can be deleted, modified, but not voided.
            MCRCouponDiscount_ds.allowDelete(!MCRCoupon.MCREnabled);
            MCRCouponDiscount_ds.allowEdit(!MCRCoupon.MCREnabled);
            MCRCouponDiscount_ds.allowCreate(!MCRCoupon.MCREnabled);

            MCRCouponCustomer.enabled(MCRCoupon.MCRCustomerSpecific);
            MCRCouponCatalog.enabled(MCRCoupon.MCRIncludeExcludeCatalog != MCRCouponInclude::None);
            MCRCouponItem.enabled(MCRCoupon.MCRIncludeExcludeItem != MCRCouponInclude::None);
        }
    }

]]></Source>
