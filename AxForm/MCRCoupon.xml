				<Source><![CDATA[
    /// <summary>
    /// Creates the <c>CouponId</c> number sequence value.
    /// </summary>
    /// <returns>
    /// The <c>NumberSeqFormHandler</c> object for the <c>CouponId</c> number sequence.
    /// </returns>
    NumberSeqFormHandler numberSeqFormHandler()
    {
        if (!numberSeqFormHandler)
        {
            numberSeqFormHandler = NumberSeqFormHandler::newForm(
                NumberSeqReference::findReference(
                    extendedTypeNum(MCRCouponId)).NumberSequenceId,
                    element,
                    mcrCoupon_DS,
                    fieldNum(MCRCoupon, mcrCouponId));
        }
        return numberSeqFormHandler;
    }

]]></Source>
