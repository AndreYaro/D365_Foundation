			<Control>
				<Name>EnableCoupon</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        /// Enables the coupon.
        /// </summary>
        void clicked()
        {
            MCRCoupon           localMCRCoupon;
            MCRCouponPost       MCRCouponPost;
            MCRCouponDiscount   localMCRCouponDiscount;
            MCRCouponCatalog    localMCRCouponCatalog;
            MCRCouponItem       localMCRCouponItem;
            MCRCouponCustomer   localMCRCouponCustomer;
            // Save currently selected record.
            MCRCoupon           currentRecord = MCRCoupon.data();
            boolean             ok = true;
            super();

            if (MCRCoupon.MCRCouponParentId)
            {
                throw error("@MCR26538");
            }

            // Validate that that at least one discount record is setup.
            select firstonly RecId from localMCRCouponDiscount
            where localMCRCouponDiscount.MCRCouponId == MCRCoupon.MCRCouponId;
            if (!localMCRCouponDiscount.RecId)
            {
                throw error("@MCR26395");
            }
            // Validate that at least one item record is setup.
            if (MCRCoupon.MCRIncludeExcludeItem != MCRCouponInclude::None)
            {
                select firstonly RecId from localMCRCouponItem
                where localMCRCouponItem.MCRCouponId == MCRCoupon.MCRCouponId;
                if (!localMCRCouponItem.RecId)
                {
                    throw error("@MCR26397");
                }
            }
            // Validate that at least one catalog record is setup.
            if (MCRCoupon.MCRIncludeExcludeCatalog != MCRCouponInclude::None)
            {
                select firstonly RecId from localMCRCouponCatalog
                where localMCRCouponCatalog.CouponId == MCRCoupon.MCRCouponId;
                if (!localMCRCouponCatalog.RecId)
                {
                    throw error("@MCR26398");
                }
            }
            // Validate that at leat one customer record is setup for parent coupons.
            if (MCRCoupon.MCRCustomerSpecific
            && MCRCoupon.MCRCouponParentId == '')
            {
                select firstonly RecId from localMCRCouponCustomer
                where localMCRCouponCustomer.MCRCouponId == MCRCoupon.MCRCouponId;
                if (!localMCRCouponCustomer.RecId)
                {
                    throw error("@MCR26396");
                }
            }

            if (MCRCoupon.MCRCouponExpDate
                && MCRCoupon.MCRToDate
                && MCRCoupon.MCRCouponExpDate < MCRCoupon.MCRToDate)
            {
                throw error(strfmt("@MCR28577", MCRCoupon.MCRCouponExpDate, MCRCoupon.MCRToDate));
            }

            ttsBegin;

            if (Box::yesNo(strfmt("@MCR25579", MCRCoupon.MCRCouponId), 2) == 1)
            {
                MCRCoupon.MCREnabled = NoYes::Yes;
                MCRCoupon.write();

                if (MCRCouponPost::isCouponElegibleForAccrualPosting(MCRCoupon, true))
                {
                    MCRCouponPost = new mcrCouponPost(MCRCoupon);
                    ok = MCRCouponPost.post();
                }

                // Enable the child coupons.
                ok = ok && localMCRCoupon.enableChildCoupon(MCRCoupon.MCRCouponId);

                if (!ok)
                {
                    throw error("@Retail:CouponActivationFailed");
                }
            }

            ttscommit;

            MCRCoupon_ds.executeQuery();

            // Re-select the previously selected record.
            MCRCoupon_ds.positionToRecord(currentRecord);
        }

]]></Source>
