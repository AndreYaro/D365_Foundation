    public void enableActions()
    {
        boolean canPost = false;
        boolean canDelete = false;
       
        if (vendRecurrenceInvoiceGroup)
        {
            if (!vendRecurrenceInvoiceGroup.Posted)
            {
                canPost = true;
                canDelete = true;
            }
        }

        else
        {
            vendRecurrenceInvoiceCheck.enabled(false);
            vendRecurrenceInvoicePost.enabled(false);
            vendRecurrenceInvoicePostTransfer.enabled(false);
        }

        if (canDelete)
        {
            vendRecurrenceInvoiceGroup_ds.allowDelete(true);
        }
        else
        {
            vendRecurrenceInvoiceGroup_DS.allowDelete(false);
        }
    }

]]></Source>
