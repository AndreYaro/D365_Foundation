					<Method>
						<Name>interestTransactionType</Name>
						<Source><![CDATA[
        display CustInterestTransactionType interestTransactionType()
        {
            CustInterestTransactionType custInterestTransactionType;
            CustInvoiceTable custInvoiceTable;
            CustInvoiceLine custInvoiceLine;

            switch (custTrans.TransType)
            {
                case LedgerTransType::Cust:
                    custInvoiceTable = CustInvoiceTable::findPostedInvoice(custTrans.AccountNum, custTrans.Invoice, custTrans.TransDate);
                    select sum(AmountCur) from custInvoiceLine
                        where custInvoiceLine.ParentRecId == custInvoiceTable.RecId;
                    custInterestTransactionType = custInvoiceLine.AmountCur < 0 ? CustInterestTransactionType::CreditNote : CustInterestTransactionType::Invoice;
                    break;

                case LedgerTransType::Interest:
                    custInterestTransactionType = CustInterestTransactionType::InterestNote;
                    break;

                case LedgerTransType::Payment:
                    custInterestTransactionType = CustInterestTransactionType::Payment;
                    break;
            }

            return custInterestTransactionType;
        }

]]></Source>
					</Method>
					<Method>
						<Name>checkAllPrevious</Name>
						<Source><![CDATA[
        private void checkAllPrevious(CustInterestTrans _custInterestTrans, boolean _isMarked)
        {
            boolean showInfo = false;
            CustInterestTrans localCustInterestTrans;
            CustInterestJour localCustInterestJour;
            boolean refreshDataSource = false;

            // This select statement does not limit the transactions considered based on their existing
            // adjustment status since the adjustment process will handle that.
            while select localCustInterestTrans
                where localCustInterestTrans.CustTransId == _custInterestTrans.CustTransId
                    && localCustInterestTrans.AdjustmentStatus != CustInterestAdjustmentStatus::Reverse
                    exists join localCustInterestJour
                    where localCustInterestJour.ToDate > _custInterestTrans.custInterestJour().ToDate &&
                          localCustInterestJour.InterestNote == localCustInterestTrans.InterestNote
            {
                if (_isMarked && !selectedInterestTrans.find(localCustInterestTrans))
                {
                    selectedInterestTrans.ins(localCustInterestTrans);
                    showInfo = true;
                }
                else if (!_isMarked && selectedInterestTrans.find(localCustInterestTrans))
                {
                    selectedInterestTrans.del(localCustInterestTrans);
                }

                refreshDataSource = true;
            }

            if (showInfo)
            {
                info("@SYS329108");
            }

            if (refreshDataSource)
            {
                custInterestJour_ds.research(true);
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>markLine</Name>
						<Source><![CDATA[
        edit NoYes markLine(boolean _set, CustInterestTrans _custInterestTrans, NoYes _value)
        {
            if (_set)
            {
                if (_value)
                {
                    selectedInterestTrans.ins(CustInterestTrans::find(_custInterestTrans.InterestNote, _custInterestTrans.RecId));

                    this.checkWaivedTransaction(_custInterestTrans);

                    if (custInterestAdjustmentAction == CustInterestAdjustmentAction::Reverse)
                    {
                        this.checkAllPrevious(_custInterestTrans, _value);
                    }
                }
                else
                {
                    if (custInterestAdjustmentAction == CustInterestAdjustmentAction::Reverse)
                    {
                        // If other transactions have been marked that are related to this one, they should also
                        // be removed from the list.
                        this.checkAllPrevious(_custInterestTrans, _value);
                    }

                    selectedInterestTrans.del(_custInterestTrans);
                }

                if (isConfigurationkeyEnabled(configurationKeyNum(PublicSector)))
                {
                    element.updateDesign();
                }
            }

            return selectedInterestTrans.find(_custInterestTrans);
        }

]]></Source>
