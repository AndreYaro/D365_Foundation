			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        element.parmCustInterestAdjustmentAction(element.args().parmEnum());

        CustTable custTable = element.args().record();
        element.parmAccountNum(custTable.AccountNum);

        super();

        filterBillingClassification.visible(CustParameters::isBillingClassificationEnabled());

        selectedInterestTrans = new RecordSortedList(tableNum(CustInterestTrans));
        selectedInterestTrans.sortOrder(fieldNum(CustInterestTrans, GeneralJournalEntry), fieldNum(CustInterestTrans, RecId));

        commandButtonAdjust.helpText("@SYS330133");
        switch (tableName2id(element.args().parm()))
        {
            case tableNum(CustInterestJour):
                custInterestAdjustmentType = element.parmCustInterestAdjustmentType(CustInterestAdjustmentType::InterestNote);
                break;

            case tableNum(CustInterestFee):
                custInterestAdjustmentType = element.parmCustInterestAdjustmentType(CustInterestAdjustmentType::InterestFees);
                break;

            case tableNum(CustInterestTrans):
                custInterestAdjustmentType = element.parmCustInterestAdjustmentType(CustInterestAdjustmentType::InterestTransaction);
                break;
        }

        switch (custInterestAdjustmentAction)
        {
            case CustInterestAdjustmentAction::Waive:
                commandButtonAdjust.text("@SYS313600");
                markCheckBox.label("@SYS313600");
                markCheckBox.helpText("@SYS330123");

                if (custInterestAdjustmentType == CustInterestAdjustmentType::InterestTransaction)
                {
                    element.design().caption("@SYS322801");
                    supplementalInstruction.text("@SYS328016");
                }
                break;

            case CustInterestAdjustmentAction::ReInstate:
                commandButtonAdjust.text("@SYS313608");
                markCheckBox.label("@SYS313608");
                markCheckBox.helpText("@SYS330134");
                if (custInterestAdjustmentType == CustInterestAdjustmentType::InterestTransaction)
                {
                    element.design().caption("@SYS328010");
                    supplementalInstruction.text("@SYS328015");
                }
                break;
            case CustInterestAdjustmentAction::Reverse:
                commandButtonAdjust.text("@SYS22850");
                markCheckBox.label("@SYS22850");
                markCheckBox.helpText("@SYS330135");
                element.design().caption("@SYS328009");
                supplementalInstruction.text("@SYS328014");
                break;
        }

        if (isConfigurationkeyEnabled(configurationKeyNum(PublicSector)))
        {
            element.updateDesign();
        }
    }

]]></Source>
