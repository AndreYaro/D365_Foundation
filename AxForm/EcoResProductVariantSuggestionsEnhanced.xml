			<Method>
				<Name>getSelectedProductDimensionValues</Name>
				<Source><![CDATA[
    private List getSelectedProductDimensionValues(FormDataSource _dataSource, FieldId _fieldId)
    {
        List selectedRecordDimensionValues;

        if (_dataSource.getFirst(true)) // only if marked in the grid
        {
            MultiSelectionHelper selectionHelper = MultiSelectionHelper::construct();
            selectionHelper.parmDatasource(_dataSource);

            selectedRecordDimensionValues = new List(Types::Int64);
            Common selectedRecord = selectionHelper.getFirst();
            while (selectedRecord)
            {
                selectedRecordDimensionValues.addEnd(selectedRecord.(_fieldId));
                selectedRecord = selectionHelper.getNext();
            }
        }

        return selectedRecordDimensionValues;
    }

]]></Source>
