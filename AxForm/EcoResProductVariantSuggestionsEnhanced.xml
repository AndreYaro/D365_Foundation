			<Method>
				<Name>disableDatasourcesAndControlsForDisabledProductDimensions</Name>
				<Source><![CDATA[
    private void disableDatasourcesAndControlsForDisabledProductDimensions()
    {
        ecoResTmpProductVariantSuggestion_Color.visible(isColorDimensionActive);
        ColorCriteriaTabPage.visible(isColorDimensionActive);
        EcoResProductMasterColor_ds.query().dataSourceTable(tableNum(EcoResProductMasterColor)).enabled(isColorDimensionActive);

        ecoResTmpProductVariantSuggestion_Size.visible(isSizeDimensionActive);
        SizeCriteriaTabPage.visible(isSizeDimensionActive);
        EcoResProductMasterSize_ds.query().dataSourceTable(tableNum(EcoResProductMasterSize)).enabled(isSizeDimensionActive);

        ecoResTmpProductVariantSuggestion_Style.visible(isStyleDimensionActive);
        StyleCriteriaTabPage.visible(isStyleDimensionActive);
        EcoResProductMasterStyle_ds.query().dataSourceTable(tableNum(EcoResProductMasterStyle)).enabled(isStyleDimensionActive);

        ecoResTmpProductVariantSuggestion_Configuration.visible(isConfigurationDimensionActive);
        ConfigurationCriteriaTabPage.visible(isConfigurationDimensionActive);
        EcoResProductMasterConfiguration_ds.query().dataSourceTable(tableNum(EcoResProductMasterConfiguration)).enabled(isConfigurationDimensionActive);

        ecoResTmpProductVariantSuggestion_Version.visible(isVersionDimensionActive);
        VersionCriteriaTabPage.visible(isVersionDimensionActive);
        EcoResProductMasterVersion_ds.query().dataSourceTable(tableNum(EcoResProductMasterVersion)).enabled(isVersionDimensionActive);
    }

]]></Source>
