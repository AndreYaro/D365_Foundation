[Form]
public class EcoResProductVariantSuggestionsEnhanced extends FormRun
{
    private EcoResProductVariantCreationMgr ecoResProductVariantCreationMgr;
    private EcoResProductMasterRecId        productMaster;

    private boolean                         isColorDimensionActive;
    private boolean                         isSizeDimensionActive;
    private boolean                         isStyleDimensionActive;
    private boolean                         isConfigurationDimensionActive;
    private boolean                         isVersionDimensionActive;
    
}
]]></Source>
			</Method>
			<Method>
				<Name>setActiveDimension</Name>
				<Source><![CDATA[
    /// <summary>
    /// Set the active dimension variable corresponding to the specified product master table ID.
    /// </summary>
    /// <param name = "_tableId">A dimension product master table id.</param>
    [Wrappable, Hookable]
    protected void setActiveDimension(TableId _tableId)
    {
        switch (_tableId)
        {
            case tableNum(EcoResProductMasterColor):
                isColorDimensionActive = true;
                break;
            case tableNum(EcoResProductMasterStyle):
                isStyleDimensionActive = true;
                break;
            case tableNum(EcoResProductMasterVersion):
                isVersionDimensionActive = true;
                break;
            case tableNum(EcoResProductMasterSize):
                isSizeDimensionActive = true;
                break;
            case tableNum(EcoResProductMasterConfiguration):
                isConfigurationDimensionActive = true;
                break;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>buildProductDimensionFilterCriteriaMap</Name>
				<Source><![CDATA[
    protected Map buildProductDimensionFilterCriteriaMap()
    {
        Map productDimensionFilterCriteriaMap = new Map(Types::Integer, Types::Class);

        if (ColorCriteriaTabPage.visible())
        {
            this.addProductDimensionValuesFilterToMap(productDimensionFilterCriteriaMap, EcoResProductMasterColor_ds, fieldNum(EcoResProductMasterColor, Color), tableNum(EcoResProductMasterColor));
        }

        if (SizeCriteriaTabPage.visible())
        {
            this.addProductDimensionValuesFilterToMap(productDimensionFilterCriteriaMap, EcoResProductMasterSize_ds, fieldNum(EcoResProductMasterSize, Size), tableNum(EcoResProductMasterSize));
        }

        if (StyleCriteriaTabPage.visible())
        {
            this.addProductDimensionValuesFilterToMap(productDimensionFilterCriteriaMap, EcoResProductMasterStyle_ds, fieldNum(EcoResProductMasterStyle, Style), tableNum(EcoResProductMasterStyle));
        }
        
        if (ConfigurationCriteriaTabPage.visible())
        {
            this.addProductDimensionValuesFilterToMap(productDimensionFilterCriteriaMap, EcoResProductMasterConfiguration_ds, fieldNum(EcoResProductMasterConfiguration, Configuration), tableNum(EcoResProductMasterConfiguration));
        }

        if (VersionCriteriaTabPage.visible())
        {
            this.addProductDimensionValuesFilterToMap(productDimensionFilterCriteriaMap, EcoResProductMasterVersion_ds, fieldNum(EcoResProductMasterVersion, ProductVersion), tableNum(EcoResProductMasterVersion));
        }

        return productDimensionFilterCriteriaMap;
    }

]]></Source>
			</Method>
			<Method>
				<Name>addProductDimensionValuesFilterToMap</Name>
				<Source><![CDATA[
    [Hookable(false)]
    protected void addProductDimensionValuesFilterToMap(
        Map             _productDimensionFilterCriteriaMap, 
        FormDataSource  _productDimensionDataSource,
        FieldId         _productDimensionValueRecIdOnDataSourceFieldId,
        TableId         _mapKey)
    {
        if (_productDimensionDataSource.queryBuildDataSource().enabled())
        {
            List productDimensionValuesFilterList = this.getSelectedProductDimensionValues(_productDimensionDataSource, _productDimensionValueRecIdOnDataSourceFieldId);

            if (productDimensionValuesFilterList
                && productDimensionValuesFilterList.elements() > 0)
            {
                _productDimensionFilterCriteriaMap.insert(_mapKey, productDimensionValuesFilterList);
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>setActiveDimensions</Name>
				<Source><![CDATA[
    private void setActiveDimensions(Set _set)
    {
        SetEnumerator se = _set.getEnumerator();

        while (se.moveNext())
        {
            this.setActiveDimension(se.current());
        }
    }

]]></Source>
