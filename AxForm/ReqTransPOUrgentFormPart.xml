			<Method>
				<Name>createArgsWithBuffer</Name>
				<Source><![CDATA[
    /// <summary>
    /// Creates an <c>Args</c> instance with the record member set to the buffer passed as argument.
    /// </summary>
    /// <param name = "_record">A buffer to be set as the record member.</param>
    /// <returns>An <c>Args</c> instance.</returns>
    protected Args createArgsWithBuffer(Common _record)
    {
        var args = new Args();
        args.record(_record);
        args.caller(element);

        return args;
    }

]]></Source>
