				<Methods>
					<Method>
						<Name>markChanged</Name>
						<Source><![CDATA[
        public void markChanged()
        {
            super();
                        
            multiSelectionState.removeNotMarkedRecords();

            if (reqPo_DS.anyMarked())
            {
                Map selectedRecords = new Map(typeName2Type(extendedTypeStr(recId)), Types::Record);
                ReqPO reqPOMarked;
                for ( reqPOMarked = reqPo_DS.getFirst(1); reqPOMarked; reqPOMarked = reqPo_DS.getNext())
                {
                    ReqTrans reqTransSelected = reqPOMarked.reqTrans();
                    selectedRecords.insert(reqTransSelected.RecId, reqTransSelected);
                }

                multiSelectionState.addRecords(selectedRecords);
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            ReqIReqPlanVersionFilterProvider reqPlanProvider = element.args().caller() as ReqIReqPlanVersionFilterProvider;
            if (reqPlanProvider)
            {                
                ReqPlanVersion planVersion = reqPlanProvider.planVersion();
                if (planVersion)
                {
                    SysQuery::findOrCreateRange(ReqPO_ds.queryBuildDataSource(), fieldNum(ReqPo, PlanVersion)).value(queryValue(planVersion.RecId));
                }
            }

            super();
        }

]]></Source>
					</Method>
