						<Source><![CDATA[
        public void init()
        {
            super();
            int reqRefType = element.args().parmEnum();
            if (reqRefType)
            {
                var dataSource = this.queryBuildDataSource();
                SysQuery::findOrCreateRange(dataSource, fieldNum(ReqTrans, FuturesDays)).value(strFmt('(%1.%2 >0)',
                    dataSource.name(),
                    fieldStr(ReqTrans, FuturesDays)));
            }
        }

]]></Source>
