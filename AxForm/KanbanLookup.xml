				<Source><![CDATA[
    /// <summary>
    /// Runs the lookup.
    /// </summary>
    public void run()
    {
        FormStringControl   callerControl = SysTableLookup::getCallerStringControl(element.args());
        boolean             filterLookup = false;

        if (callerControl.text() && callerControl.hasChanged())
        {
            filterLookup = true;
            kanban_ds.autoSearch(false);
        }

        super();

        if (filterLookup)
        {
            kanban_ds.research();
            kanban_KanbanId.filter(callerControl.text());
        }

        super();
    }

]]></Source>
