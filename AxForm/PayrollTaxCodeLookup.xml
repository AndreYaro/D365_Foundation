			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (element.args() && element.args().parmObject() && element.args().parmObject() is PayrollTaxCodeLookup)
        {
            taxCodeLookup = element.args().parmObject();
        }
        else
        {
            // If no tax code lookup object was provided, default to all records.
            taxCodeLookup = PayrollTaxCodeLookup::newAllTaxCodes();
        }

        if (!taxCodeLookup.parmCountryRegionId())
        {
            // Only show the country region id column if there isn't a filter on that field
            PayrollTaxCode_CountryRegionId.visible(true);
        }

        PayrollTaxCode_ds.query(taxCodeLookup.updateQuery());
    }

]]></Source>
