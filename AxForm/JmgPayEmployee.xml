                public int active()
        {
            int ret;

            ret = super();

            if ((select RefPayCostType from jmgPayTable where jmgPayTable.PayCostType == jmgPayEmployee.PayCostType).RefPayCostType == '')
            {
                jmgPayEmployee_ds.object(fieldnum(JmgPayEmployee,Percent)).allowEdit(false);
            }
            else
            {
                jmgPayEmployee_ds.object(fieldnum(JmgPayEmployee,Percent)).allowEdit(true);
            }

            return ret;
        }

]]></Source>
