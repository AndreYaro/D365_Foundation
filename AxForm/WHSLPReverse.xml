				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class WHSLPReverse extends FormRun
{
    WHSLicensePlateId   licensePlateId;
    WHSLPReverse        lpReverse;
    ProdTable           prodTable;
    ProdId              prodId;
    boolean             inFGLocation;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (element.args().dataset() == tableNum(ProdTable))
        {
            prodId = element.args().record().(fieldNum(ProdTable, ProdId));
            prodTable = ProdTable::find(prodId);
        }

        super();

        if (prodId)
        {
            prodIdField.text(prodId);
            prodIdField.allowEdit(false);
            whsLicensePlateId.setFocus();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>populateFields</Name>
				<Source><![CDATA[
    void populateFields()
    {
        lpReverse.populateFields(prodTable.inventDim(), licensePlateId);

        wmsLocationId.text(lpReverse.parmWmsLocationId());
        itemId.text(lpReverse.parmItemId());
        curQty.realValue(lpReverse.parmPhysicalInvent());
        unitId.text(lpReverse.parmUnitid());
        inFGLocation = lpReverse.parmInFGLocation();
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        super();

        lpReverse = WHSLPReverse::formRun(prodId, whsLicensePlateId);
    }

]]></Source>
			</Method>
