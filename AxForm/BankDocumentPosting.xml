				<Methods>
					<Method>
						<Name>jumpRef</Name>
						<Source><![CDATA[
        public void jumpRef()
        {
            switch (bankDocumentPosting.AccountCode)
            {
                case TableGroupAll::Table :
                    BankDocumentFacilityType::jumpRef(
                        BankDocumentFacilityType::find(bankDocumentPosting.BankDocumentFacilityType));
                    break;
                case TableGroupAll::GroupId :
                    BankDocumentFacilityGroup::jumpRef(
                        BankDocumentFacilityGroup::find(bankDocumentPosting.BankDocumentFacilityGroup));
                    break;
                case TableGroupAll::All :
                    return;
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>lookup</Name>
						<Source><![CDATA[
        public void lookup()
        {
            switch (bankDocumentPosting.AccountCode)
            {
                case TableGroupAll::Table :
                    BankDocumentFacilityType::lookupFacilityTypeName(this);
                    break;
                case TableGroupAll::GroupId :
                    BankDocumentFacilityGroup::lookupFacilityGroupName(this);
                    break;
                case TableGroupAll::All :
                    break;
            }
        }

]]></Source>
