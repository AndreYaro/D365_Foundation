				<Source><![CDATA[
    public void init()
    {
        super();

        if (BankLGFeatureChecker::checkBankLGEnabled())
        {
            bankLGLiquidationAccount.visible(true);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>setAccountRelationLookupButton</Name>
				<Source><![CDATA[
    /// <summary>
    /// The method to set the lookup button for account relation field based on account code value.
    /// </summary>
    public void setAccountRelationLookupButton()
    {
        switch (bankDocumentPosting.AccountCode)
        {
            case TableGroupAll::All:
                bankDocumentAccountRelationRefRecId.allowEdit(false);
                bankDocumentAccountRelationRefRecId.skip(true);
                bankDocumentAccountRelationRefRecId.lookupButton(1);
                bankDocumentPosting.BankDocumentFacilityGroup = 0;
                bankDocumentPosting.BankDocumentFacilityType = 0;
                break;
            case TableGroupAll::Table:
                bankDocumentAccountRelationRefRecId.allowEdit(true);
                bankDocumentAccountRelationRefRecId.skip(false);
                bankDocumentAccountRelationRefRecId.lookupButton(0);
                bankDocumentPosting.BankDocumentFacilityGroup = 0;
                break;
            case TableGroupAll::GroupId:
                bankDocumentAccountRelationRefRecId.allowEdit(true);
                bankDocumentAccountRelationRefRecId.skip(false);
                bankDocumentAccountRelationRefRecId.lookupButton(0);
                bankDocumentPosting.BankDocumentFacilityType = 0;
                break;
        }
    }

]]></Source>
			</Method>
