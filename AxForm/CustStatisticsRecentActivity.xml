						<Source><![CDATA[
        public void linkActive()
        {
            Object callerForm = element.args().caller();
            CustInvoiceTable linkedCustInvoiceTable;

            super();

            if (callerForm && callerForm.name() == formstr(CustFreeInvoice))
            {
                linkedCustInvoiceTable = element.args().record();
                linkedCustTable = CustTable::find(linkedCustInvoiceTable.OrderAccount);
            }
            else
            {
                linkedCustTable = element.args().record();
            }

            custStatisticsContainer = CustStatisticsUS::CalculateStatistics(linkedCustTable);
            s_InvoiceDate.dateValue(conpeek(custStatisticsContainer,1));
            s_InvoiceTotal.realValue(conpeek(custStatisticsContainer,2));

            s_PaymDate.dateValue(conpeek(custStatisticsContainer,3));
            s_PaymTotal.realValue(conpeek(custStatisticsContainer,4));
        }

]]></Source>
