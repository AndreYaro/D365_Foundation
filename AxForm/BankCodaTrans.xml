        if (element.args().dataset() == tablenum(BankCodaTransDefTable))
        {
            bankCodaTransDefTable = element.args().record();
            bankCodaTransFamily = BankCodaTransFamily::findTransactionFamily(bankCodaTransDefTable.TransactionFamily);
