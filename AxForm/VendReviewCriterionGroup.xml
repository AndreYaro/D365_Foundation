        {
            return true;
        }

]]></Source>
					</Method>
					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        public void write()
        {
            if (vendReviewCriterionGroup.RecId == 0 || vendReviewCriterionGroupTranslation.RecId == 0)
            {
                if (vendReviewCriterionGroup.validateWrite())
                {
                    vendReviewCriterionGroup.write();
                    vendReviewCriterionGroupTranslation.CriterionGroup  = vendReviewCriterionGroup.RecId;
                    vendReviewCriterionGroupTranslation.LanguageId      = defaultLanguageId;
                }
            }

            vendReviewCriterionGroupTranslation.LanguageId      = defaultLanguageId;
            if (vendReviewCriterionGroupTranslation.validateWrite())
            {
                super();
            }
        }

]]></Source>
