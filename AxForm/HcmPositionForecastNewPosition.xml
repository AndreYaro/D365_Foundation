			<Control>
				<Name>CreatePosition</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            boolean ret;
            HcmJobRecId jobRecId;
            HcmPositionActivationDateTime activationDate;
            HcmPositionRetirementDateTime retirementDate;
            HcmForecastScenario           hcmForecastScenarioLocal;
            utcdatetime nowDateTime;
            utcdatetime positionDurationStart;

            // buffer values in case of a failed save
            jobRecId = hcmPositionDetail.Job;
            activationDate = hcmPositionDuration.ValidFrom;
            retirementDate = hcmPositionDuration.ValidTo;

            hcmPosition_ds.allowCreate(true);
            hcmPositionDetail_ds.allowCreate(true);
            hcmPositionDuration_ds.allowCreate(true);

            hcmPositionForecast_ds.allowCreate(true);
            hcmPositionForecastDetail_ds.allowCreate(true);
            hcmPositionForecastDetailV2_ds.allowCreate(true);

            // force the tables to save
            hcmPosition_ds.forceWrite(true);
            hcmPositionDetail_ds.forceWrite(true);
            hcmPositionDuration_ds.forceWrite(true);

            hcmPositionForecast_ds.forceWrite(true);
            hcmPositionForecastDetail_ds.forceWrite(true);
            hcmPositionForecastDetailV2_ds.forceWrite(true);
            hcmPositionForecastHierarchy_ds.forceWrite(true);
            hcmPositionForecastLastModified_ds.forceWrite(true);

            nowDateTime = DateTimeUtil::utcNow();
            positionDurationStart = DateTimeUtil::removeTimeZoneOffset(hcmPositionDuration_ValidFrom.dateTimeValue(), DateTimeUtil::getUserPreferredTimeZone());

            // use the duration start date as the ValidFrom for the detail record if the duration began in the past
            hcmPositionDetail.ValidFrom = (positionDurationStart < nowDateTime) ? positionDurationStart : nowDateTime;

            hcmForecastScenarioLocal = HcmForecastScenario::findByPlanningProcessAndScenario(HcmForecastScenario.BudgetPlanningProcess,
                                                                                                HcmForecastScenario.BudgetPlanScenario);
            // save the records
            ttsBegin;
            if (!hcmForecastScenarioLocal)
            {
                HcmForecastScenario.insert();
                hcmForecastScenarioLocal = HcmForecastScenario;
            }

            HcmPositionForecastScenario.ForecastScenario = hcmForecastScenarioLocal.RecId;

            this.initPositionForecastNewPosition(hcmForecastScenarioLocal);

            ret = hcmPosition_ds.leaveRecord();
            ttsCommit;
            if (ret)
            {
                super();
                info(strFmt(("@Budget:BudgetPlan_PositionForecasting_HcmPositionForecast_CreateNewPositionSuccessMessage"), HcmPosition::find(hcmPositionDetail.Position).PositionId));
            }
            else
            {
                // reset table values to values that were on the window
                hcmPositionDetail.Job = jobRecId;
                hcmPositionDetail.ValidFrom = hcmPositionDuration_ValidFrom.dateTimeValue();

                hcmPositionDuration.ValidFrom = hcmPositionDuration_ValidFrom.dateTimeValue();
                hcmPositionDuration.ValidTo = hcmPositionDuration_ValidTo.dateTimeValue();
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>initPositionForecastNewPosition</Name>
						<Source><![CDATA[
        private void initPositionForecastNewPosition(HcmForecastScenario _hcmForecastScenario)
        {
            // Set the Position Forecast Detail fields.
            hcmPositionForecastDetail.Job = hcmPositionDetail.Job;
            hcmPositionForecastDetail.Title = hcmPositionDetail.Title;
            hcmPositionForecastDetail.Description = HcmJobDetail::findByJob(hcmPositionForecastDetail.Job).Description;
            hcmPositionForecastDetailV2.Job = hcmPositionDetail.Job;
            hcmPositionForecastDetailV2.Title = hcmPositionDetail.Title;
            hcmPositionForecastDetailV2.Description = HcmJobDetail::findByJob(hcmPositionForecastDetailV2.Job).Description;

            // Set the Position Forecast Hierarchy fields.
            hcmPositionForecastHierarchy.PositionHierarchyType = HcmPositionHierarchyType::lineHierarchyType();

            // Set the Position Forecast Duration fields.
            HcmPositionForecastScenario.ActivationDateTime = hcmPositionDuration.ValidFrom;
            HcmPositionForecastScenario.RetirementDateTime = hcmPositionDuration.ValidTo;

            HcmPositionForecastScenario.LegalEntity = _hcmForecastScenario.LegalEntity;
            HcmPositionForecastScenario.VersionDateTime = DateTimeUtil::getSystemDateTime();
            HcmPositionForecastScenario.FullTimeEquivalency = hcmPositionDetail.FullTimeEquivalency;
        }

]]></Source>
