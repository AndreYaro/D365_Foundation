				<Source><![CDATA[
    public void closeOk()
    {
        Args args;
        MenuFunction menuFunction;
        FormRun formRun;
        HcmPositionForecastScenario newHcmPositionForecastScenario;

        if (formHasMethod(element.args().caller(), identifierStr(refreshCaller)))
        {
            element.args().caller().refreshCaller(hcmPositionForecastScenario.RecId);
        }

        element.close();

        // creating a new instance of the HcmPositionForecastScenario table so this form's query is not passed
        newHcmPositionForecastScenario = HcmPositionForecastScenario::find(hcmPositionForecastScenario.RecId);

        if (element.args().caller() == null || element.args().caller().name() != formStr(HcmPositionForecast))
        {
            args = new Args();

            args.lookupRecord(newHcmPositionForecastScenario);

            args.caller(element);
            args.openMode(OpenMode::Edit);

            menuFunction = new MenuFunction(menuitemDisplayStr(HcmPositionForecast), MenuItemType::Display);
            formRun = menuFunction.create(args);
            formRun.run();
        }
    }

]]></Source>
