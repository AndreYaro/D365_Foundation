				<Source><![CDATA[
[Form]
public class smmTMCancelled extends FormRun
{
    smmTMCallListTable      smmTMCallListTable;
    FormDataSource          formDataSource;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        super();

        if (!this.args().caller())
        {
            // Form was called incorrectly.
            throw error("@SYS22996");
        }

        // Is form called from the telemarketing call list form
        if (element.args().record() && element.args().record().TableId == tablenum(smmTMCallListTable))
        {
            // Save link to the caller form data source
            formDataSource = FormDataUtil::getFormDataSource(element.args().record());

            smmTMCallListTable = element.args().record();

            // Set new call status to cancelled and make sure that status can't be changed be the user
            tmStatus.selection(smmTMCallStatus::Cancelled);
            tmStatus.enabled(false);
        }
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="" />
		<DataControls xmlns="">
			<Control>
				<Name>OK</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            // Check that a reason for cancelling the call was specified
            if (!reasonId.text())
            {
                // Set reason cancelled.
                throw error ("@SYS80828");
            }

            smmTMCallListTable.ReasonId = reasonId.text();
            if (smmTMCallListTable.validateField(fieldNum(smmTMCallListTable, ReasonId)))
            {
                // Update call list to status cancelled
                smmTMCallListTable::updateCallListTable(smmTMCallListTable.CallListId, smmTMCallListTable.ContactPersonId, reasonId.text());

                // Refresh datasource on the caller form
                formDataSource.reread();

                super();
            }
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
		</DataControls>
	</SourceCode>
