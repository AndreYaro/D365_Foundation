    LedgerIExchAdj ledgerIExchAdj;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (element.args().caller() && element.args().caller() is LedgerIExchAdj)
        {
            ledgerIExchAdj = element.args().caller();
            exchAdj = ledgerIExchAdj.ledgerExchAdj();
        }
        else
        {
            throw error(Error::wrongUseOfFunction(element.name()));
        }

        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>getSelectedLegalEntities</Name>
				<Source><![CDATA[
    private container getSelectedLegalEntities()
    {
        container selectedLegalEntities;

        MultiSelectionHelper multiSelectionHelper = MultiSelectionHelper::construct();
        multiSelectionHelper.parmDatasource(this.CompanyInfo_ds);

        CompanyInfo companyInfoLocal = multiSelectionHelper.getFirst();

        while (companyInfoLocal)
        {
            selectedLegalEntities += companyInfoLocal.DataArea;

            companyInfoLocal = multiSelectionHelper.getNext();
        }

        return selectedLegalEntities;
    }

]]></Source>
			</Method>
