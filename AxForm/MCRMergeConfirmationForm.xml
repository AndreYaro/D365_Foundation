	<Name>MCRMergeConfirmationForm</Name>
	<SourceCode>
		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
/// <summary>
///     The <c>MCRMergeConfirmationForm</c> form displays the two customers seleced for a merge.
/// </summary>
/// <remarks>
///     A user will confirm the swap, or possibly flip
///     the survivor or phantom records.
/// </remarks>
public class MCRMergeConfirmationForm extends FormRun
{
    CustAccount mergeFromCust, mergeToCust;
    CustAccount survivorAccount;
}
]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        element.args().parm(survivorAccount);

        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Obtains values from the arguments.
    /// </summary>
    public void init()
    {
        MCRMergeConfirmationConfigure mergeConfirmationConfigure;

        if (element.args().parmObject()
            && classIdGet(element.args().parmObject()) == classIdGet(mergeConfirmationConfigure))
        {
            mergeConfirmationConfigure = element.args().parmObject();
            mergeFromCust = mergeConfirmationConfigure.parmMergedFromCust();
            MergeToCust = mergeConfirmationConfigure.parmMergedToCust();
        }

        super();
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>CustTableMergedFrom</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Selects on the merge from datasource.
        /// </summary>
        public void executeQuery()
        {
            QueryBuildDataSource queryBuildDataSource;

            queryBuildDataSource = CustTableMergedFrom_ds.query().dataSourceTable(tableNum(CustTable));
            queryBuildDataSource.clearRanges();

            queryBuildDataSource.addRange(fieldNum(CustTable, AccountNum)).value(mergeFromCust);

            super();
        }

]]></Source>
					</Method>
				</Methods>
				<Fields />
			</DataSource>
			<DataSource>
				<Name>CustTableMergedTo</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Selects on the merge to datasource.
        /// </summary>
        public void executeQuery()
        {
            QueryBuildDataSource queryBuildDataSource;

            queryBuildDataSource = CustTableMergedTo_ds.query().dataSourceTable(tableNum(CustTable));
            queryBuildDataSource.clearRanges();

            queryBuildDataSource.addRange(fieldNum(CustTable, AccountNum)).value(mergeToCust);
            super();
        }

]]></Source>
					</Method>
				</Methods>
				<Fields />
			</DataSource>
		</DataSources>
		<DataControls xmlns="">
			<Control>
				<Name>SwitchButton</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Swaps the merge from and merge to customer and refreshes the form.
        /// </summary>
        void clicked()
        {
            CustAccount tempForSwap;

            super();

            tempForSwap   = mergeToCust;
            mergeToCust   = mergeFromCust;
            mergeFromCust = tempForSwap;

            CustTableMergedFrom_ds.executeQuery();
            CustTableMergedTo_ds.executeQuery();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>ApplyMerge</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Calls merge logic, with one final confirmation or warning dialog.
        /// </summary>
        void clicked()
        {
            DialogButton db;

            super();

            db = box::okCancel(strfmt("@MCR12281", CustTableMergedFrom.AccountNum, CustTableMergedTo.AccountNum),
                                    DialogButton::Ok, "@SYS866");

            if (db == dialogButton::Cancel)
            {
                return;
            }

            // Call merge logic
            MCRCustomerMerge::Applymerge(CustTableMergedFrom.AccountNum, CustTableMergedTo.AccountNum);

            // Return the survivor, set in close method
            survivorAccount = CustTableMergedTo.AccountNum;

            element.close();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
		</DataControls>
	</SourceCode>
