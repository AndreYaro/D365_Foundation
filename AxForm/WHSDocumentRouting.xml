			<Method>
				<Name>queryUpdated</Name>
				<Source><![CDATA[
    public void queryUpdated(FormRun _sender, container _packedQuery)
    {
        whsDocumentRoutingTable.Query = _packedQuery;
        whsDocumentRoutingTable.update();
        whsDocumentRoutingTable_ds.research(true);
    }

]]></Source>
			</Method>
			<Method>
				<Name>enableQueryButtons</Name>
				<Source><![CDATA[
    /// <summary>
    /// Determines whether or not the query buttons should be enabled on the form
    /// </summary>
    /// <param name="_forceEnable">
    /// Forces the query buttons to become enabled
    /// </param>
    public void enableQueryButtons(boolean _forceEnable = false)
    {
