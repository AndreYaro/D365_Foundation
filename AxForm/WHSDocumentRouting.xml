				<Methods>
					<Method>
						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            boolean ret = true;

            switch (this.valueStr())
            {
                case enum2str(WHSWorkTransType::CycleCount):
                    warning(strFmt("@WAX4786", enum2value(WHSWorkTransType::CycleCount)));
                    ret = false;
                    break;

                case enum2str(WHSWorkTransType::WorkCancel):
                    warning(strFmt("@WAX4786", enum2value(WHSWorkTransType::WorkCancel)));
                    ret = false;
                    break;
            }

            if (ret)
            {
                ret = super();

                workTransTypeSelect = this.valueStr();
                workTransTypeRange.value(workTransTypeSelect);
                whsDocumentRoutingTable_ds.executeQuery();
            }

            return ret;
        }

]]></Source>
