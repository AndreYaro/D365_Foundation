				<Source><![CDATA[
    private void initQueryFormParts()
    {
        if (whsDocumentRoutingTable)
        {
            QueryRun defaultQuery = new QueryRun(whsDocumentRoutingTable.Query);
            str fieldNameQuery = fieldId2Name(tableNum(whsDocumentRoutingTable), fieldNum(whsDocumentRoutingTable, Query));
            headerRangePart = WHSQueryHelper::initializeQueryFormPart(element, WHSDocumentRoutingQueryPart_Range, fieldNameQuery, '', null, defaultQuery, true, false);
        }
    }

]]></Source>
