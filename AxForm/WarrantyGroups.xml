{
    boolean isCalledFromRetail;

}
]]></Source>
			</Method>
			<Method>
				<Name>getPickerInitParameters</Name>
				<Source><![CDATA[
    public RetailOperatingUnitPickerData getPickerInitParameters()
    {
        // no need to pass already selected orgs so we just pass the hierarchy purpose as an init parameter for the picker.
        return RetailOperatingUnitPickerData::createInstance(HierarchyPurpose::RetailAssortment);
    }

]]></Source>
			</Method>
			<Method>
				<Name>processOperatingUnitPickerData</Name>
				<Source><![CDATA[
    void processOperatingUnitPickerData(RetailOperatingUnitPickerData _operatingUnitPickerData)
    {
        boolean hasChannelAdded =
            WarrantyGroupFacade::addApplicableChannelsFromPicker(
                _operatingUnitPickerData,
                WarrantyGroupHeader);

        if (hasChannelAdded)
        {
            RetailWarrantyApplicableChannel_ds.research(true);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateFormDesign</Name>
				<Source><![CDATA[
    /// <summary>
    /// Update controls based on current data in the form.
    /// </summary>
    internal void updateFormDesign()
    {
        boolean isEditable = false;
        
        switch (WarrantyGroupHeader.Status)
        {
            case WarrantyGroupStatusType::Draft:
                // Warranty group header must be saved first
                if (WarrantyGroupHeader.RecId)
                {
                    isEditable = true;
                }
                break;

            case WarrantyGroupStatusType::Published:
            default:
                isEditable = false;
                break;
        }

        if (isEditable)
        {
            element.design().viewEditMode(ViewEditMode::Edit);
        }
        else
        {
            element.design().viewEditMode(ViewEditMode::Auto);
        }

        PublishButton.enabled(isEditable);
        RevertButton.enabled(isEditable);

        WarrantyGroupData_ds.allowCreate(isEditable);
        WarrantyGroupData_ds.allowEdit(isEditable);
        WarrantyGroupData_ds.allowDelete(isEditable);

        WarrantyApplicableProduct_ds.allowCreate(isEditable);
        WarrantyApplicableProduct_ds.allowEdit(isEditable);
        WarrantyApplicableProduct_ds.allowDelete(isEditable);

        RetailWarrantyApplicableChannel_ds.allowCreate(isEditable);
        RetailWarrantyApplicableChannel_ds.allowEdit(isEditable);
        RetailWarrantyApplicableChannel_ds.allowDelete(isEditable);
        ApplicableChannelsButtoAddLine.enabled(isEditable);
    }

]]></Source>
			</Method>
			<Method>
				<Name>task</Name>
				<Source><![CDATA[
    public int task(int _taskId)
    {
        #Task

        int ret;

        DialogButton dialogButton;

        if (_taskId == #TaskEditRecord && 
            WarrantyGroupHeader.Status ==  WarrantyGroupStatusType::Published)
        {
            dialogButton = Box::yesNo("@Warranty:SetWarrantyGroupDraftConfirmation",
                DialogButton::No,
                "@Warranty:SetWarrantyGroupDraftDialog");
            if (dialogButton == DialogButton::Yes)
            {
                WarrantyGroupFacade::setWarrantyGroupToDraft(WarrantyGroupHeader);
                WarrantyGroupHeader_ds.research(true);
                element.updateFormDesign();
            }
            else
            {
                // Avoid going into edit mode.
                return 0;
            }
        }
    
        ret = super(_taskId);
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        isCalledFromRetail = 
            isConfigurationkeyEnabled(configurationKeyNum(Retail)) &&
            this.args().menuItemName() == menuItemDisplayStr(RetailWarrantyGroups);

        super();

        if (isCalledFromRetail)
        {
            ApplicableChannels.visible(true);
        }

        if (element.args().record() is WarrantyGroupHeader)
        {
            WarrantyGroupHeader_ds.query().dataSourceTable(tableNum(WarrantyGroupHeader)).addDynalink(
                fieldNum(WarrantyGroupHeader, RecId),
                element.args().record(),
                fieldNum(WarrantyGroupHeader, RecId));
        }

        // make sure form is in propert status at the start when active is not yet called.
        this.updateFormDesign();
    }

]]></Source>
			</Method>
