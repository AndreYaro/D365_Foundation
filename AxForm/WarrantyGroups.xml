				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            DialogButton dialogButton;

            #task

            dialogButton = Box::yesNo(
                "@Warranty:PublishWarrantyGroupConfirmation", 
                DialogButton::No, 
                "@Warranty:PublishWarrantyGroupDialog");
            if (dialogButton == DialogButton::Yes)
            {
                WarrantyGroupFacade::publishWarrantyGroup(WarrantyGroupHeader);
                WarrantyGroupHeader_ds.research(true);
            }

            super();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>RevertButton</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            DialogButton dialogButton;

            if (WarrantyGroupHeader.Status == WarrantyGroupStatusType::Draft)
            {
                dialogButton = Box::yesNo("@Warranty:RevertWarrantyGroupConfirmation", DialogButton::No, "@Warranty:RevertWarrantyGroupDialog");
                if (dialogButton == DialogButton::Yes)
                {
                    WarrantyGroupFacade::revertWarrantyGroup(WarrantyGroupHeader);

                    WarrantyGroupHeader_ds.research(true);
                    element.updateFormDesign();
                }
            }

            super();
        }

]]></Source>
					</Method>
