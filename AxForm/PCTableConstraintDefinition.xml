					<Method>
						<Name>validateDelete</Name>
						<Source><![CDATA[
        public boolean validateDelete()
        {
            boolean ret = true;
            PCTableConstraint       tableConstraint;

            select firstonly RecId from tableConstraint
                where tableConstraint.TableConstraintDefintion == tableConstraintDefinition.RecId;

            if (tableConstraint.RecId != 0)
            {
                ret = checkFailed("@SYS309778");
            }
            else
            {
                ret = super();
            }

            return ret;
        }

]]></Source>
