				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            Args args = new Args();

            super();

            if (tableConstraintDefinition.RecId != 0)
            {
                args.record(PCTableConstraintDefinition::find(tableConstraintDefinition.RecId, true));

                if (tableConstraintDefinition is PCGlobalTableConstraintDefinition)
                {
                    new MenuFunction(menuitemDisplayStr(PCTableConstraintContentUserDefined), MenuItemType::Display).run(args);
                }
                else
                {
                    new MenuFunction(menuitemDisplayStr(PCTableConstraintColumnSystem), MenuItemType::Display).run(args);
                }
            }
        }

]]></Source>
