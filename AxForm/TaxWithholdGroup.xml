			<Method>
				<Name>buildJumprefQuery</Name>
				<Source><![CDATA[
    public void buildJumprefQuery(TaxWithholdGroup         _taxWithholdGroup,
                                  TaxWithholdCodeType_IN   _taxType)
    {
        QueryBuildDataSource            queryBuildDataSource;
        Query                           query;

        query = new Query();
        if (companyId)
        {
            query.allowCrossCompany(true);
            query.addCompanyRange(companyId);
        }
        queryBuildDataSource = query.addDataSource(tableNum(TaxWithholdGroupHeading));
        if (_taxWithholdGroup)
        {
            queryBuildDataSource.addRange(fieldNum(TaxWithholdGroupHeading, TaxWithholdGroup)).value(queryValue(_taxWithholdGroup));
        }
        queryBuildDataSource.addRange(fieldNum(TaxWithholdGroupHeading, TaxType_IN)).value(queryValue(_taxType));
        taxWithholdGroupHeading_DS.query(query);
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        // <GIN>
        Args args                       = new Args();
        Common                          record;
        LedgerJournalTrans              ledgerJournalTrans;
        boolean                         showIndia   = TaxWithholdParameters_IN::checkTaxParameters();

        showTDS = TaxWithholdParameters_IN::isTDSActivate();
        if (TaxWithholdParameters_IN::checkTaxParameters() &&
            element.args().parmEnumType())
        {
            record              = element.args().record();
            if (tableNum(LedgerJournalTrans) == element.args().dataset())
            {
                ledgerJournalTrans.data(record);
            }
            companyId = ledgerJournalTrans ? ledgerJournalTrans.Company : curext();
        }
        // </GIN>
        super();

        tmpTaxWithholdList = new RecordSortedList(tableNum(TmpTaxWithhold));
        tmpTaxWithholdList.sortOrder(fieldNum(TmpTaxWithhold, TaxWithholdCode));

        // <GIN>
        taxType_IN.visible(showIndia);
        taxWithholdGroupHeading_ds.object(fieldNum(TaxWithholdGroupHeading, TaxType_IN)).visible(showIndia);
        taxWithholdGroupData_OverlookTax_IN.visible(showIndia);
        taxWithholdGroupData_Exempt_IN.visible(showIndia);

        Transporter_IN.visible(showTDS);
        TaxWithholdGroupHeading_ds.object(fieldnum(TaxWithholdGroupHeading, IsTransporter_IN)).visible(showTDS);

        // To generate the TaxType lookup
        GTEEnable = TaxSolutionScopeIntegrationUtil::isCompanyEnabled();
        if (showIndia)
        {
            TaxWithholdValue.noOfDecimals(5);

            excludeIndirectTaxCtrl = SysLookupMultiSelectCtrl::construct(element, ExcludeIndirectTax_IN, queryStr(TaxTypeLookup_IN),false,[tableNum(TaxTypeTable_IN), fieldNum(TaxTypeTable_IN, TaxType)]);
            // <GTE>
            if (GTEEnable)
            {
                IncludeIndirectTax_IN.visible(true);
                includeIndirectTaxCtrl = SysLookupMultiSelectCtrl::construct(element, IncludeIndirectTax_IN, queryStr(TaxRuntimeComponentLookupQuery), false, [tableNum(TaxRuntimeComponent), fieldNum(TaxRuntimeComponent, Name)]);
            }
            // </GTE>
        }
        // </GIN>
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateLookupControl_IN</Name>
				<Source><![CDATA[
    /// <summary>
    /// This method sets the previously selected tax type values to the SysLookupMultiLookupControl
    /// </summary>
    /// <remarks>
    /// Recid and TaxTypes are populated to the container.
    /// </remarks>
    public void updateLookupControl_IN()
    {
        container localExcludedIndirectTax;
        container localIncludedIndirectTax;

        excludeIndirectTaxCtrl.set(conNull());
        localExcludedIndirectTax = TaxWithholdExcludeIndirectTax_IN::getExcludedIndirectTaxContainer(taxWithholdGroupHeading.Recid);
        if (conPeek(localExcludedIndirectTax, 1) != conNull())
        {
            excludeIndirectTaxCtrl.set(localExcludedIndirectTax);
        }
        
        // <GTE>
        if (GTEEnable)
        {
            includeIndirectTaxCtrl.set(conNull());
            localIncludedIndirectTax = TaxWithholdIncIndirectTaxComponentsUtil::getIncludedTaxComponentsContainer(taxWithholdGroupHeading.Recid);
      
            if (conPeek(localIncludedIndirectTax, 1) != conNull())
            {
                includeIndirectTaxCtrl.set(localIncludedIndirectTax);
            }
        }
        // </GTE>
    }

]]></Source>
