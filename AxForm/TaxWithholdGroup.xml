					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            boolean isMarked = TaxWithholdParameters_IN::checkTaxParameters();
            int ret;

            ret = super();
            taxWithholdGroupData_DS.object(fieldNum(TaxWithholdGroupData, OverlookTax_IN)).visible((taxWithholdGroupHeading.TaxType_IN != TaxWithholdCodeType_IN::None) && isMarked);
            taxWithholdGroupData_DS.object(fieldNum(TaxWithholdGroupData, Exempt_IN)).visible((taxWithholdGroupHeading.TaxType_IN != TaxWithholdCodeType_IN::None) && isMarked);
            designerButton_IN.enabled((taxWithholdGroupHeading.TaxType_IN != TaxWithholdCodeType_IN::None) && isMarked);

            if (ret)
            {
                TaxWithholdGroupHeading_ds.object(fieldnum(TaxWithholdGroupHeading, IsTransporter_IN)).enabled(
                    (taxWithholdGroupHeading.TaxType_IN == TaxWithholdCodeType_IN::TDS) && showTDS);
                TaxWithholdGroupHeading_ds.object(fieldnum(TaxWithholdGroupHeading, ExcludeCharges_IN)).visible((taxWithholdGroupHeading.TaxType_IN != TaxWithholdCodeType_IN::None) && isMarked);
                ExcludeIndirectTax_IN.visible(taxWithholdGroupHeading.TaxType_IN != TaxWithholdCodeType_IN::None && isMarked);
                TaxWithholdGroupHeading_ds.object(fieldnum(TaxWithholdGroupHeading, ExcludeCharges_IN)).enabled((taxWithholdGroupHeading.TaxType_IN != TaxWithholdCodeType_IN::None) && isMarked);
                ExcludeIndirectTax_IN.enabled(taxWithholdGroupHeading.TaxType_IN != TaxWithholdCodeType_IN::None && isMarked);
                General_IN.visible(taxWithholdGroupHeading.TaxType_IN != TaxWithholdCodeType_IN::None && isMarked);

                TaxWithholdGroupHeading_ds.object(fieldNum(TaxWithholdGroupHeading, TaxLiabilityOnPayment_IN)).visible((taxWithholdGroupHeading.TaxType_IN == TaxWithholdCodeType_IN::TCS) && isMarked);

                if (isMarked)
                {
                    element.updateLookupControl_IN();
                }
            }
            return ret;
        }

]]></Source>
