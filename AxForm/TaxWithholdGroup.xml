						<Name>write</Name>
						<Source><![CDATA[
        public void write()
        {
            //<GIN>
            TaxWithholdExcludeIndirectTax_IN        taxWithholdExcludeIndirectTax;
            container                               taxTypeContainer;
            boolean                                 showIndia   = TaxWithholdParameters_IN::checkTaxParameters();
            TaxWithholdIncIndirectTaxComponent_IN   taxWithholdIncludeComponentTax;
            container                               componentRecIdContainer;

            if (showIndia)
            {
                taxTypeContainer = excludeIndirectTaxCtrl.getSelectedFieldValues();
                taxWithholdExcludeIndirectTax.writeExcludedTaxes(taxTypeContainer, excludedIndirectTaxCtrlText, taxWithholdGroupHeading.RecId);
                
                // <GTE>
                if (GTEEnable)
                {
                    componentRecIdContainer = includeIndirectTaxCtrl.get();
                    taxWithholdIncludeComponentTax.writeIncludedTaxComponents(componentRecIdContainer, includedIndirectTaxCtrlText, taxWithholdGroupHeading.RecId);
                }
                // </GTE>

                if (taxWithholdGroupHeading_ds.forceWrite(true))
                {
                    taxWithholdGroupHeading_ds.forceWrite(false);
                }
            }
            //</GIN>
            super();
        }

]]></Source>
