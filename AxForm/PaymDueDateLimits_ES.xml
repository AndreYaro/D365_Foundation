						<Source><![CDATA[
        public int active()
        {
            int ret = super();

            if (dueDateLimit.GroupId)
            {
                PaymDueDateLimitValues_ES defaultValue = PaymDueDateLimitValues_ES::find(dueDateLimit.GroupId, DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone()), true);

                if (!defaultValue)
                {
                    defaultValue.GroupId    = dueDateLimit.GroupId;
                    defaultValue.PeriodUnit = dueDateLimit.PeriodUnit;
                    defaultValue.QtyUnit    = dueDateLimit.QtyUnit;
                    defaultValue.insert();
                    dueDateLimitValues_ds.executeQuery();
                }

                dueDateLimitValues_ds.allowCreate(true);
                dueDateLimitValues_ds.allowEdit(true);
                dueDateLimitValues_ds.allowDelete(true);
            }
            else
            {
                dueDateLimitValues_ds.allowCreate(false);
                dueDateLimitValues_ds.allowEdit(false);
                dueDateLimitValues_ds.allowDelete(false);
            }

            return ret;
        }

]]></Source>
