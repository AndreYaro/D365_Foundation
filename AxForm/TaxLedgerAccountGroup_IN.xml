			<Method>
				<Name>checkTaxType</Name>
				<Source><![CDATA[
    boolean checkTaxType()
    {
        TaxParameters taxParameters = TaxParameters::find();
        boolean       ret;

        if (taxParameters.vat_in    || taxParameters.SalesTax_IN ||
           taxParameters.Excise_IN || taxParameters.ServiceTax_IN || taxParameters.Customs_IN)
        {
            ret = true;
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        if (!this.checkTaxType())
        {
            element.close();
        }
        registrationNumber  = taxLedgerAccountSetup_TaxRegistrationNumberTable;
        salesTax.visible(false);
        excise.visible(false);
        serviceTax.visible(false);
        customs.visible(false);

        ledgerDimension.parmPostingType(LedgerPostingType::None);
    }

]]></Source>
			</Method>
			<Method>
				<Name>setLookupButton</Name>
				<Source><![CDATA[
    void setLookupButton()
    {
        registrationNumber.allowEdit(true);
        registrationNumber.skip(false);

        if (taxLedgerAccountSetup.AccountCode == TaxAccountCode_IN::All)
        {
            registrationNumber.allowEdit(false);
            registrationNumber.skip(true);
            taxLedgerAccountSetup.TaxRegistrationNumberTable = 0;
            taxLedgerAccountSetup_ds.rereadReferenceDataSources();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>showHideLedgerAccount</Name>
				<Source><![CDATA[
    void showHideLedgerAccount()
    {
        switch (taxLedgerAccountGroup.TaxType)
        {
            case TaxTypeComponent_IN::VAT:
                vat.visible(true);
                salesTax.visible(false);
                excise.visible(false);
                serviceTax.visible(false);
                customs.visible(false);
                break;

            case TaxTypeComponent_IN::SalesTax:
                vat.visible(false);
                salesTax.visible(true);
                excise.visible(false);
                serviceTax.visible(false);
                customs.visible(false);
                break;

            case TaxTypeComponent_IN::Excise:
                vat.visible(false);
                salesTax.visible(false);
                excise.visible(true);
                serviceTax.visible(false);
                customs.visible(false);
                break;

            case TaxTypeComponent_IN::ServiceTax:
                vat.visible(false);
                salesTax.visible(false);
                excise.visible(false);
                serviceTax.visible(true);
                customs.visible(false);
                break;

            case TaxTypeComponent_IN::Customs:
                vat.visible(false);
                salesTax.visible(false);
                excise.visible(false);
                serviceTax.visible(false);
                customs.visible(true);
                break;
        }
    }

]]></Source>
