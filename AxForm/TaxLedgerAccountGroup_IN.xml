						<Name>initValue</Name>
						<Source><![CDATA[
        public void initValue()
        {
            super();
            switch (taxLedgerAccountGroup.TaxType)
            {
                case TaxTypeComponent_IN::VAT:
                    taxLedgerAccounts.AccountType = TaxLedgerAccounts_IN::accountTypeVAT2AccountType(vat.selection());
                    break;
                case TaxTypeComponent_IN::SalesTax:
                    taxLedgerAccounts.AccountType = TaxLedgerAccounts_IN::accountTypeSales2AccountType(salesTax.selection());
                    break;
                case TaxTypeComponent_IN::ServiceTax:
                    taxLedgerAccounts.AccountType = TaxLedgerAccounts_IN::accountTypeService2AccountType(serviceTax.selection());
                    break;
                case TaxTypeComponent_IN::Excise:
                    taxLedgerAccounts.AccountType = TaxLedgerAccounts_IN::accountTypeExcise2AccountType(excise.selection());
                    break;
                case TaxTypeComponent_IN::Customs:
                    taxLedgerAccounts.AccountType = TaxLedgerAccounts_IN::accountTypeCustoms2AccountType(customs.selection());
                    break;
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            TaxAccountType_IN       accountType;
            Query                   query;
            QueryBuildDataSource    qbD;

            switch (taxLedgerAccountGroup.TaxType)
            {
                case TaxTypeComponent_IN::VAT:
                    accountType = TaxLedgerAccounts_IN::accountTypeVAT2AccountType(vat.selection());
                    break;
                case TaxTypeComponent_IN::SalesTax:
                    accountType = TaxLedgerAccounts_IN::accountTypeSales2AccountType(salesTax.selection());
                    break;
                case TaxTypeComponent_IN::ServiceTax:
                    accountType = TaxLedgerAccounts_IN::accountTypeService2AccountType(serviceTax.selection());
                    break;
                case TaxTypeComponent_IN::Excise:
                    accountType = TaxLedgerAccounts_IN::accountTypeExcise2AccountType(excise.selection());
                    break;
                case TaxTypeComponent_IN::Customs:
                    accountType = TaxLedgerAccounts_IN::accountTypeCustoms2AccountType(customs.selection());
                    break;
            }

            query   = new Query();
            qbD     = query.addDataSource(tablenum(TaxLedgerAccounts_IN));
            qbD.addRange(fieldnum(TaxLedgerAccounts_IN, AccountType)).value(queryValue(accountType));
            qbD.addRange(fieldnum(TaxLedgerAccounts_IN, TaxLedgerAccountSetup)).value(queryValue(taxLedgerAccountSetup.RecId));
            this.query(query);

            super();
        }

]]></Source>
