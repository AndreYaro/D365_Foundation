				<Source><![CDATA[
[Form]
public class RetailHardwareConfiguration extends FormRun
{
    private RetailHardwareProfileId hardwareProfileId;
    private RecId hardwareConfigurationRecId;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (this.args() && this.args().record())
        {
            Common record = this.args().record();
            if (record.TableId == tableNum(RetailStoreHardwareStationTable))
            {
                RetailStoreHardwareStationTable storeHardwareStation = record;
                RetailStoreTable store = RetailStoreTable::findRecId(storeHardwareStation.Store);

                // Update header fields based on store hardware station.
                StoreNumberControl.text(store.StoreNumber);
                StoreNumberControl.visible(true);
                StoreHardwareStationDescriptionControl.text(storeHardwareStation.Description);
                StoreHardwareStationDescriptionControl.visible(true);

                hardwareProfileId = storeHardwareStation.HardwareProfileId;
                hardwareConfigurationRecId = RetailHardwareConfiguration::findOrCreateByHardwareStation(storeHardwareStation.RecId).RecId;
            }
            else if (record.TableId == tableNum(RetailTerminalTable))
            {
                RetailTerminalTable terminal = record;

                // Update header fields based on register.
                TerminalIdControl.text(terminal.terminalId);
                TerminalIdControl.visible(true);

                hardwareProfileId = terminal.hardwareProfile;
                hardwareConfigurationRecId = RetailHardwareConfiguration::findOrCreateByTerminal(terminal.terminalId).RecId;
            }
            else
            {
                error("@Retail:RetailHardwareConfigurationFormInvalidRequest");
            }

            if (!hardwareProfileId || !hardwareConfigurationRecId)
            {
                error("@Retail:RetailHardwareConfigurationFormInvalidRequest");
            }
        }
        else
        {
            error("@Retail:RetailHardwareConfigurationFormInvalidRequest");
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>UpdateHardwareControls</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates the hardware controls based device types.
    /// </summary>
    private void UpdateHardwareControls()
    {
        PrinterIPAddress.enabled(RetailHardwareProfile.printer == RetailPeripheralType::Network);
        PrinterPort.enabled(RetailHardwareProfile.printer == RetailPeripheralType::Network);
        Printer2IPAddress.enabled(RetailHardwareProfile.printer2 == RetailPeripheralType::Network);
        Printer2Port.enabled(RetailHardwareProfile.printer2 == RetailPeripheralType::Network);
        DrawerIPAddress.enabled(RetailHardwareProfile.drawer == RetailDrawer::Network);
        DrawerPort.enabled(RetailHardwareProfile.drawer == RetailDrawer::Network);
        Drawer2IPAddress.enabled(RetailHardwareProfile.drawer2 == RetailDrawer::Network);
        Drawer2Port.enabled(RetailHardwareProfile.drawer2 == RetailDrawer::Network);
        PinpadIPAddress.enabled(RetailHardwareProfile.pinpad == RetailPinpadType::Network);
        PinpadPort.enabled(RetailHardwareProfile.pinpad == RetailPinpadType::Network);
    }

]]></Source>
			</Method>
