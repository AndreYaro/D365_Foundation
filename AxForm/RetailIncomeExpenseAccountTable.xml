            }
        }
        else if (element.args().record() && element.args().record().TableId == tablenum(RetailIncomeExpenseAccountTable))
        {
            RetailIncomeExpenseAccountTable record = element.args().record() as RetailIncomeExpenseAccountTable;
            QueryBuildDataSource queryDS = RetailIncomeExpenseAccountTable_ds.query().dataSourceTable(tableNum(RetailIncomeExpenseAccountTable));

            queryDS.addRange(fieldNum(RetailIncomeExpenseAccountTable, storeId)).value(queryValue(record.storeId));
            queryDS.addRange(fieldNum(RetailIncomeExpenseAccountTable, AccountNum)).value(queryValue(record.accountNum));
        }
    }

