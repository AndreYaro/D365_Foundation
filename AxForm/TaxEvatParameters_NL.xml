						<Source><![CDATA[
        void executeQuery()
        {
            ;
            if (runExecuteDirect)
            {
                super();
            }
            else
            {
                runExecuteDirect = true;
                this.queryRun(NumberSeqReference::buildQueryRunMulti(numberSequenceReference,
                                                                     tmpIdRef,
                                                                     numberSequenceTable,
                                                                     numberSequenceModules,
                                                                     scope));

                numbersequenceReference_ds.research();

                if (new MultipleTaxIdICPReportToggle().isEnabled())
                {
                    this.queryRun().query().dataSourceName('NumbersequenceReference_1')
                        .addRange(fieldNum(NumbersequenceReference, NumberSequenceDatatype))
                            .value(SysQuery::valueNot(NumberSequenceDatatype::findByDatatypeId(extendedtypenum(TaxTurnOverID)).RecId));

                    numbersequenceReference_ds.research();
                }
            }
        }

