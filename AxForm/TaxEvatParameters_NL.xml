				<Source><![CDATA[
    public void init()
    {
        this.numberSeqPreInit();

        super();
        boolean useAdvCert = SystemParameters::find().UseAdvCertificateStore;
        TaxEvatParameters_NL_ds.object(fieldNum(TaxEvatParameters_NL, CertificateCA)).visible(!useAdvCert);
        TaxEvatParameters_NL_ds.object(fieldNum(TaxEvatParameters_NL, CertificateAuth)).visible(!useAdvCert);
        TaxEvatParameters_NL_ds.object(fieldNum(TaxEvatParameters_NL, KeyVaultCertificateCA)).visible(useAdvCert);
        TaxEvatParameters_NL_ds.object(fieldNum(TaxEvatParameters_NL, KeyVaultCertificateAuth)).visible(useAdvCert);
        CompanyInfo::find();
        TaxEvatParameters_NL::find();

        ElectronicReporting.visible(!new MultipleTaxIdICPReportToggle().isEnabled());

        this.numberSeqPostInit();
    }

]]></Source>
