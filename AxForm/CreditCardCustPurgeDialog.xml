				<Source><![CDATA[
[Form]
public class CreditCardCustPurgeDialog extends FormRun
{
    private utcdatetime startDate = DateTimeUtil::minValue();
    private utcdatetime endDate = DateTimeUtil::minValue();

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    [Hookable(false)]
    public void init()
    {
        super();

        endDate = DateTimeUtil::addDays(DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone()), -any2Int(element.args().parm()));
        int numOfTokens = CreditCardCustPurge::getPurgeEligibleRecordCount(startDate, endDate);

        CreditCardCustPurgeMessage.text(strFmt("@AccountsReceivable:CreditCardCustPurgeDialogText", numOfTokens));
    }

]]></Source>
