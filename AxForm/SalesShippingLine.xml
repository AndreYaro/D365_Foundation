			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        super();

        // Refresh the SalesShippingStat data source on the caller.
        if (FormDataUtil::isFormDataSource(salesParmTable))
        {
            FormRun formRun = element.args().caller() as FormRun;

            if (formRun)
            {
                FormDataSource formDataSource = formRun.dataSource(tablestr(SalesShippingStat));

                if (formDataSource)
                {
                    formDataSource.research(true);
                }
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        super();

        salesParmTable = element.args().record();

        Query queryParmid = new Query();
        queryParmid.addDataSource(tablenum(SalesShippingStat));
        queryParmid.dataSourceTable(tablenum(SalesShippingStat)).addRange(fieldnum(SalesShippingStat, ParmId)).value(queryValue(salesParmTable.ParmId));
        queryParmid.dataSourceTable(tablenum(SalesShippingStat)).addRange(fieldnum(SalesShippingStat, TableRefId)).value(queryValue(salesParmTable.TableRefId));

        salesShippingStat_ds.query(queryParmid);
    }

]]></Source>
