				<Methods>
					<Method>
						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            boolean ret = super();
            MCRChannelUser locChannelUser;

            if (ret)
            {
                // if the user Id already exsit, need throw customized error.
                select firstonly locChannelUser where locChannelUser.User == this.text();
                if (locChannelUser)
                {
                    ret = checkFailed(strFmt("@MCR40164", locChannelUser.User, locChannelUser.channelName()));
                }
            }

            return ret;
        }

]]></Source>
