			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        // Show only affiliations, hide the field from filter pane
        QueryBuildDataSource qbds = retailAffiliation_ds.query().dataSourceTable(tableNum(RetailAffiliation));
        QueryBuildRange qbr = qbds.addRange(fieldNum(RetailAffiliation, AffiliationType));
        qbr.value(queryValue(RetailAffiliationType::General));
        qbr.status(RangeStatus::Hidden);

        // Add Name into filter pane
        qbds.addRange(fieldNum(RetailAffiliation, Name));

        if (element.args().record() && element.args().record().TableId == tablenum(RetailAffiliation))
        {
            RetailAffiliation record = element.args().record() as RetailAffiliation;
            qbds.addRange(fieldNum(RetailAffiliation, Name)).value(record.Name);
        }

        this.setPriceGroupsButtonVisibility();
    }

]]></Source>
			</Method>
			<Method>
				<Name>setPriceGroupsButtonVisibility</Name>
				<Source><![CDATA[
    private void setPriceGroupsButtonVisibility()
    {
        MenuItemButtonPriceGroups.visible(!RetailPricingFeatureToggle::isRetailPricingPropertyFeatureEnabled());
    }

]]></Source>
