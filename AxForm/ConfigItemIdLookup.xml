				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class ConfigItemIdLookup extends FormRun
{
    BOMId               bomId;
    ConfigGroupId       configGroupId;
    boolean             constructExclIncl;
    ConfigRuleMethod    method;
    boolean             showInvent;

    FormStringControl   callerControl;
    FormStringControl   selectedControl;
    FormDataSource      selectedFormDataSource;
    boolean             filterLookup;
    Map                 tab2ComboBoxItemMap;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        super();

        callerControl   = SysTableLookup::getCallerStringControl(element.args());

        if (! bomId)
        {
            throw error(strFmt("@SYS22800",element.name()));
        }

        if (! configGroupId)
        {
            throw error(strFmt("@SYS27846",element.name()));
        }

        element.selectMode(bom_ItemId);
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    void run()
    {
        this.setSelectMode();

        filterLookup = SysTableLookup::filterLookupPreRun_2DS(callerControl,
                                                              selectedControl,
                                                              bom_ds,
                                                              inventTable_ds);

        super();

        SysTableLookup::filterLookupPostRun(filterLookup,
                                            callerControl.text(),
                                            selectedControl,
                                            selectedFormDataSource);

        // Generate view combobox based on tabs
        tab2ComboBoxItemMap = SysLookup::tab2ComboBox(Tab, switchView);
        switchView.visible(showInvent);
    }

]]></Source>
			</Method>
			<Method>
				<Name>setBOMId</Name>
				<Source><![CDATA[
    void setBOMId(BOMId _bomId)
    {
        bomId = _bomId;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setConfigGroupId</Name>
				<Source><![CDATA[
    void setConfigGroupId(ConfigGroupId _groupId)
    {
        configGroupId = _groupId;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setMethod</Name>
				<Source><![CDATA[
    void setMethod(ConfigRuleMethod    _method)
    {
        method              = _method;
        constructExclIncl   = true;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setSelectMode</Name>
				<Source><![CDATA[
    void setSelectMode()
    {
        #define.tabPageBOM(1)
        #define.tabPageInvent(2)

        void setSelectedControlAndDataSource(FormStringControl  _selectedControl,
                                             FormDataSource     _selectedFormDataSource)
        {
            selectedControl         = _selectedControl;
            selectedFormDataSource  = _selectedFormDataSource;
        }

        switch (tab.tab())
        {
            case #tabPageInvent :
                element.selectMode(inventTable_ItemId);
                inventTable_ItemId.setFocus();

                setSelectedControlAndDataSource(inventTable_ItemId, inventTable_ds);
                break;

            case #tabPageBOM    :
            default             :
                element.selectMode(bom_itemId);
                bom_itemId.setFocus();

                setSelectedControlAndDataSource(bom_itemId, bom_ds);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>setShowInvent</Name>
				<Source><![CDATA[
    void setShowInvent(boolean show)
    {
        showInvent = show;
    }

]]></Source>
			</Method>
