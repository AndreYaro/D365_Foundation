			<DataSource>
				<Name>BOM</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        void executeQuery()
        {
            Query query;

            if (constructExclIncl)
            {
                query = new Query(ConfigExcludeInclude::construct(method).configItemIdLookupQuery(
                    bomId,
                    configGroupId));
            }
            else
            {
                query = new Query(new ConfigExclIncl_1Level().configItemIdLookupQuery(
                    bomId,
                    configGroupId));
            }

            this.query(query);

            super();
        }

]]></Source>
					</Method>
				</Methods>
