				<Source><![CDATA[
    public void closeOk()
    {
        InventJournalTable.InventDimFixed = inventDimParm.inventDimFixed();
        InventJournalTable_ds.write();

        super();

        saveLast.data(inventDimParm);
        xSysLastValue::saveLast(this);

        InventJournalFormTable journalFormCaller = element.args().caller();
        if (journalFormCaller)
        {
            var journalTableCaller_ds = journalFormCaller.journalTable_DS();
            if (journalTableCaller_ds)
            {
                journalTableCaller_ds.executeQuery();
                if (InventJournalCreatePositionRecordReplacementFlight::instance().isEnabled())
                {
                    journalTableCaller_ds.findRecord(InventJournalTable);
                }
                else
                {
                    journalTableCaller_ds.positionToRecord(InventJournalTable);
                }

                journalFormCaller.switchFormView(FormViewOption::Details);
            }
        }
    }

]]></Source>
