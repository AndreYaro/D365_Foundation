						<Source><![CDATA[
        public int active()
        {
            int ret = super();
            boolean enablePrepayInvoice = false;

            if (purchPrepayOpen.PrepayAvailable >= 0.01 && !VendInvoiceTrans::existForPurchId(purchPrepayOpen.PurchId) && !PurchTable::find(PurchPrepayOpen.PurchId).isInterCompanyOrder())
            {
                enablePrepayInvoice = true;
            }

            buttonUpdatePrepayInvoice.enabled(enablePrepayInvoice);

            return ret;
        }

]]></Source>
