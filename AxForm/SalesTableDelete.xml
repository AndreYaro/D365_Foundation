						<Name>linkActive</Name>
						<Source><![CDATA[
        void linkActive()
        {
            ProjTable       projTable;

            switch (element.args().dataset())
            {
                case(tableNum(ProjTable)):
                    projTable   = element.args().record();
                    criteriaProjId.value(projTable.ProjId+'*');

                    break;
            }

            super();
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        void  init()
        {
            Query                   query;
            QueryBuildDataSource    qB;

            switch (element.args().dataset())
            {
                case(tableNum(ProjTable)):
                    salesTableDelete_ds.autoQuery(false);
                    query   = new Query();
                    qB      = query.addDataSource(tableNum(SalesTableDelete));

                    criteriaProjId      = qB.addRange(fieldNum(SalesTableDelete, ProjId));
                    salesTableDelete_ds.query(query);

                    break;
            }

            super();
        }

]]></Source>
