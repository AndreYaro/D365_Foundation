					<Method>
						<Name>enableFields</Name>
						<Source><![CDATA[
        public void enableFields()
        {
            boolean enableReportFormat;
            boolean enableEFDocumentCity;
            boolean isEFDocument = fiscalDocumentType_BR.isEfDocument();
            boolean hasInvoices = fiscalDocumentType_BR.hasInvoices();

            enableReportFormat   =  fiscalDocumentType_BR_ds.requiresFormat() && fiscalDocumentType_BR.ElectronicInvoice == false;

            enableEFDocumentCity = this.editEFDocument() == false;

            if (!enableReportFormat)
            {
                FiscalDocumentFormat_BR_ds.clearReportFormatFields();
            }

            tabSetupFormat.allowEdit        (enableReportFormat);
            this.setupReportMandatoryFields (enableReportFormat);
            this.adjustEFDocumentCity       (enableEFDocumentCity);
            nFeFederal.enabled              (!fiscalDocumentType_BR.ElectronicInvoice);
            FiscalDocumentType_BR_ElectronicInvoice.enabled(!nFeFederal.checked());

            fiscalDocumentType_BR_ds.object(fieldnum(FiscalDocumentType_BR, FiscalDocumentModel)).allowEdit(!isEFDocument);
            this.enableFiscalDocumentTypeFields(!hasInvoices);
            this.enableFiscalDocumentFormatFields(!hasInvoices);
            FiscalDocumentType_BR_ds.object(fieldNum(FiscalDocumentType_BR, InvoiceIssueDate)).allowEdit(BrazilParameters::find().ChronologicalInvoiceNumbering);
        }

]]></Source>
					</Method>
					<Method>
						<Name>adjustEFDocumentCity</Name>
						<Source><![CDATA[
        public void adjustEFDocumentCity(boolean _enable = true)
        {
            if (!_enable)
            {
                fiscalDocumentType_BR.ElectronicInvoice = NoYes::No;
            }

            if (fiscalDocumentType_BR.ElectronicInvoice == NoYes::No)
            {
                fiscalDocumentFormat_BR.EInvoiceExportLayout = '';
                fiscalDocumentFormat_BR.EInvoiceImportLayout = '';
                FiscalDocumentFormat_BR.ERFormatMapping = nullValueBaseType(Types::Int64);
            }
            else
            {
                fiscalDocumentType_BR.FiscalDocumentModel = FiscalDocModel_BR::getServiceModel().Model;
            }

            fiscalDocumentType_BR_ds.object(fieldnum(FiscalDocumentType_BR, ElectronicInvoice)).allowEdit(_enable);
            fiscalDocumentFormat_BR_ds.object(fieldnum(FiscalDocumentFormat_BR, EInvoiceExportLayout)).allowEdit(FiscalDocumentType_BR.ElectronicInvoice);
            fiscalDocumentFormat_BR_ds.object(fieldnum(FiscalDocumentFormat_BR, EInvoiceImportLayout)).allowEdit(FiscalDocumentType_BR.ElectronicInvoice);
            fiscalDocumentFormat_BR_ds.object(fieldNum(FiscalDocumentFormat_BR, ERFormatMapping)).allowEdit(FiscalDocumentType_BR.ElectronicInvoice);
            fiscalDocumentFormat_BR_ds.object(fieldnum(FiscalDocumentFormat_BR, EInvoiceImportLayout)).mandatory(FiscalDocumentType_BR.ElectronicInvoice);
        }

]]></Source>
