            FiscalDocumentType_BR_ds.object(fieldNum(FiscalDocumentType_BR, ElectronicInvoice))             .allowEdit(_enable);
            FiscalDocumentType_BR_ds.object(fieldNum(FiscalDocumentType_BR, FiscalDocumentModel))           .allowEdit(_enable);
            FiscalDocumentType_BR_ds.object(fieldNum(FiscalDocumentType_BR, FiscalDocumentSeries))          .allowEdit(_enable);
            FiscalDocumentType_BR_ds.object(fieldNum(FiscalDocumentType_BR, FiscalDocumentSpecie))          .allowEdit(_enable);
            FiscalDocumentType_BR_ds.object(fieldNum(FiscalDocumentType_BR, FiscalDocumentTypeId))          .allowEdit(_enable);
            FiscalDocumentType_BR_ds.object(fieldNum(FiscalDocumentType_BR, Name))                          .allowEdit(_enable);
            FiscalDocumentType_BR_ds.object(fieldNum(FiscalDocumentType_BR, FiscalDocumentNumberSequence))  .allowEdit(_enable);

            nFeFederal.allowEdit(_enable);
        }

]]></Source>
