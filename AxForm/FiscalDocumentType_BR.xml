        private void enableFiscalDocumentFormatFields(boolean _enable)
        {
            boolean isElectronicInvoice = FiscalDocumentType_BR.ElectronicInvoice == NoYes::Yes;
            boolean isERFormatMappingEditable = FiscalDocumentFormat_BR_ds.object(fieldNum(FiscalDocumentFormat_BR, ERFormatMapping)).allowEdit();
            Global::allowEditFieldsOnFormDS_W(FiscalDocumentFormat_BR_ds, _enable);
            FiscalDocumentFormat_BR_ds.object(fieldNum(FiscalDocumentFormat_BR, EInvoiceExportLayout)).allowEdit(_enable && isElectronicInvoice);
            FiscalDocumentFormat_BR_ds.object(fieldNum(FiscalDocumentFormat_BR, EInvoiceImportLayout)).allowEdit(_enable && isElectronicInvoice);
            FiscalDocumentFormat_BR_ds.object(fieldNum(FiscalDocumentFormat_BR, ERFormatMapping)).allowEdit(isERFormatMappingEditable);
        }

]]></Source>
