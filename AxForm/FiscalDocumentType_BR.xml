					<Method>
						<Name>editEFDocument</Name>
						<Source><![CDATA[
        public edit NoYes editEFDocument(boolean _set = false, NoYes _noYes = false)
        {
            if (_set)
            {
                if (_noYes)
                {
                    fiscalDocumentType_BR.FiscalDocumentModel = #EFDDocModel;
                }
                else
                {
                    fiscalDocumentType_BR.FiscalDocumentModel = '';
                }

                this.enableFields();
            }

            return fiscalDocumentType_BR.isEfDocument();
        }

]]></Source>
