			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    public boolean canClose()
    {
        boolean ret;

        ret = super() && ((TaxBranchType::checkAllLocationIdFilledIn() && TaxBranchType::checkHeadOfficeSelected())
                || FeatureStateProvider::isFeatureEnabled(TaxInvoiceSerialNumberGenerateIndonesiaFeature::instance()));

        if (!ret)
        {
            taxBranch_ds.research(true);
        }

        return ret;
    }

]]></Source>
