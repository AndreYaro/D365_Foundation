				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();
            if (!TaxThaiGovCertificationFeatureChecker::isTaxBranchParamMarked()
                && !FeatureStateProvider::isFeatureEnabled(TaxInvoiceSerialNumberGenerateIndonesiaFeature::instance()))
            {
                taxBranch_DS.allowCreate(false);
                taxBranch_DS.allowEdit(false);
                taxBranch_DS.allowDelete(false);

                warning(
                    "@SYS4082530",
                    '',
                    SysInfoAction_Formrun::newFormnameControlnameDesc(
                        formStr(LedgerParameters),
                        formControlStr(LedgerParameters, TaxBranch_TH_TaxBranchEnabled_TH),
                        "@SYS4082531"));
            }
        }

]]></Source>
					</Method>
				</Methods>
