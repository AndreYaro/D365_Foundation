]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        addressController = LogisticsPostalAddressFormHandler::newParameters(taxIntervatGeneral_ds, logisticsPostalAddress_ds);
        addressController.callerUpdateQuery(fieldnum(TaxIntervatGeneral,Location));
    }

]]></Source>
			</Method>
			<Method>
				<Name>vatPeriod</Name>
				<Source><![CDATA[
    display TaxPeriod vatPeriod()
    {
        if (taxIntervatGeneral.TaxIntervatFrequency == TaxIntervatFrequency::Monthly)
        {
            return date2str(taxIntervatGeneral.TaxIntervatStartDate,321,0,0,1,0,0,DateFlags::None);
        }
        else
        {
            return int2str(any2int(trunc(mthofyr(taxIntervatGeneral.TaxIntervatStartDate)/3))+1);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>vaTyear</Name>
				<Source><![CDATA[
    display TaxPeriod vaTyear()
    {
        return date2str(taxIntervatGeneral.TaxIntervatStartDate,321,0,0,0,0,4,DateFlags::None);
    }

]]></Source>
			</Method>
