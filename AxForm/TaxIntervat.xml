						<Source><![CDATA[
        public void clicked()
        {
            super();
            if (taxIntervatGeneral.TaxIntervatFilename != '')
            {
                if (this.canRecreateFile())
                {
                    TaxIntervatDiskBE::reCreateFile(taxIntervatGeneral);
                    info("@SYS92388");
                }
            }
            else
            {
                error("@SYS92389");
            }
        }

]]></Source>
