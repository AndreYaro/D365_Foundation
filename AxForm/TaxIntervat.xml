						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean ret;

            ret = super();

            if (ret)
            {
                if (mthofyr(taxIntervatGeneral.TaxIntervatEndDate) != MonthsOfYear::December &&
                    taxIntervatGeneral.NihilAnnualListing)
                {
                    if (Box::yesNo(strfmt("@SYS334838", fieldpname(TaxIntervatGeneral, NihilAnnualListing)), DialogButton::No) == DialogButton::No)
                    {
                        ret = false;
                    }
                }
            }

            return ret;
        }

]]></Source>
