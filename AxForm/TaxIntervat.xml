				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            #ISOCountryRegionCodes

            super();
            if (taxIntervatGeneral.TaxIntervatStatus == TaxIntervatStatus::Created)
            {
                Browser browser = new Browser();
                switch (substr(infolog.language(),1,2))
                {
                    case #isoFR:
                        browser.navigate(TaxIntervatConfiguration::findByTaxIntervatElement(TaxIntervatElement::FRBE).TaxIntervatConfValue, true, false);
                        break;
                    case #isoDE:
                        browser.navigate(TaxIntervatConfiguration::findByTaxIntervatElement(TaxIntervatElement::DE).TaxIntervatConfValue, true, false);
                        break;
                    default:
                        browser.navigate(TaxIntervatConfiguration::findByTaxIntervatElement(TaxIntervatElement::NLBE).TaxIntervatConfValue, true, false);
                        break;
                }
            }
            else
            {
                error("@SYS92384");
            }
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>ChangeStatus</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            super();
            if (taxIntervatGeneral.TaxIntervatStatus == TaxIntervatStatus::Created)
            {
                taxIntervatGeneral.TaxIntervatStatus = TaxIntervatStatus::Sent;
                taxIntervatGeneral.update();
                taxIntervatGeneral_ds.executeQuery();
                info ("@SYS92283");
            }
            else
            {
                error ("@SYS92384");
            }
        }

]]></Source>
