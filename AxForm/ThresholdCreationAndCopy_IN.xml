        {
            Object      callerForm;

            super();

            ThresholdType_IN::cloneThresholdDesigner(fromThreshold.RecId, Threshold_IN.RecId);

            callerForm = element.args().caller();
            if (formHasMethod(callerForm, identifierStr(refresh)))
