			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Initialize method of form.
    /// </summary>
    public void init()
    {
        if (!element.args().caller() || !element.args().dataset() || element.args().dataset() != tableNum(Threshold_IN))
        {
            //Select a threshold record to copy from.
            throw error("@SYS4003345");
        }

        fromThreshold = element.args().record();

        if (!fromThreshold.RecId)
        {
            throw error("@SYS4003345");
        }
        super();
    }

]]></Source>
