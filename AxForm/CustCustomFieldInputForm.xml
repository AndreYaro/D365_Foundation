			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    void closeOk()
    {
        DialogRunbase dialog = element.args().caller();
        dialog.updateServer();
        if (runbaseForm.checkCloseDialog())
        {
            super();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (!element.args().caller())
            throw error("@SYS8500");

        runbaseForm =  element.args().caller().runbase();

        super();

        switch (runbaseForm.parmCustCustomFieldSourceType())
        {
            case CustCustomFieldSourceType::CustRecurrenceInvoice :
                gridGroup.visible(true);
                fieldGroup.visible(true);
                splitterGroup.visible(true);
                commandButtonOk.text("@SYS301990");
                commandButtonCancel.visible(false);
                element.design().height(480);
            break;
        }
    }

]]></Source>
			</Method>
