			<DataSource>
				<Name>CustInvoiceStandardLineTemplate</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            CustRecurrenceInvoice custRecurrenceInvoice;

            if (runbaseForm.parmCustCustomFieldSourceType() == CustCustomFieldSourceType::CustRecurrenceInvoice)
            {
                custRecurrenceInvoice = runbaseForm.parmCustRecurrenceInvoice();
                this.query().dataSourceTable(tableNum(CustInvoiceStandardLineTemplate)).clearRanges();
                this.query().dataSourceTable(tableNum(CustInvoiceStandardLineTemplate)).addRange(
                                                                                                fieldNum(CustInvoiceStandardLineTemplate,
                                                                                                CustInvoiceTemplate)).value(
                                                                                                SysQuery::value(custRecurrenceInvoice.CustInvoiceTemplate));
                super();

                custInvoiceStandardLineTemplate_DS.active();
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;

            ret = super();

            if (runbaseForm.parmCustCustomFieldSourceType() == CustCustomFieldSourceType::CustRecurrenceInvoice)
            {
                runbaseForm.setGroupVisible(custInvoiceStandardLineTemplate.RecId);
            }

            return ret;
        }

]]></Source>
