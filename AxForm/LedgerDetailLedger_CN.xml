			<Control>
				<Name>GeneralLedger</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            Args                            args = new Args();
            MenuFunction                    menuFunc;
            LedgerInquiryGeneral_CN         ledgerInquiryGeneral_CN;
            FiscalCalendarPeriod            fiscalCalendarPeriod;
            FiscalCalendarYear              fiscalCalendarYear;

            Query                           query;
            QueryRun                        queryRun;
            QueryBuildDataSource            ds;
            QueryBuildRange                 range;

            super();
            if (tmpDailyLedger_CN.LedgerAccount == '')
            {
                // Account number must be filled in.
                warning("@GLS54968");
            }
            else
            {
                query   = new Query();

                ds      = query.addDataSource(tablenum(MainAccount));
                ds.addRange(fieldnum(MainAccount, LedgerChartOfAccounts)).value(queryValue(LedgerChartOfAccounts::current()));
                ds.addRange(fieldnum(MainAccount, MainAccountId)).value(
                    LedgerDimensionFacade::getMainAccountIdFromLedgerDimension(tmpDailyLedger_CN.LedgerDimension));

                queryRun    = new QueryRun(query);

                fiscalCalendarPeriod = LedgerFiscalCalendar::findPeriodByPeriodCodeDate(
                    CompanyInfoHelper::fiscalCalendarRecId(),
                    tmpDailyLedger_CN.TransDate);
                fiscalCalendarYear = FiscalCalendarYear::find(fiscalCalendarPeriod.FiscalCalendarYear);

                ledgerInquiryGeneral_CN = new LedgerInquiryGeneral_CN(queryRun);

                ledgerInquiryGeneral_CN.parmLedgerDimensionFromOtherForm(tmpDailyLedger_CN.LedgerDimension);
                ledgerInquiryGeneral_CN.parmPackedExpressionBuilderHelper(element.getPackedDimensionExpressionBuilderHelper());
                ledgerInquiryGeneral_CN.parmFiscalPeriodNameAndYearName(fiscalCalendarPeriod.Name, fiscalCalendarYear.Name);
                args.parmObject(ledgerInquiryGeneral_CN);
                args.caller(this);
                menuFunc = new MenuFunction(menuitemdisplaystr(LedgerGeneralLedger_CN), MenuItemType::Display);
                menuFunc.run(args);
            }
        }

]]></Source>
