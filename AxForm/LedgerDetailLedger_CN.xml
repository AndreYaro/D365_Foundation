    public void setButtonEnabled()
    {
        if (tmpDailyLedger_CN.Voucher == ''
            || tmpDailyLedger_CN.LedgerAccount == '')
        {
            buttonBase.enabled(false);
            buttonLedgerTransAccount.enabled(false);
            buttonTaxTrans.enabled(false);
            buttonOriginaldocs.enabled(false);
            buttonOffsetAccount.enabled(false);
            generalLedger.enabled(false);
            dailyledger.enabled(false);
        }
        else
        {
            buttonBase.enabled(true);
            buttonLedgerTransAccount.enabled(true);
            buttonTaxTrans.enabled(true);
            buttonOriginaldocs.enabled(true);
            buttonOffsetAccount.enabled(true);
            generalLedger.enabled(true);
            dailyledger.enabled(true);
        }
    }

]]></Source>
