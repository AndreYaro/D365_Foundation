			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        str                     fieldName;
        Query                   query;
        QueryBuildDataSource    ds;
        LedgerParameters        ledgerParameters;

        super();
        overviewGrid.widthMode(1);

        ledgerParameters = LedgerParameters::find();

        if (ledgerParameters.LedgerInquiry_CN == NoYes::Yes)
        {
            if (element.args().caller())
            {
                ledgerInquiryDaily_CN = element.args().parmObject();
            }
            else
            {
                ledgerInquiryDaily_CN = new LedgerInquiryDaily_CN();
                ledgerInquiryDaily_CN.setDailySummary(false);
                ledgerInquiryDaily_CN.initTmpDailyLedger();
                if (!ledgerInquiryDaily_CN.submitQuery())
                {
                    element.close();
                }
            }
            tmpDailyLedger_CN.linkPhysicalTableInstance(ledgerInquiryDaily_CN.tmpDailyLedger());

            query   = tmpDailyLedger_CN_ds.query();
            ds      = query.dataSourceTable(tablenum(TmpDailyLedger_CN));

            ds.addSortIndex(indexnum(TmpDailyLedger_CN, AccTransIdx));
        }
        else
        {
            fieldName = fieldid2pname(
                tablenum(LedgerParameters),
                fieldnum(LedgerParameters, LedgerInquiry_CN));
            // This form cannot be activated unless the parameter '%1' is checked.
            warning(strfmt("@GLS54626",fieldName));
            this.close();
        }

        // Set the LedgerDimension reference on controls displaying LedgerAccount in order for the preview to work off a string field
        TmpDailyLedger_CN_LedgerAccount.userDataItem(fieldNum(TmpDailyLedger_CN, LedgerDimension));
    }

]]></Source>
