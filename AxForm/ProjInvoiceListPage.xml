				<Methods>
					<Method>
						<Name>lookup</Name>
						<Source><![CDATA[
        public void lookup()
        {
            Args args = new Args(formStr(ProjResourceCompanyLookup));
            FormRun formRun;

            args.callerFormControl(CtrlCompanyLookup);
            formRun = classfactory.formRunClass(args);
            formRun.init();
            this.performFormLookup(formRun);
            formRun.wait();
            ProjLegalEntityLookupListPageHelper::setFilterValuesFromControl(projInvoiceJour_ds, ctrlCompanyLookup.valueStr());
        }

]]></Source>
