						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            return ProjLegalEntityLookupListPageHelper::setFilterValuesFromControl(projInvoiceJour_ds, ctrlCompanyLookup.valueStr());
        }

]]></Source>
