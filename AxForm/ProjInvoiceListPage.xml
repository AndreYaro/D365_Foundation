				<Source><![CDATA[
    /// <summary>
    /// Initialize the company lookup value to be this LE.
    /// </summary>
    public void init()
    {
        super();

        CtrlCompanyLookup.text(curExt());

        //<GTH>
        boolean enableSubstitutionInvoices = TaxThaiGovCertificationFeatureChecker::isProjInvoicesSubstitutionAdjustmentEnabled();

        PrintProjInvoiceSubstitution_TH.visible(enableSubstitutionInvoices);
        PrintProjInvoiceAdjustment_TH.visible(enableSubstitutionInvoices);
        ProjInvoiceJourAdjustment_TH.visible(enableSubstitutionInvoices);
        //</GTH>

        boolean projOperationsIntegrationFeatureIsNotEnabled = !ProjProjectOperationsIntegrationFeatureHelper::isFeatureEnabled();
        FunctionGroup.visible(projOperationsIntegrationFeatureIsNotEnabled);

        ProjParameters projParameters = ProjParameters::find();
        InvoiceSummary.visible(projParameters.ShowInvoiceSummary);
    }

]]></Source>
			</Method>
