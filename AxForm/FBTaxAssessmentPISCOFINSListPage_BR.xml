		<DataControls xmlns="">
			<Control>
				<Name>LedgerTransVoucher</Name>
				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            Args args;
            MenuFunction menuFunction;
            TaxTrans taxTrans;

            menuFunction = new menuFunction(menuitemDisplayStr(ledgerTransVoucher), MenuItemType::Display);

            changeCompany (FBTaxAssessment_BR.VoucherDataArea)
            {
                select firstOnly taxTrans
                    where taxTrans.Voucher     == FBTaxAssessment_BR.Voucher
                        && taxTrans.TransDate   == FBTaxAssessment_BR.VoucherTransDate;

                args = new args(this);
                args.record(taxTrans);
                menuFunction.run(args);
            }
        }

]]></Source>
