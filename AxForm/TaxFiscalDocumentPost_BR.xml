[Form]
public class TaxFiscalDocumentPost_BR extends FormRun
{
    TaxFiscalDocumentPostJob_BR fiscalDocPostJob;
    // Local variable needed for "Proforma/Posting" edit method
    NoYes updateNow;

}
]]></Source>
			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        DialogRunbase dialog = element.args().caller();

        // Set the form's field values back into the RunBase class
        fiscalDocPostJob.initParameters(printOut.selection(),
                                        updateNow,
                                        printNoYes.value());

        dialog.updateServer();
        if (fiscalDocPostJob.checkCloseDialog())
        {
            super();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (!element.args().caller())
        {
            throw error("@SYS22539");
        }

        fiscalDocPostJob =  element.args().caller().runbase();
        super();

        // Default the form's fields from the last RunBase values
        updateNow = fiscalDocPostJob.parmProforma();
        printOut.selection(fiscalDocPostJob.parmPrintOut());
        printNoYes.value(fiscalDocPostJob.parmPrintOut());

        // Enable/disable appropriate fields
        element.updateDesign();
    }

]]></Source>
			</Method>
			<Method>
				<Name>runBase</Name>
				<Source><![CDATA[
    RunBase runBase()
    {
        return fiscalDocPostJob;
    }

]]></Source>
