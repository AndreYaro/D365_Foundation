					<Method>
						<Name>linkActive</Name>
						<Source><![CDATA[
        public void linkActive()
        {
            #define.star('*')
            ProjTable               projTable;
            ProjInvoiceTable        projInvoiceTable;
            ProjCategory            projCategory;
            SMASubscriptionGroup    smaSubscriptionGroup;
            ProjId                  projId;
            ProjInvoiceProjId       projInvoiceProjId;

            switch (element.args().dataset())
            {
                case(tableNum(ProjTable)):
                    projTable              = element.args().record();
                    projId                 = projTable.ProjId;
                    criteriaProjId.value(projId + #star);
                    break;

                case(tableNum(ProjInvoiceTable)):
                    projInvoiceTable       = element.args().record();
                    projInvoiceProjId          = projInvoiceTable.ProjInvoiceProjId;
                    criteriaProjInvoiceId.value(queryValue(projInvoiceProjId));
                    break;

                case(tableNum(ProjCategory)):
                    projCategory           = element.args().record();
                    criteriaCategoryId.value(queryValue(projCategory.CategoryId));
                    break;

                case(tableNum(SMASubscriptionGroup)):
                    smaSubscriptionGroup   = element.args().record();
                    criteriaGroupId.value(queryValue(smaSubscriptionGroup.GroupId));
                    break;
            }

            super();
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            if (element.args().menuItemName() == menuItemDisplayStr(SMASubscriptionTableListPage_Active))
            {
                this.queryBuildDataSource().addRange(fieldNum(SMASubscriptionTable, Active)).value(queryValue(NoYes::Yes));
            }

            criteriaProjId              = this.queryBuildDataSource().addRange(fieldNum(SMASubscriptionTable, ProjId));
            criteriaCategoryId          = this.queryBuildDataSource().addRange(fieldNum(SMASubscriptionTable, ProjCategoryId));
            criteriaGroupId             = this.queryBuildDataSource().addRange(fieldNum(SMASubscriptionTable, GroupId));
            criteriaSubscriptionId      = this.queryBuildDataSource().addRange(fieldNum(SMASubscriptionTable, SubscriptionId));

            QueryBuildDataSource queryBuildDataSourceProj = this.queryBuildDataSource().addDataSource(tableNum(ProjTable));

            queryBuildDataSourceProj.addLink(fieldNum(SMASubscriptionTable,ProjId), fieldNum(ProjTable, ProjId));
            queryBuildDataSourceProj.joinMode(JoinMode::ExistsJoin);

            criteriaProjInvoiceId       = queryBuildDataSourceProj.addRange(fieldNum(ProjTable, ProjInvoiceProjId));
        }

]]></Source>
