			<Method>
				<Name>groupIsModified</Name>
				<Source><![CDATA[
    void groupIsModified()
    {
        SMASubscriptionGroup    smaSubscriptionGroup = SMASubscriptionGroup::find(smaSubscriptionTable.GroupId);

        if (smaSubscriptionGroup.NumberSequenceRecId)
        {
            smaNumberseq_Subscription.groupId(smaSubscriptionTable.GroupId);

            if (!smaSubscriptionTable.SubscriptionId || !SMAChangeGroupImmutableSubscriptionIdFlight::instance().isEnabled())
            {
                smaSubscriptionTable.SubscriptionId = smaNumberseq_Subscription.getNextNum();
            }
        }
        else
        {
            smaNumberseq_Subscription.abort();
        }

        Identification_SubscriptionId.allowEdit(smaSubscriptionGroup.NumberSequenceRecId 
                                                ? NumberSequenceTable::find(smaSubscriptionGroup.NumberSequenceRecId).numAllowEdit() 
                                                : true);
    }

]]></Source>
