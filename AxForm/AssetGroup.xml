				<Source><![CDATA[
[Form]
public class AssetGroup extends FormRun
{
    RecordSortedList    recordSortedList;
    TmpAssetGroupCOT    tmpAssetGroupCOT;

    #isoCountryRegionCodes
    boolean             isIsoJP;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        // <GJP>
        isIsoJP = SysCountryRegionCode::isLegalEntityInCountryRegion([#isoJP]);
        // </GJP>

        super();

        recordSortedList = new RecordSortedList(tablenum(TmpAssetGroupCOT));
        recordSortedList.sortOrder(fieldnum(TmpAssetGroupCOT, AssetGroupId));

        AssetImageController imageController = AssetImageController::construct(
            TableGroupAll::GroupId,
            assetGroup_ds,
            this.control(this.controlId(formControlStr(AssetGroup, TabImage))),
            this.control(this.controlId(formControlStr(AssetGroup, ImageControl))));
        imageController.parmButtonControlUploadImage(this.control(this.controlId(formControlStr(AssetGroup, ButtonImageUpload))));
        imageController.parmButtonControlRemoveImage(this.control(this.controlId(formControlStr(AssetGroup, ButtonImageRemove))));
        imageController.initialize();
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign_BR</Name>
				<Source><![CDATA[
    void updateDesign_BR()
    {
        FBPISCOFINSFixedAssetAppropMethod_BR appropriationMethod;
        boolean                              allowPISCOFINSCredit = AssetGroup.PISCOFINSReceivableLongTerm_BR;

        if (BrazilParameters::isEnabled())
        {
            assetGroup_DS.object(fieldNum(AssetGroup, PISCOFINSCreditGroup_BR)).allowEdit(allowPISCOFINSCredit);
            assetGroup_DS.object(fieldNum(AssetGroup, PISCOFINSCreditPurpose_BR)).allowEdit(allowPISCOFINSCredit);
            assetGroup_DS.object(fieldNum(AssetGroup, PISCOFINSAppropriationMethod_BR)).allowEdit(allowPISCOFINSCredit);
            assetGroup_DS.object(fieldNum(AssetGroup, PISTaxationCode_BR)).allowEdit(allowPISCOFINSCredit);
            assetGroup_DS.object(fieldNum(AssetGroup, COFINSTaxationCode_BR)).allowEdit(allowPISCOFINSCredit);

            appropriationMethod = assetGroup_DS.object(fieldNum(AssetGroup, PISCOFINSAppropriationMethod_BR)).getValue();
            assetGroup_DS.object(fieldNum(AssetGroup, PISCOFINSCreditInstallments_BR)).allowEdit(appropriationMethod == FBPISCOFINSFixedAssetAppropMethod_BR::Acquisition);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateAssetTypeDesign_JP</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates the design based on asset type.
    /// </summary>
    public void updateAssetTypeDesign_JP()
    {
        if (AssetGroup.AssetType == AssetType::Deferred_JP)
        {
            AssetGroup_ds.object(fieldNum(AssetGroup, AssetClassification_JP)).visible(false);
            AssetGroup_ds.object(fieldNum(AssetGroup, AssetDeferredType_JP)).visible(true);
            AssetGroup_ds.object(fieldNum(AssetGroup, AssetDeferredType_JP)).mandatory(true);
        }
        else
        {
            AssetGroup_ds.object(fieldNum(AssetGroup, AssetClassification_JP)).visible(true);
            AssetGroup_ds.object(fieldNum(AssetGroup, AssetDeferredType_JP)).visible(false);
            AssetGroup_ds.object(fieldNum(AssetGroup, AssetDeferredType_JP)).mandatory(false);
        }
    }

]]></Source>
			</Method>
