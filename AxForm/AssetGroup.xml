					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        int active()
        {
            int ret;

            ret = super();

            assetGroup_ds.findTmpAssetGroupCOT(assetGroup.GroupId, true);

            // <GJP>
            if (isIsoJP)
            {
                element.updateAssetTypeDesign_JP();
            }
            // </GJP>

            element.updateDesign_BR();

            assetGroup_DS.object(fieldNum(AssetGroup, AutoNumberSequenceTable)).allowEdit(assetGroup.AutoNumber);

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>findTmpAssetGroupCOT</Name>
						<Source><![CDATA[
        void findTmpAssetGroupCOT(AssetGroupId _assetGroupId, boolean _forceUpdate = false)
        {
            tmpAssetGroupCOT.AssetGroupId = _assetGroupId;
            if ((!recordSortedList.find(tmpAssetGroupCOT)) || _forceUpdate)
            {
                tmpAssetGroupCOT.data(AssetGroupBookSetup::findTmpAssetGroupCOT(_assetGroupId));
                recordSortedList.ins(tmpAssetGroupCOT, true);
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        public void write()
        {
            if  (BrazilParameters::isEnabled())
            {
                if (!assetGroup.PISCOFINSReceivableLongTerm_BR)
                {
                    assetGroup.PISCOFINSCreditGroup_BR          = FBPISCOFINSFixedAssetCreditGroup_BR::None;
                    assetGroup.PISCOFINSCreditInstallments_BR   = 0;
                    assetGroup.PISCOFINSCreditPurpose_BR        = FBPISCOFINSFixedAssetCreditPurpose_BR::None;
                    assetGroup.PISCOFINSAppropriationMethod_BR  = FBPISCOFINSFixedAssetAppropMethod_BR::None;
                    assetGroup.PISTaxationCode_BR               = '';
                    AssetGroup.COFINSTaxationCode_BR            = '';
                }
            }

            super();
        }

]]></Source>
