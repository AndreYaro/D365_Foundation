					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            if (element.args().record().TableId == tableNum(RDeferralsBook))
            {
                rDeferralsWritingOffFactor_ds.allowCreate(false);
                deferralsGroupId.visible(false);
                deferralsGroup.allowEdit(false);
                printWritingOffFactor.visible(false);
            }

            printWritingOffFactor.text("@SYS55777");

            super();
        }

]]></Source>
