			<Method>
				<Name>enableFields</Name>
				<Source><![CDATA[
    void enableFields()
    {
        boolean notExistTrans = RTax25StdChannel_DeferralFactor::notExistWritingOffTrans(rDeferralsWritingOffFactor.DeferralsGroupId,
                                      rDeferralsWritingOffFactor.rTax25ProfitTable,
                                      rDeferralsWritingOffFactor.StartDate,
                                      rDeferralsWritingOffFactor.EndDate,
                                      false);

        boolean notExistNextFactor = RDeferralsWritingOffFactor::findNextPeriod(rDeferralsWritingOffFactor.EndDate,
                                      rDeferralsWritingOffFactor.rTax25ProfitTable,
                                      rDeferralsWritingOffFactor.DeferralsGroupId).RecId ? false : true;

        boolean notExistNextCalc = RDeferralsFactorCalculation::findNextPeriod(
                                       RDeferralsFactorCalculation::findRecId(rDeferralsWritingOffFactor.RefRecId).EndDate).RecId ?
                                       false : true;

        rDeferralsWritingOffFactor_ds.allowEdit(notExistTrans && notExistNextFactor && notExistNextCalc);
        rDeferralsWritingOffFactor_ds.allowCreate(notExistNextCalc);
    }

]]></Source>
