			<Method>
				<Name>allowToDeleteHierarchy</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if the selected hierarchy can be deleted. If not, displays the error message.
    /// </summary>
    /// <returns>
    /// True if the selected hierarchy can be deleted, false otherwise.
    /// </returns>
    boolean allowToDeleteHierarchy()
    {
        TaxReportHierarchyVersion_IN    hierarchyVersion;
        boolean                         ret = true;

        while select hierarchyVersion
            where hierarchyVersion.TaxReportHierarchy_IN == taxReportHierarchy_IN.RecId
        {
            ret = ret && element.allowToDeleteHierarchyVersion(hierarchyVersion);
        }

        if (!ret)
        {
            error(strFmt(
                "@SYS4005618",
                taxReportHierarchy_IN.Name));
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>allowToDeleteHierarchyVersion</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if the selected hierarchy version can be deleted. If not, displays the error message.
    /// </summary>
    /// <param name="_hierarchyVersion">
    /// The <c>TaxReportHierarchyVersion_IN</c> record.
    /// </param>
    /// <returns>
    /// True if the selected hierarchy version can be deleted, false otherwise.
    /// </returns>
    boolean allowToDeleteHierarchyVersion(
        TaxReportHierarchyVersion_IN _hierarchyVersion = taxReportHierarchyVersion_IN)
    {
        if (TaxReportHierarchyHelper_IN::allowToDeleteHierarchyVersion(_hierarchyVersion.RecId))
        {
            return true;
        }

        return checkFailed(strFmt(
            "@SYS4005619",
            taxReportHierarchyVersion_IN.Version));
    }

]]></Source>
