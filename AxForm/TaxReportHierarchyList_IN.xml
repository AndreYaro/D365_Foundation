				<Name>performAddVersion</Name>
				<Source><![CDATA[
    void performAddVersion()
    {
        if (FeatureStateProvider::isFeatureEnabled(TaxSetOffHierarchyAddSyncBatchFeature::instance()))
        {
            Args args = new Args();
            args.record(taxReportHierarchy_IN);
            TaxReportCreateHierarchyVersionController::main(args);
        }
        else
        {
            TaxReportHierarchyHelper_IN::createTaxReportHierarchyVersion(taxReportHierarchy_IN);
        }

        // Refresh the version grid
        taxReportHierarchyVersion_IN_ds.research();
    }

]]></Source>
