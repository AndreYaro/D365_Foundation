				<Name>toggleActivateVersion</Name>
				<Source><![CDATA[
    void toggleActivateVersion()
    {
        if (taxReportHierarchyVersion_IN.IsActive)
        {
            // Deactivate
            if (!TaxReportHierarchyHelper_IN::allowToDeactivateHierarchyVersion(taxReportHierarchyVersion_IN))
            {
                throw error(strFmt(
                    "@SYS4005620",
                    taxReportHierarchy_IN.Name,
                    taxReportHierarchyVersion_IN.Version));
            }

            ttsBegin;

            taxReportHierarchyVersion_IN.IsActive = NoYes::No;
            taxReportHierarchyVersion_IN.update();

            ttsCommit;
        }
        else
        {
            // Activate
            TaxReportHierarchyHelper_IN::activateTaxReportHierarchy(taxReportHierarchyVersion_IN.RecId);
        }

        taxReportHierarchyVersion_IN_DS.reread();
        element.updateActivateVersionButton();
    }

]]></Source>
