			<Method>
				<Name>updateOutline</Name>
				<Source><![CDATA[
    void updateOutline(TaxReportHrchyStructureAttribute_IN _designAttr = null)
    {
        TaxReportHrchyNode_IN               rootNode;

        // Clear the outline tree
        outlineTree.deleteAll();

        // Populate the outline tree starting from the root node
        rootNode = TaxReportHierarchyHelper_IN::findRootNode(TaxReportHierarchyVersion_IN.RecId);
        this.populateOutlineTree(rootNode, FormTreeAdd::Root);

        // Expand the outline tree
        SysFormTreeControl::expandTree(outlineTree, outlineTree.getRoot(), 10);
    }

]]></Source>
