			<Method>
				<Name>performViewVersion</Name>
				<Source><![CDATA[
    /// <summary>
    /// Views the selected hierarchy in the designer form.
    /// </summary>
    void performViewVersion()
    {
        FormRun                         formRun;
        Args                            args;
        TaxReportHierarchyInstance_IN   instance;

        instance = new TaxReportHierarchyInstance_IN(taxReportHierarchyVersion_IN);

        args = new Args();
        args.caller(element);
        args.parmObject(instance);

        formRun = new MenuFunction(
            menuitemDisplayStr(TaxReportHierarchyDesigner_IN),
            MenuItemType::Display).create(args);
        formRun.run();
    }

]]></Source>
