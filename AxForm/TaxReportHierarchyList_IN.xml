			<Method>
				<Name>updateActivateVersionButton</Name>
				<Source><![CDATA[
    void updateActivateVersionButton()
    {
        if (TaxReportHierarchyVersion_IN.IsActive)
        {
            // Deactivate
            activateVersionButton.text("@GLS70567");
            activateVersionButton.helpText("@SYS4005613");
            activateVersionButton.normalImage("11370");
        }
        else
        {
            // Activate
            activateVersionButton.text("@sys50567");
            activateVersionButton.helpText("@SYS4005612");
            activateVersionButton.normalImage("11420");
        }
    }

]]></Source>
