			<Method>
				<Name>performNew</Name>
				<Source><![CDATA[
    /// <summary>
    /// Only entry for creating a new tax report hierarchy.
    /// 1) Opens a "New hierarch" form for user inputs
    /// 2) Clicks the OK button to create the new hierarchy
    /// 3) Refreshes the overview grid after the new hierarchy has been created
    /// </summary>
    void performNew()
    {
        MenuFunction    menuFunction;
        FormRun         formRun;
        Args            args;

        args = new Args();
        args.caller(element);

        menuFunction = new MenuFunction(
            menuitemDisplayStr(TaxReportHierarchyNew_IN),
            MenuItemType::Display);

        formRun = menuFunction.create(args);
        formRun.run();
        formRun.wait(true); // Modal window
    }

]]></Source>
			</Method>
			<Method>
				<Name>performSynchronizeVersion</Name>
				<Source><![CDATA[
    void performSynchronizeVersion()
    {
        if (FeatureStateProvider::isFeatureEnabled(TaxSetOffHierarchyAddSyncBatchFeature::instance()))
        {
            taxReportHierarchyVersion_IN_DS.reread();

            if (this.canSyncVersion())
            {
                Args args = new Args();
                args.record(taxReportHierarchyVersion_IN);
                TaxReportSyncHierarchyVersionController::main(args);
            }
        }
        else
        {
            TaxReportHierarchySynchronizer_IN::synchronizeVersion(taxReportHierarchyVersion_IN);

            info(strFmt(
                "@SYS4005681",
                taxReportHierarchy_IN.Name,
                taxReportHierarchyVersion_IN.Version));
        }

        taxReportHierarchyVersion_IN_DS.reread();
        element.updateSynchronizeButton();
    }

]]></Source>
