			<Method>
				<Name>populateOutlineTree</Name>
				<Source><![CDATA[
    void populateOutlineTree(TaxReportHrchyNode_IN _node, TreeItemIdx _parentIdx, TaxReportHrchyStructureDesign_IN _design = null)
    {
        str                     outlineStr;
        TreeItemIdx             itemIdx;
        ListEnumerator          childNodeEnumerator;
        TaxReportHrchyNode_IN   childNode;
        List                    childNodes;

        outlineStr = element.getOutlineNodeStr(_node);

        itemIdx = SysFormTreeControl::addTreeItem(
                outlineTree,
                outlineStr,
                _parentIdx,
                outlineStr,
                10052,
                true);

        childNodes = TaxReportHierarchyHelper_IN::findAllDescendantsByDepth(_node, 1);
        childNodeEnumerator = childNodes.getEnumerator();

        while (childNodeEnumerator.moveNext())
        {
            childNode = childNodeEnumerator.current();
            element.populateOutlineTree(childNode, itemIdx);

            // Only tax type nodes, GTE tax type nodes and GTE tax component nodes should be thoroughly iterated
            if (childNode.parmClassId() != classNum(TaxReportHrchyTaxTypeNode_IN) &&
                childNode.parmClassId() != classNum(TaxReportHrchyGTETaxTypeNode_IN) &&
                childNode.parmClassId() != classNum(TaxReportHrchyGTETaxComponentNode_IN))
                break;
        }
    }

]]></Source>
