            this.queryBuildDataSource().addRange(fieldNum(TaxReportHierarchy_IN, TaxSolutionScope)).value(con2Str(taxSolutionScopeRangeValuesContainer));

            if (selectedHierarchy)
            {
                // Clicks the "Hierarchy details" button in the designer form
                this.queryBuildDataSource().addRange(
                    fieldNum(TaxReportHierarchy_IN, Name)).value(selectedHierarchy);
            }

            super();
        }

