			<Method>
				<Name>fileNameLookUpFileName</Name>
				<Source><![CDATA[
    public str fileNameLookUpFileName()
    {
        int      fileNamePos;
        Filename lookupFileName = ledgerRRGReportTable.Template;

        if (lookupFileName)
        {
            fileNamePos    = strFind(lookupFileName, '\\', strLen(lookupFileName), -strLen(lookupFileName));
            lookupFileName = subStr(lookupFileName, fileNamePos + 1, strLen(lookupFileName) - fileNamePos);
            return lookupFileName;
        }

        return conPeek(this.fileNameLookupFilter(), Value2);
    }

]]></Source>
			</Method>
