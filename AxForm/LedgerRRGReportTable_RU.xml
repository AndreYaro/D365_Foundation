						<Source><![CDATA[
        public void write()
        {
            boolean updateCells;

            if (ledgerRRGReportTable.LedgerRRGETemplates_W && ledgerRRGReportTable.orig().LedgerRRGETemplates_W != ledgerRRGReportTable.LedgerRRGETemplates_W)
            {
                updateCells = Box::yesNo("@LedgerRRG:LedgerRRG_UpdateCells", DialogButton::Yes) == DialogButton::Yes;
            }

            super();

            if (updateCells)
            {
                LedgerRRGECreateReportLines_W::updateReportCells(ledgerRRGReportTable.RepCode);
            }
        }

]]></Source>
