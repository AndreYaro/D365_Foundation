			<Method>
				<Name>refresh</Name>
				<Source><![CDATA[
    void refresh(LedgerRRGRepCode_RU _repCode = '')
    {
        LedgerRRGReportTable_RU reportTable;

        if (_repCode)
        {
            ledgerRRGReportTable_ds.executeQuery();
            reportTable = LedgerRRGReportTable_RU::findByRepCode(_repCode);
            ledgerRRGReportTable_ds.positionToRecord(reportTable);
        }
        else
        {
            ledgerRRGReportTable_ds.reread();
            ledgerRRGReportTable_ds.refresh();
            ledgerRRGDimensionInterval_ds.executeQuery();
            ledgerRRGTransType_ds.executeQuery();
            ledgerRRGTransOperationsTax_ds.executeQuery();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>reportDataTypeModified</Name>
				<Source><![CDATA[
    public void reportDataTypeModified()
    {
        boolean enabledTrans = ledgerRRGReportTable.DataType == LedgerRRGDataType_RU::Trans;
    
        LedgerRRGReportTable.TypeByCorrect = LedgerRRGTypeByCorrect_RU::None;

		ReversingEntry.visible(enabledTrans); 
        TransOperationsTaxPage.visible(enabledTrans || ledgerRRGReportTable.DataType == LedgerRRGDataType_RU::DimFocusBalance);
        operationTransTypePage.visible(enabledTrans);
    
        ledgerRRGReportTable_ds.object(fieldNum(LedgerRRGReportTable_RU, CurrencyForCalc)).enabled(ledgerRRGReportTable.DataType != LedgerRRGDataType_RU::Register);
    
        if (ledgerRRGReportTable.DataType        == LedgerRRGDataType_RU::Register &&
            ledgerRRGReportTable.CurrencyForCalc == LedgerRRGCurrencyForCalc_RU::AmountMSTSecond)
        {
            ledgerRRGReportTable.CurrencyForCalc = LedgerRRGCurrencyForCalc_RU::AmountMST;
        }
    
        ledgerRRGReportTable_ds.object(fieldNum(LedgerRRGReportTable_RU, ModelNum)).enabled(ledgerRRGReportTable.DataType == LedgerRRGDataType_RU::Budget);
    }

]]></Source>
