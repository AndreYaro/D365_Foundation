			<Method>
				<Name>fileNameLookupInitialPath</Name>
				<Source><![CDATA[
    public str fileNameLookupInitialPath()
    {
        str path;

        if (ledgerRRGReportTable.Template)
        {
            path = ledgerRRGReportTable.Template;
            path = subStr(path, 1, strFind(path, '\\', strLen(path), -strLen(path)));
        }
        else if (SysCountryRegionCode::isLegalEntityInCountryRegion([IsoRU]))
        {
            try
            {
                path = GlobalEx::templateFolder_W();
            }
            catch(Exception::Error)
            {
                path = '';
            }
        }

        return path;
    }

]]></Source>
