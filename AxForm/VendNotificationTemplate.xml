					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        /// <summary>
        /// Calls data and control initializations for the form.
        /// </summary>
        /// <returns>
        /// An <c>integer</c> value returned from the base class.
        /// </returns>
        public int active()
        {
            int ret = super();

            if (vendNotificationTemplate.Status == VendNotificationStatus::NotSent)
            {
                this.enableSend(true);
                createMessageButton.visible(true);
                viewMessageButton.visible(false);
            }
            else
            {
                this.enableSend(false);
                createMessageButton.visible(false);
                viewMessageButton.visible(true);
            }

            if ((vendNotificationTemplate.EnableCategorySelection == VendNotificationCategorySelection::SelectedOnly) &&
                (vendNotificationTemplate.Status == VendNotificationStatus::NotSent) )
            {
                element.enableCategorySelection(true);
            }
            else
            {
                element.enableCategorySelection(false);
            }

            return ret;
        }

]]></Source>
