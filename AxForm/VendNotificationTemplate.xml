						<Source><![CDATA[
        void clicked()
        {
            super();
            QueryRun queryRun = new QueryRun(vendNotificationTemplate.QueryData);
            queryRun.name(strFmt("@SCM:VendorNotificationSelectQueryFormCaption", vendNotificationTemplate.Topic));

            queryRun.saveUserSetup(false);

            if (queryRun.prompt())
            {
                vendNotificationTemplate.QueryData = queryRun.pack();
            }
        }

]]></Source>
