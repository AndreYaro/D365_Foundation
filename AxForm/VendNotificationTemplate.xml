				<Source><![CDATA[
    /// <summary>
    /// Enables or disable the controls that are used for filtering categories.
    /// </summary>
    /// <param name="_enable">
    /// A value that determines if the category controls should be enabled or disabled.
    /// </param>
    public void enableCategorySelection(boolean _enable)
    {
        categoryGrid.enabled(_enable);
        vendNotificationTemplateCategory_ds.allowCreate(_enable);
        vendNotificationTemplateCategory_ds.allowEdit(_enable);
        vendNotificationTemplateCategory_ds.allowDelete(_enable);
    }

]]></Source>
