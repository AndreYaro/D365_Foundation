								<Source><![CDATA[
            /// <summary>
            /// Resolves reference for the category lookup.
            /// </summary>
            /// <param name="_formReferenceControl">
            /// A <c>FormReferenceControl</c> control for procurement categories.
            /// </param>
            /// <returns>
            /// The record returned from the call to the <c>resolveCategoryHierarchy</c> method.
            /// </returns>
            public Common resolveReference(FormReferenceControl _formReferenceControl)
            {
                Common                      ret;
                EcoResCategoryHierarchy     procurementHierarchy;

                procurementHierarchy = EcoResCategoryHierarchy::find(EcoResCategoryHierarchyRole::getHierarchiesByRole(EcoResCategoryNamedHierarchyRole::Procurement).CategoryHierarchy);
                ret                  = EcoResCategory::resolveCategoryHierarchy(_formReferenceControl,  procurementHierarchy);

                return ret.RecId? ret : null;
            }

]]></Source>
