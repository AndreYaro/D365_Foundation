						<Source><![CDATA[
        /// <summary>
        /// Enables or disables the form controls.
        /// </summary>
        /// <param name="_enable">
        /// true if the form should be enabled, otherwise false.
        /// </param>
        private void enableSend(boolean _enable)
        {
            sendButtonGroup.enabled(_enable);
            selectButtonGroup.enabled(_enable);

