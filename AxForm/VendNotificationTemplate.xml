					<Method>
						<Name>delete</Name>
						<Source><![CDATA[
        /// <summary>
        /// If any buffer is not empty opens a dialog asking if sent messages should be deleted. Calls business operation accordingly.
        /// </summary>
        public void delete()
        {
            if (vendNotificationTemplate)
            {
                DialogButton ret = Box::yesNoCancel("@SCM:VendorNotificationDeleteAlertCaption", DialogButton::No);

                if (ret != DialogButton::Cancel)
                {
                    ttsbegin;
                    vendNotificationTemplate.markAsDeleted(ret == DialogButton::Yes);
                    ttscommit;

                    vendNotificationTemplate_ds.research();
                }
            }
        }

]]></Source>
