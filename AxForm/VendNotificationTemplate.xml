				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        /// Sends the notification to the vendor contact.
        /// </summary>
        void clicked()
        {
            DialogButton                            dialogButton;
            VendNotificationTemplateTranslation     templateTranslation;
            int                                     recipientCounter = 0;

            // check if there is an existing message that uses the company's default language
            templateTranslation = VendNotificationTemplateTranslation::find(vendNotificationTemplate.RecId, CompanyInfo::languageId());
            if (!templateTranslation)
            {
                // You must create a message for your company’s default language before you can send the notification.
                Box::warning("@SYS152663");
                return;
            }

            if (vendNotificationTemplate.Status == VendNotificationStatus::NotSent)
            {
                // This notification will be sent to all selected recipients. Do you want to continue?
                dialogButton = Box::yesNo("@SYS152661", DialogButton::No);
                if (dialogButton == DialogButton::Yes)
                {
                    recipientCounter = VendNotificationHelper::sendNotification(vendNotificationTemplate, vendNotificationTemplateTranslation);

                    // inform the user that no notification or email was sent to the intended recipients
                    if (recipientCounter == 0)
                    {
                        // There is no valid vendor contact person recipient for this message. The notification was not sent.
                        error("@SYS152698");
                    }
                }
            }

            super();
        }

]]></Source>
