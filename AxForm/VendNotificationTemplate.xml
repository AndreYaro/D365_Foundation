        {
            QueryBuildRange                     range;
            LanguageId                          languageId;
            super();

            languageId = CompanyInfo::languageId();

            range = this.query().dataSourceTable(tableNum(VendNotificationTemplateTranslation)).addRange(fieldNum(VendNotificationTemplateTranslation, LanguageId));
            range.status(RangeStatus::Hidden);
            range.value(SysQuery::value(languageId));
        }

