            /// The record returned from the call to the <c>lookupCategoryHierarchy</c> method.
            /// </returns>
            public Common lookupReference(FormReferenceControl _formReferenceControl)
            {
                #define.ReferenceStringControlName('VendNotificationTemplateCategory_EcoResCategory')
