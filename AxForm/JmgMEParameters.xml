			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class JmgMEParameters extends FormRun
{
    NumberSeqReference          numberSeqReference;
    boolean                     runExecuteDirect;
    TmpIdRef                    tmpIdRef;
    FormStringControl           fileNameControl;
    NumberSeqScope              scope;
    NumberSeqApplicationModule  numberSeqApplicationModule;
    container                   numberSequenceModules;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        this.numberSeqPreInit();
        super();
        JmgParameters::find();
    }

]]></Source>
			</Method>
			<Method>
				<Name>numberSeqPreInit</Name>
				<Source><![CDATA[
    void numberSeqPreInit()
    {
        runExecuteDirect   = false;

        numberSequenceModules = [NumberSeqModule::SFC, NumberSeqModule::ProductionAndShopFloor];

        numberSeqApplicationModule = new NumberSeqModuleJobManager();
        scope = NumberSeqScopeFactory::createDataAreaScope();
        NumberSeqApplicationModule::createReferencesMulti(numberSequenceModules, scope);
        tmpIdRef.setTmpData(NumberSequenceReference::configurationKeyTableMulti(numberSequenceModules));
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>NumberSequenceReference</Name>
				<Methods>
					<Method>
						<Name>removeFilter</Name>
						<Source><![CDATA[
        void removeFilter()
        {
            runExecuteDirect = false;
            numbersequenceReference_ds.executeQuery();
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        void executeQuery()
        {
            if (runExecuteDirect)
            {
                super();
            }
            else
            {
                runExecuteDirect = true;

                QueryRun qr = this.queryRun(NumberSeqReference::buildQueryRunMulti(numberSequenceReference,
                                                                     tmpIdRef,
                                                                     numberSequenceTable,
                                                                     numberSequenceModules,
                                                                     scope));

                if (FeatureStateProvider::isFeatureEnabled(ProductionAndShopFloorJobIdNumberSeqFeature::instance()))
                {
                    ProductionAndShopFloorJobIdNumberSeqHelper::removeNumberSequenceDatatype(qr, extendedTypeNum(JmgJobId));
                }
                else
                {
                    ProductionAndShopFloorJobIdNumberSeqHelper::removeNumberSequenceDatatype(qr, extendedTypeNum(ProductionAndShopFloorJobId));
                    ProductionAndShopFloorJobIdNumberSeqHelper::canGiveOverlappingNumbers();
                }

                numbersequenceReference_ds.research();
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>validateWrite</Name>
						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean ret = super();

            if (ret 
                && !FeatureStateProvider::isFeatureEnabled(ProductionAndShopFloorJobIdNumberSeqFeature::instance())
                && !ProductionAndShopFloorJobIdNumberSeqHelper::isValidJobIdNumberSequence(numberSequenceReference))
            {
                Message::Add(MessageSeverity::Warning, "@SCM:ProductionAndShopFloorJobIdNumberSeqCanGiveOverlappingNumbers");
            }
        
            return ret;
        }

]]></Source>
					</Method>
				</Methods>
