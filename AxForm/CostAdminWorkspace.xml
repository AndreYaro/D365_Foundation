				<Methods>
					<Method>
						<Name>createPart</Name>
						<Source><![CDATA[
        /// <summary>
        /// Initializes the period on the form part and hook up the event handler.
        /// </summary>
        /// <returns>The <c>FormRun</c> corresponding to the created form part.</returns>
        public FormRun createPart()
        {
            FormRun ret = super();

            var inventoryStatementsFormRun = ret as CostISetPeriod;
            inventoryStatementsFormRun.setPeriod(viewModel.getPeriod());
            viewModel.onPeriodChange += eventhandler(inventoryStatementsFormRun.setPeriod);

            return ret;
        }

]]></Source>
