						<Source><![CDATA[
        /// <summary>
        /// Opens the period selection lookup.
        /// </summary>
        public void lookup()
        {
            super();

            Args    args = new Args();
            FormRun periodLookUp;

            args.name(formstr(CostSelectPeriodDialogForm));
            args.caller(element);
            periodLookUp = new FormRun(args);
            periodLookUp.init();
            this.performFormLookup(periodLookUp);
        }

]]></Source>
