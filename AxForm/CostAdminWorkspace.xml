        public void pageActivated()
        {
            if (!inventoryStatusTabInitialized)
            {
                CostManagementPBIWorkspaceController controller = new CostManagementPBIWorkspaceController();
                controller.initializeReportControl(controller.setupReportRunParams(InventoryStatusContentPackResourceName, InventoryStatusReportName), PowerBIInventoryStatusReportGroup);
                inventoryStatusTabInitialized = true;
            }
        }

]]></Source>
