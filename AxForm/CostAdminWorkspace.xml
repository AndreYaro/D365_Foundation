				<Source><![CDATA[
[Form]
public class CostAdminWorkspace extends FormRun
    implements CostICostSelectPeriodDialogFormCaller, CostICostStatementCacheCardRefresh, SysIFilterProvider
{
    CostWorkspaceViewModel viewModel;
    SysFilterChangeEvent changeEvent;

    private const str InventoryStatusContentPackResourceName = 'CostManagementInvnetoryStatusPbiContentPack';
    private const str ManufacturingStatusContentPackResourceName = 'CostManagementManufacturingStatusPbiContentPack';
    private const str InventoryStatusReportName = 'CostManagementInventoryStatus';
    private const str ManufacturingStatusReportName = 'CostManagementManufacturingStatus';

    private boolean inventoryStatusTabInitialized;
    private boolean manufacturingStatusTabInitialized;

}
]]></Source>
			</Method>
			<Method>
				<Name>parmFilter</Name>
				<Source><![CDATA[
    public SysIFilter parmFilter()
    {
        SysIFilter  filter = SysFilterFactory::createEmptyFilter();
        str periodValue = FilterStringControl.valueStr();

        SysIFilterValue filterValue = SysFilterValueFactory::createFilterValueForDate(viewModel.getPeriod().parmFromDate(), extendedTypeNum(FromDate));
        filter.addItem(filterValue, SysFilterOperator::GreaterEqual, SysFilterValueResolutionMethod::None);

        filterValue = SysFilterValueFactory::createFilterValueForDate(viewModel.getPeriod().parmToDate(), extendedTypeNum(ToDate));
        filter.addItem(filterValue, SysFilterOperator::LessEqual, SysFilterValueResolutionMethod::None);

        return filter;
    }

]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        super();

        SysFilterLastValueHelper::saveLastValue(FilterStringControl);
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmChangeEvent</Name>
				<Source><![CDATA[
    public SysFilterChangeEvent parmChangeEvent()
    {
        return changeEvent;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        viewModel = CostWorkspaceViewModel::newFromParameters(this);
        viewModel.init();

        FilterStringControl.text(viewModel.getPeriod().asDateInterval());

        changeEvent = SysFilterChangeEvent::newFromFormControl(FilterStringControl);
    }

]]></Source>
			</Method>
