				<Source><![CDATA[
    public void init()
    {
        super();

        this.deSerializeTerminalOfflineStatusData();

        CDXTroubleshootHtmlViewer.parmHtml(strFmt(
            "<html><body>%1&nbsp;<a href='https://go.microsoft.com/fwlink/?linkid=2169401'>%2</a>&nbsp;%3</body></html>",
            "@Retail:RetailCDXTroubleshootingStartText",
