								<Source><![CDATA[
            /// <summary>
            /// Adds the view details link to the project menu.
            /// </summary>
            public void jumpRef()
            {
                if (purchReqTable.ProjIdDataArea && CompanyInfo::findDataArea(purchReqTable.ProjIdDataArea).RecId)
                {
                    ProjTable projTable = ProjTable::findByDataArea(purchReqTable.ProjId, purchReqTable.ProjIdDataArea);
                
                    Args args = new Args();
                    args.record(projTable);
        
                    MenuFunction menuFunction = new MenuFunction(menuitemDisplayStr(ProjTable), MenuItemType::Display);

                    changecompany(purchReqTable.ProjIdDataArea)
                    {
                        menuFunction.run(args);
                    }
                }
            }

]]></Source>
