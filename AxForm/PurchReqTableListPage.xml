				<Source><![CDATA[
[Form]
public class PurchReqTableListPage extends FormRun
    implements OfficeIGeneratedWorkbookCustomExporter
{
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        super();

        if (TaxSolutionScopeIntegrationUtil::isCompanyEnabled())
        {
            TaxDocumentLauncher.visible(true);
        }

        // It only shows for the form when it's opened via "Purchase requisitions assigned to my queues"
        if (element.args().parmEnum() == WorkflowWorkListFilter::AssignedToMyQueues &&
            !PurchReqWorkItemAssignedToQueueAcceptedColumnHidden::instance().isEnabled())
        {
            PurchReqTable_Accepted.visible(true);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>canSubmitToWorkflow</Name>
				<Source><![CDATA[
    public boolean canSubmitToWorkflow()
    {
        return purchReqTable.canSubmit();
    }

]]></Source>
			</Method>
			<Method>
				<Name>customizeMenuOptions</Name>
				<Source><![CDATA[
    public void customizeMenuOptions(OfficeMenuOptions _menuOptions)
    {
        OfficeMenuDataEntityOptions entityOptions = _menuOptions.getOptionsForEntity(tableStr(PurchaseRequisitionHeaderV2Entity));
        if (entityOptions)
        {
            // Don't include the default export menu item, as it will use the public entity to export all purchase requisition headers, and not only those that the user can see on the list page.
            // From PurchReqTable form user can edit in excel any given purchase requisition (which the user can see) with its lines.
            entityOptions.includeDefault(false); 
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>getDataEntityContext</Name>
				<Source><![CDATA[
    public ExportToExcelDataEntityContext getDataEntityContext(OfficeGeneratedExportMenuItem _menuItem)
    {
        // Method is required for interface OfficeIGeneratedWorkbookCustomExporter, but we don't want to expose the Purchase requisition header entity on the list page.
        return null;
    }

]]></Source>
			</Method>
			<Method>
				<Name>task</Name>
				<Source><![CDATA[
    /// <summary>
    /// Overriding task method to be able to detect an error condition
    /// if worker is not assigned to the user upon creation of a new record.
    /// </summary>
    /// <param name = "_taskID">task ID</param>
    /// <returns></returns>
    public int task(int _taskID)
    {
        #task
        int ret;

        if ((_taskID == #taskNew)
            && (!HcmWorkerLookup::currentWorker()))
        {
            throw error("@SYS330588"); // To enter a purchase requisition your user ID must be assigned to an active worker. Contact your Microsoft Dynamics Rainier system administrator.
        }

        ret = super(_taskID);

        return ret;
    }

]]></Source>
			</Method>
