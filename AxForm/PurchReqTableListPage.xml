			<Control>
				<Name>PurchRequisitionerFilter</Name>
				<Type>ReferenceGroup</Type>
				<Methods>
					<Method>
						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            HcmWorkerRecId                      requisitionerRecId  = this.value();
            boolean                             ret                 = true;
            PurchReqTableListPageInteraction    interaction         = element.pageInteraction();

            if (interaction.listPage().listPageArgs().menuItemName() != menuitemDisplayStr(PurchReqTableListPage))
            {
                super();

                interaction.applyFilter(purchReqTable_ds.query(), requisitionerRecId);

                purchReqTable_ds.executeQuery();
            }

            return ret;
        }

]]></Source>
