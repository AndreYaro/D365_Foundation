						<Source><![CDATA[
        void clicked()
        {
            Args            args = new Args();

            if (hasMenuItemAccess(menuitemActionStr(PurchReqTableResetApprovedToDraft), MenuItemType::Action))
            {
                args.record(purchReqTable);
                new MenuFunction(menuitemActionStr(PurchReqTableResetApprovedToDraft), MenuItemType::Action).run(args);
            }

            super();
        }

]]></Source>
