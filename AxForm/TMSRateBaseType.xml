				<Source><![CDATA[
    public void init()
    {
        TMSRateBase         rateBase;

        if (element.args().dataset() == tablenum(TMSRateBase))
        {
            rateBase    = element.args().record();
            type        = TMSRateMaster::find(rateBase.RateMasterCode).RateBaseTypeCode;
        }

        super();

        if (element.args().parmEnum() == TMSFieldType::Assignment)
        {
            FieldType.selection(TMSFieldType::Assignment);
        }

        fieldTypeValue = str2enum(fieldTypeValue, FieldType.valueStr());
    }

]]></Source>
