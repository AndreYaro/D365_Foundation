			<Method>
				<Name>splitLine</Name>
				<Source><![CDATA[
    /// <summary>
    /// Splits the currently selected line.
    /// </summary>
    public void splitLine(Amount quantity)
    {
        if (quantity <= 0 ||
            quantity >= TMSTmpInvReconcile.Amount)
        {
            throw error(strFmt("@TRX:WrongSplitQuantity", TMSTmpInvReconcile.Amount));
        }

        TMSTmpInvReconcile.setTmpData(TMSTmpInvReconcile.splitReconcile(TMSTmpInvReconcile, quantity));
        TMSTmpInvReconcile_ds.executeQuery();
        TMSInvoiceLine_ds.executeQuery();
    }

]]></Source>
			</Method>
