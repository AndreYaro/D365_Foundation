			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
		Common argsRecord = this.args().record();

		if (argsRecord && argsRecord.TableId == tableNum(TMSInvoiceTable))
		{
			invoiceTable = TMSInvoiceTable::findByRecId(argsRecord.RecId);
		}

        super();

        TMSTmpInvReconcile.setTmpData(TMSInvoiceApproval::popTmpInvReconcile(invoiceTable));
    }

]]></Source>
