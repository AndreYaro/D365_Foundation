			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        TransTaxInformation         transTaxInformation;
        TaxRegistrationNumbers_IN   localTaxRegistrationNumbers;

        super();

        transTaxInformation = TransTaxInformationHelper::findOrCreateTransTaxInformation(element.args().dataset(), element.args().record().RecId);

        switch (element.args().dataset())
        {
            case tablenum(PurchLine):
            case tablenum(SalesLine):
            case tablenum(InventTransferLine):
            case tablenum(projCostTrans):
            case tablenum(projItemTrans):
            case tablenum(projOnAccTrans):
                localTaxRegistrationNumbers = TransTaxInformationHelper::getTaxRegistrationNumbers(transTaxInformation, TaxType_IN::Excise);
                break;

            default:
                break;
        }

        if (localTaxRegistrationNumbers.RecId)
        {
            eccNumberLoc = localTaxRegistrationNumbers.RegistrationNumber;
            taxRegistrationNumberRecId = localTaxRegistrationNumbers.RecId;
        }
    }

]]></Source>
