        public void executeQuery()
        {
            PurchTable  purchTableParent;
            purchTableParent = element.args().record() as PurchTable;
            if (purchTableParent.RecId)
            {
                PurchTotalsSummaryController::updateTotals(purchTableParent, tableNum(PurchTotalsSummary));

                super();
            }
        }

]]></Source>
