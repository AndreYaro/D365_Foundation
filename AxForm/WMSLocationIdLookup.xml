			<Method>
				<Name>unpack</Name>
				<Source><![CDATA[
    public boolean unpack(container _packedClass)
    {
        Integer     version     = conPeek(_packedClass,1);
        Integer     tab;

        switch (version)
        {
            case #CurrentVersion    :
                [version,#CurrentList,tab] = _packedClass;
                break;
            default :
                return false;
        }

        if (!showItemInfo && tab == 2)
        {
            ctrlTab.tab(1);
        }
        else
        {
            ctrlTab.tab(tab);
        }

        ctrlShowAvail.visible(ctrlTabPageItem.isActivePage());
        ctrlLinkDim.visible(ctrlTabPageItem.isActivePage());

        return true;
    }

]]></Source>
