						<Source><![CDATA[
        void init()
        {
            Query                   q;
            QueryBuildDataSource    qbds;

            super();

            q = new Query();
            qbds = q.addDataSource(tableNum(WMSLocation));

            this.query(q);
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        void executeQuery()
        {
            QueryBuildDataSource    qbds;
            DictRelation            dr;
            Counter                 i;
            FieldId                 inventLocationFieldId;
            FormDataSource          callerFormDatasource;
            Common                  cursor;

            //if queryRun object is created use query from it
            //this allows to modify query that is used in filtered lookups
            if (this.queryRun())
            {
                qbds = this.queryRun().query().dataSourceTable(tableNum(WMSLocation));
            }
            else
            {
                qbds = this.query().dataSourceTable(tableNum(WMSLocation));
            }

            inventLocationFieldId = inventDimFormSetup.callerInventLocationFieldId();

            qbds.clearDynalinks();

            if (inventLocationFieldId)
            {
                cursor = inventDimFormSetup.callerInventLocationIdCursor();
                if (cursor.TableId != tableNum(Common) && cursor.(inventLocationFieldId))
                {
                    qbds.addDynalink(
                        fieldNum(WMSLocation,InventLocationId),
                        cursor,
                        inventLocationFieldId);
                }
            }

            FormStringControl ctrl = element.selectTarget() as FormStringControl;
            if (ctrl)
            {
                ExtendedTypeId extendedTypeId = ctrl.extendedDataType();
                if (extendedTypeId)
                {
                    DictType dt = new DictType(extendedTypeId);
                    dr = dt.relationObject();
                }
                else
                {
                    callerFormDatasource    = inventDimFormSetup.callerFormDatasource();
                    if (callerFormDatasource)
                    {
                        if (callerFormDatasource.table() == tableNum(InventDim) && callerFormDatasource.query())
                        {
                            isCrossCompanyQuery = callerFormDatasource.query().allowCrossCompany();                            
                        }

                        dr = new DictRelation(callerFormDatasource.table());
                        dr.loadFieldRelation(ctrl.dataField());
                    }
                }
            }
            if (dr)
            {
                for (i=1;i<=dr.lines();i++)
                {
                    switch (dr.lineType(i))
                    {
                        case TableRelation::ExternFixed:
                            qbds.addRange(dr.lineExternTableValue(i)).value(queryValue(dr.lineTableValue(i)));
                            break;
                    }
                }
            }

            super();
        }

]]></Source>
