    void setSelectMode()
    {
        void setSelectedControlAndDataSource(FormStringControl  _selectedControl,
                                             FormDataSource     _selectedFormDataSource)
        {
            selectedControl         = _selectedControl;
            selectedFormDataSource  = _selectedFormDataSource;
        }

        switch (ctrlTab.tab())
        {
            case 2  :
                element.selectMode(InventoryDimensions_wMSLocationId);
                InventoryDimensions_wMSLocationId.setFocus();
                setSelectedControlAndDataSource(InventoryDimensions_wMSLocationId, inventDim_ds);
                break;

            case 1  :
            default :
                element.selectMode(ctrlWMSLocation_wMSLocationId);
                ctrlWMSLocation_WMSLocationId.setFocus();
                setSelectedControlAndDataSource(ctrlWMSLocation_wMSLocationId, wmsLocation_ds);
        }
    }

