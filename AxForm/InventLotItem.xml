        InventTransSum          inventTransSum;

        [FormObservable]
        boolean                 calcOpenValues;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        inventTransSum = InventTransSum::newStandard();
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateReceiptIssueGroupVisibility</Name>
				<Source><![CDATA[
    public void updateReceiptIssueGroupVisibility(RecId _inventTransOriginRecId)
    {
        InventTrans     inventTransLocal = InventTrans::findByInventTransOrigin(_inventTransOriginRecId);
        InventDirection direction        = inventTransLocal.direction();

        ReceiptTab.visible(direction == InventDirection::Receipt);
        IssueTab.visible(direction == InventDirection::Issue);
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateUnitValue</Name>
				<Source><![CDATA[
    public void updateUnitValue(ItemId _itemid)
    {
        UnitOfMeasureSymbol symbol = InventTable::find(_itemId).inventUnitId();

        ReceiptTab.caption(strfmt("@SCM:InventLotReceiptTotalOverview", symbol));
        IssueTab.caption(strfmt("@SCM:InventLotIssueTotalOverview", symbol));
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateFormControls</Name>
				<Source><![CDATA[
    public void updateFormControls(ItemId _itemIdCaller)
    {
        calcOpenValues = InventTable::find(_itemIdCaller).modelGroup().inventModelType().isSettled();

        IssueFinOpen.visible(calcOpenValues);
        ReceiptFinOpen.visible(calcOpenValues);
    }

]]></Source>
			</Method>
