]]></Source>
			</Method>
			<Method>
				<Name>totalCWQty</Name>
				<Source><![CDATA[
    display PdsCWInventQty totalCWQty()
    {
        PdsCWInventQty     qty;
        SalesQuotationLine localSalesQuotationLine;

        localSalesQuotationLine.setTmp();
        localSalesQuotationLine.setTmpData(salesQuotationLine_ds.cursor());

        while select RecId, PdsCWqty from localSalesQuotationLine
        {
            qty += (localSalesQuotationLine.RecId == salesQuotationLine.RecId ? salesQuotationLine.PdsCWQty : localSalesQuotationLine.PdsCWQty);
        }

        return qty;
    }

]]></Source>
			</Method>
			<Method>
				<Name>totalQty</Name>
				<Source><![CDATA[
    display SalesQty totalQty()
    {
        SalesQty qty;
        SalesQuotationLine localSalesQuotationLine;

        localSalesQuotationLine.setTmp();
        localSalesQuotationLine.setTmpData(salesQuotationLine_ds.cursor());

        while select RecId, SalesQty from localSalesQuotationLine
        {
            qty += (localSalesQuotationLine.RecId == salesQuotationLine.RecId ? salesQuotationLine.SalesQty : localSalesQuotationLine.SalesQty);
        }

        return qty;
    }

]]></Source>
