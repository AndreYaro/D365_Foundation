        public void linkActive()
        {
            SalesQuotationLine localSalesQuotationLine;

            if (element.args().record().TableId == tablenum(SalesQuotationLine))
            {
                localSalesQuotationLine = element.args().record();
                salesQuotationTable = localSalesQuotationLine.salesQuotationTable();

                salesQuotationTableForm = SalesQuotationTableForm::construct(element.args().parmEnum(), localSalesQuotationLine);
                salesQuotationTableForm.salesQuotationLine_Init(salesQuotationLine);
                if (#PdsCatchWeightEnabled)
                {
                    cwFormCtrl.parmSalesQuotationTableForm_DlvSchedule(salesQuotationTableForm);
                }

                TabPageCharges.visible(localSalesQuotationLine.LineDeliveryType == LineDeliveryType::OrderLine);
                dataChanged = false;
            }

            super();
        }

]]></Source>
