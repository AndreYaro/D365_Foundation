			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void  init()
    {
        if (! element.args().caller()
        ||  ! element.args().record())
        {
           throw error("@SYS22539");
        }

        if (#PdsCatchWeightEnabled)
        {
            cwFormCtrl = PdsCWFormCtrl_SalesQuotationDelSched::construct();
        }

        super();

        if (cwFormCtrl)
        {
            cwFormCtrl.parmFormRun(element);
            cwFormCtrl.setDatasources(salesQuotationLine);
            cwformCtrl.initPost();
            cwFormCtrl.dataSourceInitPost(salesQuotationLine);
        }
    }

]]></Source>
