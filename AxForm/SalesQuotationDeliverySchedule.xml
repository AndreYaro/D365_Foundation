					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        void  write()
        {
            NumberSeq   numberSeq;

            if (salesQuotationTable.checkUpdate())
            {
                salesQuotationTableForm.salesQuotationLine_Write(salesQuotationLine);

                if (this.validateWrite())
                {
                    if (salesQuotationLine.RecId)
                    {
                        salesQuotationLine.doUpdate();
                    }
                    else
                    {
                        numberSeq = NumberSeq::newGetNum(InventParameters::numRefInventTransId());
                        salesQuotationLine.InventTransId = numberSeq.num();
                        
                        SalesQuotationLine quotationLine;
                        quotationLine.setTmp();
                        quotationLine.setTmpData(SalesQuotationLine_ds.cursor());
                        salesQuotationLine.LineCreationSequenceNumber = salesQuotationTableForm.nextCreationSequenceNumber(quotationLine, salesQuotationLine.QuotationId);
                        
                        salesQuotationLine.doInsert();
                    }

                    dataChanged = true;

                    salesQuotationLine_ds.reread();
                    salesQuotationLine_ds.refresh();
                }
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>delete</Name>
						<Source><![CDATA[
        public void delete()
        {
            if (salesQuotationLine.RecId)
            {
                if (salesQuotationLine.checkDelete())
                {
                    super();
                    dataChanged = true;
                }
            }
            else
            {
                super();
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>validateWrite</Name>
						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean                     ret;
            SalesQuotationLine          salesQuotationLineOrig;

            salesQuotationTableForm.setTouched(salesQuotationTable);

            ret = super();

            if (ret)
            {
               ret = salesQuotationTableForm.salesQuotationLine_ValidateWrite(salesQuotationLine, salesQuotationLine.inventDim());
            }

            if (ret && salesQuotationLine.SalesQty < 0)
            {
                // Quantity cannot be negative on a delivery line.
                ret = checkFailed("@SYS131411");
            }

            if (ret && salesQuotationLine.RecId)
            {
                salesQuotationLineOrig = salesQuotationLine.orig();

                if (salesQuotationLineOrig
                    && abs(salesQuotationLine.SalesQty)  <  abs(salesQuotationLineOrig.SalesQty)
                    && salesQuotationLine.SalesQty * salesQuotationLineOrig.SalesQty >= 0 )
                {
                    if (salesQuotationLine.RemainSalesPhysical * salesQuotationLineOrig.RemainSalesPhysical   < 0 ||
                        salesQuotationLine.RemainSalesPhysical * salesQuotationLineOrig.RemainSalesFinancial  < 0)
                    {
                        // Quantity cannot be reduced because the remaining update quantity changes sign.
                        ret = checkFailed("@SYS25508");
                    }
                }
            }

            return ret;
        }

]]></Source>
