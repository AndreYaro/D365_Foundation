			<Method>
				<Name>getAddressController</Name>
				<Source><![CDATA[
    public LogisticsPostalAddressFormHandler getAddressController()
    {
        return addressController;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        #isoCountryRegionCodes
        super();
        this.updateDesign();

        Roundoff_accountRoundOffGain.parmPostingType(LedgerPostingType::None);

        Roundoff_accountRoundOffLoss.parmPostingType(LedgerPostingType::None);

        addressController = LogisticsPostalAddressFormHandler::newParameters(taxAuthorityAddress_ds, logisticsPostalAddress_ds);
        addressController.callerUpdateQuery(fieldNum(TaxAuthorityAddress,Location));

        if (!TaxIntegrationUtils::isMultipleTaxIdEnabled())
        {
            TabTaxregistration.visible(false);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    void updateDesign()
    {
        if (taxAuthorityAddress.isIT())
        {
            reportLayout_PrintBlankPage.visible(true);
            reportLayout_SeparateTaxSummary.visible(true);
            TaxAuthorityAddress_ds.object(fieldNum(TaxAuthorityAddress, RoundOffGainLedgerDimension)).visible(true);
            TaxAuthorityAddress_ds.object(fieldNum(TaxAuthorityAddress, RoundOffLossLedgerDimension)).visible(true);
        }
        else
        {
            reportLayout_PrintBlankPage.visible(false);
            reportLayout_SeparateTaxSummary.visible(false);
            TaxAuthorityAddress_ds.object(fieldNum(TaxAuthorityAddress, RoundOffGainLedgerDimension)).visible(false);
            TaxAuthorityAddress_ds.object(fieldNum(TaxAuthorityAddress, RoundOffLossLedgerDimension)).visible(false);
        }

        if (TaxParameters::find().TaxReportFormatMappingID_UK)
        {
            ReportLayout_UseDefaultLayout.visible(true);
            boolean isDefaultLayout = taxAuthorityAddress.TaxReportLayout == TaxReportLayout::Default;
            ReportLayout_UseDefaultLayout.enabled(isDefaultLayout);
        }
    }

]]></Source>
			</Method>
