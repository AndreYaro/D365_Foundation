				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        /// Catalog Edit.
        /// </summary>
        public void clicked()
        {
            RetailCatalogInternalOrg catalogInternalOrg;
            Args args = new Args(element);
            MenuFunction menuFunction;

            if (Catalog.State == CatalogState::Published)
            {
                menuFunction = new MenuFunction(menuItemActionStr(RetailCatalogEditAction), MenuItemType::Action);
            }
            else
            {
                menuFunction = new MenuFunction(menuItemDisplayStr(RetailCatalogEdit), MenuItemType::Display);
                menuFunction.openMode(OpenMode::Edit);
            }

            args.record(Catalog);

            menuFunction.run(args);
        }

]]></Source>
					</Method>
				</Methods>
