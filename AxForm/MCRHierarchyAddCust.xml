			<Method>
				<Name>deselectCustomer</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Deletes the selected record from the temporary table.
    /// </summary>
    void deselectCustomer()
    {
        ttsbegin;

        if (tmpMCRCustCategorySelected_ds.anyMarked())
        {
            tmpMCRCustCategorySelected_ds.deleteMarked();
        }
        else if (tmpMCRCustCategorySelected_ds.getFirst())
        {
            tmpMCRCustCategorySelected_ds.delete();
        }

        ttscommit;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        Common                  commonForEmptyTitleDS;
        QueryBuildDataSource    qbds;

        if (!element.args() ||
            !element.args().caller() ||
            !element.args().record() ||
            !element.args().dataset() ||
            element.args().dataset() != tablenum(MCRCustCategory))
        {
            throw error(strfmt("@SYS22678",element.name()));
        }

        caller = element.args().caller();
        currentCategory.data(element.args().record());

        super();

        //set category filter, based on inheritance.
        qbds = mcrCustCategory_ds.query().dataSourceTable(tablenum(MCRCustCategory));
        qbds.clearRange(fieldnum(MCRCustCategory, CategoryHierarchy));
        qbds.addRange(fieldnum(MCRCustCategory, CategoryHierarchy))
            .value(queryValue(currentCategory.CategoryHierarchy));

        // To clear dynamic Title dataSource, need to pass either null or empty table buffer.
        element.args().record(commonForEmptyTitleDS);
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        super();
        element.wait(true);
    }

]]></Source>
			</Method>
			<Method>
				<Name>selectCustomer</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Adds the selected customer record to the temporary table.
    /// </summary>
    void selectCustomer()
    {
        CustTable           selectedCustomer;
        MCRCustCategory     tmpCustCategory;

        tmpCustCategory.setTmp();

        // Are there any marked records?
        for ( selectedCustomer = custTable_ds.getFirst(1)?custTable_ds.getFirst(1):custTable_ds.cursor();
              selectedCustomer;
              selectedCustomer = custTable_ds.getNext())
        {
            tmpCustCategory.setTmpData(tmpMCRCustCategorySelected);

            select firstonly RecId from tmpCustCategory
                where tmpCustCategory.CategoryHierarchy == currentCategory.CategoryHierarchy
                    && tmpCustCategory.Customer == selectedCustomer.RecId;

            if (!tmpCustCategory.RecId)
            {
                tmpCustCategory.clear();
                tmpCustCategory.ParentCategory     = currentCategory.RecId;
                tmpCustCategory.CategoryHierarchy  = currentCategory.CategoryHierarchy;
                tmpCustCategory.Customer           = selectedCustomer.RecId;
                tmpCustCategory.Name               = selectedCustomer.name();
                tmpCustCategory.doInsert();
            }
            else
            {
                error (strFmt("@MCR39551",
                            CustTable::findRecId(selectedCustomer.RecId).name(),
                            MCRCustCategoryHierarchy::find(currentCategory.CategoryHierarchy).Name));
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>setControlState</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Sets the state of the control on the depending on the formdatasource record selected.
    /// </summary>
    /// <param name="_formDataSource">
    ///  The form dataSource.
    /// </param>
    /// <param name="_control">
    ///  The FormControl that needs to be enabled or disabled based on the FormdataSource selected record.
    /// </param>
    void setControlState(FormDataSource _formDataSource, FormControl _control)
    {
        Common selectedRecord;

        selectedRecord = _formDataSource.getFirst(true)? _formDataSource.getFirst(true): _formDataSource.cursor();
        _control.enabled(selectedRecord.RecId);
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateCategoryCustomer</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Adds all the records from the temporary table to the <c>MCRCustCategory</c> table.
    /// </summary>
    void updateCategoryCustomer()
    {
        MCRCustCategory             mcrCustCategoryLocal;

        while select forUpdate * from tmpMCRCustCategorySelected
        {
            mcrCustCategoryLocal.clear();
            mcrCustCategoryLocal.initValue();
            mcrCustCategoryLocal.initFromParent(currentCategory);

            mcrCustCategoryLocal.Customer = tmpMCRCustCategorySelected.Customer;
            mcrCustCategoryLocal.Name = CustTable::findRecId(
                                            tmpMCRCustCategorySelected.Customer).name();
            if (!mcrCustCategoryLocal.Name)
            {
                mcrCustCategoryLocal.Name  = MCRCustCategory::findNonExistNameInHierarchy(currentCategory.CategoryHierarchy);
            }

            mcrCustCategoryLocal.addToHierarchy();
        }
    }

]]></Source>
