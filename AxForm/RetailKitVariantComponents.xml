				<Methods>
					<Method>
						<Name>UpdateCaption</Name>
						<Source><![CDATA[
        /// <summary>
        /// Update the form caption.
        /// </summary>
        /// <param name="_kitVariantRecId">
        /// The kit variant identifier for the kit description to use.
        /// </param>
        /// <remarks>
        /// Sets the form caption using the kit variant description in the current language.
        /// </remarks>
        private void UpdateCaption(RefRecId _kitVariantRecId)
        {
            str kitVariantDescription = EcoResDistinctProductVariant::productDescriptionBasedOnDimValues(
                                            EcoResDistinctProductVariant::find(_kitVariantRecId));

            // Override the form caption
            element.design().caption(kitVariantDescription);
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        /// <summary>
        /// RetailKitVariantComponent datasource initialization.
        /// </summary>
        /// <remarks>
        /// Initialize the RetailKitVariantComponent datasource for the given kit.
        /// </remarks>
        public void init()
        {
            SalesLine callerSalesLine;
            SalesParmLine callerSalesParmLine;
            InventTransId callerInventTransId;
            RefRecId callerKitVariantRecId;

            InventTransOrigin inventTransOrigin;
            InventDimCombination inventDimCombo;
            QueryBuildDataSource qbds;

            super();

            if (element.args().record() is SalesLine)
            {
                // if from Sales Order form...
                callerSalesLine = element.args().record();

                // ... and it's a kit...
                if (RetailKit::existByProductMaster(callerSalesLine.inventTable().Product))
                {
                    callerInventTransId = callerSalesLine.InventTransId;
                }
            }
            else if (element.args().record() is SalesParmLine)
            {
                // if from Picking/Packing list...
                callerSalesParmLine = element.args().record();

                // ... and it's a kit...
                if (RetailKit::existByProductMaster(callerSalesParmLine.inventTable().Product))
                {
                    callerInventTransId = callerSalesParmLine.InventTransId;
                }
            }

            if (callerInventTransId)
            {
                // Get the DistinctProductVariant by way of the InventDimCombination (invent dim w/o site & locataion information) and InventTransOrigin
                select DistinctProductVariant from inventDimCombo
                exists join inventTransOrigin where
                    inventDimCombo.ItemId == inventTransOrigin.ItemId
                    && inventDimCombo.InventDimId == inventTransOrigin.ItemInventDimId
                    && inventTransOrigin.InventTransId == callerInventTransId;

                if (inventDimCombo)
                {
                    // DistinctProductVariant is the Kit Variant.
                    callerKitVariantRecId = inventDimCombo.DistinctProductVariant;
                }
            }

            // Filter the list for only those components belonging to the given Kit Variant.
            qbds = this.query().dataSourceTable(this.table());
            qbds.addRange(fieldNum(RetailKitVariantComponent, KitVariant)).value(queryValue(callerKitVariantRecId));

            // Update the caption with the Kit Variant.
            this.UpdateCaption(callerKitVariantRecId);
        }

]]></Source>
