						<Name>editConfigGroup</Name>
						<Source><![CDATA[
        /// <summary>
        /// Controls the value of the <c>ConfigGroupId</c> <c>ReferenceGroup</c>.
        /// </summary>
        /// <param name="_set">
        /// A Boolean value that specifies whether a new value has been specified.
        /// </param>
        /// <param name="_ecoResNomenclatureSegment">
        /// A <c>EcoResNomenclatureSegment</c> record.
        /// </param>
        /// <param name="_value">
        /// A string containing the ConfigGroupId.
        /// </param>
        /// <returns>
        /// A string containing the ConfigGroupId for the <c>EcoResNomenclatureSegment</c>.
        /// </returns>
        public edit str editConfigGroup(
            boolean                     _set,
            EcoResNomenclatureSegment   _ecoResNomenclatureSegment,
            str                         _value)
        {
            if (_set)
            {
                if(_ecoResNomenclatureSegment is EcoResNomenclatureSegmentConfigGroupItemId)
                {
                    EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemId = _ecoResNomenclatureSegment as EcoResNomenclatureSegmentConfigGroupItemId;
                    EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemId.ConfigGroupId = _value;

                    return EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemId.ConfigGroupId;
                }
                else if(_ecoResNomenclatureSegment is EcoResNomenclatureSegmentConfigGroupItemName)
                {
                    EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemName = _ecoResNomenclatureSegment as EcoResNomenclatureSegmentConfigGroupItemName;
                    EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemName.ItemNameConfigGroupId = _value;

                    return EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemName.ItemNameConfigGroupId;
                }
            }
    
            if(_ecoResNomenclatureSegment is EcoResNomenclatureSegmentConfigGroupItemId)
            {
                EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemId = _ecoResNomenclatureSegment as EcoResNomenclatureSegmentConfigGroupItemId;
                return EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemId.ConfigGroupId;
            }
            else if (_ecoResNomenclatureSegment is EcoResNomenclatureSegmentConfigGroupItemName)
            {
                EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemName = _ecoResNomenclatureSegment as EcoResNomenclatureSegmentConfigGroupItemName;
                return EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemName.ItemNameConfigGroupId;
            }
            
            return _value;
        }

]]></Source>
