[Form]
public class EcoResNomenclature extends FormRun
{
    TableId nomenclatureType;

    PCClass pcClass;
    BOMTable bomTable;

    Map allowedSegmentsMap;
    Map controlIdToSegmentTableId;

    NumberSeqScope scope;

    EcoResNomenclatureForm nomenclatureForm;

}
]]></Source>
			</Method>
			<Method>
				<Name>setSimpleMode</Name>
				<Source><![CDATA[
    /// <summary>
    /// Sets the form in simple segment entry mode.
    /// </summary>
    public void setSimpleMode()
    {
        NavigationListGroup.visible(false);
        HeaderDetails.visible(false);
        EcoResNomenclature_ds.allowCreate(false);
        EcoResNomenclature_DS.allowDelete(false);
        element.design().showNewButton(false);
        element.design().showDeleteButton(false);
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        nomenclatureForm = EcoResNomenclatureForm::construct();

        nomenclatureForm.initNomenclatureType(element.args().dataset(), element.args().record());
        nomenclatureType = nomenclatureForm.parmNomenclatureType();

        pcClass = nomenclatureForm.parmPCClass();
        bomTable = nomenclatureForm.parmBOMTable();

        super();

        allowedSegmentsMap = EcoResNomenclatureForm::getAllowedNomenclatureSegments(nomenclatureType, element.args().lookupRecord());

        element.addSegmentButtons();
        element.toggleSegmentFieldVisibility();

        if (nomenclatureForm.runInSingleRecordMode())
        {
            element.setSimpleMode();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>addSegmentButtons</Name>
				<Source><![CDATA[
    /// <summary>
    /// Adds menu buttons for allowed segment types.
    /// </summary>
    private void addSegmentButtons()
    {
        controlIdToSegmentTableId = new Map(Types::Int64, Types::Int64);

        MapEnumerator tablesEnumerator = allowedSegmentsMap.getEnumerator();

        while (tablesEnumerator.moveNext())
        {
            TableId segmentTableId = tablesEnumerator.currentValue();
            str segmentLabel = tablesEnumerator.currentKey();
            str segmentButtonName = 'AddSegmentButton' + int2Str(segmentTableId);
            
            if (segmentLabel == "@SCM:EcoResNomenclatureSegmentAttributeName")
            {
                segmentButtonName += 'Name';
            }

            FormButtonControl segmentButton = NewSegmentMenu.addControl(FormControlType::Button, segmentButtonName);
            segmentButton.text(segmentLabel);
            segmentButton.registerOverrideMethod(methodStr(FormButtonControl, clicked), formMethodStr(EcoResNomenclature, 'AddSegmentButton_Clicked'), element);

            controlIdToSegmentTableId.insert(segmentButton.id(), segmentTableId);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>toggleSegmentFieldVisibility</Name>
				<Source><![CDATA[
    /// <summary>
    /// Toggles visibility on segment fields depending if the corresponding segment type is allowed.
    /// </summary>
    private void toggleSegmentFieldVisibility()
    {
        DictTable segmentTable;

        segmentTable =  new SysDictTable(EcoResNomenclatureSegment_EcoResNomenclatureSegmentAttributeValue_ds.table());
        EcoResNomenclatureSegmentAttributeValue_Attribute.visible(allowedSegmentsMap.exists(segmentTable.label()));

        segmentTable =  new SysDictTable(EcoResNomenclatureSegment_EcoResNomenclatureSegmentConfigGroupItemId_ds.table());
        EcoResNomenclatureSegmentConfigGroup.visible(allowedSegmentsMap.exists(segmentTable.label()));

        segmentTable =  new SysDictTable(EcoResNomenclatureSegment_EcoResNomenclatureSegmentTextConstant_ds.table());
        EcoResNomenclatureSegmentTextConstant_Text.visible(allowedSegmentsMap.exists(segmentTable.label()));

        segmentTable =  new SysDictTable(EcoResNomenclatureSegment_EcoResNomenclatureSegmentNumberSequence_ds.table());
        EcoResNomenclatureSegmentNumberSequence_NumberSequence.visible(allowedSegmentsMap.exists(segmentTable.label()));
    }

]]></Source>
			</Method>
			<Method>
				<Name>getNumberSeqScope</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets or creates the number sequence scope.
    /// </summary>
    /// <returns>The current number sequence scope.</returns>
    public NumberSeqScope getNumberSeqScope()
    {
        if (!scope)
        {
            container numberSequenceModules = [NumberSeqModule::EcoResProduct];
            scope = NumberSeqScopeFactory::createGlobalScope();
            NumberSeqApplicationModule::createReferencesMulti(numberSequenceModules, scope);
        }

        return scope;
    }

]]></Source>
			</Method>
			<Method>
				<Name>AddSegmentButton_Clicked</Name>
				<Source><![CDATA[
    /// <summary>
    /// Creates a new record based on the segment button clicked.
    /// </summary>
    /// <param name = "_formButtonControl">The clicked button.</param>
    public void AddSegmentButton_Clicked(FormButtonControl _formButtonControl)
    {
        EcoResNomenclatureSegment_ds.last();

        Map tableNamesMap = new Map(Types::String, Types::String);
        tableNamesMap.insert(EcoResNomenclatureSegment_ds.name(), tableId2Name(controlIdToSegmentTableId.lookup(_formButtonControl.id())));
        EcoResNomenclatureSegment_ds.createTypes(tableNamesMap, true);

        if (_formButtonControl.text() == "@SCM:EcoResNomenclatureSegmentAttributeName")
        {
            EcoResNomenclatureSegment_EcoResNomenclatureSegmentAttributeValue.AttributeSegmentType = EcoResNomenclatureAttributeSegmentType::Name;
        }
    }

]]></Source>
