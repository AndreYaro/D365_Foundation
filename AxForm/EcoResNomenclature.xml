			<Method>
				<Name>createRecord</Name>
				<Source><![CDATA[
    public void createRecord(str _formDataSourceName, boolean _append = false)
    {
        Map tableNamesMap = new Map(Types::String, Types::String);

        if (_formDataSourceName == EcoResNomenclature_ds.name())
        {
            // Set the specific sub table
            tableNamesMap.insert(_formDataSourceName, tableId2Name(nomenclatureType));
            EcoResNomenclature_ds.createTypes(tableNamesMap, _append);
        }
        else if (_formDataSourceName == EcoResNomenclatureSegment_ds.name())
        {
            // Give the user a dialog in which to select the segment type
            TableId selectedSubTableId = EcoResPolymorphicCreateSelector::promptForTableId("@SCM:EcoResCreateNewSegment", "@SCM:EcoResNomenclatureSegmentType", allowedSegmentsMap);
            if (selectedSubTableId)
            {
                EcoResNomenclatureSegment_ds.last();

                tableNamesMap.insert(_formDataSourceName, tableId2Name(selectedSubTableId));
                EcoResNomenclatureSegment_ds.createTypes(tableNamesMap, true);
            }
        }
        else
        {
            super(_formDataSourceName, _append);
        }
    }

]]></Source>
