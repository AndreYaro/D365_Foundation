						<Name>lookupReference</Name>
						<Source><![CDATA[
        public Common lookupReference()
        {
            Query                   lookupQuery;
            SysReferenceTableLookup sysTableLookup  = SysReferenceTableLookup::newParameters(tableNum(NumberSequenceTable), this);

            sysTableLookup.addLookupfield(fieldNum(NumberSequenceTable, NumberSequence));
            sysTableLookup.addLookupfield(fieldNum(NumberSequenceTable, Txt));

            lookupQuery = new Query();
            QueryBuildDataSource qbds = lookupQuery.addDataSource(tableNum(NumberSequenceTable));

            // Only include numbersequences currently set to automatic
            qbds.addRange(fieldNum(NumberSequenceTable, Manual)).value(queryValue(enum2int(NoYes::No)));

            // Only include number sequences in the scope
            qbds.addRange(fieldNum(NumberSequenceTable, NumberSequenceScope)).value(queryValue(element.getNumberSeqScope().getId(true)));

            sysTableLookup.parmQuery(lookupQuery);
            return sysTableLookup.performFormLookup();
        }

]]></Source>
