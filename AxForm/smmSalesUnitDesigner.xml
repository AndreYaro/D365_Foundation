						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            QueryBuildDataSource    queryBuildDataSource;
            QueryBuildRange         rangeSalesUnitType;
            QueryBuildRange         rangeShowOnlyActive;

            super();

            this.query().dataSourceTable(tablenum(smmSalesUnitMembers)).clearDynalinks();

            // Get the manual created sales unit section datasource
            queryBuildDataSource = this.query().dataSourceTable(tablenum(smmSalesUnit));

            // If the datasource isn't created already
            if (!queryBuildDataSource)
            {
                // Add the extra sales unit selection data source
                queryBuildDataSource    = this.query().dataSourceTable(tablenum(smmSalesUnitMembers)).addDataSource(tablenum(smmSalesUnit));

                // Add  new range for filtering on sales unit type, active
                rangeSalesUnitType   = queryBuildDataSource.addRange(fieldnum(smmSalesUnit, UnitType));
                rangeShowOnlyActive  = queryBuildDataSource.addRange(fieldnum(smmSalesUnit, Active));

                // Link the new datasource to the existing query
                queryBuildDataSource.joinMode(JoinMode::ExistsJoin);
                queryBuildDataSource.fetchMode(QueryFetchMode::One2One);
                queryBuildDataSource.addLink(fieldnum(smmSalesUnitMembers, SalesUnitId), fieldnum(smmSalesUnit, SalesUnitId));

                // The new datasource should be hidden in the query selection form
                rangeSalesUnitType.status(RangeStatus::Hidden);
                rangeShowOnlyActive.status(RangeStatus::Hidden);
            }

            // Filter on the selected sales unit type
            rangeSalesUnitType.value(queryValue(salesUnitType));
            rangeShowOnlyActive.value(queryValue(showOnlyActive));
            rangeShowOnlyActive.enabled(showOnlyActive);
        }

]]></Source>
