			<Method>
				<Name>setButtonPanel</Name>
				<Source><![CDATA[
    void setButtonPanel()
    {
        boolean   enabledEmployee;
        boolean   enabledTreeRoot;
        boolean   enabledDelete;

        enabledEmployee = smmSalesUnitMembers.SalesPersonWorker ? true : false;
        if ( !enabledEmployee && smmSalesUnitMembers)
        {
            enabledTreeRoot = true;
        }
        else
        {
            enabledTreeRoot = false;
        }

        enabledDelete = enabledTreeRoot || enabledEmployee;

        if (editEmplButton.enabled() != enabledEmployee)
        {
            editEmplButton.enabled(enabledEmployee);
        }

        if (salesManagerButton.enabled() != enabledEmployee)
        {
            salesManagerButton.enabled(enabledEmployee);
        }

        if (RemoveButton.enabled() != enabledDelete)
        {
            RemoveButton.enabled(enabledDelete);
        }

        if (upperSetup.enabled() != !enabledTreeRoot)
        {
            upperSetup.enabled(!enabledTreeRoot);
        }
    }

]]></Source>
