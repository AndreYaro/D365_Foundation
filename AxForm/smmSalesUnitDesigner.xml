				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            int             selected = salesUnitTree.getFirstSelected();
            FormTreeItem    item = salesUnitTree.getItem(selected);
            str             name = item.text();
            smmSalesUnitMembers::setAsSalesUnitManager(smmSalesUnitMembers);

            salesUnitMembersTree.updateParent(salesUnitTree.getParent(salesUnitTree.getSelection()));

            //SELECT THE ORIGINAL ITEM
            selected = salesUnitTree.getFirstSelected();
            selected = salesUnitTree.getChild(selected);
            while (selected)
            {
                item = salesUnitTree.getItem(selected);
                if (item.text() == name)
                {
                    salesUnitTree.select(selected);
                    break;
                }
                selected = salesUnitTree.getNextSibling(selected);
            }

            if (callerObj && callerObj is FormRun &&
                formHasMethod(callerObj,identifierstr(reRead)))
            {
                callerObj.reRead();
            }
        }

]]></Source>
