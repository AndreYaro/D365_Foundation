[Form]
public class smmSalesUnitDesigner extends FormRun
{
    #macrolib.resAppl
    #macrolib.resource

    Object                      callerObj;
    smmSalesUnit                callerSalesUnit;
    smmSalesUnitType            salesUnitType;
    NoYesId                     showOnlyActive;
    smmSalesUnitMembersTree     salesUnitMembersTree;
    int                         treeDragIdx;
    HcmWorkerLookup             hcmWorkerLookupCurrentCompany;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        callerObj = element.args().caller();

        // If it is called from sales unit
        if (element.args().dataset() == tablenum(smmSalesUnit))
        {
            callerSalesUnit = element.args().record();
        }

        // If unit type is passed as enum
        if (element.args().parmEnumType() == enumnum(smmSalesUnitType))
        {
            salesUnitType = element.args().parmEnum();
        }
        // If unit type is coming from caller form
        else if (callerObj && callerObj is FormRun
                  && SysFormRun::hasMethod(callerObj,identifierstr(unitTypeSelection)))
        {
            salesUnitType = callerObj.unitTypeSelection();

            if (SysFormRun::hasMethod(callerObj,identifierstr(showOnlyActiveSelection)))
            {
                showOnlyActive = callerObj.showOnlyActiveSelection();
            }
        }
        // If no unit type is specified
        else
        {
            // Form was called incorrectly.
            throw error("@SYS22996");
        }

        super();

        hcmWorkerLookupCurrentCompany = HcmWorkerLookup::newCustomOptions(true, true, true, false, true, true, true, true, true);
    }

]]></Source>
			</Method>
			<Method>
				<Name>initCaption</Name>
				<Source><![CDATA[
    void initCaption()
    {
        if (!element.args().dataset())
        {
            return;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initRightView</Name>
				<Source><![CDATA[
    void initRightView()
    {
        const str divider = " /--/ ";

        // Write employee id and name
        formatExplain.text(strfmt("@SYS53179", "@SYS81251", divider, "@SYS54564"));
    }

]]></Source>
			</Method>
			<Method>
				<Name>initTree</Name>
				<Source><![CDATA[
    void initTree()
    {
        salesUnitMembersTree = new smmSalesUnitMembersTree(smmSalesUnitMembers_DS,
                                                           salesUnitTree,
                                                           fieldnum(smmSalesUnitMembers, MemberId),
                                                           fieldnum(smmSalesUnitMembers, ParentId),
                                                           fieldnum(smmSalesUnitMembers, Identification),
                                                           false);

        salesUnitMembersTree.initRoot("@SYS79797", '');

        if (callerSalesUnit)
        {
            // Select the caller sales unit in tree
            salesUnitMembersTree.selectSalesUnitMember(callerSalesUnit.SalesUnitId);
        }
    }

]]></Source>
