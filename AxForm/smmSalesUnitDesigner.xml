			<Control>
				<Name>AddButton</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            boolean     salesUnitSelected = (smmSalesUnitMembers && !smmSalesUnitMembers.SalesPersonWorker) ? true : false;

            super();

            FormTreeItem formTreeItem = salesUnitTree.getItem(salesUnitTree.getSelection());

            if (salesUnitSelected)
            {
                treeDragIdx = formTreeItem.idx();
            }
            else
            {
                treeDragIdx = salesUnitTree.getParent(formTreeItem.idx());
            }

            if (workerTableDrag.RecId)
            {
                if (treeDragIdx > 0)
                {
                    salesUnitMembersTree.workerTableDrag(workerTableDrag);
                    salesUnitMembersTree.addMember(treeDragIdx);
                }
            }
        }

]]></Source>
