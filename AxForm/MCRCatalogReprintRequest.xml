			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Confirms that the value in the log ID field is a valid
    ///     log ID for catalog requests, otherwise an error is thrown.
    /// </summary>
    public void closeOk()
    {
        if (MCRLogIdField.text())
        {
            if (!MCRCatalogRequest::existsCatalogRequest(MCRLogIdField.text()))
            {
                checkFailed(strfmt("@MCR11529", MCRLogIdField.text()));
                return;
            }

            this.args().parm(MCRLogIdField.text());
        }
        super();
    }

]]></Source>
