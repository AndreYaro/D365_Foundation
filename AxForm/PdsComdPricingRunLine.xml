				<Source><![CDATA[
    public void updateDesign()
    {
        inventDim_ds.enableField(fieldnum(InventDim, InventSiteId),
            !pdsComdPricingRun.InventSiteId);

        pdsComdPricingRunLine_ds.enableField(fieldnum(PdsComdPricingRunLine, Cost),
            !(pdsComdPricingRunLine.UseStandardCost
                || pdsComdPricingRunLine.BaseItemId));

        pdsComdPricingRunLine_ds.enableField(
            fieldnum(PdsComdPricingRunLine, BaseItemId),
                !pdsComdPricingRunLine.UseStandardCost);

        pdsComdPricingRunLine_ds.enableField(fieldnum(PdsComdPricingRunLine,
            BaseInventSiteId),
                !pdsComdPricingRunLine.UseStandardCost
                && pdsComdPricingRunLine.BaseItemId);
        pdsComdPricingRunLine_ds.enableField(fieldnum(PdsComdPricingRunLine,
            BaseInventLocationId),
                !pdsComdPricingRunLine.UseStandardCost
                    && pdsComdPricingRunLine.BaseItemId);
        pdsComdPricingRunLine_ds.enableField(
            fieldnum(PdsComdPricingRunLine, PriceChange),
            !pdsComdPricingRunLine.UseStandardCost
                && pdsComdPricingRunLine.BaseItemId);
        pdsComdPricingRunLine_ds.enableField(
            fieldnum(PdsComdPricingRunLine, PricePerc),
            !pdsComdPricingRunLine.UseStandardCost
                && pdsComdPricingRunLine.BaseItemId);
        pdsComdPricingRunLine_ds.enableField(
            fieldnum(PdsComdPricingRunLine, PriceMult),
            !pdsComdPricingRunLine.UseStandardCost
                && pdsComdPricingRunLine.BaseItemId);
    }

