        void clicked()
        {
            MenuFunction        menufunction;
            VendTable           vendTableloc;
            CustTable           custTableloc;
            Args                args;
            FormRun             formRun;

            super();

            if (TmpTaxWithholdTaxJournal.VendAccount != '')
            {
                vendTableloc = VendTable::find(TmpTaxWithholdTaxJournal.VendAccount);
                menuFunction = new MenuFunction(menuitemDisplayStr(VendTrans), MenuItemType::Display);
                args = new Args(menuFunction.object());
                args.record(vendTableloc);
            }
            else
            {
                custTableloc = CustTable::find(TmpTaxWithholdTaxJournal.CustAccount);
                menuFunction = new MenuFunction(menuitemDisplayStr(CustTrans), MenuItemType::Display);
                args = new Args(menuFunction.object());
                args.record(custTableloc);
            }
            args.caller(element);

            formRun = ClassFactory::formRunClassOnClient(args);
            formRun.init();
            formRun.run();

            if (!formRun.closed())
            {
                formRun.wait();
            }
            TmpTaxWithholdTaxJournal.SourceTransVoucher = sourceTransVoucher;
            TmpTaxWithholdTaxJournal.SourceTransDate    = sourceTransDate;
            TmpTaxWithholdTaxJournal.SourceTransTableId = sourceTransTableId;
            TmpTaxWithholdTaxJournal.SourceTransRecId   = sourceTransRecId;
            TmpTaxWithholdTaxJournal_ds.write();
        }

