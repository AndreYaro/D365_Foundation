					<Method>
						<Name>markTrans</Name>
						<Source><![CDATA[
        public edit NoYes markTrans(
            boolean set,
            CustVendSettlement _vendSettlement,
            NoYes _markTrans)
        {
            NoYes markTransLocal = _markTrans;

            if (set)
            {
                if (_markTrans == NoYes::Yes)
                {
                    if (specTransManager.exist(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId) == false) //! specOffsetVoucher.isMarked(_vendSettlement.TableId, _vendSettlement.RecId))
                    {
                        settle.insert(_vendSettlement.RecId, _vendSettlement.SettleAmountCur);
                        specTransManager.insert(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId, _vendSettlement.SettleAmountCur, vendTrans.CurrencyCode);
                    }
                    else
                    {
                        markTransLocal = NoYes::No;
                        error("@SYS29065");
                    }
                }
                else
                {
                    specTransManager.delete(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId);

                    settle.insert(_vendSettlement.RecId, _vendSettlement.SettleAmountCur);
                }

                vendTrans_ds.refreshEx(-1);
            }
            else
            {
                markTransLocal = (specTransManager.exist(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId)
                    && specTransManager.existForOtherSpec(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId) == false);
            }

            return markTransLocal;
        }

]]></Source>
					</Method>
					<Method>
						<Name>isMarked</Name>
						<Source><![CDATA[
        public display ImageRes isMarked(VendSettlement _vendSettlement)
        {
            if (specTransManager.existForOtherSpec(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId))
            {
                return imageList.image(#ImageAboveEstimate);
            }

            return -1;
        }

]]></Source>
					</Method>
					<Method>
						<Name>settleAmountCur</Name>
						<Source><![CDATA[
        public edit Amount settleAmountCur(
            boolean set,
            VendSettlement _vendSettlement,
            Amount settleAmount)
        {
            Amount settleAmountCur;
            VendTrans localVendTrans;

            if (set)
            {
                this.settleAmountMST(
                    true,
                    _vendSettlement,
                    CurrencyExchangeHelper::curAmount2CurAmount(settleAmount, ledgerJournalTrans.CurrencyCode, localVendTrans.CurrencyCode, ledgerJournalTrans.TransDate, false));
            }

            if (settle.exists(_vendSettlement.RecId))
            {
                settleAmountCur = settle.lookup(_vendSettlement.RecId);
            }

            return CurrencyExchangeHelper::curAmount2CurAmount(settleAmountCur, vendTrans.CurrencyCode, ledgerJournalTrans.CurrencyCode, ledgerJournalTrans.TransDate, false);
        }

]]></Source>
					</Method>
					<Method>
						<Name>settleAmountMST</Name>
						<Source><![CDATA[
        public edit Amount settleAmountMST(
            boolean set,
            VendSettlement _vendSettlement,
            Amount settleAmount)
        {
            Amount tmpAmount;
            Amount bufferAmount;

            if (set)
            {
                tmpAmount = settleAmount;

                if (!tmpAmount)
                {
                    if (specTransManager.exist(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId) == false)
                    {
                        tmpAmount =  (select firstonly Balance01 from specTrans
                                        where specTrans.RefTableId  == _vendSettlement.TableId
                                           && specTrans.RefRecId    == _vendSettlement.RecId).Balance01;
                    }
                }

                settle.insert(_vendSettlement.RecId, tmpAmount);

                if (specTransManager.exist(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId) == true)
                {
                    specTransManager.updateSettleAmount(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId, tmpAmount);
                }
            }
            else
            {
                if (_vendSettlement.RecId)
                {
                    if (!settle.exists(_vendSettlement.RecId) || !settle.lookup(_vendSettlement.RecId))
                    {
                        if (specTransManager.exist(_vendSettlement.DataAreaId, _vendSettlement.TableId, _vendSettlement.RecId) == true)
                        {
                            bufferAmount = (select firstonly Balance01 from specTrans
                                                where specTrans.RefTableId  == _vendSettlement.TableId
                                                   && specTrans.RefRecId    == _vendSettlement.RecId).Balance01;
                        }
                        else
                        {
                            bufferAmount = _vendSettlement.SettleAmountCur;
                        }

                        settle.insert(_vendSettlement.RecId, bufferAmount);
                    }
                }
            }

            if (_vendSettlement.RecId)
            {
                if (settle.exists(_vendSettlement.RecId))
                {
                    return settle.lookup(_vendSettlement.RecId);
                }
            }

            return 0;
        }

]]></Source>
