			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class VendVendorPortalInvoiceWorkspace extends FormRun implements SysIFilterProvider, VendIInvoiceRefresh
{
    SysFilterChangeEvent changeEvent;
    boolean formInitError;

}
]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        super();

        if (!formInitError)
        {
            SysFilterLastValueHelper::saveLastValue(vendAccountFilter);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        VendTable vendTable;
        select firstonly AccountNum from vendTable;

        if (!vendTable)
        {
            formInitError = true;
            error("@AccountsPayable:VendorPortalInsufficientRights");
            this.close();
        }

        super();

        SysFilterLastValueHelper::getLastValue(vendAccountFilter);

        VendAccount vendAccount = vendAccountFilter.valueStr();

        if (!vendAccount || !VendTable::find(vendAccount))
        {
            vendAccountFilter.text(vendTable.AccountNum);
        }

        changeEvent = SysFilterChangeEvent::newFromFormControl(vendAccountFilter);
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmFilter</Name>
				<Source><![CDATA[
    public SysIFilter parmFilter()
    {
        SysIFilter filter = SysFilterFactory::createEmptyFilter();

        SysIFilterValue filterValue = SysFilterValueFactory::createFilterValueForString(vendAccountFilter.valueStr(), extendedTypeNum(VendAccount));
        filter.addItem(filterValue, SysFilterOperator::Equal, SysFilterValueResolutionMethod::None);
        return filter;
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmChangeEvent</Name>
				<Source><![CDATA[
    public SysFilterChangeEvent parmChangeEvent()
    {
        return changeEvent;
    }

]]></Source>
			</Method>
			<Method>
				<Name>refresh</Name>
				<Source><![CDATA[
    public void refresh()
    {
        TileDataService::forceRefresh(identifierStr(VendVendorPortalInvoiceListPageDraft), SysDataServiceFormHelper::newFromCaller(this));
        TileDataService::forceRefresh(identifierStr(VendVendorPortalInvoiceListPageInReview), SysDataServiceFormHelper::newFromCaller(this));

        this.doRefresh();
    }

]]></Source>
			</Method>
