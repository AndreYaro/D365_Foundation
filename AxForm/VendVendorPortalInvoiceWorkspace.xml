				<Name>CreateInvoiceFromPO</Name>
				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            // Manually run the VendCreateInvoiceFromVendor menu item, to set the parm() value to vendAccountFilter.valueStr()
            Args args = new Args();

            args.caller(element);
            args.parm(vendAccountFilter.valueStr());

            MenuFunction menuFunction = new MenuFunction(menuitemDisplayStr(VendCreateInvoiceFromVendor), MenuItemType::Display);
            menuFunction.run(args);
        }

]]></Source>
					</Method>
