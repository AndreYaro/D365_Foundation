			<Method>
				<Name>populateCDXSessionsSummaryData</Name>
				<Source><![CDATA[
    /// <summary>
    /// Builds an aggregated summary of the upload sessions history.
    /// </summary>
    public void populateCDXSessionsSummaryData()
    {
        // Retrieve the workspace level filter from the RetailITWorkspace
        SysIFilter  filter = this.parmFilterProvider().parmFilter();

        wsDataGroupFilter = SysFilterValueHelper::getFirstValueRecordFromFilterForType(filter, SysFilterValueTypeEnum::Integer, extendedTypeNum(RetailCDXDataGroupRefRecId));

        // prepares the session summary data and insert it to RetailCDXUploadSummaryCache cache table
        RetailCDXUploadSummaryCacheConsumer::prepareDataSet(wsDataGroupFilter);

        // filter the  upload session summary data using the specified workspace level datagroup filter value.
        QueryBuildDataSource qbds = RetailCDXUploadSummaryCache_DS.query().dataSourceTable(tablenum(RetailCDXUploadSummaryCache));
        qbds.clearRanges();

        QueryBuildRange dataGroupRange = qbds.addRange(fieldnum(RetailCDXUploadSummaryCache, DataGroupRecId));
        dataGroupRange.value(SysQuery::value(wsDataGroupFilter));

        RetailCDXUploadSummaryCache_DS.executeQuery();
    }

]]></Source>
