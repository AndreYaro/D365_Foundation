					<Method>
						<Name>loadCompensationGrid</Name>
						<Source><![CDATA[
        private void loadCompensationGrid()
        {
            compGridView = new HcmPositionForecastCompGridView(compensationRate.RecId);

            // Loading the data into the temp table
            compGridView.setTmpGridData();

            // Updating the labels of the grid and hiding the columns based on reference point setup
            HcmPositionForecastComTmpGrid.setTmpData(compGridView.parmTmpGrid());
            compGridView.setControls(HcmPositionForecastComTmpGrid_ds, element.design());
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            if (!element.args() || (element.args().dataset() != tableNum(HcmPositionForecastCompensationRate)))
            {
                throw error(strFmt("@SYS25516",element.name()));
            }

            // Setting title text from the parent form.
            compensationRate = element.args().record();

            super();
            HcmPositionForecastComTmpGrid_ds.loadCompensationGrid();
        }

]]></Source>
