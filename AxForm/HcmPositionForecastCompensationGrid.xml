			<Method>
				<Name>updateHCMPositionForecastComTmpGrid</Name>
				<Source><![CDATA[
    /// <summary>
    /// Perform mass update on the <c>HcmPositionForecastComTmpGrid</c> table.
    /// </summary>
    /// <param name = "_adjustmentType">The <c>HcmPositionForecastCompAdjustmentType</c> enum value</param>
    /// <param name = "_adjustmentValue">The adjustment value for mass update</param>
    public void updateHCMPositionForecastComTmpGrid(
                      HcmPositionForecastCompAdjustmentType _adjustmentType = HcmPositionForecastCompAdjustmentType::FlatAmount,
                      HcmPositionForecastAmountCur _adjustmentValue = 0)
    {
        if (HcmPositionForecastCompGridView::massUpdateCompensationGrid(HcmPositionForecastComTmpGrid, compGridView, _adjustmentType, _adjustmentValue))
        {
            hcmPositionForecastComTmpGrid_ds.research(true);
        }
    }

]]></Source>
