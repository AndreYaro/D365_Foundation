				<Methods>
					<Method>
						<Name>validateDelete</Name>
						<Source><![CDATA[
        public boolean validateDelete()
        {
            boolean isValid;
            isValid = super();

            // Check if a Forecast Position exists that has been assigned the selected rate table's Compensation Group, and the selected Compensation Level.
            if (HcmPositionForecastScenario::existsByCompGroupAndCompLevel(compensationRate.CompensationGroup, hcmPositionForecastComTmpGrid.CompensationLevel))
            {
                isValid = checkFailed("@SYS4005558");
            }

            if (isValid)
            {
                compGridView.deleteCompensationLevel(HcmPositionForecastComTmpGrid.CompensationLevel);
            }
            return isValid;
        }

]]></Source>
					</Method>
					<Method>
						<Name>initValue</Name>
						<Source><![CDATA[
        public void initValue()
        {
            super();
            HcmPositionForecastComTmpGrid.CompensationGroupRefPoint = compGridView.parmCompGroupRefPoint().RecId;
        }

]]></Source>
					</Method>
					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        public void write()
        {
            super();
            compGridView.updatePositionForecastCompGrid(HcmPositionForecastComTmpGrid);
        }

]]></Source>
