			<Control>
				<Name>OKButton</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            InventTransferOrderCopying_BR    inventTransferOrderCopying;
            boolean existsThirdPartyReceivedOrderRejected = element.checkThirdPartyReceivedOrderRejected(fiscalDocument_BR);

            if (inventTransferCancelReturn == InventTransferCancelReturn_BR::Cancel)
            {
                if (!element.canCancelFiscalDocument(fiscalDocument_BR))
                {
                    throw Exception::Error;
                }

                if (EFDocUserInterfaceUtil_BR::constructFromFiscalDocument(fiscalDocument_BR).checkReversal())
                {
                    inventTransferOrderCopying = InventTransferOrderCopying_BR::newCancelInventTransferJour(inventTransferJour, reasonTableRef);
                    inventTransferRecId = inventTransferOrderCopying.run();
                }
            }
            else if (inventTransferCancelReturn == InventTransferCancelReturn_BR::FiscalEstablishmentReturn)
            {
                if (!element.canReturnFiscalDocument(fiscalDocument_BR))
                {
                    throw Exception::Error;
                }

                boolean canReturn = true;

                if (existsThirdPartyReceivedOrderRejected)
                {
                    DialogButton dlgBtn = Box::okCancel(
                        strFmt(
                            "@NFE:MessageForCancelRejectedFD",
                            fiscalDocument_BR.FiscalDocumentNumber,
                            fiscalDocument_BR.Status,
                            FiscalDocumentStatus_BR::Discarded),
                        DialogButton::Cancel);

                    canReturn = dlgBtn == DialogButton::Ok ? true : false;
                }

                if (canReturn)
                {
                    inventTransferOrderCopying = InventTransferOrderCopying_BR::newReturnInventTransferJour(inventTransferJour);
                    inventTransferRecId = inventTransferOrderCopying.run();
                }
            }

            if (inventTransferRecId)
            {
                super();
            }
        }

]]></Source>
