						<Source><![CDATA[
        public void lookup()
        {
            boolean useMainAccount, useOffsetAccount;

            if (InventTransferJour.inventTransferTable().inventTransferTableFiscalInfo_BR().isFiscalEstablishmentOrder())
            {
                useMainAccount = true;
                useOffsetAccount = true;
            }
            else
            {
                if (InventLocation::find(InventTransferJour.InventLocationIdTo).VendAccount)
                {
                    useMainAccount = true;
                }
                else
                {
                    useOffsetAccount = true;
                }
            }

            ReasonFormTable::reasonCodeLookupMultipleAccounts(
                    this,
                    LedgerJournalACType::Cust,
                    LedgerJournalACType::Vend,
                    useMainAccount,
                    useOffsetAccount);
        }

]]></Source>
