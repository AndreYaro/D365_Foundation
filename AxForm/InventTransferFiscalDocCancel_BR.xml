        {
            element.design().caption("@GLS63948");
            GroupReasonCode.visible(NoYes::Yes);
        }
        else
        {
            GlobalizationInstrumentationHelper::featureRun(GlobalizationConstants::FeatureReferenceBR00046, funcName());
            element.design().caption("@SYS105841");
        }
    }

