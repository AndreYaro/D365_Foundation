			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        FormDataSource formDataSource;
        InventTransferTable inventTransfer;

        super();

        if (inventTransferRecId)
        {
            select inventTransfer where inventTransfer.RecId == inventTransferRecId;

            formDataSource = FormDataUtil::getFormDataSource(this.args().record());
            formDataSource.positionToRecord(inventTransfer);
        }
    }

]]></Source>
