						<Source><![CDATA[
        void clicked()
        {
            CustFreeInvoiceCancel_BR    custFreeInvoiceCancel;

            if (element.checkOkButtonToEnable())
            {
                custInvoiceTable.AdjustingInvoiceDate   = adjustInvoiceDate.dateValue();
                custInvoiceTable.write();

                custFreeInvoiceCancel = CustFreeInvoiceCancel_BR::newCancelInvoice(custInvoiceTable.RecId);
                custInvoiceTableRecId = custFreeInvoiceCancel.createCancelInvoice(
                            custInvoiceTable.CorrectionReasonCode,
                            adjustInvoiceDate.dateValue(),
                            custInvoiceTable.InvoiceDate);

                if (custInvoiceTableRecId)
                {
                    super();
                }
            }
        }

]]></Source>
