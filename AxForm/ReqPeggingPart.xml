		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class ReqPeggingPart extends FormRun
{
    ReqPeggingBatchQueryBuilder peggingBatchQueryBuilder;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        Query batchReqTransQuery = batchReqTrans_DS.query();

        peggingBatchQueryBuilder = new ReqPeggingBatchQueryBuilder();
        peggingBatchQueryBuilder.batchReqTransDsInit(batchReqTransQuery);

        QueryBuildDataSource qbdsReqTransCov = reqTransCov_DS.queryBuildDataSource();
        qbdsReqTransCov.addLink(fieldNum(ReqTrans,RecId), fieldNum(ReqTransCov, ReceiptRecId));

        var qbdsReqTransSettled = reqTransSettled_ds.queryBuildDataSource();
        qbdsReqTransSettled.clearLinks();
        qbdsReqTransSettled.addLink(fieldNum(ReqTransCov,IssueRecId),fieldNum(ReqTrans,RecId));
        qbdsReqTransSettled.clearRanges();
        qbdsReqTransSettled.addRange(fieldNum(ReqTrans, Direction)).value(queryValue(InventDirection::Issue));
    }

]]></Source>
			</Method>
		</Methods>
