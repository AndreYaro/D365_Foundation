				<Source><![CDATA[
    public void init()
    {
        super();
        #ISOCountryRegionCodes
        boolean notRUAndPettyCashEnabled = !SysCountryRegionCode::isLegalEntityInCountryRegion([#isoRU]) && BankParameters::getEnablePettyCash();
        rCashDestinationTable_ds.allowEdit(notRUAndPettyCashEnabled);
        rCashDestinationTable_ds.object(fieldNum(RCashDestinationTable, Description)).visible(notRUAndPettyCashEnabled);
        rCashDestinationTable_ds.object(fieldNum(RCashDestinationTable, DestinationCode)).visible(notRUAndPettyCashEnabled);
        RCashDestination.visible(notRUAndPettyCashEnabled);
    }

]]></Source>
