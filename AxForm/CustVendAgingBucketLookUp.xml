	<Name>CustVendAgingBucketLookUp</Name>
	<SourceCode>
		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
    [Form]
    public class CustVendAgingBucketLookUp extends FormRun
    {
        int agingBuckets;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        agingBuckets = CustVendBalanceList::getMaximumAgingBuckets();
        element.selectMode(statRepInterval_Name);
    }

]]></Source>
			</Method>
			<Method>
				<Name>selectRecord</Name>
				<Source><![CDATA[
    public void selectRecord(CustVendReportName _name)
    {
        StatRepInterval localStatRepInterval;

        select localStatRepInterval where localStatRepInterval.Name == _name;

        element.args().lookupRecord(localStatRepInterval);
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>StatRepInterval</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            StatRepIntervalLine statRepIntervalLine;
            boolean foundValidDef = false;

            while select count(Qty) from statRepIntervalLine
                group by Name
            {
                if (statRepIntervalLine.Qty > 0 && statRepIntervalLine.Qty <= agingBuckets)
                {
                    this.query().dataSourceNo(1).addRange(fieldnum(StatRepInterval, Name)).value(queryValue(statRepIntervalLine.Name));
                    foundValidDef = true;
                }
            }

            if (foundValidDef)
            {
                super();
                element.selectRecord(element.args().lookupValue());
            }
        }

]]></Source>
					</Method>
				</Methods>
