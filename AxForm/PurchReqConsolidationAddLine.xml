			<Method>
				<Name>isCalledFromListPage</Name>
				<Source><![CDATA[
    /// <summary>
    /// This method checks the passed args() and validates that the caller is from <c>PurchReqAggregation</c>
    /// </summary>
    /// <returns>
    /// True if calling form is <c>PurchReqAggregation</c>; otherwise False.
    /// </returns>
    private boolean isCalledFromListPage()
    {
        Object      myCaller;
        FormRun     callerForm;
        boolean     calledFromListPage = false;

        if (element.args())
        {
            myCaller = element.args().caller();
            if (myCaller && myCaller is FormRun)
            {
                callerForm = myCaller;
                if (callerForm.name() == formStr(PurchReqConsolidation))
                {
                    calledFromListPage = true;
                }
            }
        }

        return calledFromListPage;
    }

]]></Source>
