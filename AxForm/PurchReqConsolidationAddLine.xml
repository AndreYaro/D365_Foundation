					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            QueryBuildRange qbr;
            qbr = purchReqLine_ds.query().dataSourceTable(tableNum(PurchReqLine)).addRange(fieldNum(PurchReqLine,RequisitionPurpose));
            qbr.value(SysQuery::value(RequisitionPurpose::Consumption));
            qbr.status(RangeStatus::Locked);

            qbr = purchReqLine_ds.query().dataSourceTable(tableNum(PurchReqLine)).addRange(fieldNum(PurchReqLine,PurchLineCreated));
            qbr.value(SysQuery::value(NoYes::No));
            qbr.status(RangeStatus::Locked);

            qbr = purchReqLine_ds.query().dataSourceTable(tableNum(PurchReqLine)).addRange(fieldNum(PurchReqLine,RequisitionStatus));
            qbr.value(queryValue(PurchReqRequisitionStatus::Approved));
            qbr.status(RangeStatus::Locked);

            purchReqLine.setTmp();

            this.initializeLinesToConsolidate();
        }

]]></Source>
					</Method>
					<Method>
						<Name>insertReadyToBeConsolidatedPurchReqLine</Name>
						<Source><![CDATA[
        private void insertReadyToBeConsolidatedPurchReqLine(PurchReqLine _tmpPurchReqLine)
        {
            if (PurchReqPurchaseOrderGenerationRule::canConsolidateStatic(_tmpPurchReqLine))
            {
                purchReqLine.data(_tmpPurchReqLine.data());

                // Since tempory table will overwrite the RecId value, so assign RecId into an unused field
                purchReqLine.AddressRefRecId = _tmpPurchReqLine.RecId;

                purchReqLine.doInsert();
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>initializeLinesToConsolidate</Name>
						<Source><![CDATA[
        private void initializeLinesToConsolidate()
        {
            PurchReqLine                tmpPurchReqLine;
            PurchReqTable               tmpPurchReqTable;
            BudgetSource                budgetSource;
            SourceDocumentLine          sourceDocumentLine;

            if (PurchReqConsolidationAddLineImprovePerformanceFlight::instance().isEnabled())
            {
                PurchReqConsolidationLine purchReqConsolidationLine;

                using (var cache = PurchReqPurchaseOrderGenerationRuleCache::construct())
                {
                    while select tmpPurchReqLine
                        where tmpPurchReqLine.RequisitionPurpose               == RequisitionPurpose::Consumption
                            && tmpPurchReqLine.PurchLineCreated                == NoYes::No
                            && tmpPurchReqLine.RequisitionStatus               == PurchReqRequisitionStatus::Approved
                            && tmpPurchReqLine.IsPreEncumbranceRequired        != UnknownNoYes::Unknown
                            && tmpPurchReqLine.IsPurchaseOrderGenerationManual == NoYes::Yes
                        exists join tmpPurchReqTable
                            where tmpPurchReqTable.RecId == tmpPurchReqLine.PurchReqTable
                        notexists join budgetSource
                            where budgetSource.SourceDocumentLine == tmpPurchReqLine.SourceDocumentLine
                                && budgetSource.CheckResult       == BudgetCheckResult::Failed
                        notexists join sourceDocumentLine
                            where sourceDocumentLine.RecId             == tmpPurchReqLine.SourceDocumentLine
                                && sourceDocumentLine.AccountingStatus != SourceDocumentLineAccountingStatus::Completed
                                && sourceDocumentLine.AccountingStatus != SourceDocumentLineAccountingStatus::Finalized
                        notexists join purchReqConsolidationLine
                                where purchReqConsolidationLine.PurchReqLineID == tmpPurchReqLine.RecId
                    {
                        this.insertReadyToBeConsolidatedPurchReqLine(tmpPurchReqLine);
                    }
                }
            }
            else
            {
                while select tmpPurchReqLine
                    where tmpPurchReqLine.RequisitionPurpose               == RequisitionPurpose::Consumption
                        && tmpPurchReqLine.PurchLineCreated                == NoYes::No
                        && tmpPurchReqLine.RequisitionStatus               == PurchReqRequisitionStatus::Approved
                        && tmpPurchReqLine.IsPreEncumbranceRequired        != UnknownNoYes::Unknown
                        && tmpPurchReqLine.IsPurchaseOrderGenerationManual == NoYes::Yes
                    exists join tmpPurchReqTable
                        where tmpPurchReqTable.RecId == tmpPurchReqLine.PurchReqTable
                    notexists join budgetSource
                        where budgetSource.SourceDocumentLine == tmpPurchReqLine.SourceDocumentLine
                            && budgetSource.CheckResult       == BudgetCheckResult::Failed
                    notexists join sourceDocumentLine
                        where sourceDocumentLine.RecId             == tmpPurchReqLine.SourceDocumentLine
                            && sourceDocumentLine.AccountingStatus != SourceDocumentLineAccountingStatus::Completed
                            && sourceDocumentLine.AccountingStatus != SourceDocumentLineAccountingStatus::Finalized
                {
                    if (!PurchReqConsolidationLine::findByReqLineId(tmpPurchReqLine.RecId))
                    {
                        this.insertReadyToBeConsolidatedPurchReqLine(tmpPurchReqLine);
                    }
                }
            }
        }

]]></Source>
