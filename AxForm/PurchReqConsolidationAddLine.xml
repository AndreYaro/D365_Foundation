				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            PurchReqLine    currentLine;
            RefRecId        purchReqLineId;
            FormDataSource  fds           = FormDataUtil::getFormDataSource(purchReqLine);
            List            linesSelected = new List(Types::Int64);

            super();

            for (currentLine = fds.getFirst(true) ? fds.getFirst(true) : fds.cursor(); currentLine; currentLine = fds.getNext())
            {
                // Since the unused field is used to hold the RecId value, so we will use it here
                purchReqLineId = currentLine.AddressRefRecId;

                linesSelected.addEnd(purchReqLineId);
            }

            if (linesSelected)
            {
                PurchReqConsolidationLine::addLinesFromPurchReqLines(linesSelected, purchReqConsolidation.RecId);
            }
        }

]]></Source>
					</Method>
