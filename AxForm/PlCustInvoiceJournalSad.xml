            public void activate(boolean _active)
    {
        super(_active);

        this.enableControls();
        custInvoiceJour_ds.refresh();
    }

]]></Source>
			</Method>
			<Method>
				<Name>enableControls</Name>
				<Source><![CDATA[
    /// <summary>
    /// Enables controls on the form.
    /// </summary>
    public void enableControls()
    {
        RecId                                   localExportSalesInvoiceId;
        boolean                                 enableVATIn;
        boolean                                 enableVATOut;
        ExportSalesInvoiceDomesticTaxVoucher    localExportSalesInvoiceDomesticTaxVoucher;

        if (custInvoiceJour)
        {
            enableVATIn = true;
            enableVATOut = true;

            localExportSalesInvoiceId = ExportSalesInvoice::findRecIdByCustInvoiceJour(custInvoiceJour.RecId);

            if (ExportSalesInvoiceDomesticTaxVoucher::existReferenceDomesticSalesTaxVoucher(localExportSalesInvoiceId))
            {
                enableVATIn = false;
                enableVATOut = false;
            }
            else
            {
                localExportSalesInvoiceDomesticTaxVoucher = ExportSalesInvoiceDomesticTaxVoucher::findDomesticSalesTaxVoucher(localExportSalesInvoiceId, false);

                if (localExportSalesInvoiceDomesticTaxVoucher)
                {
                    enableVATIn = false;

                    if (!localExportSalesInvoiceDomesticTaxVoucher.IsPosted)
                    {
                        enableVATOut = false;
                    }
                }
                else
                {
                    enableVATOut = false;
                }
            }
        }
        else
        {
            enableVATIn = false;
            enableVATOut = false;
        }

        if (enableVATIn)
        {
            taxIn_PL.enabled(true);
            ExportSalesInvoiceLineDetail_M_editDomesticSalesTaxGroup_PL.enabled(true);
            ExportSalesInvoiceLineDetail_M_editDomesticSalesTaxGroup_PL.allowEdit(true);
        }
        else
        {
            taxIn_PL.enabled(false);
            ExportSalesInvoiceLineDetail_M_editDomesticSalesTaxGroup_PL.enabled(false);
            ExportSalesInvoiceLineDetail_M_editDomesticSalesTaxGroup_PL.allowEdit(false);
        }

        if (enableVATOut)
        {
            taxOut_PL.enabled(true);
        }
        else
        {
            taxOut_PL.enabled(false);
        }
    }

]]></Source>
