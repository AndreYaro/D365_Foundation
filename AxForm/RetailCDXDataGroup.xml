        public int active()
        {
            int ret;
            ret = super();

            element.setDatabaseProfileFieldsAccess();
            this.enableEditDelete();

            RetailCDXDataGroup_ds.object(fieldNum(RetailCDXDataGroup, maxNumberOfRecordsAllowedInsideStoreHours)).enabled(RetailCDXDataGroup.PauseSyncDuringStoreHours);

            return ret;
        }

]]></Source>
