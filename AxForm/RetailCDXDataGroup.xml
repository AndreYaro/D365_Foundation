				<Name>setDatabaseProfileFieldsAccess</Name>
				<Source><![CDATA[
    public void setDatabaseProfileFieldsAccess()
    {
        // The following 2 channel database group name should not be allowed to edit
        // 1. The default channel database group name which was generated in the deployment process, and the environment reprovisioning tool expects this to be present.
        // 2. The custoemr search database group name.
        boolean isEditAllowed = RetailCDXDataGroup.Name != RetailCDXConstants::RetailCdxDataGroup_DefaultName &&
                                RetailCDXDataGroup.Name != RetailCustomerSearchConstants::CdxDataGroupName;

        RetailCDXDataGroup_Name1.allowEdit(isEditAllowed);
    }

]]></Source>
			</Method>
			<Method>
				<Name>gridSelectedCount</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets number of selected records from the grid.
    /// </summary>
    /// <returns>Number of selected records.</returns>
    public int gridSelectedCount(FormGridControl grid)
    {
        struct gridData = grid.getSelectedData();
        const int DataLinesIndex = 1;
        int selectedCount = gridData.value(gridData.fieldName(DataLinesIndex)).lastIndex();
        
        // when no record selected then the grid data array count will be 1 and the element index will be -1
        if (selectedCount == 1 && gridData.value(gridData.fieldName(DataLinesIndex)).value(1).value(1) < 1)
        {
            return 0;
        }

        return selectedCount;
    }

]]></Source>
			</Method>
