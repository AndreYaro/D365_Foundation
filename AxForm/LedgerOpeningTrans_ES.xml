						<Source><![CDATA[
        public void initValue()
        {
            RecId accountStructureRecId;

            super();

            ledgerOpeningTrans.initFromOpeningSheet(LedgerOpeningSheet_ES::find(ledgerOpeningTable.Sheet));

            if (LedgerOpeningTable.MainAccount)
            {
                accountStructureRecId = DimensionHierarchy::getAccountStructure(ledgerOpeningTable.MainAccount, Ledger::primaryLedger(CompanyInfo::current(ledgerOpeningTable.company())));
                ledgerOpeningTrans.LedgerDimension = LedgerDimensionDefaultingEngine::getLedgerDimensionFromAccountAndDim(ledgerOpeningTable.MainAccount, accountStructureRecId);
                ledgerOpeningTrans_DS.rereadReferenceDataSources();
            }
        }

]]></Source>
