						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            boolean isAllowDelete = true;
            boolean refreshCaller = false;

            // Check if this is the last version of interest code to be allowed to delete.
            SysDateEffectiveDataManager sysDateEffectiveDataManager = SysDateEffectiveDataManager::construct(SysDateEffectiveSetupType::CustInterest);

            if (custInterest && sysDateEffectiveDataManager.isOnlyVersion(custInterest.RecId))
            {
                isAllowDelete = custInterest.validateDelete();
                refreshCaller = isAllowDelete;
            }

            if (isAllowDelete)
            {
                custInterestVersion_ds.delete();
                element.close();
            }

            CustInterestIRefreshable parentForm = element.args().caller() as CustInterestIRefreshable;

            if (refreshCaller && parentForm)
            {
                parentForm.refreshRootDataSource();
            }
        }

]]></Source>
