			<Method>
				<Name>createQueryForOtherMembers</Name>
				<Source><![CDATA[
    private Query createQueryForOtherMembers()
    {
        Query                   query;
        QueryBuildDataSource    qDS_PayCounterTable, qDS_PayFundMember;

        query = new Query();

        qDS_PayCounterTable = query.addDataSource(tablenum(RPayCounterTable));
        qDS_PayCounterTable.addSelectionField(fieldnum(RPayCounterTable,CounterUnit));
        qDS_PayCounterTable.addSelectionField(fieldnum(RPayCounterTable, Name));

        qDS_PayCounterTable.addRange(fieldnum(RPayCounterTable,CounterType));
        qDS_PayCounterTable.addRange(fieldnum(RPayCounterTable,LineType));

        qDS_PayFundMember = qDS_PayCounterTable.addDataSource(tablenum(RPayFundMember));

        qDS_PayFundMember.addRange(fieldnum(RPayFundMember,PayCostType));

        qDS_PayFundMember.joinMode(JoinMode::NoExistsJoin);
        qDS_PayFundMember.addLink(fieldnum(RPayCounterTable,CounterUnit), fieldnum(RPayFundMember,CounterUnit));

        return query;
    }

]]></Source>
