			<Method>
				<Name>initListOtherMembers</Name>
				<Source><![CDATA[
    #resappl
    private void initListOtherMembers()
    {
        int                 iIdx;
        QueryRun            qr;
        RPayCounterTable    payCounterTable;
        FormListItem        formListItem;
        ;

        this.setWhereForListOtherMembers(payTable.PayCtype);
        qr = new QueryRun(m_qListOtherMembers);
        ListOtherMembers.deleteAll();

        while (qr.next())
        {
            payCounterTable = qr.get(tablenum(RPayCounterTable));

            formListItem = new FormListItem(payCounterTable.CounterUnit,m_imageList.image(1200));

            formListItem.data(payCounterTable.CounterUnit);

            iIdx = ListOtherMembers.addItem(formListItem);

            formListItem = new FormListItem(payCounterTable.Name);
            formListItem.subItem(1);
            formListItem.idx(iIdx);
            ListOtherMembers.setItem(formListItem);
        }
    }

]]></Source>
