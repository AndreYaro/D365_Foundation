				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            Dialog         dialog =  new Dialog("@GLS105867");
            DialogField    dialogFieldPayCtype;
            RPayCostType   newPayCtype;
            RPayTable      RPayTable;
            RPayFundMember RPayFundMember, payFundMember;

            dialogFieldPayCtype = dialog.addField(extendedtypestr(RPayCostType));
            dialogFieldPayCtype.lookupButton(1);
            dialogFieldPayCtype.mandatory_RU(true);
            dialog.run();
            if (dialog.closedOk())
            {
                newPayCtype = dialogFieldPayCtype.value();
                //validate
                if ( !newPayCtype)
                {
                    error(strFmt("@SYS136411", "@RUP309"));
                    return;
                }
                //do copy
                RPayTable.clear();
                RPayTable.data(payTable);
                RPayTable.PayCtype = newPayCtype;
                RPayTable.insert();
                while select payFundMember
                    where payFundMember.PayCostType == payTable.PayCtype
                {
                    RPayFundMember.data(payFundMember);
                    RPayFundMember.PayCostType = newPayCtype;
                    RPayFundMember.insert();
                }
                //refresh form
                payTable_ds.research();
                payTable_ds.positionToRecord(RPayTable);
            }
        }

]]></Source>
