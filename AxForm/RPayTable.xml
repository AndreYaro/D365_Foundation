			<Method>
				<Name>setWhereForListOtherMembers</Name>
				<Source><![CDATA[
    private void setWhereForListOtherMembers(RPayCostType _payCostType)
    {
        QueryBuildDataSource    qDS;
        QueryBuildRange         qRange;
        ;

        qDS = m_qListOtherMembers.dataSourceTable(tablenum(RPayFundMember));
        qRange = qDS.rangeField(fieldnum(RPayFundMember,PayCostType));
        qRange.value(_payCostType);

        qDS = m_qListOtherMembers.dataSourceTable(tablenum(RPayCounterTable));
        qRange = qDS.rangeField(fieldnum(RPayCounterTable,CounterType));
        qRange.value(queryValue(RPayCounterType::Fund));
        qRange = qDS.rangeField(fieldnum(RPayCounterTable,LineType));
        if (RPayTable::find(_payCostType).PayChargeType == RPayChargeType::AddSum)
        {
            qRange.value(SysQuery::valueUnlimited());
        }
        else
        {
            qRange.value(queryValue( RPayCtypeType::BaseTotal));
        }
    }

]]></Source>
