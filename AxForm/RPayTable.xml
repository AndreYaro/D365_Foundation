				<Name>refreshFundTabVisibility</Name>
				<Source><![CDATA[
    public void refreshFundTabVisibility()
    {
        if (payTable.PayChargeType == RPayChargeType::AddSum)
        {
            payTable_RpayTaxIncomeCode.allowEdit(true);
            FundsTabPage.visible(true);
        }
        else
        {
            payTable_RpayTaxIncomeCode.allowEdit(false);
            FundsTabPage.visible(false);
        }
    }

]]></Source>
