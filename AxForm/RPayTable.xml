						<Source><![CDATA[
        int active()
        {
            int ret;

            ret = super();

            DependenceButton.enabled(payTable.RecId);

            if (payTable.PayChargeType == RPayChargeType::AddSum)
            {
                payTable_RpayPeriodFrequency.allowEdit(true);
                payTable_RpayTaxIncomeCode.allowEdit(true);
                FundsTabPage.caption("@RUP6347");
            }
            else
            {
                payTable.RpayPeriodFrequency = RPayPeriodFrequency::None;
                payTable_RpayPeriodFrequency.allowEdit(false);
                payTable_RpayTaxIncomeCode.allowEdit(false);
                FundsTabPage.caption("@SYS16127");
            }

            if (FundsTabPage.isActivePage())
            {
                element.initMembersLists();
            }

            return ret;
        }

]]></Source>
