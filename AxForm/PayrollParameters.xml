					<Field>
						<DataField>PaymentJournalName</DataField>
						<Methods>
							<Method>
								<Name>validate</Name>
								<Source><![CDATA[
            public boolean validate()
            {
                boolean ret;
                LedgerJournalName journalName;

                ret = super();

                if (ret)
                {
                    journalName = LedgerJournalName::find(PayrollParameters.PaymentJournalName);

                    // Journal name must either be left blank or have the type of payroll disbursements
                    if ((journalName.RecId != 0) && (journalName.JournalType != LedgerJournalType::PayrollDisbursement))
                    {
                        ret = checkFailed("@Payroll:OnlyJournalsThatHaveAJournalTypeOfPayrollDisbursementCanBeUs");
                    }
                }

                return ret;
            }

]]></Source>
							</Method>
							<Method>
								<Name>lookup</Name>
								<Source><![CDATA[
            public void lookup(FormControl _formControl, str _filterStr)
            {
                SysTableLookup      sysTableLookup  = SysTableLookup::newParameters(tableNum(LedgerJournalName), _formControl);
                Query               query           = new Query();

                // Filter lookup to only show Payroll disbursement type
                query.addDataSource(tableNum(LedgerJournalName)).addRange(fieldNum(LedgerJournalName, JournalType)).value(queryValue(LedgerJournalType::PayrollDisbursement));

                // Sort the lookup by journal name
                query.dataSourceTable(tableNum(LedgerJournalName)).addOrderByField(fieldNum(LedgerJournalName, JournalName), SortOrder::Ascending);

                // Add fields
                sysTableLookup.addLookupfield(fieldNum(LedgerJournalName, JournalName));
                sysTableLookup.addLookupfield(fieldNum(LedgerJournalName, Name));

                // Run lookup
                sysTableLookup.parmQuery(query);
                sysTableLookup.performFormLookup();
            }

]]></Source>
