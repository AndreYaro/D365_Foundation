						<Methods>
							<Method>
								<Name>validate</Name>
								<Source><![CDATA[
            public boolean validate()
            {
                boolean ret;
                CustVendorBlocked   vendBlocked;

                ret = super();

                if (ret)
                {
                    vendBlocked = VendTable::find(PayrollParameters.VendorAccountNum).Blocked;

                    if (vendBlocked == CustVendorBlocked::All
                        || vendBlocked == CustVendorBlocked::Invoice
                        || vendBlocked == CustVendorBlocked::Payment)
                    {
                        // Warn the user about the hold
                        info(strFmt("@SYS128411", PayrollParameters.VendorAccountNum, vendBlocked));
                    }
                }

                return ret;
            }

]]></Source>
