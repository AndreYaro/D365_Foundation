    public void init()
    {
        super();

        whsWorkAuditTemplateForm = new WHSWorkAuditTemplateForm();

        whsTmpFieldName.populateFieldNames(tableNum(WHSWorkTable), false);
        whsTmpMethodName.populateMethodNames();
        whsTmpAuditTemplateStatus.populateStatus();

        WHSSequenceNumberMoverForm::newFromDataSource(
                WHSWorkAuditTemplateLine_ds,
                fieldStr(WHSWorkAuditTemplateLine, AuditSeqNum),
                [fieldStr(WHSWorkAuditTemplateLine, WorkAuditTemplateId)])
            .initNewButtons(ButtonGroup);

        ButtonMoveUpLine.visible(false);
        ButtonMoveDownLine.visible(false);
    }

]]></Source>
