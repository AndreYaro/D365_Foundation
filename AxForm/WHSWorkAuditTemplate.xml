			<Method>
				<Name>toggleFunctionFields</Name>
				<Source><![CDATA[
    void toggleFunctionFields()
    {
        switch (whsWorkAuditTemplateLine.AuditBreakFunction)
        {
            case WHSAuditBreakFunction::CaptureData:
                whsWorkAuditTemplateLine_WorkAuditTemplateReportName.allowEdit(false);
                whsWorkAuditTemplateLine_WorkAuditTemplateEvents.allowEdit(false);
                whsWorkAuditTemplateLine_WorkAuditTemplateCustomMethod.allowEdit(true);
                printSettings.enabled(false);

                whsWorkAuditTemplateLine.WorkAuditTemplateReportName = '';
                whsWorkAuditTemplateLine.WorkAuditTemplateEvents = WHSWorkAuditTemplateEvents::Blank;
                break;

            case WHSAuditBreakFunction::Custom:
                whsWorkAuditTemplateLine_WorkAuditTemplateReportName.allowEdit(false);
                whsWorkAuditTemplateLine_WorkAuditTemplateEvents.allowEdit(false);
                whsWorkAuditTemplateLine_WorkAuditTemplateCustomMethod.allowEdit(true);
                printSettings.enabled(false);

                whsWorkAuditTemplateLine.WorkAuditTemplateReportName = '';
                whsWorkAuditTemplateLine.WorkAuditTemplateEvents = WHSWorkAuditTemplateEvents::Blank;
                break;

            case WHSAuditBreakFunction::Display:
                whsWorkAuditTemplateLine_WorkAuditTemplateReportName.allowEdit(false);
                whsWorkAuditTemplateLine_WorkAuditTemplateEvents.allowEdit(false);
                whsWorkAuditTemplateLine_WorkAuditTemplateCustomMethod.allowEdit(false);
                printSettings.enabled(false);

                whsWorkAuditTemplateLine.WorkAuditTemplateReportName = '';
                whsWorkAuditTemplateLine.WorkAuditTemplateEvents = WHSWorkAuditTemplateEvents::Blank;
                whsWorkAuditTemplateLine.WorkAuditTemplateCustomMethod = '';
                break;

            case WHSAuditBreakFunction::Event:
                whsWorkAuditTemplateLine_WorkAuditTemplateReportName.allowEdit(false);
                whsWorkAuditTemplateLine_WorkAuditTemplateEvents.allowEdit(true);
                whsWorkAuditTemplateLine_WorkAuditTemplateCustomMethod.allowEdit(false);
                printSettings.enabled(false);

                whsWorkAuditTemplateLine.WorkAuditTemplateReportName = '';
                whsWorkAuditTemplateLine.WorkAuditTemplateCustomMethod = '';
                break;

            case WHSAuditBreakFunction::Print:
                whsWorkAuditTemplateLine_WorkAuditTemplateReportName.allowEdit(true);
                whsWorkAuditTemplateLine_WorkAuditTemplateEvents.allowEdit(false);
                whsWorkAuditTemplateLine_WorkAuditTemplateCustomMethod.allowEdit(false);
                printSettings.enabled(true);

                whsWorkAuditTemplateLine.WorkAuditTemplateEvents = WHSWorkAuditTemplateEvents::Blank;
                whsWorkAuditTemplateLine.WorkAuditTemplateCustomMethod = '';
                break;
        }
    }

]]></Source>
