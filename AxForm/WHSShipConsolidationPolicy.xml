            element.buildTmpShipConsolidationPolicyField();
            shipConsolidationPolicyFieldToSearch.setTmp();
            shipConsolidationPolicyFieldToSearch.setTmpData(tmpShipConsolidationPolicyField);

            select firstonly shipConsolidationPolicyFieldToSearch
                where shipConsolidationPolicyFieldToSearch.ShipConsolidationPolicyName == shipConsolidationPolicyField.ShipConsolidationPolicyName &&
                      shipConsolidationPolicyFieldToSearch.ShipConsolidationPolicyType == shipConsolidationPolicyField.ShipConsolidationPolicyType &&
                      shipConsolidationPolicyFieldToSearch.SourceTableName             == shipConsolidationPolicyField.SourceTableName             &&
                      shipConsolidationPolicyFieldToSearch.SourceFieldName             == shipConsolidationPolicyField.SourceFieldName;

            tmpShipConsolidationPolicyField_ds.research();
            tmpShipConsolidationPolicyField_ds.positionToRecord(shipConsolidationPolicyFieldToSearch);
            whsShipConsolidationPolicy_ds.reread();
            whsShipConsolidationPolicy_ds.refresh();
        }

