						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            boolean       ret;
            TransDateTime issueDateTimeLoc;

            ret = super();

            if (ret && issueDateTime.dateTimeValue())
            {
                issueDateTimeLoc = DateTimeUtil::newDateTime(
                                    EximUpdate_IN::importExpiryDate(
                                        schemeType,
                                        DateTimeUtil::date(issueDateTime.dateTimeValue())),
                                    0);

                issueDateTimeLoc = DateTimeUtil::addSeconds(DateTimeUtil::addDays(issueDateTimeLoc, 1), -1);

                impExpiryDate.dateTimeValue(issueDateTimeLoc);
            }

            return ret;
        }

]]></Source>
