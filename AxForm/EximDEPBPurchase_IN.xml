								<Source><![CDATA[
            public boolean validate()
            {
                boolean              ret;
                TaxComponentTable_IN taxComponentTableloc = TaxComponentTable_IN::find(tmpEximTaxDetails.TaxComponentTable);

                ret = super();

                if (ret && taxComponentTableloc && taxComponentTableloc.TaxType != TaxType_IN::Customs)
                {
                   ret = checkFailed(strfmt("@GLS61651",
