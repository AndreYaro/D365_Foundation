			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        Object caller;

        super();

        caller = element.args().caller();

        if (!element.args() ||
            !caller ||
            !(caller is EximUpd_Purchase_IN))
        {
            throw error(strfmt("@SYS22862", element.name()));
        }

        eximUpdatePurchase = caller;

        if (element.args().parmEnumType() != enumnum(EximAuthorizationType_IN))
        {
            throw error("@SYS22539");
        }

        schemeType = element.args().parmEnum();

        taxAccountType = TaxAccountType_IN::EximDEPBReceivableAccount;

        OffsetLedgerDimension.parmPostingType(LedgerPostingType::Customs_IN);
    }

]]></Source>
			</Method>
			<Method>
				<Name>setValues</Name>
				<Source><![CDATA[
    public void setValues()
    {
        eximUpdatePurchase.parmLicenseNumber(authNum.valueStr());
        eximUpdatePurchase.parmIssueDateTime(issueDateTime.dateTimeValue());
        eximUpdatePurchase.parmImportExpiryDateTime(impExpiryDate.dateTimeValue());
        eximUpdatePurchase.parmPurchValue(purchValue.realValue());
        eximUpdatePurchase.parmTxt(text.valueStr());
        eximUpdatePurchase.parmTaxAccountGroup(taxLedgerAccountGroup.valueStr());
        eximUpdatePurchase.parmIECNumber(taxRegistrationNumber.valueStr());
        eximUpdatePurchase.parmTransDate(transDate.dateValue());
        eximUpdatePurchase.parmTmpEximTaxDetails(tmpEximTaxDetails);
        eximUpdatePurchase.parmTmpEximEntitlementDetails(tmpEximEntitlementDetails);
    }

]]></Source>
			</Method>
			<Method>
				<Name>validatePostingParameters</Name>
				<Source><![CDATA[
    public boolean validatePostingParameters()
    {
        boolean isParmValid = true;

        if (!totalDutyEntitlement.realValue()
            || (totalDutyEntitlement.realValue() != totalComponentAmount.realValue()))
        {
            isParmValid = checkFailed("@GLS62353");
        }

        if (!authNum.valueStr())
        {
            isParmValid = checkFailed(strfmt("@SYS116347", "@SYS114298"));
        }

        if (!issueDateTime.dateTimeValue())
        {
            isParmValid = checkFailed(strfmt("@SYS116347", "@GLS62266"));
        }

        if (!impExpiryDate.dateTimeValue())
        {
            isParmValid = checkFailed(strfmt("@SYS116347", "@GLS62344"));
        }

        if (!taxLedgerAccountGroup.valueStr())
        {
            isParmValid = checkFailed(strfmt("@SYS116347", "@GLS5094"));
        }

        if (!taxRegistrationNumber.valueStr())
        {
            isParmValid = checkFailed(strfmt("@SYS116347", "@GLS5100"));
        }

        if (!transDate.dateValue())
        {
            isParmValid = checkFailed(strfmt("@SYS116347", "@SYS67"));
        }

        return isParmValid;
    }

]]></Source>
