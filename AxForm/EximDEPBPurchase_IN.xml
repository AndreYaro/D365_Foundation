			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    public boolean canClose()
    {
        boolean ret;

        ret = super();

        if (ret && this.closedOk() && !this.closedCancel())
        {
            ret = element.validatePostingParameters();

            if (ret)
            {
                if (Box::yesNo("@GLS61903", DialogButton::No) == DialogButton::Yes)
                {
                    element.setValues();
                    eximUpdatePurchase.parmPostingStatus(true);
                }
                else
                {
                    ret = false;
                    element.close();
                }
            }
        }

        return ret;
    }

]]></Source>
