							<Method>
								<Name>lookupReference</Name>
								<Source><![CDATA[
            public Common lookupReference(FormReferenceControl _formReferenceControl)
            {
                Common                          ret;
                TaxRegistrationNumbers_IN       taxRegistrationNumberTable;
                TaxLedgerAccountGroup_IN        taxLedgerAccountGroupLoc;
                TaxLedgerAccountSetup_IN        taxLedgerAccountSetupLoc;

                taxRegistrationNumberTable = TaxRegistrationNumbers_IN::findByNaturalKey(TaxRegistrationType_IN::Company,
                                                                                             TaxType_IN::Customs,
                                                                                             taxRegistrationNumber.text());
                taxLedgerAccountGroupLoc = TaxLedgerAccountGroup_IN::findByTaxAccountGroup(taxLedgerAccountGroup.valueStr());
                taxLedgerAccountSetupLoc = TaxLedgerAccountSetup_IN::findByTaxAccountGroup(taxLedgerAccountGroupLoc.RecId,
                                                                                           TaxAccountCode_IN::Table,
                                                                                           taxRegistrationNumberTable.RecId);
                if (!taxLedgerAccountSetupLoc)
                {
                        taxLedgerAccountSetupLoc = TaxLedgerAccountSetup_IN::findByTaxAccountGroup(taxLedgerAccountGroupLoc.RecId,
                                                                                                   TaxAccountCode_IN::All);
                }

                if (taxRegistrationNumberTable)
                {
                    ret = TaxLedgerAccounts_IN::lookupCustomsComponent(_formReferenceControl,
                                                                 taxLedgerAccountSetupLoc.RecId,
                                                                 TaxTypeComponent_IN::Customs,
                                                                 taxAccountType);
                }

                return ret;
            }

]]></Source>
