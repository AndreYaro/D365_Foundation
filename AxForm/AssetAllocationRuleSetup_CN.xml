						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            DimensionAttribute dimAttr;
            int ret;

            ret = super();

            if (AssetAllocationRule_CN)
            {
                dimAttr = DimensionAttribute::find(AssetAllocationRule_CN.DimensionAttribute);

                if (!dimAttr)
                {
                    // @SYS4002359 - Invalid dimension %1
                    checkFailed(strFmt("@SYS4002359", AssetAllocationRule_CN.DimensionAttribute));
                }
                else
                {
                    dimAttrName = dimAttr.localizedName();
                }
            }
            else
            {
                dimAttrName = '';
            }

            DetailsRuleId.setFocus();
            DimensionValue.label(dimAttr.localizedName());

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>leave</Name>
						<Source><![CDATA[
        public boolean leave()
        {
            boolean ret;

            ret = super();

            // Cannot move focus to other record if validation failed on current row
            // Because leave() method will be called when form initializes, and we don't need to validate at that time.
            if (ret && validationOnLeave && AssetAllocationRule_CN)
            {
                ret = AssetAllocationRule_CN::validateAllocationRule_CN(AssetAllocationRule_CN);
            }

            validationOnLeave = true;

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>create</Name>
						<Source><![CDATA[
        public void create(boolean _append = false)
        {
            // Cannot create new rule if validation failed on any existing rules
            if (AssetAllocationRule_CN::validateAllAllocationRule_CN())
            {
                super(_append);
            }
        }

]]></Source>
