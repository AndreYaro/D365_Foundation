        public boolean modified()
        {
            boolean needModify;
            boolean ret;

            needModify = dimAttrController.validateDimensionName(AssetAllocationRule_CN, this.text());

            if (needModify)
            {
                ret = super();

                dimAttrController.modifyDimensionName(AssetAllocationRule_CN, this.text());

                if (AssetAllocationRule_CN)
                {
                    AssetAllocationRule_CN_DS.write();
                }

                DimensionValue.label(dimAttrName);
            }

            // If return false, there will be a warning icon on this control.
            ret = !needModify || ret;
            return ret;
        }

