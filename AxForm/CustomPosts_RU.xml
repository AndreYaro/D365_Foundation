                public void init()
        {
            QueryBuildDataSource    qbds;
            QueryBuildDynalink      qbdl;
            FormDataObject          fdo;
            int                     i, cnt;

            super();

            qbds = this.queryBuildDataSource();
            cnt = qbds.dynalinkCount();
            for (i = 1; i <= cnt; i++)
            {
                qbdl = qbds.dynalink(i);
                fdo  = this.object(qbdl.field());
                if (fdo)
                {
                    fdo.visible(false);
                }
            }
        }

