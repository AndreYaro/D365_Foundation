						<Source><![CDATA[
        /// <summary>
        /// Returns the budget control category value for the <c>BudgetSourceTracking</c> record.
        /// </summary>
        /// <param name="_budgetSourceTracking">
        /// The <c>BudgetSourceTracking</c> record for which the budget control category value needs to be obtained.
        /// </param>
        /// <returns>
        /// The budget control category value for the <c>BudgetSourceTracking</c> record.
        /// </returns>
        public display String255 displayCategory(BudgetSourceTracking _budgetSourceTracking)
        {
            // Using str type makes each row in grid wider so i used this String255.
            String255 category = enum2str(_budgetSourceTracking.Category);

            if (_budgetSourceTracking.Category == BudgetControlCategory::EncumbranceReduction &&
                _budgetSourceTracking.Status == BudgetSourceTrackingStatus::Confirmed)
            {
                category = "@SYS300929";
            }

            return category;
        }

]]></Source>
