				<Source><![CDATA[
/// <summary>
/// Enhanced Preview form for InventTable.
/// </summary>
[Form]
public class InventTablePreview extends FormRun
{
    EcoResProductRecId  productRecId;
    QueryBuildRange     productRecIdRange; // defines join between EcoResProduct and InventTable from code, as the EcoResProduct can be distinct or a variant

    Common          callerRecord;
    ItemId          callerItemId;
    InventDim       callerInventDim;

    VendExternalItemDescription vendExternalItemDescription;
    BOMVersion activeVersion;
    RouteVersion routeVersion;
    InventDimCombination variantCombination;

    [FormObservable]
    container productImage;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (!element.args() || !element.args().record().TableId)
        {
            throw error(Error::missingFormActiveBuffer(element.name()));
        }

        callerRecord = element.args().record();
        callerInventDim = InventDimLocator::findAnyFromRecord(callerRecord);

        super();

        this.setupFallbackProductTranslation();
    }

]]></Source>
			</Method>
			<Method>
				<Name>setupFallbackProductTranslation</Name>
				<Source><![CDATA[
    private void setupFallbackProductTranslation()
    {
        List fallbackStringControls = new List(Types::Integer);
        List ecoResProductTranslationStringControls = new List(Types::Integer);

        ecoResProductTranslationStringControls.addEnd(element.controlId(formControlStr(InventTablePreview, EcoResProductTranslation_Description)));
        fallbackStringControls.addEnd(element.controlId(formControlStr(InventTablePreview, EcoResProductTranslationFallbackView_Description)));

        EcoResProductTranslationFallbackViewModel::setupFallbackTranslationForm(element,
                                                                                element.EcoResProductTranslationFallbackView_ds,
                                                                                element.EcoResProduct_ds,
                                                                                fallbackStringControls,
                                                                                ecoResProductTranslationStringControls);
    }

]]></Source>
			</Method>
