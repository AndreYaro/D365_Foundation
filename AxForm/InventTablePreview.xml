					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            super();

            if (InventTable)
            {
                productRecId = InventTable.Product;

                if (InventTable.productSubtype() == EcoResProductSubtype::ProductMaster)
                {
                    variantCombination = InventDimCombination::findByInventDim(InventTable.ItemId, callerInventDim);
                    if (variantCombination)
                    {
                        productRecId = variantCombination.DistinctProductVariant;
                        productImage = variantCombination.productImageThumbnail();
                    }
                }

                productRecIdRange.value(queryValue(productRecId));

                if (productImage == conNull())
                {
                    productImage = InventTable.productImageThumbnail();
                }

                element.updateDesign(InventItemSetupSupplyType.DefaultOrderType);
            }
        }

]]></Source>
