			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    private void updateDesign(ReqPOType _defaultOrderType)
    {
        if (_defaultOrderType == ReqPOType::Production || _defaultOrderType == ReqPOType::Kanban)
        {
            ProductionSpecificGroup.visible(true);
        }
        if (_defaultOrderType == ReqPOType::Purch)
        {
            PurchaseSpecificGroup.visible(true);
            vendExternalItemDescription = new VendExternalItemDescription(InventTable.ItemId, callerInventDim, element.displayPrimaryVendor());
            vendExternalItemDescription.findExternalDescription();
        }
    }

]]></Source>
