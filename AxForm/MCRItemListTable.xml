				<Methods>
					<Method>
						<Name>create</Name>
						<Source><![CDATA[
        public void create(boolean _append = false)
        {
            if (!MCRItemListTable.RecId && numberSeq)
            {
                numberSeq.abort();
            }

            MCRItemListTable_ItemListId.setFocus();

            super(_append);

            if (! MCRItemListTable.ItemListId)
            {
                numberSeq = MCRItemListTable::numberSeq(true,true);
                if (numberSeq)
                {
                    ttsBegin;
                    MCRItemListTable.ItemListId  = MCRItemListTable::numberSeq().num();
                    ttsCommit;
                }
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>delete</Name>
						<Source><![CDATA[
        void delete()
        {
            ttsbegin;

            if (!MCRItemListTable.RecId && numberSeq)
            {
                numberSeq.abort();
            }

            super();

            ttscommit;
        }

]]></Source>
					</Method>
					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        void write()
        {
            ttsbegin;

            if (numberSeq)
            {
                numberSeq.used();
            }

            super();

            ttscommit;
        }

]]></Source>
