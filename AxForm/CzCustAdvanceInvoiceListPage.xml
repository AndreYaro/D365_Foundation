						<Source><![CDATA[
        /// <summary>
        /// Displays advance invoice Id.
        /// </summary>
        /// <param name = "_custAdvanceInvoiceTable">Advance invoice record</param>
        /// <returns>Id of the advance invoice.</returns>
        public display str displayInvoiceNumber(CzCustAdvanceInvoiceTable _custAdvanceInvoiceTable)
        {
            return _custAdvanceInvoiceTable.InvoiceId == "" ? "@SYS11670" : _custAdvanceInvoiceTable.InvoiceId;
        }

