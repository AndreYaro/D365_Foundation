						<Source><![CDATA[
        void clicked()
        {
            PrintMgmtSetupContext setupContext = new PrintMgmtSetupContext();

            if (czCustAdvanceInvoiceTable_ds.cursor() == null)
            {
                return;
            }

            super();

            setupContext.addHierarchyContext(PrintMgmtHierarchyType::Sales, PrintMgmtNodeType::SalesAdvanceInvoice);

            setupContext.parmCaller(element);
            setupContext.parmReferencedTableBuffer(czCustAdvanceInvoiceTable_ds.cursor());
            PrintMgmt::launchSetup(setupContext);
        }

]]></Source>
