			<Control>
				<Name>ButtonAdvanceInvoiceLink</Name>
				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>checkPrepaymentLink_W</Name>
						<Source><![CDATA[
        public boolean checkPrepaymentLink_W()
        {
            ErrorTolerance prepaymentLink = CustParameters::find().PrepaymentLink_W;

            if (prepaymentLink == ErrorTolerance::Accept || ! czCustAdvanceInvoiceTable.Posted)
                return true;

            if (prepaymentLink                            == ErrorTolerance::Warning &&
                Box::yesNo("@GLS114096", DialogButton::No) == DialogButton::No)
                return checkFailed("@SYS107501");

            if (prepaymentLink == ErrorTolerance::Error)
                return checkFailed("@GLS114097");

            return true;
        }

]]></Source>
					</Method>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            if (this.checkPrepaymentLink_W())
            {
                super();
            }
        }

]]></Source>
