			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        QueryBuildDataSource qbds = rTax25RegisterJournalTable_ds.query()
			.dataSourceTable(tablenum(RTax25RegisterJournalTable));

        callerRecord = element.args().record();

        reportingPeriodType = RTax25PeriodType::Quarter;

        if (element.args().parmEnumType() == enumNum(RTax25RegisterJournalType))
        {
            journalType = element.args().parmEnum();

            qbds.addRange(fieldnum(RTax25RegisterJournalTable, RegisterJournalType))
				.value(queryValue(journalType));
        }

        rTax25RegisterJournalTable_ds.object(fieldnum(RTax25RegisterJournalTable, AssetTaxType))
			.visible(journalType == RTax25RegisterJournalType::Asset_RU);

        //if opening corrective tax registers
        if (element.args().parmEnumType() == enumNum(NoYes))
        {
            linkBySourceRecId = element.args().parmEnum();

            if (callerRecord)
            {
                journalType = callerRecord.RegisterJournalType;
            }

            qbds.clearDynalinks();

            if (linkBySourceRecId)
            {
                qbds.addDynalink(
                    fieldNum(RTax25RegisterJournalTable, SourceRecId),
                    callerRecord,
                    fieldNum(RTax25RegisterJournalTable, RecId));

                if (callerRecord.RegisterJournalType == RTax25RegisterJournalType::Asset_RU)
                {
                    RTax25RegisterJournalTable_DS.allowCreate(false);
                }
            }
            else
            {
                qbds.addDynalink(fieldNum(RTax25RegisterJournalTable, CorrectedRecId), callerRecord, fieldNum(RTax25RegisterJournalTable, RecId));
                qbds.addDynalink(fieldNum(RTax25RegisterJournalTable, PeriodNum), callerRecord, fieldNum(RTax25RegisterJournalTable, PeriodNum));
                qbds.addDynalink(fieldNum(RTax25RegisterJournalTable, PeriodType), callerRecord, fieldNum(RTax25RegisterJournalTable, PeriodType));
                qbds.addDynalink(fieldNum(RTax25RegisterJournalTable, FiscalYear), callerRecord, fieldNum(RTax25RegisterJournalTable, FiscalYear));
                qbds.addDynalink(fieldNum(RTax25RegisterJournalTable, RegisterJournalType), callerRecord, fieldNum(RTax25RegisterJournalTable, RegisterJournalType));

                RTax25RegisterJournalTable_DS.object(fieldNum(RTax25RegisterJournalTable, PeriodNum))
					.allowEdit(false);

                RTax25RegisterJournalTable_DS.object(fieldNum(RTax25RegisterJournalTable, PeriodType))
					.allowEdit(false);

                RTax25RegisterJournalTable_DS.object(fieldNum(RTax25RegisterJournalTable, FiscalYear))
					.allowEdit(false);


                if (callerRecord.RegisterJournalType == RTax25RegisterJournalType::ProfitTax)
                {
                    RTax25RegisterJournalTable_DS.allowCreate(false);
                }
            }

            Correction.visible(false);
        }

        if (!element.args().dataset() && element.args().menuItemName()) //checking that its not called from SysInfoAction
        {
            qbds.addRange(fieldnum(RTax25RegisterJournalTable, CorrectedRecId))
				.value(queryValue(0));
        }

        RTax25RegisterJournalTable_ds.object(fieldnum(RTax25RegisterJournalTable, AssetTaxType))
			.visible(journalType == RTax25RegisterJournalType::Asset_RU);

        RTax25RegisterJournalTable_DS.object(fieldnum(RTax25RegisterJournalTable, Version)).visible(
            element.args().dataset() == tableNum(RTax25RegisterJournalTable) ||
           !element.args().menuItemName());

        RTax25RegisterJournalTable_DS.object(fieldnum(RTax25RegisterJournalTable, Accepted))
			.visible(journalType == RTax25RegisterJournalType::ProfitTax
				&& (element.args().dataset() == tableNum(RTax25RegisterJournalTable)
					|| !element.args().menuItemName()));
    }

]]></Source>
