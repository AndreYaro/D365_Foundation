			<Method>
				<Name>setButtonAccess</Name>
				<Source><![CDATA[
    public void setButtonAccess()
    {
        rTax25LedgerJournal.visible(journalType == RTax25RegisterJournalType::ProfitTax);

        RTax25RegisterJournalTrans rTax25RegisterJournalTransConstDiff = RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
            RTax25RegisterTable::find(RTax25RegisterType::ConstantDifferences).RegisterId);
        RTax25RegisterJournalTrans rTax25RegisterJournalTransTempDiff = RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
            RTax25RegisterTable::find(RTax25RegisterType::TemporaryDifferences).RegisterId);
        RTax25RegisterJournalTrans rTax25RegisterJournalTransConstDiffByBalance = RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
            RTax25RegisterTable::find(RTax25RegisterType::ConstantDifferencesByBalance).RegisterId);
        RTax25RegisterJournalTrans rTax25RegisterJournalTransTempDiffByBalance = RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
            RTax25RegisterTable::find(RTax25RegisterType::TemporaryDifferencesByBalance).RegisterId);

        rTax25TaxDiffByBalanceLedgerJournalTable.visible(rTax25RegisterJournalTransConstDiffByBalance && rTax25RegisterJournalTransTempDiffByBalance);
        RTax25LedgerJournalTable.visible(rTax25RegisterJournalTransConstDiff && rTax25RegisterJournalTransTempDiff);

        boolean notExistReverseJournalForTaxDiffByBalance = LedgerJournalCreate_Tax25_RU::notExistReverseJournal(rTax25RegisterJournalTable, LedgerJournalType::RTax25TaxDiffByBalance, false);
        boolean notExistReverseJournalForTaxDiff = LedgerJournalCreate_Tax25_RU::notExistReverseJournal(rTax25RegisterJournalTable, LedgerJournalType::RTax25, false);

        RTax25TaxDiffByBalanceLedgerJournalTable.enabled(
            rTax25RegisterJournalTransConstDiffByBalance.Approved
            && rTax25RegisterJournalTransTempDiffByBalance.Approved
            && notExistReverseJournalForTaxDiff);
        rTax25LedgerJournalTable.enabled(
            rTax25RegisterJournalTransConstDiff.Approved
            && rTax25RegisterJournalTransTempDiff.Approved
            && notExistReverseJournalForTaxDiffByBalance);

        rTax25AmountDiffLedgerJournalTable.enabled(
            RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
				RTax25RegisterTable::find(RTax25RegisterType::AmountDifference).RegisterId).Approved);

        rTax25ExchDiffLedgerJournalTable.enabled(
            RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
				RTax25RegisterTable::find(RTax25RegisterType::TaxExchangeAdj).RegisterId).Approved);

        rTax25TaxRemainGoodsLedgerJournalTable.enabled(
            RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
				RTax25RegisterTable::find(RTax25RegisterType::TaxRemainGoods).RegisterId).Approved);

        rTax25DebtDebitReserveLedgerJournalTable.enabled(
            RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
				RTax25RegisterTable::find(RTax25RegisterType::DebtDebitReservationDetail).RegisterId).Approved
					|| RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
						RTax25RegisterTable::find(RTax25RegisterType::DebtDebitReservationDetailRAP).RegisterId).Approved);

        rAssetLedgerJournal.visible(journalType == RTax25RegisterJournalType::Asset_RU);
        rAssetLedgerJournal.enabled(rTax25RegisterJournalTable.EndDate == dateEndQtr(rTax25RegisterJournalTable.EndDate));
        rAssetAssessedLedgerJourTable.visible(rTax25RegisterJournalTable.isOfTaxType(RAssetTaxType::AssessedTax));
        rAssetTransportLedgerJourTable.visible(rTax25RegisterJournalTable.isOfTaxType(RAssetTaxType::TransportTax));
        rAssetLandLedgerJourTable.visible(rTax25RegisterJournalTable.isOfTaxType(RAssetTaxType::LandTax));

        rAssetAssessedLedgerJourTable.enabled(
            RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
				new DictEnum(enumNum(RAssetAssessedTaxRegType)).value2Symbol(RAssetAssessedTaxRegType::AssessedTax)).Approved);

        rAssetTransportLedgerJourTable.enabled(
            RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
				new DictEnum(enumNum(RAssetAssessedTaxRegType)).value2Symbol(RAssetAssessedTaxRegType::TransportTax)).Approved);

        rAssetLandLedgerJourTable.enabled(
            RTax25RegisterJournalTrans::findLink(rTax25RegisterJournalTable,
                new DictEnum(enumNum(RAssetAssessedTaxRegType)).value2Symbol(RAssetAssessedTaxRegType::LandTax)).Approved);

        RTax25RegisterJournalTableCorr.enabled(rTax25RegisterJournalTable.approved());

        Correction.enabled(rTax25RegisterJournalTable.RecId
			&& (rTax25RegisterJournalTable.RegisterJournalType == RTax25RegisterJournalType::Asset_RU
				|| rTax25RegisterJournalTable.PeriodType == reportingPeriodType));

        RTax25RegisterJournalTable_DS.object(fieldNum(RTax25RegisterJournalTable, Accepted))
			.allowEdit(rTax25RegisterJournalTable.Version > 0
				&& !rTax25RegisterJournalTable.existsFurtherCorrection()
				&& !rTax25RegisterJournalTable.existsPrevCorrections());

        RTax25RegisterJournalTrans.enabled(rTax25RegisterJournalTable.RecId);

        Correction.visible(rTax25RegisterJournalTable.Version == 0);
    }

]]></Source>
