					<Method>
						<Name>create</Name>
						<Source><![CDATA[
        public void create(boolean _append = false)
        {
            Args args = new Args();
            args.caller(element);
            args.parmEnumType(enumNum(RTax25RegisterJournalType));
            args.parmEnum(journalType);
            args.record(callerRecord);

            FormRun registerJournalCreate = new MenuFunction(menuItemDisplayStr(RTax25RegisterJournalCreate), MenuItemType::Display)
                .create(args);

            registerJournalCreate.run();

            if (!registerJournalCreate.closed())
            {
                registerJournalCreate.wait();
            }

            if (registerJournalCreate.closedOk())
            {
                RTax25RegisterJournalTable newRecord = RTax25RegisterJournalTable::findRecId(newJournalRecId);

                super(_append);

                RTax25RegisterJournalTable.data(newRecord);
                RTax25RegisterJournalTable_DS.reread();
            }
        }

]]></Source>
