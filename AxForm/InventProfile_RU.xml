			<Method>
				<Name>priorityDown</Name>
				<Source><![CDATA[
    void priorityDown()
    {
        InventProfile_RU    after;
        InventProfile_RU    cur;
        LineNum             priority;

        ttsbegin;

        select firstonly forupdate after
            order by SelectionPriority
            where after.SelectionPriority > inventProfileOrdered.SelectionPriority;

        if (after &&
            inventProfileOrdered &&
            after.SelectionPriority > inventProfileOrdered.SelectionPriority)
        {
            cur = InventProfile_RU::findRecId(inventProfileOrdered.RecId,true);

            if (cur)
            {
                priority = inventProfileOrdered.SelectionPriority;
                cur.SelectionPriority = after.SelectionPriority;

                after.SelectionPriority = priority + (after.SelectionPriority - priority) / 2;

                after.update();
                cur.update();
            }
        }

        ttscommit;

        if (cur)
        {
            inventProfileOrdered_ds.research();
            inventProfileOrdered_ds.positionToRecord(cur);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>priorityUp</Name>
				<Source><![CDATA[
    void priorityUp()
    {
        InventProfile_RU    before;
        InventProfile_RU    save;
        LineNum             priority;

        ttsbegin;

        select firstonly forupdate before
            order by SelectionPriority desc
            where before.SelectionPriority < inventProfileOrdered.SelectionPriority;

        if (before &&
            inventProfileOrdered &&
            before.SelectionPriority < inventProfileOrdered.SelectionPriority)
        {
            priority = inventProfileOrdered.SelectionPriority;
            inventProfileOrdered.SelectionPriority = before.SelectionPriority;

            before.SelectionPriority = before.SelectionPriority + (priority - before.SelectionPriority) / 2;

            before.update();
            inventProfileOrdered.update();
            save = inventProfileOrdered.data();
        }

        ttscommit;

        if (save)
        {
            inventProfileOrdered_ds.research();
            inventProfileOrdered_ds.positionToRecord(save);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>setFields</Name>
				<Source><![CDATA[
    void setFields()
    {
        inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdInit)).allowEdit(true);
        inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdSplitSalesInvoice)).allowEdit(true);
        inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdCheckPurchOrder)).allowEdit(true);
        inventProfile_ds.object(fieldNum(InventProfile_RU, LockCostAdjustment)).allowEdit(true);
        inventProfile_ds.object(fieldNum(InventProfile_RU, LockMarkupCustVend)).allowEdit(true);
        inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdCheckSalesOrder)).allowEdit(true);
        inventProfile_ds.object(fieldNum(InventProfile_RU, StockInTransit)).allowEdit(false);

        switch (inventProfile.InventProfileType)
        {
            case InventProfileType_RU::CommissionPrincipalAgent:
                inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdInit)).allowEdit(false);
                inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdSplitSalesInvoice)).allowEdit(false);
                inventProfile_ds.object(fieldNum(InventProfile_RU, LockCostAdjustment)).allowEdit(false);
                inventProfile_ds.object(fieldNum(InventProfile_RU, LockMarkupCustVend)).allowEdit(false);
                break;
            case InventProfileType_RU::CommissionAgent:
                inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdInit)).allowEdit(false);
                inventProfile_ds.object(fieldNum(InventProfile_RU, LockCostAdjustment)).allowEdit(false);
                inventProfile_ds.object(fieldNum(InventProfile_RU, LockMarkupCustVend)).allowEdit(false);
                break;
            case InventProfileType_RU::Bailee:
                inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdInit)).allowEdit(false);
                inventProfile_ds.object(fieldNum(InventProfile_RU, LockCostAdjustment)).allowEdit(false);
                inventProfile_ds.object(fieldNum(InventProfile_RU, LockMarkupCustVend)).allowEdit(false);
                inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdCheckSalesOrder)).allowEdit(false);
                break;
            case InventProfileType_RU::General:
                inventProfile_ds.object(fieldNum(InventProfile_RU, StockInTransit)).allowEdit(true);
                break;
            default:
                break;
        }

        if (inventProfile.InventOwnerIdInit && inventProfile.InventProfileType != InventProfileType_RU::General)
        {
            inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdCheckPurchOrder)).allowEdit(false);
        }

        switch (inventProfile.StockInTransit)
        {
            case StockInTransit_RU::BoughtInTransit:
                inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdInit)).allowEdit(false);
                inventProfile_ds.object(fieldNum(InventProfile_RU, InventOwnerIdCheckPurchOrder)).allowEdit(true);
                inventProfile.InventOwnerIdInit            = true;
                inventProfile.InventOwnerIdCheckPurchOrder = true;
                break;
        }
    }

]]></Source>
