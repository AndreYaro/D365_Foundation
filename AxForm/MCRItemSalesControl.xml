	<SourceCode>
		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
    [Form]
    /// <summary>
    ///     The <c>MCRItemSalesControl</c> form is used for sales order templates.
    /// </summary>
    public class MCRItemSalesControl extends FormRun
    {
        InventDimCtrl_Frm_Mov   inventDimFormSetup;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        // Call updateDesign so set item dimension attributes based on user's settings.
        element.updateDesign(InventDimFormDesignUpdate::Init);
    }

]]></Source>
			</Method>
			<Method>
				<Name>inventDimSetupObject</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Retuns an object that indicates what item dimensions to display.
    /// </summary>
    /// <returns>
    ///     An <c>Object</c> indicating which item dimensions to display.
    /// </returns>
    Object inventDimSetupObject()
    {
        return inventDimFormSetup;
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Updates the form based on Invent Dimension display set up.
    /// </summary>
    /// <param name="mode">
    ///     A <c>InventDimFormDesignUpdate</c> object which indicates the mode of the form.
    /// </param>
    void updateDesign(InventDimFormDesignUpdate mode)
    {
        SalesLine   salesLine;
        InventMovement movement;
        salesLine.ItemId = MCRItemSalesControl.ItemId;
        movement = InventMovement::construct(salesLine);

        switch (mode)
        {
            case InventDimFormDesignUpdate::Init:
                if (!inventDimFormSetup)
                {
                    inventDimFormSetup = InventDimCtrl_Frm_Mov::newFromFormMovement(element, movement);
                }
            case InventDimFormDesignUpdate::Active:
                inventDimFormSetup.formActiveSetup(movement.inventDimGroupSetup(), movement);
                inventDimFormSetup.formSetControls(true);
                break;
            case InventDimFormDesignUpdate::FieldChange:
                inventDimFormSetup.formActiveSetup(movement.inventDimGroupSetup(), movement);
                inventDimFormSetup.formSetControls(false);
                break;
            default: throw error(strfmt("@SYS54195", funcname()));
        }
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>MCRItemSalesControl</Name>
				<Methods>
					<Method>
						<Name>hasDimensions</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Determines if user has entered dimensions for the sales control record.
        /// </summary>
        /// <returns>
        ///     true if the user has entered dimensions; otherwise, false.
        /// </returns>
        boolean hasDimensions()
        {
            return (InventDim.anyProductDimensionSpecified() ||
                    InventDim.anyTrackingDimensionSpecified() ||
                    InventDim.InventLocationId || 
                    InventDim.wMSLocationId);
        }

]]></Source>
					</Method>
					<Method>
						<Name>create</Name>
						<Source><![CDATA[
        public void create(boolean _append = false)
        {
            super(_append);

            // hen a new record is created set the focus on item id.
            MCRItemSalesControl_ItemId.setFocus();
        }

]]></Source>
					</Method>
					<Method>
						<Name>validateWrite</Name>
						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean ret = true;

            // Validate item dimensions if any are populated.
            if (this.hasDimensions() && !InventDim.validateWriteProductDimensions(MCRItemSalesControl.ItemId, true))
            {
                InventDim_ds.reread();
                ret = false;
            }

            // Dimensions are valid or none are populated then create/find the InventDim
            // record and link it to the MCRItemSalesControl record.
            if (ret)
            {
                MCRItemSalesControl.InventDimId = InventDim::findOrCreate(inventDim).InventDimId;
            }

            ret = super() && ret;

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        public void write()
        {
            super();

            if (MCRItemSalesControl.InventDimId != inventDim.InventDimId)
            {
                // Reload the inventDim record after the write since a new one
                // may be created.
                inventDim.data(InventDim::find(MCRItemSalesControl.InventDimId));
                inventDim_ds.setCurrent();
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;

            ret = super();

            // Each record may have differen properties based on its set up
            // so re-run the inv. dimension logic to make sure all dimension
            // field properties are correctly set.
            element.updateDesign(InventDimFormDesignUpdate::Active);

            InventDim_ds.allowEdit(true);

            return ret;
        }

]]></Source>
