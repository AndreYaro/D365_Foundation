				<Source><![CDATA[
[Form]
public class MCRCustPaymInstallments extends FormRun
{
    SalesTable                      salesTable;
    // Saves whether or not one of the lies on the installment plan has been paid
    MCRCustPaymTotals               mcrCustPaymTotals;
    MCRSalesOrderTotals             mcrSalesOrderTotals;
    AmountCur                       totalEligibleIBAmount;
    boolean                         formAllowEdit;
}
]]></Source>
			</Method>
			<Method>
				<Name>addNewCardHolder</Name>
				<Source><![CDATA[
    /// <summary>
    /// Loads customer credit card creation form and populates
    ///     selected payment record with returned values.
    /// </summary>
    void addNewCardHolder()
    {
        CreditCardPaymentCardTokenize   cardTokenize;
        CreditCardProcessors            activeCreditCardProcessors;
        CreditCardCust                  newCreditCardCust;

        if (MCRCustPaymTable_InstBill.TenderTypeId == '' && FeatureStateProvider::isFeatureEnabled(RetailRestrictCreditCardUsageFeature::instance()))
        {
            // Must set a payment method before adding a new credit card.
            warning("@Retail:PaymentMethodMustBeSelectedBeforeAddingNewCreditCard");
            return;
        }


        activeCreditCardProcessors = CreditCardProcessors::findActiveProcessor();

        cardTokenize = CreditCardPaymentCardTokenize::construct();

        cardTokenize.init(activeCreditCardProcessors.Name);
        cardTokenize.parmCustAccount(MCRCustPaymTable_InstBill.CustAccount);
        cardTokenize.parmIsSupportSalesOrderScope(true);

        if (!cardTokenize.getFromDialog())
        {
            return;
        }

        newCreditCardCust = CreditCard::addCreditCard(cardTokenize.getXmlString(),
                                                        MCRCustPaymTable_InstBill.CustAccount,
                                                        activeCreditCardProcessors.RecId);
        if (newCreditCardCust)
        {
            CreditCardCust::scopeCreditCardToken(newCreditCardCust.RecId, RetailTokenScope::SalesOrder);

            MCRCustPaymTable_InstBill.PaymInfoRecId = newCreditCardCust.RecId;
            MCRCustPaymTable_InstBill.PaymInfoTableId = newCreditCardCust.TableId;
            MCRCustPaymTable_InstBill_ds.cacheCalculateMethod(tableMethodStr(MCRCustPaymTable, editCreditCard));
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (!element.args()
            || !element.args().record()
            || element.args().dataset() != tableNum(SalesTable))
        {
            throw error(strfmt("@MCR10663", element.name()));
        }

        salesTable = element.args().record();

        mcrSalesOrderTotals = new MCRSalesOrderTotals(salesTable);
        totalEligibleIBAmount = mcrSalesOrderTotals.getInstallmentEligibleTotal();
        mcrCustPaymTotals = MCRCustPaymTotals::construct(salesTable.TableID, salesTable.RecId, mcrSalesOrderTotals);

        if (element.args().parm() == 'InquiryOnly')
        {
            formAllowEdit = false;
        }
        else
        {
            formAllowEdit = true;
        }

        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        super();

        this.setInstBillingControls();

        // If the form was open in inquiry only mode
        // Do not allow payment modification.
        if (!formAllowEdit)
        {
            MCRCustPaymTable_InstBill_ds.allowEdit(false);
            MCRCustPaymTable_InstBill_ds.allowDelete(false);
        }

        // If the form was open in inquiry only mode
        // Or any of the lines have been paid or invoiced
        // Do not allow modification
        if (!formAllowEdit
            || (MCRInstallmentPlanSalesTable
            && (MCRInstallmentPlanSalesTable.hasAnyPayments()
            || MCRInstallmentPlanSalesTable.hasBeenInvoiced())))
        {
            MCRInstallmentPlanSalesTable_ds.allowEdit(false);
            MCRInstallmentPlanSalesTable_ds.allowDelete(false);
        }

        element.updatePaymTypeControls();
    }

]]></Source>
			</Method>
			<Method>
				<Name>setInstBillingControls</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Determines if the Installment billing controls need to be displayed or not.
    /// </summary>
    /// <remarks>
    ///     Only display if the Installment billing amount is > 0 and eligible plans exist.
    /// </remarks>
    public void setInstBillingControls()
    {
        AmountCur                       salesTotalAmount;
        MCRInstallmentPlanSalesTable    mcrInstallmentPlanSalesTableLocal;

        // Show plan info if it exists on the order even
        // if the customer is currently ineligible
        // (this means at one point they were eligible & used a plan to finance the order).
        if (mcrInstallmentPlanSalesTable
            || CustTable::find(salesTable.InvoiceAccount).mcrCustTable().InstallmentEligible )
        {
            salesTotalAmount = mcrSalesOrderTotals.getTotalOrder();

            if (totalEligibleIBAmount > 0)
            {
                // Set the total based on what's eligible, min/max amts & paydowns
                // so that plans are validated correctly.
                mcrInstallmentPlanSalesTableLocal = MCRInstallmentPlanSalesTable::findSalesId(salesTable.SalesId);
            }

            // Set the values
            SalesTotal_InstBilling.realValue(totalEligibleIBAmount);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updatePaymTypeControls</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Updates properties of form controls based upon the payment type of the
    ///     active payment.
    /// </summary>
    public void updatePaymTypeControls()
    {
        if (MCRCustPaymTable_InstBill.CustPaymType == MCRCustPaymType::CreditCard)
        {
            CCInfo.allowEdit(true);
            CCInfo.skip(false);
        }
        else
        {
            CCInfo.allowEdit(false);
            CCInfo.skip(true);
        }
    }

]]></Source>
