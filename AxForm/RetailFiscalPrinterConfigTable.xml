				<Source><![CDATA[
    public void init()
    {
        super();

        configMgr = RetailFiscalPrinterConfigManager::construct();
        
        ExportSampleConfig.visible(configMgr.sampleConfigResourceName() != '');

        this.viewEditModeHelper().EditModeSwitched += eventhandler(this.enableExportImportButtons);
        this.viewEditModeHelper().ViewModeSwitched += eventhandler(this.enableExportImportButtons);
    }

]]></Source>
