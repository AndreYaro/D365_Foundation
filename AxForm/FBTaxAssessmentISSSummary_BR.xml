    FBTaxAssessment_BR              fbTaxAssessment;

}
]]></Source>
			</Method>
			<Method>
				<Name>updateValues</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates the fields values
    /// </summary>
    public void updateValues()
    {
        FBTaxAssessmentCalculator_BR taxAssessmentCalculatorIss;

        fbTaxAssessment = element.args().record();

        if (fbTaxAssessment)
        {
            taxAssessmentCalculatorIss = fbTaxAssessment.getCalculator();
            taxAssessmentCalculatorIss.calculate();

            Debits.realValue(taxAssessmentCalculatorIss.getDebits());
            Retained.realValue(taxAssessmentCalculatorIss.getRetained());
        }
        else
        {
            Debits.realValue(0);
            Retained.realValue(0);
        }
    }

]]></Source>
