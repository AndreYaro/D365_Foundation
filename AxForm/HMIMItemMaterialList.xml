			<Method>
				<Name>customizeMenuOptions</Name>
				<Source><![CDATA[
    public void customizeMenuOptions(OfficeMenuOptions _menuOptions)
    {
        // If feature HMIMMaterialDivisionUniqueInClassFeature is enabled, hide the V2 data entity, otherwise hide the V3.
        var entityNameToDelete = isHMIMMaterialDivisionUniqueInClassFeatureEnabled
            ? dataEntityViewStr(HMIMItemMaterialDetailV2Entity)
            : dataEntityViewStr(HMIMItemMaterialDetailV3Entity);

        ListIterator dataEntityIterator = new ListIterator(_menuOptions.dataEntityOptions());

        while (dataEntityIterator.more())
        {
            if (dataEntityIterator.value().dataEntityName() == entityNameToDelete)
            {
                dataEntityIterator.delete();
                break;
            }
            else
            {
                dataEntityIterator.next();
            }
        }
    }

]]></Source>
			</Method>
