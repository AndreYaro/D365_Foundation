            /// <summary>
            ///  Default/Override item material data when ClassGroupCode is modified
            /// </summary>
            public void modified()
            {
                super();

                if (HMIMItemMaterial.ClassGroupCode != HMIMItemMaterial.orig().ClassGroupCode)
                {
                    if (EcoResProductParameters::find().UpdateShippingPrintTextItem)
                    {
                        HMIMItemMaterial_ds.initFromClassGroupCode();
                    }
                }
            }

