						<Name>initFromMaterialCode</Name>
						<Source><![CDATA[
        /// <summary>
        /// Default/Override item material data based on Material code
        /// </summary>
        public void initFromMaterialCode()
        {
            if (Box::yesNo("@SCM:InitFromMaterialCode", DialogButton::Yes) == DialogButton::Yes)
            {
                HMIMMaterial material = HMIMMaterial::find(HMIMItemMaterial.MaterialCode);

                HMIMItemMaterial.RegCode                = material.RegCode ? material.RegCode : HMIMItemMaterial.orig().RegCode;
                HMIMItemMaterial.MaterialDescription    = material.MaterialDescription;
                HMIMItemMaterial.Multiplier             = material.Multiplier;
                HMIMItemMaterial.ClassCode              = material.ClassCode;
                HMIMItemMaterial.DivisionCode           = material.DivisionCode;
                HMIMItemMaterial.PackingGroupCode       = material.PackingGroupCode;
                HMIMItemMaterial.IdentificationCode     = material.IdentificationCode;
                HMIMItemMaterial.TechnicalNameCode      = material.TechnicalNameCode;
                HMIMItemMaterial.TunnelCode             = material.TunnelCode;
                HMIMItemMaterial.TransportCategoryCode  = material.TransportCategoryCode;
                HMIMItemMaterial.MarinePollutant        = material.MarinePollutant;
                HMIMItemMaterial.EnviroDangerous        = material.EnviroDangerous;
                HMIMItemMaterial.LimitedQty             = material.LimitedQty;
                HMIMItemMaterial.LabelCode              = material.LabelCode;
                HMIMItemMaterial.NOS                    = material.NOS;
                HMIMItemMaterial.StowageCode            = material.StowageCode;
                HMIMItemMaterial.IATAStar               = material.IATAStar;
                HMIMItemMaterial.EMSCode                = material.EMSCode;
                HMIMItemMaterial.AirType                = material.AirType;
                HMIMItemMaterial.PackInstCodePassenger  = material.PackInstCodePassenger;
                HMIMItemMaterial.PackInstCodeCargo      = material.PackInstCodeCargo;
                HMIMItemMaterial.UnitOfMeasure          = InventTableModule::find(HMIMItemMaterial.ItemId, ModuleInventPurchSales::Invent).UnitId;
                HMIMItemMaterial.Flashpoint             = material.Flashpoint;
                HMIMItemMaterial.PackCode               = material.PackCode;
                         
                HMIMItemMaterial_ds.research(true);

                HMIMShippingPrintText::construct().copyShippingPrintTxtLanguage(HMIMItemMaterial);

            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>initFromClassGroupCode</Name>
						<Source><![CDATA[
        /// <summary>
        /// Default/Override item class group data based on Group code
        /// </summary>
        public void initFromClassGroupCode()
        {
            if (Box::yesNo("@SCM:InitFromClassCode", DialogButton::Yes) == DialogButton::Yes)
            {
                HMIMClassGroup classgroup = HMIMClassGroup::find(HMIMItemMaterial.ClassGroupCode);

                HMIMItemMaterial.Multiplier             = classgroup.Multiplier;
                HMIMItemMaterial.ClassCode              = classgroup.ClassCode;
                HMIMItemMaterial.DivisionCode           = classgroup.DivisionCode;
                HMIMItemMaterial.PackingGroupCode       = classgroup.PackingGroupCode;
                HMIMItemMaterial.TransportCategoryCode  = classgroup.TransportCategoryCode;

                HMIMItemMaterial_ds.research(true);
            }
        }

]]></Source>
