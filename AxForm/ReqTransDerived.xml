		<DataSources xmlns="">
			<DataSource>
				<Name>ReqTrans</Name>
				<Methods>
					<Method>
						<Name>linkActive</Name>
						<Source><![CDATA[
        void linkActive()
        {
            ReqTrans reqTransCaller;

            if (element.args().caller() && element.args().dataset() == tablenum(ReqTrans))
            {
                reqTransCaller = element.args().record();

                criteriaRefType.value(queryValue(-1));

                if (ReqTrans::derivedRefType(reqTransCaller.RefType) != ReqRefType::None)
                {
                    criteriaRefType.value(queryValue(ReqTrans::derivedRefType(reqTransCaller.RefType)));
                }

                criteriaRefId.value(queryValue(reqTransCaller.RefId));
                criteriaReqPlanVersion.value(queryValue(reqTransCaller.PlanVersion));
            }

            pmfFormCtrl_ReqTransDerived.dataSourceLinkActivePre();

            super();
        }

]]></Source>
					</Method>
					<Method>
						<Name>actionSum</Name>
						<Source><![CDATA[
        display ActionQty actionSum(ReqTrans _reqTrans)
        {
            return _reqTrans.actionSum() != _reqTrans.Qty ? _reqTrans.actionSum() : 0;
        }

]]></Source>
					</Method>
					<Method>
						<Name>actionDate</Name>
						<Source><![CDATA[
        display ActionDate actionDate(ReqTrans _reqTrans)
        {
            return _reqTrans.ActionDate != _reqTrans.ReqDate ? reqTrans.ActionDate : dateNull();
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        void init()
        {
            super();

            reqTrans_ds.cacheAddMethod(tablemethodstr(ReqTrans, itemName));
            reqTrans_ds.cacheAddMethod(tablemethodstr(ReqTrans, displayWrkCtrConsumption));

            Query q = new Query();
            QueryBuildDataSource qbdsReqTrans = q.addDataSource(tablenum(ReqTrans));

            criteriaReqPlanVersion  = q.dataSourceNo(1).addRange(fieldnum(ReqTrans,PlanVersion));
            criteriaRefType         = q.dataSourceNo(1).addRange(fieldnum(ReqTrans,RefType));
            criteriaRefId           = q.dataSourceNo(1).addRange(fieldnum(ReqTrans,RefId));

            QueryBuildDataSource qbdsBOM = qbdsReqTrans.addDataSource(tableNum(BOM));
            qbdsBOM.joinMode(JoinMode::OuterJoin);
            qbdsBOM.relations(true);

            q.dataSourceNo(2).addSortField(fieldNum(BOM,Position));


            //criteriaReqPlanVersion.

            this.query(q);

            pmfFormCtrl_ReqTransDerived.dataSourceInitPost(reqTrans);
        }

]]></Source>
