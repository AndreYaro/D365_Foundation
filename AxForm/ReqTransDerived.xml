				<Source><![CDATA[
[Form]
public final class ReqTransDerived extends FormRun
{
    QueryBuildRange     criteriaRefType;
    QueryBuildRange     criteriaRefId;
    QueryBuildRange     criteriaReqPlanVersion;

    InventDimCtrl_Frm   inventDimFormSetup;

    PmfFormCtrl_ReqTransDerived pmfFormCtrl_ReqTransDerived;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        ReqParameters::checkPlanningProcessesAreEnabled();

        pmfFormCtrl_ReqTransDerived =
            PmfFormCtrl_ReqTransDerived::newFromForm(this);
        pmfFormCtrl_ReqTransDerived.initPre();

        super();

        element.updateDesign(InventDimFormDesignUpdate::Init);

        pmfFormCtrl_ReqTransDerived.setDatasources(reqTrans);
        pmfFormCtrl_ReqTransDerived.initPost();

        if (element.menuItemName() == menuitemDisplayStr(PmfCoByReqTransDerived))
        {
            displayWrkCtrConsumption.visible(false);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>inventDimSetupObject</Name>
				<Source><![CDATA[
    Object inventDimSetupObject()
    {
        return inventDimFormSetup;
    }

]]></Source>
			</Method>
			<Method>
				<Name>menuItemName</Name>
				<Source><![CDATA[
    public str menuItemName()
    {
        Args args = element.args();

        return !args ? '' : args.menuItemName();
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        super();

        if (element.args().parm() == '1')
        {
            tabPageAction.setFocus();
        }
        else if (element.args().parm() == '2')
        {
            tabPageFuture.setFocus();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    void updateDesign(InventDimFormDesignUpdate mode)
    {
        switch (mode)
        {
            case InventDimFormDesignUpdate::Init        :
                if (! inventDimFormSetup)
                {
                    inventDimFormSetup  = InventDimCtrl::newFromForm(element);
                }

            case InventDimFormDesignUpdate::Active      :
                inventDimFormSetup.formSetControls(true);
                break;
        }
    }

]]></Source>
			</Method>
