					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            switch (RecordState.selection())
            {
                case TrvCreditCardRecordState::All:
                    expenseLineDataSource.joinMode(JoinMode::ExistsJoin);
                    showTransferred.value("");
                    showMatched.value("");
                    break;

                case TrvCreditCardRecordState::Unattached:
                    expenseLineDataSource.joinMode(JoinMode::ExistsJoin);
                    showTransferred.value(SysQueryRangeUtil::valueEmptyString());
                    showMatched.value(SysQuery::valueNot(TrvAppStatus::Matched));
                    break;

                case TrvCreditCardRecordState::Matched:
                    expenseLineDataSource.joinMode(JoinMode::ExistsJoin);
                    showTransferred.value("");
                    showMatched.value(queryValue(TrvAppStatus::Matched));
                    break;

                case TrvCreditCardRecordState::Error:
                    expenseLineDataSource.joinMode(JoinMode::NoExistsJoin);
                    showTransferred.value("");
                    showMatched.value("");
                    break;
            }
            super();
        }

]]></Source>
