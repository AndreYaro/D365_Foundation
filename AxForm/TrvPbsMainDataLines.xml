						<Source><![CDATA[
        /// <summary>
        /// Generates expense lines for selected unreconciled expenses
        /// </summary>
        public void clicked()
        {
            MultiSelectionHelper selectedCreditCardTransactions = MultiSelectionHelper::createFromCaller(element);

            for (TrvPBSMaindata selectedCreditCardTransaction = selectedCreditCardTransactions.getFirst();
                selectedCreditCardTransaction;
                selectedCreditCardTransaction = selectedCreditCardTransactions.getNext())
            {
                setPrefix(strFmt("@Expense:CreateExpenseFromCreditCardInfo", selectedCreditCardTransaction.AmountCurr, selectedCreditCardTransaction.TransDate, HcmWorker::find(selectedCreditCardTransaction.Worker).name()));
                TrvExpTrans trvExpTrans = TrvUnreconciledExpenseHelper::insertExpenseLineFromCreditCard(selectedCreditCardTransaction);
            }

            super();
        }

