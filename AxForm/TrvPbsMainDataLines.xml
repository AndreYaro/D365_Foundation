			<Method>
				<Name>populateCreditCardTransControl</Name>
				<Source><![CDATA[
    /// <summary>
    /// Pre-populate credit card transaction into the entity name control.
    /// </summary>
    /// <param name="_formRun">
    /// The <c>FormRun</c> object of ImportTransactions form.
    /// </param>
    /// <returns>
    /// The <c>FormRun</c> object to ImportTransactions form.
    /// </returns>
    protected FormRun populateCreditCardTransControl(FormRun _formRun)
    {
        DMFEntity dmfEntity;
        const str entityNameControlName = 'EntityNameControl';

        // Pre-populate credit card transaction into the entity name control.
        FormStringControl entityNameControl = _formRun.control(_formRun.controlId(entityNameControlName)) as FormStringControl;
        if (entityNameControl)
        {
            select firstonly EntityName from dmfEntity
                    where dmfEntity.TargetEntity == tableId2Name(tableNum(TrvCreditCardTransactionEntity));
            entityNameControl.text(dmfEntity.EntityName);
            entityNameControl.modified();
            entityNameControl.allowEdit(false);
        }

        return _formRun;
    }

]]></Source>
