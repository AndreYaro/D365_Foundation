			<Method>
				<Name>setReassignTransactionsButtonAvailability</Name>
				<Source><![CDATA[
    /// <summary>
    /// Enable or disable reassign transactions button according to the status of associated employee of selected credit card transaction.
    /// Enabled when all selected records have same employee and the employee is terminated, otherwise disabled.
    /// </summary>
    private void setReassignTransactionsButtonAvailability()
    {
        TrvPBSMaindata	            selectedCreditCardTransaction;
        HcmWorkerRecId	            workerRecId;
        HcmEmployment	            workerEmploymentStatus;
        MultiSelectionHelper        multiSelectionHelper = MultiSelectionHelper::construct();
        boolean                     isWorkerSame = true;

        // Disable the button by default.
        ReassignTransactions.enabled(false);

        multiSelectionHelper.parmDatasource(TrvPBSMainData_DS);
        selectedCreditCardTransaction = multiSelectionHelper.getFirst();

        // Figure out whether worker is same for all selected records.
        workerRecId = selectedCreditCardTransaction.Worker;
        while (selectedCreditCardTransaction)
        {
            selectedCreditCardTransaction = multiSelectionHelper.getNext();

            if (selectedCreditCardTransaction && (workerRecId != selectedCreditCardTransaction.Worker))
            {
                isWorkerSame = false;
                break;
            }
        }

        // Enable reassign transactions only if the worker is terminated.
        if (isWorkerSame && workerRecId)
        {
            workerEmploymentStatus = HcmEmployment::findByWorkerLegalEntity(workerRecId,
                                                        CompanyInfo::current(selectedCreditCardTransaction.DataAreaId));

            if (!workerEmploymentStatus)
            {
                ReassignTransactions.enabled(true);
            }
        }
    }

]]></Source>
