					<Method>
						<Name>selectionChanged</Name>
						<Source><![CDATA[
        /// <summary>
        /// When selection changes, enable/disable reassign transactions button based on the employment status.
        /// </summary>
        public void selectionChanged()
        {
            super();

            TrvExpTrans trvExpTrans;
            select firstonly RecId, ApprovalStatus from trvExpTrans where trvExpTrans.PBSRecId == trvPBSMaindata.RecId;

            if (trvExpTrans.RecId && trvExpTrans.ApprovalStatus != TrvAppStatus::Create)
            {
				// If expense line associated with cc transaction has already been submitted for review, do not allow changes to record.
                TrvPBSMaindata_ds.allowEdit(false);
                ReassignTransactions.enabled(false);
            }
			else
            {
                TrvPBSMaindata_ds.allowEdit(true);
                element.setReassignTransactionsButtonAvailability();
            }    
        }

]]></Source>
