						<Methods>
							<Method>
								<Name>lookup</Name>
								<Source><![CDATA[
            /// <summary>
            /// Filters the valid payment method according to the current expense category.
            /// Includes methods that are marked Import only from the lookup.
            /// </summary>
            /// <param name = "_formControl">Form control</param>
            /// <param name = "_filterStr">Filter string</param>
            public void lookup(FormControl _formControl, str _filterStr)
            {
                TrvExpenseLookupHelper::createPaymentMethodLookup(_formControl, TrvPBSMaindata.CostType, false);
            }

]]></Source>
							</Method>
							<Method>
								<Name>validate</Name>
								<Source><![CDATA[
            /// <summary>
            /// Validates the entered payment method, applying the same filtering that is applied to the lookup.
            /// </summary>
            /// <returns>
            /// True if the new payment method is valid; otherwise, false.
            /// </returns>
            public boolean validate()
            {
                boolean ret;
            
                ret = super();

				if (ret)
                {
                    if (!TrvExpenseLookupHelper::validatePaymentMethod(TrvPBSMaindata.Paymethod, TrvPBSMaindata.CostType, false))
                    {
                        ret = ret && checkFailed(strFmt("@SYS107056", TrvPBSMaindata.Paymethod, TrvPBSMaindata.CostType));
                    }
                }

                if (ret)
                {
                    TrvExpTrans trvExpTrans, ccExpenseLine;

                    // Verify payment method will be valid on all expense lines associated with record
                    while select CostType from trvExpTrans
                        join ccExpenseLine 
                            where ccExpenseLine.PBSRecId == trvPBSMaindata.RecId
                            && (trvExpTrans.ParentRecId == ccExpenseLine.RecId // all child lines -> itemized, personal, or split
                            || trvExpTrans.RecId == ccExpenseLine.RecId) // parent line
                    {
                        if (!TrvExpenseLookupHelper::validatePaymentMethod(TrvPBSMaindata.Paymethod, trvExpTrans.CostType, false))
                        {
                            warning("@Expense:InvalidPaymentMethodSelectedForChildExpenseLine");
                            ret = ret && checkFailed(strFmt("@SYS107056", TrvPBSMaindata.Paymethod, trvExpTrans.CostType));
                            break;
                        }
                    }
                }

                return ret;
            }

]]></Source>
