					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            expenseLineDataSource = this.query().dataSourceNo(1).addDataSource(tableNum(TrvExpTrans));
            expenseLineDataSource.addLink(fieldNum(TrvPBSMaindata, RecId), fieldNum(TrvExpTrans, PBSRecId));
            expenseLineDataSource.joinMode(JoinMode::ExistsJoin);

            showTransferred = expenseLineDataSource.addRange(fieldNum(TrvExpTrans, ExpNumber));
            showTransferred.status(RangeStatus::Hidden);

            showMatched = expenseLineDataSource.addRange(fieldNum(TrvExpTrans, ApprovalStatus));
            showMatched.status(RangeStatus::Hidden);
        }

]]></Source>
