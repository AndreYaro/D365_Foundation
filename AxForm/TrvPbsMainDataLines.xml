						<Methods>
							<Method>
								<Name>validate</Name>
								<Source><![CDATA[
            /// <summary>
            /// Validates if cost type selected is valid for a credit card expense.
            /// </summary>
            /// <returns>
            /// True if cost type is valid for expense line; otherwise, false.
            /// </returns>
            public boolean validate()
            {
                boolean ret;
        
                ret = super();

                if (ret)
                {
                    TrvExpTrans trvExpTrans;
                    select firstonly RecId, SplitLineType, CostType from trvExpTrans where trvExpTrans.PBSRecId == trvPBSMaindata.RecId;

                    if (trvExpTrans.RecId && trvExpTrans.SplitLineType == TrvExpSplitLineType::SplitHeader)
                    {
                        // Cannot change category on split line.  Provide warning and reset.
                        TrvPBSMaindata.CostType = trvExpTrans.CostType;
                        warning("@Expense:ErrorChannotChangeCategoryOnCCForSplitLine");
                    }
                    else
                    {
                        TrvCostType costType = TrvCostType::find(TrvPBSMaindata.CostType);
                        if (!TrvUnreconciledExpenseHelper::isSupportedCategory(costType.ExpType, true))
                        {
                            ret = checkFailed("@Expense:InvalidCategoryForCreditCard");
                        }
                    }
                }
        
                return ret;
            }

]]></Source>
