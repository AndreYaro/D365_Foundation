				<Source><![CDATA[
    public void updateFooterAmount()
    {
        AmountCur   newTotalBud;
        AmountCur	revisedBud;

        newTotalBud = newTotalBudget.realValue(projBudgetLine.TotalBudget + projBudgetLine.UncommittedRevisions);

        revisedBud = revisedBudget.realValue(projBudgetRevisionLine.RevisionAmount);
        previousApprovedBudget.realValue(newTotalBud - revisedBud);
    }

