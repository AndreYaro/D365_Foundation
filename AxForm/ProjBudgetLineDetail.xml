			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        ProjBudget      budget;
        ProjBudgetLine  budgetLine;
        ProjBudgetRevision  projBudgetRevision;
        TmpProjBudgetRevisionLine   tmpProjBudgetRevisionLine;

        ProjBudgetRevisionLine projBudgetrevisionline1;
        ProjBudgetRevision  projBudgetRevision1;

        super();

        budgetLineRecId = 0;

        if (element.args() && element.args().record())
        {
            if (element.args().dataset() == tableNum(ProjBudgetLine))
            {
                budgetLine = element.args().record();
                budgetLineRecId = budgetLine.RecId;

                select firstonly RecId from projBudgetrevisionline1 join RecId from projBudgetRevision1 order by projBudgetRevision1.RevisionDate desc
                where projBudgetrevisionline1.ProjBudgetLine == budgetLine.RecId &&
                projBudgetrevisionline1.ProjBudgetRevision == projBudgetRevision1.RecId &&
                projBudgetRevision1.RevisionWorkflowStatus == ProjBudgetRevisionWFStatus::Approved;

                budgetRevisionLineRecId =  projBudgetrevisionline1.RecId;
            }
            if (element.args().dataset() == tableNum(TmpProjBudgetRevisionLine))
            {
                tmpProjBudgetRevisionLine = element.args().record();
                budgetLine = ProjBudgetLine::find(tmpProjBudgetRevisionLine.ProjBudgetLine);
                budgetLineRecId = tmpProjBudgetRevisionLine.ProjBudgetLine;
                budgetRevisionLineRecId = tmpProjBudgetRevisionLine.ProjBudgetRevisionLine;
                if (budgetRevisionLineRecId == 0)
                {
                        throw error("@SYS22996"); // Form called incorrectly
                }
            }
        }

        if (budgetLineRecId == 0)
        {
             throw error("@SYS22996"); // Form called incorrectly
        }

        //
        originalAmount = ProjBudgetLineDetail::calcProjBudgetLineOrigAmount(budgetLineRecId, budgetRevisionLineRecId);
        projBudgetLineType = budgetLine.ProjBudgetLineType;
        projTransType = budgetLine.ProjTransType;

        if (budgetRevisionLineRecId == 0)
        {
            budget = ProjBudget::find(budgetLine.ProjBudget);
            if (budget && (budget.BudgetWorkflowStatus == ProjBudgetWorkflowStatus::Created
                || budget.BudgetWorkflowStatus == ProjBudgetWorkflowStatus::Rejected))
            {
                isReadOnly = false;
            }
            else
            {
                isReadOnly = true;
            }
        }
        else
        {
            projBudgetRevision = ProjBudgetRevision::find(tmpProjBudgetRevisionLine.ProjBudgetRevision);
            if (projBudgetRevision &&(projBudgetRevision.RevisionWorkflowStatus == ProjBudgetRevisionWFStatus::Created ||
                projBudgetRevision.RevisionWorkflowStatus == ProjBudgetRevisionWFStatus::Rejected))
            {
                isReadOnly = false;
            }
            else
            {
                isReadOnly = true;
            }
        }

        element.initControls();

        element.updateDesign(InventDimFormDesignUpdate::Init);
    }

]]></Source>
