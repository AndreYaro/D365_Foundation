			<Method>
				<Name>initControls</Name>
				<Source><![CDATA[
    private void initControls()
    {
        if (isReadOnly)
        {
            projBudgetLineDetail_ds.allowEdit(false);
            projBudgetLineDetail_ds.allowCreate(false);
            projBudgetLineDetail_ds.allowDelete(false);
            element.design().viewEditMode(ViewEditMode::View);
        }

        // Show controls if this is a new budget.
        ProjBudgetLine_OriginalBudget.visible(budgetRevisionLineRecId == 0);
        ProjBudgetLine_CommittedRevisions.visible(budgetRevisionLineRecId == 0);
        ProjBudgetLine_UnCommittedRevisions.visible(budgetRevisionLineRecId == 0);
        ProjBudgetLine_TotalBudget.visible(budgetRevisionLineRecId == 0);

        // Show controls if this is a revision.
        ProjBudgetLine_TotalBudget1.visible(budgetRevisionLineRecId != 0);
        RevisedBudget.visible(budgetRevisionLineRecId != 0);
        NewTotalBudget.visible(budgetRevisionLineRecId != 0);

        if (projBudgetLineType == ProjBudgetLineType::Cost)
        {
            projBudgetLineDetail_CostPrice.visible(true);
            projBudgetLineDetail_TotalCostPrice.visible(true);
            projBudgetLineDetail_SalesPrice.visible(false);
            projBudgetLineDetail_TotalSalesPrice.visible(false);
        }
        else
        {
            projBudgetLineDetail_CostPrice.visible(false);
            projBudgetLineDetail_TotalCostPrice.visible(false);
            projBudgetLineDetail_SalesPrice.visible(true);
            projBudgetLineDetail_TotalSalesPrice.visible(true);
        }

        switch (projTransType)
        {
            case ProjTransType::Hour:
                projBudgetLineDetail_Resource.visible(true);
                projBudgetLineDetail_ResourceCategory.visible(true);
                projBudgetLineDetail_Quantity.visible(true);
                projBudgetLineDetail_ItemId.visible(false);
                projBudgetLineDetail_SalesUnitId.visible(false);
                tabPageDimension.visible(false);
                break;
            case ProjTransType::Cost:
                projBudgetLineDetail_Resource.visible(true);
                projBudgetLineDetail_ResourceCategory.visible(false);
                projBudgetLineDetail_Quantity.visible(true);
                projBudgetLineDetail_ItemId.visible(false);
                projBudgetLineDetail_SalesUnitId.visible(false);
                tabPageDimension.visible(false);
                break;
            case ProjTransType::Item:
                projBudgetLineDetail_Resource.visible(false);
                projBudgetLineDetail_ResourceCategory.visible(false);
                projBudgetLineDetail_Quantity.visible(true);
                projBudgetLineDetail_ItemId.visible(true);
                projBudgetLineDetail_SalesUnitId.visible(true);
                tabPageDimension.visible(true);
                break;
            case ProjTransType::Revenue:
                projBudgetLineDetail_Resource.visible(true);
                projBudgetLineDetail_ResourceCategory.visible(false);
                projBudgetLineDetail_Quantity.visible(false);
                projBudgetLineDetail_ItemId.visible(false);
                projBudgetLineDetail_SalesUnitId.visible(false);
                tabPageDimension.visible(false);
                break;

            default:
                break;
        }
    }

]]></Source>
