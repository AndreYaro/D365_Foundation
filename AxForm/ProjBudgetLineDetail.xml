			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        AmountCur   newAmount;
        ProjBudgetLine projBudgetLineTmp;
        ProjBudgetRevisionLine projBudgetRevisionLineTmp;

        super();

        if (!isReadOnly)
        {
            newAmount = ProjBudgetLineDetail::calcProjBudgetLineOrigAmount(budgetLineRecId, budgetRevisionLineRecId);

            if (newAmount != originalAmount)
            {
                ttsbegin;
                if (budgetRevisionLineRecId == 0)
                {
                    projBudgetLineTmp = ProjBudgetLine::find(budgetLineRecId,true);
                    if (projBudgetLineTmp )
                    {
                        projBudgetLineTmp.OriginalBudget = newAmount;
                        projBudgetLineTmp.update();
                    }
                }
                else
                {
                    projBudgetRevisionLineTmp = ProjBudgetRevisionLine::find(budgetRevisionLineRecId, true);
                    if (projBudgetRevisionLineTmp)
                    {
                        projBudgetRevisionLineTmp.RevisionAmount = newAmount - projBudgetline.TotalBudget;
                        projBudgetRevisionLineTmp.update();
                    }
                }
                ttscommit;

                if (element.args() && element.args().caller() && formHasMethod(element.args().caller(), identifierStr(refreshBudgetLine)))
                {
                    element.args().caller().refreshBudgetLine(budgetLineRecId);
                }
            }
        }
    }

]]></Source>
