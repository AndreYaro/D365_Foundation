			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (element.args()
            && element.args().menuItemName() == menuItemDisplayStr(RDeferralsTransLink))
        {
            element.showTransLink(element.args().record());
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>showTransLink</Name>
				<Source><![CDATA[
    public void showTransLink(RDeferralsTrans _source)
    {
        RDeferralsTrans_Q.dataSourceTable(tablenum(RDeferralsTrans))
            .clearDynalinks();

        QueryBuildDataSource qbds = RDeferralsTrans_Q.dataSourceTable(tablenum(TransLink_RU));
        qbds.joinMode(JoinMode::InnerJoin);

        findOrCreateRange_W(qbds, fieldnum(TransLink_RU, ReversalGroup), queryValue(TransLink_RU::findSec(_source.TableId, _source.RecId).ReversalGroup));

        CorrectionType.visible(true);

        RDeferralsTransLink.visible(false);
        TransactionReversal.visible(false);
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>RDeferralsTrans</Name>
				<Methods>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret = super();

            TransactionReversal.enabled(RDeferralsTrans.RecId
                && !RDeferralsTrans.StornoRecId
                && RDeferralsTable::find(RDeferralsTrans.DeferralsId).RefTableId != tablenum(RAssetTable)
                && RDeferralsTransactionReversal::allowReversTransType(RDeferralsTrans.TransType));

            RDeferralsTransLink.enabled(TransLink.RecId != 0);
            CorrectionGroup.visible(TransLink.RecId != 0);

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>displayOption</Name>
						<Source><![CDATA[
        public void displayOption(Common _deferralsTrans, FormRowDisplayOption _options)
        {
            RDeferralsTrans deferralsTrans = _deferralsTrans;

            if (deferralsTrans.StornoRecId )
            {
                _options.backColor(180 << 16 + 180 << 8 + 180);
            }

            super(_deferralsTrans, _options);
        }

]]></Source>
					</Method>
