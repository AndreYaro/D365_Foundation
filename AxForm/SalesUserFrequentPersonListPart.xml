			<Method>
				<Name>addUserFrequentPersonListEntry</Name>
				<Source><![CDATA[
    public void addUserFrequentPersonListEntry(DirPersonRecId _personFound)
    {
        UserInfo    userInfoLocal;
        UserId      currentUser;

        if (SalesUserFrequentPersonList.RecId)
        {
            currentUser = SalesUserFrequentPersonList.User;
        }
        else
        {
            RefRecId value = SysFilterValueHelper::getFirstValueRecordFromFilterForType(this.parmSourceFilter(), SysFilterValueTypeEnum::Record, tableNum(HcmWorker));

            if (value)
            {
                currentUser = HcmWorker::findPersonUser(value).User;
            }
            else
            {
                currentUser = curUserId();
            }
        }

        if (_personFound)
        {
            if (!SalesUserFrequentPersonList::userFrequentPersonListEntryExists(_personFound, currentUser))
            {
                SalesUserFrequentPersonList::addUserFrequentPersonListEntry(_personFound, currentUser);
                SalesUserFrequentPersonList_ds.research();
            }
        }
    }

]]></Source>
