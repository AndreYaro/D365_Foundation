			<Method>
				<Name>customizeMenuOptions</Name>
				<Source><![CDATA[
    public void customizeMenuOptions(OfficeMenuOptions _menuOptions)
    {
        // Clear the menu options which are driven by the root data source of an entity,
        ListIterator dataEntityIterator = new ListIterator(_menuOptions.dataEntityOptions());
        while (dataEntityIterator.more())
        {
            if (dataEntityIterator.value().dataEntityName() == dataentityviewstr(TMSTransportationAppointmentEntity))
            {
                dataEntityIterator.delete();
                break;
            }
        }

        List exportOptions = _menuOptions.customMenuItems();

        var exportOption = OfficeGeneratedExportMenuItem::construct(dataEntityViewStr(TMSTransportationAppointmentDriverLogEntity), TMSDriverLogExportToExcelOptionId);
        exportOption.displayName(strfmt("@SYS76681", "@TRX:TMSTransportationAppointmentDriverLogEntityName", '', curExt()));
        exportOptions.addEnd(exportOption);
    }

]]></Source>
			</Method>
			<Method>
				<Name>getDataEntityContext</Name>
				<Source><![CDATA[
    public ExportToExcelDataEntityContext getDataEntityContext(OfficeGeneratedExportMenuItem _menuItem)
    {
        ExportToExcelDataEntityContext context = null;

        switch (_menuItem.id())
        {
            case TMSDriverLogExportToExcelOptionId:
                context = ExportToExcelDataEntityContext::construct(dataEntityViewStr(TMSTransportationAppointmentDriverLogEntity), tableFieldGroupStr(TMSTransportationAppointmentDriverLogEntity, AutoReport));

                ListEnumerator enumerator = context.entities().getEnumerator();
                while (enumerator.moveNext())
                {
                    ExportToExcelDataEntityInfo entity = enumerator.current();

                    if (entity.entityName() == tableStr(TMSTransportationAppointmentDriverLogEntity))
                    {
                        ExportToExcelFilterTreeBuilder filterBuilder = new ExportToExcelFilterTreeBuilder(tablestr(TMSTransportationAppointmentDriverLogEntity));

                        var filter = filterBuilder.and(
                        filterBuilder.areEqual(fieldStr(TMSTransportationAppointmentDriverLogEntity, DataAreaId), curExt()),
                        filterBuilder.areEqual(fieldStr(TMSTransportationAppointmentDriverLogEntity, AppointmentId), tmsDriverLog.ApptId));

                        entity.filter(filter);
                        break;
                    }
                }
                break;
        }

        return context;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        switch (element.args().dataset())
        {
            case tableNum(TMSDriverLog):
                TMSDriverLog driverLog   = element.args().record();
                apptId      = driverLog.ApptId;
                break;
            case tableNum(TMSAppointment):
                TMSAppointment appointment = element.args().record();
                apptId      = appointment.ApptId;
                break;
            case tableNum(WHSLoadTable):
                WHSLoadTable loadTable   = element.args().record();
                apptId      = loadTable.tmsDisplayAppointmentId();
                break;
            case tableNum(TMSTransportationAppointmentDriverLogStaging):                
                TMSTransportationAppointmentDriverLogStaging transportationAppointmentDriverLogStaging = element.args().record();
                apptId = transportationAppointmentDriverLogStaging.AppointmentId;
                break;
        }

        super();
    }

]]></Source>
