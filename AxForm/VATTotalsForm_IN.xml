			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
    [Form]
    public class VATTotalsForm_IN extends FormRun
    {
        FormDataSource                  formDataSource;
        TaxTotalsArguments_IN           taxTotalsArguments;
        container                       fieldNames;
        VATTransInquiry_IN              vatTransInquiry;
        SelectQuery_IN                  selectQuery;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        TmpVATInquiryTable_IN           callerTmpTable;

        super();
        GlobalizationInstrumentationHelper::featureRun(GlobalizationConstants::FeatureReferenceIN00111, funcName());

        taxTotalsArguments = new TaxTotalsArguments_IN();
        taxTotalsArguments = element.args().parmObject();
        formDataSource     = taxTotalsArguments.getFormDataSource();
        fieldNames         = taxTotalsArguments.getFieldNames();
        vatTransInquiry    = taxTotalsArguments.getVATTransInquiryObject();
        callerTmpTable     = formDataSource.cursor();
        selectQuery        = element.args().parm();

        if (callerTmpTable.RecId)
        {
            VATTotal_IN::updateVATTotal(selectQuery, callerTmpTable, fieldNames, vatTransInquiry, false);
        }
        else
        {
            VATTotal_IN::updateVATTotal(selectQuery, callerTmpTable, fieldNames, vatTransInquiry, true);
        }
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>VAtTotal_IN</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            Query                       query = new Query();
            QueryBuildDataSource        queryBuildDataSource;
            QueryBuildRange             queryBuildRange;

            queryBuildDataSource    = query.addDataSource(tablenum(VATTotal_IN));
            queryBuildRange         = queryBuildDataSource.addRange(fieldnum(VATTotal_IN, SelectQuery));
            queryBuildRange.value(queryValue(selectQuery));
            this.query(query);

            super();
        }

]]></Source>
					</Method>
				</Methods>
