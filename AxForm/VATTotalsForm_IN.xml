				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            FormRun                 formRun;
            Args                    args                  = new Args();
            TaxTotalsArguments_IN   taxTotalsArgumentsLoc = new TaxTotalsArguments_IN();

            super();
            taxTotalsArgumentsLoc.setFormDataSource(vatTotal_IN_ds);
            taxTotalsArgumentsLoc.setVATTransInquiryObject(vatTransInquiry);
            args.parmObject(taxTotalsArgumentsLoc);
            formRun = new MenuFunction(menuitemdisplaystr(VATComponentTotals_IN), MenuItemType::Display).create(args);
            formRun.run();
            formRun.wait();
        }

]]></Source>
					</Method>
