				<Name>InternalUrl</Name>
				<Type>String</Type>
				<Methods>
					<Method>
						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            boolean ret;

            ret = super();

            if (internalUrl.text())
            {
                if (!externalUrl.text())
                {
                    externalUrl.text(internalUrl.valueStr());
                }
            }
            return ret;
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>OKButton</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            super();

            if (!CollabSiteSharePointOnlineProxy::validateUrl(internalUrl.text()))
            {
                warning ("@SYS91305");
                return;
            }

            collabSiteManager.linkSite(internalUrl.text(), externalUrl.text());

            if (formHasMethod( element.args().caller(), identifierStr(displayURL)))
            {
                element.args().caller().displayURL();
            }
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
