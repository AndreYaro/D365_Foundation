				<Source><![CDATA[
    [Form]
    public class BankLCImportMargin extends FormRun
    {
        BankLCImport bankLCImport;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        BankLCImportFeatureChecker::checkBankLCImportEnabled(true);

        if (element.args().dataset() != tablenum(bankLCImport))
        {
            throw error(Error::missingRecord(formStr(BankLCImportMargin)));
        }

        bankLCImport = element.args().record();

        if (!bankLCImport || bankLCImport.RecId == 0)
        {
            throw error(Error::missingRecord(formStr(BankLCImportMargin)));
        }

        super();
    }

]]></Source>
