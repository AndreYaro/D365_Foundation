						<Source><![CDATA[
                boolean tabChange(int fromTab)
        {
            boolean ret;

            ret = super(fromTab);
            switch (fromTab)
            {
                case 1:
                    if (paySheetTable.Name && paySheetTable_Ds.validateWrite())
                    {
                        paySheetTable_Ds.write();
                    }
                    break;
                case 3 :
                    if (paySheetTrans.SheetTableRecId && paySheetTrans_Ds.validateWrite())
                    {
                        paySheetTrans_Ds.write();
                    }
                    break;
            }

            return ret;
        }

