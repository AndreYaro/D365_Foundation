				<Methods>
					<Method>
						<Name>write</Name>
						<Source><![CDATA[
                public void write()
        {
            RPayAmount  origAmount = PaySheetTrans.orig().Amount;

            if (PaySheetTrans.TaxAmount != 0 && PaySheetTable.PaySheetType == RPaySheetType::Interim)
            {
                PaySheetTrans.BranchId = RPaySheetEngine::getBranchId(PaySheetTrans.Emplid, endmth(PaySheetTable.PayPeriod));
            }

            super();

            if (PaySheetTrans.Amount != origAmount)
            {
                paySheetTable.SumSheet = paySheetTable.SumSheet - origAmount + PaySheetTrans.Amount;
                paySheetTable.update();
                paySheetTable_Ds.reread();
                paySheetTable_Ds.refresh();
            }
        }

]]></Source>
