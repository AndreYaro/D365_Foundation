			<Method>
				<Name>init</Name>
				<Source><![CDATA[
        void init()
    {
        RPaySheetTable  rPaySheetTable;
        super();

        if (element.args().record() && element.args().record().TableId == tablenum(RPaySheetTable))
        {
            RPaySheetTable = element.args().record();
            periodDateControl.dateValue(RPaySheetTable.PayPeriod);
            paySheetType = RPaySheetTable.PaySheetType;
        }
        else
        {
            periodDateControl.dateValue(RPayParameters::payPeriod());
            paySheetType = element.args().parmEnum();
        }

        sheetTypeControl.selection(paySheetType);
        if (paySheetType || RPaySheetTable.RecId)
        {
            sheetTypeControl.visible(false);
        }
        paySheetEngine = RPaySheetEngine::construct(element);
        paySheetEngine.updateFormDesign();
    }

]]></Source>
