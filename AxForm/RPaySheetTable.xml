			<Control>
				<Name>PSTF_Button_SetSumm</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
                void clicked()
        {
            Dialog       dlg = new Dialog("@GLS105840");
            DialogField  dlFieldAmount;
            Amount       tmpAmount;
            Amount       totalSum;

            dlFieldAmount  = dlg.addField( extendedTypeStr(RPayAmount) );
            dlg.run();

            if (dlg.closedOk())
            {
                totalSum = 0;
                tmpAmount = dlFieldAmount.value();

                ttsbegin;
                while select forupdate paySheetTrans
                    where paySheetTrans.SheetTableRecId == paySheetTable.RecId
                {
                    paySheetTrans.Amount = tmpAmount;
                    totalSum += tmpAmount;
                    paySheetTrans.update();
                }
                paySheetTable.SumSheet = totalSum;
                paySheetTrans_Ds.research();
                paySheetTable_Ds.write();
                ttscommit;
            }

            super();
        }

]]></Source>
