                        public void modified()
            {
                RHRMEmplId    emplId;

                emplId = paySheetTrans.orig().Emplid;

                super();

                paySheetTrans_ds.cacheCalculateMethod(tablemethodstr(RPaySheetTrans, getFullEmplName));

                if (paySheetTrans.Emplid       != emplId                  &&
                    paySheetTable.PaySheetSost == RPaySheetSost::NoPaid   &&
                    paySheetTable.PaySheetType != RPaySheetType::Vacation &&
                    paySheetTable.HrmOrganisationId)
                {
                    if (RPayHistory::getHistoryLineByDate(paySheetTrans.Emplid, paySheetTable.TransDate).HrmOrganizationId !=
                        paySheetTable.HrmOrganisationId)
                    {
                        warning(strfmt("@GLS107049",
                                       paySheetTrans.Emplid, paySheetTable.TransDate, paySheetTable.HrmOrganisationId));
                    }
                }
            }

