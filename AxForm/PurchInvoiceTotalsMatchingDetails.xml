			<Method>
				<Name>updateInvoiceTotals</Name>
				<Source><![CDATA[
    public void updateInvoiceTotals()
    {
        purchTotals_Balance.realValue(purchTotals.purchBalance());
        purchTotals_TotalDiscount.realValue(purchTotals.purchEndDisc());
        purchTotals_MiscCharges.realValue(purchTotals.purchMarkup());
        purchTotals_SalesTax.realValue(purchTotals.purchTaxTotal());
        purchTotals_RoundOff.realValue(purchTotals.purchRoundOff());
        purchTotals_InvoiceAmount.realValue(purchTotals.purchTotalAmount());

        num.text(header.Num);
        purchId.text(header.displayPurchID());
    }

]]></Source>
