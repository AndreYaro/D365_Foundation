			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates access to the form when it is initially opened.
    /// </summary>
    /// <exception cref="Exception::Error">
    /// Accounts payable invoice totals matching is not enabled for this company. Use the 'Use invoice totals matching for this company' option in the Accounts payable parameters form to enable it.
    /// </exception>
    public void init()
    {
        // Window must be called with parameters
        if (!element.args().caller())
        {
            throw error("@SYS22539");
        }

        // Prevent the window from opening if the setup option is not checked
        if (VendParameters::find().UseTotalPriceMatching == NoYes::No)
        {
            throw error("@SYS130110");
        }

        super();

        purchTotals = element.args().parmObject();
        header = element.args().record();

        if (purchTotals.recordListPurchLine().len() == 0)
        {
            // If there are no PO-based lines on this invoice, display blank values
            balanceVariancePercent.showZero(RepShowZero::No);
            totalDiscountVariancePercent.showZero(RepShowZero::No);
            miscChargesVariancePercent.showZero(RepShowZero::No);
            salesTaxVariancePercent.showZero(RepShowZero::No);
            invoiceAmountVariancePercent.showZero(RepShowZero::No);
        }

        element.updateInvoiceTotals();

        if (FeatureStateProvider::isFeatureEnabled(VendInvoiceTotalsMatchingDetailFeature::instance())
            && PurchInvoiceTotalsMatchingDetailsFormDisableTolerancePercentageFlight::instance().isEnabled())
        {
            VendInvoiceMatching_ds.object(fieldNum(VendInvoiceMatching, TotalPriceTolerancePercentage)).visible(false);
        }
    }

]]></Source>
