			<Method>
				<Name>updateVariancePercentages</Name>
				<Source><![CDATA[
    public void updateVariancePercentages()
    {
        PriceVariancePercent value = VendInvoiceMatching::calcPercentVariance(purchTotals_Balance.realValue(), vendInvoiceMatching.ExpectedBalance);
        balanceVariancePercent.realValue(element.getDisplayValue(value));

        value = VendInvoiceMatching::calcPercentVariance(purchTotals_TotalDiscount.realValue(), vendInvoiceMatching.ExpectedTotalDiscount, true);
        totalDiscountVariancePercent.realValue(element.getDisplayValue(value));

        value = VendInvoiceMatching::calcPercentVariance(purchTotals_MiscCharges.realValue(), vendInvoiceMatching.ExpectedMarkup);
        miscChargesVariancePercent.realValue(element.getDisplayValue(value));

        value = VendInvoiceMatching::calcPercentVariance(purchTotals_SalesTax.realValue(), vendInvoiceMatching.ExpectedSalesTax);
        salesTaxVariancePercent.realValue(element.getDisplayValue(value));

        value = VendInvoiceMatching::calcPercentVariance(purchTotals_InvoiceAmount.realValue(), vendInvoiceMatching.ExpectedDocumentAmount);
        invoiceAmountVariancePercent.realValue(element.getDisplayValue(value));
    }

]]></Source>
