			<Method>
				<Name>updateWarnings</Name>
				<Source><![CDATA[
    public void updateWarnings()
    {
        #resAppl
        boolean isVariance;

        if (purchTotals.recordListPurchLine().len() == 0)
        {
            // If there are no PO-based lines on this invoice, display empty status
            this.setBalanceStatus(LastMatchVarianceOptions::Unknown);
            this.setTotalDiscountStatus(LastMatchVarianceOptions::Unknown);
            this.setMiscChargesStatus(LastMatchVarianceOptions::Unknown);
            this.setSalesTaxStatus(LastMatchVarianceOptions::Unknown);
            this.setRoundoffStatus(LastMatchVarianceOptions::Unknown);
            this.setInvoiceAmountStatus(LastMatchVarianceOptions::Unknown);
        }
        else
        {
            if (FeatureStateProvider::isFeatureEnabled(VendInvoiceTotalsMatchingDetailFeature::instance()))
            {
                this.updateWarningsWithFeatureEnabled();
            }
            else
            {
                // Balance
                isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveTolerance(
                                                purchTotals_Balance.realValue(),
                                                vendInvoiceMatching.ExpectedBalance,
                                                vendInvoiceMatching.TotalPriceTolerancePercentage);

                this.setBalanceStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);

                // TotalDiscount
                isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveTolerance(
                                                purchTotals_TotalDiscount.realValue(),
                                                vendInvoiceMatching.ExpectedTotalDiscount,
                                                vendInvoiceMatching.TotalPriceTolerancePercentage,
                                                true); // Negative balance

                this.setTotalDiscountStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);

                // MiscCharges
                isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveTolerance(
                                                purchTotals_MiscCharges.realValue(),
                                                vendInvoiceMatching.ExpectedMarkup,
                                                vendInvoiceMatching.TotalPriceTolerancePercentage);

                this.setMiscChargesStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);

                // SalesTax
                isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveTolerance(
                                                purchTotals_SalesTax.realValue(),
                                                vendInvoiceMatching.ExpectedSalesTax,
                                                vendInvoiceMatching.TotalPriceTolerancePercentage);

                this.setSalesTaxStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);

                // RoundOff - don't compare values

                // InvoiceAmount
                isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveTolerance(
                                                purchTotals_InvoiceAmount.realValue(),
                                                vendInvoiceMatching.ExpectedDocumentAmount,
                                                vendInvoiceMatching.TotalPriceTolerancePercentage);

                this.setInvoiceAmountStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateWarningsWithFeatureEnabled</Name>
				<Source><![CDATA[
    private void updateWarningsWithFeatureEnabled()
    {
        boolean isVariance;
        VendParameters vendParameters = VendParameters::find();

        // Balance
        if (vendParameters.UsePriceMatchingForSubTotalAmount)
        {
            isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveToleranceForSubTotalAmount(
                                                purchTotals_Balance.realValue(),
                                                vendInvoiceMatching.ExpectedBalance,
                                                vendInvoiceMatching.PriceTolerancePercentageForSubTotalAmount);

            this.setBalanceStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);
        }
        else
        {
            this.setBalanceStatus();
        }

        // Total discount
        if (vendParameters.UsePriceMatchingForTotalDiscount)
        {
            isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveToleranceForTotalDiscount(
                                                purchTotals_TotalDiscount.realValue(),
                                                vendInvoiceMatching.ExpectedTotalDiscount,
                                                vendInvoiceMatching.PriceTolerancePercentageForTotalDiscount,
                                                true); // Negative balance

            this.setTotalDiscountStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);
        }
        else
        {
            this.setTotalDiscountStatus();
        }

        // Misc charge
        if (vendParameters.UsePriceMatchingForCharges)
        {
            isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveToleranceForCharges(
                                                purchTotals_MiscCharges.realValue(),
                                                vendInvoiceMatching.ExpectedMarkup,
                                                vendInvoiceMatching.PriceTolerancePercentageForCharges);

            this.setMiscChargesStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);
        }
        else
        {
            this.setMiscChargesStatus();
        }

        // Sales tax
        if (vendParameters.UsePriceMatchingForCharges)
        {
            isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveToleranceForSalesTax(
                                                purchTotals_SalesTax.realValue(),
                                                vendInvoiceMatching.ExpectedSalesTax,
                                                vendInvoiceMatching.PriceTolerancePercentageForSalesTax);

            this.setSalesTaxStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);
        }
        else
        {
            this.setSalesTaxStatus();
        }

        // Invoice amount
        if (vendParameters.UsePriceMatchingForInvoiceAmount)
        {
            isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveToleranceForInvoiceAmount(
                                                purchTotals_InvoiceAmount.realValue(),
                                                vendInvoiceMatching.ExpectedDocumentAmount,
                                                vendInvoiceMatching.PriceTolerancePercentageForInvoiceAmount);

            this.setInvoiceAmountStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);
        }
        else
        {
            this.setInvoiceAmountStatus();
        }

        // RoundOff
        if (vendParameters.UsePriceMatchingForRoundOff)
        {
            isVariance = VendInvoiceMatching::isTotalPriceVarianceAboveToleranceForRoundOff(
                                                purchTotals_RoundOff.realValue(),
                                                vendInvoiceMatching.PriceToleranceAmountForInvoiceRoundOff);
        
            this.setRoundOffStatus(isVariance ? LastMatchVarianceOptions::Warning : LastMatchVarianceOptions::OK);
        }
        else
        {
            this.setRoundOffStatus();
        }
    }

]]></Source>
