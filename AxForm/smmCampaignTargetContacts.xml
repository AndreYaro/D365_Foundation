						<Methods>
							<Method>
								<Name>validate</Name>
								<Source><![CDATA[
            /// <summary>
            /// Validates that the party selected does not belong to a business relation that was merged or has had it's list deleted.
            /// </summary>
            /// <returns>
            /// true if the party field is valid; otherwise, false.
            /// </returns>
            public boolean validate()
            {
                boolean ret;
                MCRsmmBusRelTable mcRsmmBusRelTable;

                ret = super();

                if (isConfigurationkeyEnabled(configurationKeyNum(MCRCallCenter)))
                {
                    mcRsmmBusRelTable  = MCRsmmBusRelTable::findParty(smmCampaignSelectionResult.Party);

                    // Display error if trying to update a buisness relation that has been merged into another.
                    if (mcRsmmBusRelTable.MergedParent)
                    {
                        ret = checkFailed(strFmt("@MCR12337", mcRsmmBusRelTable.smmBusRelTable().BusRelAccount, mcRsmmBusRelTable.MergedParent));
                    }

                    if (mcRsmmBusRelTable.IsListDeleted)
                    {
                        ret = checkFailed("@MCR26131");
                    }
                }

                return ret;
            }

]]></Source>
