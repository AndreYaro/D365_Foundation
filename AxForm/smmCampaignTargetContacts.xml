				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        super();

        caller = element.args().caller();

        if (!caller)
        {
            // Form was called incorrectly.
            throw error("@SYS22996");
        }

        campaignTable               = element.args().record();
        campaignId                  = campaignTable.CampaignId;

        smmCampaignTargetSelection  = new smmCampaignTargetSelection(campaignId);

        queryClasses = smmCampaignQuery::buildClassLookup();
    }

]]></Source>
