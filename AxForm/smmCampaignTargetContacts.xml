[Form]
public class smmCampaignTargetContacts extends FormRun
{
    const int num_AND   = 1;
    const int num_OR    = 2;
    const int num_ADD   = 3;
    const int num_SUB   = 4;
    const int num_BR1   = 5;
    const int num_BR2   = 6;
    const int num_QUERY = 7;

    const str txt_AND   = "AND";
    const str txt_OR    = "OR";
    const str txt_ADD   = "+";
    const str txt_SUB   = "-";
    const str txt_BR1   = "(";
    const str txt_BR2   = ")";
    const str blank     = "";

    FormRun                     caller;

    smmCampaignTable            campaignTable;
    smmCampaignId               campaignId;

    smmCampaignTargetSelection  smmCampaignTargetSelection;
    smmCampaignTable            smmCampaignTableUpdate;

    FormDataSource              formDataSource;

    container                   queryClasses;
    boolean                     lookupFlag;

}
]]></Source>
			</Method>
			<Method>
				<Name>addSpace</Name>
				<Source><![CDATA[
    boolean addSpace(int _operatorNum)
    {
        if (!strLen(smmCampaignQueryExpression.Expression))
        {
            return false;
        }

        if (_operatorNum == num_AND || _operatorNum == num_OR)
        {
            return true;
        }

        if (strScan(smmCampaignQueryExpression.Expression, txt_AND, strLen(smmCampaignQueryExpression.Expression) - strLen(txt_AND), strLen(txt_AND)))
        {
            return true;
        }

        if (strScan(smmCampaignQueryExpression.Expression, txt_OR, strLen(smmCampaignQueryExpression.Expression) - strLen(txt_OR), strLen(txt_OR)))
        {
            return true;
        }

        return false;
    }

]]></Source>
