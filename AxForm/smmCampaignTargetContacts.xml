			<Method>
				<Name>prosessExpression</Name>
				<Source><![CDATA[
    boolean prosessExpression(int _operatorNum)
    {
        const str whiteSpace = " ";

        switch (_operatorNum)
        {
            case num_AND :

                if (element.addSpace(num_AND))
                {
                    smmCampaignQueryExpression.Expression += (whiteSpace + txt_AND);
                }
                else
                {
                    smmCampaignQueryExpression.Expression += txt_AND;
                }
                break;

            case num_OR :
                if (element.addSpace(num_OR))
                {
                    smmCampaignQueryExpression.Expression += (whiteSpace + txt_OR);
                }
                else
                {
                    smmCampaignQueryExpression.Expression += txt_OR;
                }
                break;

            case num_ADD :
                if (element.addSpace(num_ADD))
                {
                    smmCampaignQueryExpression.Expression += (whiteSpace + txt_ADD);
                }
                else
                {
                    smmCampaignQueryExpression.Expression += txt_ADD;
                }
                break;

            case num_SUB :
                if (element.addSpace(num_SUB))
                {
                    smmCampaignQueryExpression.Expression += (whiteSpace + txt_SUB);
                }
                else
                {
                    smmCampaignQueryExpression.Expression += txt_SUB;
                }
                break;

            case num_BR1 :
                if (element.addSpace(num_BR1))
                {
                    smmCampaignQueryExpression.Expression += (whiteSpace + txt_BR1);
                }
                else
                {
                    smmCampaignQueryExpression.Expression += txt_BR1;
                }
                break;

            case num_BR2 :
                if (element.addSpace(num_BR2))
                {
                    smmCampaignQueryExpression.Expression += (whiteSpace + ')');
                }
                else
                {
                    smmCampaignQueryExpression.Expression += txt_BR2;
                }
                break;

            case num_QUERY :
                if (element.addSpace(num_BR2))
                {
                    smmCampaignQueryExpression.Expression += (whiteSpace + smmCampaignQuery.NameId);
                }
                else
                {
                    smmCampaignQueryExpression.Expression += smmCampaignQuery.NameId;
                }
                break;

            default :
                return false;
        }

        if (smmCampaignQueryExpression.RecId)
        {
            smmCampaignQueryExpression.update();
        }
        else
        {
            smmCampaignQueryExpression.initValue();
            smmCampaignQueryExpression.insert();
        }

        smmCampaignQueryExpression_ds.executeQuery();

        return true;
    }

]]></Source>
