			<Method>
				<Name>calculatePosition</Name>
				<Source><![CDATA[
    int calculatePosition(str _text, smmCampaignSelectionExpression _expression, FormStringControl _control)
    {
        int         pos;
        real        posInLine;
        container   cursorPos;

        // Get the cursor position of the string control
        cursorPos = _control.getCursorPos();

        posInLine = conPeek(cursorPos, 1) / 6.075;

        if (posInLine <= 1)
        {
            pos = 1;
        }

        return pos;
    }

]]></Source>
