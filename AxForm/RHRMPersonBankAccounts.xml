					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            QueryBuildRange         qbrBank;
            QueryBuildRange         qbrCust;
            QueryBuildRange         qbrRHRMPerson;
            RecId                   ledgerDimension;
            CustVendAC              recipient;
            RHRMPersonTableRecId    rhrmPersonTableRecId;

            super();

            switch (element.args().dataset())
            {
                case tableNum(LedgerJournalTrans):
                    this.query().dataSourceNo(1).clearDynalinks();
                    qbrBank = this.query().dataSourceNo(1).addRange(fieldNum(RHRMPersonBankAccount, AccountID));
                    qbrBank.value(element.args().record().(fieldName2id(element.args().dataset(),fieldStr(LedgerJournalTrans, CustVendBankAccountId))));

                    qbrCust = this.query().dataSourceNo(1).addRange(fieldNum(CustBankAccount, CustAccount));

                    ledgerDimension = element.args().record().(fieldName2id(element.args().dataset(),fieldStr(LedgerJournalTrans, LedgerDimension)));
                    qbrCust.value(LedgerDynamicAccountHelper::getAccountNumberFromDynamicAccount(ledgerDimension));

                    break;

                case tableNum(RPayEmplStandardDeductionTable):
                    this.query().dataSourceNo(1).clearDynalinks();
                    qbrRHRMPerson = this.query().dataSourceNo(1).addRange(fieldNum(RHRMPersonBankAccount, RHRMPersonTable));

                    recipient = element.args().record().(fieldName2id(element.args().dataset(), fieldStr(RPayEmplStandardDeductionTable, Recipient)));
                    rhrmPersonTableRecId = RHRMPersonTable::findPayMainEmplId(RHRMEmplTable::find(recipient).PayMainEmplId).RecId;

                    qbrRHRMPerson.value(queryValue(rhrmPersonTableRecId));

                    break;
            }
        }

]]></Source>
