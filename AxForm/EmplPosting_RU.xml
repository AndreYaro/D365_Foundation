			<Method>
				<Name>setLookupButton</Name>
				<Source><![CDATA[
    void setLookupButton()
    {
        lineAccountField.enabled(true);
        lineAccountField.lookupButton(0);

        if (emplLedgerAccounts.AccountCode == TableGroupAll::All)
        {
            lineAccountField.enabled(false);
            lineAccountField.lookupButton(1);
            emplLedgerAccounts.Num = "";
        }
        if  (emplLedgerAccounts.AccountCode != emplLedgerAccounts.orig().AccountCode)
        {
             emplLedgerAccounts.Num                    = "";
             emplLedgerAccounts.SummaryLedgerDimension = 0;
        }
    }

]]></Source>
