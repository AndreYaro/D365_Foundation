						<Source><![CDATA[
        public void executeQuery()
        {
            QueryBuildDataSource queryBuildDataSource;

            if (combobox_filter.selection()==MCRShowRecords::All)
            {
                queryBuildDataSource = this.query().dataSourceTable(tableNum(MCRNCOAFileTable));
                queryBuildDataSource.clearRanges();
            }
            else
            {
                queryBuildDataSource = this.query().dataSourceTable(tableNum(MCRNCOAFileTable));
                queryBuildDataSource.clearRanges();
                queryBuildDataSource.addRange(fieldnum(MCRNCOAFileTable, Status)).value(int2str(Combobox_filter.selection()));
            }
            super();
        }

]]></Source>
