        void executeQuery()
        {
            QueryBuildDataSource queryBuildDataSource = this.query().dataSourceNo(1);

            queryBuildDataSource.clearDynalinks();
            if (itemId)
            {
                SysQuery::findOrCreateRange(queryBuildDataSource, fieldNum(InventItemBarcode, ItemId)).value(queryValue(itemId));
            }

            super();
        }

]]></Source>
