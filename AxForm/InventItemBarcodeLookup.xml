}
]]></Source>
			</Method>
			<Method>
				<Name>closeSelect</Name>
				<Source><![CDATA[
    void closeSelect(str p1)
    {
        PurchLine               purchLine;
        SalesLine               salesLine;
        SalesQuotationLine      salesQuotationLine;

        if (element.args() && element.args().record())
        {
            switch (element.args().dataset())
            {
                case tableNum(PurchLine)    :
                    purchLine = element.args().record() as PurchLine;
                    purchLine.BarCodeType = inventItemBarcode.BarcodeSetupId;
                    break;
                case tableNum(SalesLine)    :
                    salesLine = element.args().record() as SalesLine;
                    salesLine.BarCodeType = inventItemBarcode.BarcodeSetupId;
                    break;

                case tableNum(SalesQuotationLine)    :
                    salesQuotationLine = element.args().record() as SalesQuotationLine;
                    salesQuotationLine.BarCodeType = inventItemBarcode.BarcodeSetupId;
                    break;
            }
        }

        super(p1);
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        FormRun             callerFormRun;

        PurchLine               purchLine;
        SalesLine               salesLine;
        SalesQuotationLine      salesQuotationLine;
        RetailInventItemLabel   retailInventItemLabel;

        if (element.args() && element.args().record())
        {
            switch (element.args().dataset())
            {
                case tableNum(PurchLine)    :
                    purchLine = element.args().record() as PurchLine;
                    itemId = purchLine.ItemId;
                    break;
                case tableNum(SalesLine)    :
                    salesLine = element.args().record() as SalesLine;
                    itemId = salesLine.ItemId;
                    break;
                case tableNum(SalesQuotationLine)    :
                    salesQuotationLine = element.args().record() as SalesQuotationLine;
                    itemId = salesQuotationLine.ItemId;
                    break;
            }
        }

        if (element.args().caller())
        {
            if (element.args().caller().name() == formStr(RetailInventItemLabel))
            {
                retailInventItemLabel = element.args().record();
                itemId = retailInventItemLabel.ItemId;
            }
            callerFormRun   = element.args().caller();
            lookupControl   = element.selectTarget() as FormStringControl;
        }

        super();

        if (this.lookupControlHasWildcard())
        {
            inventItemBarcode_ds.query().dataSourceTable(tableNum(InventItemBarcode)).addRange(fieldNum(InventItemBarcode, ItemBarCode)).value(lookupControl.text());
        }

        element.updateDesign(InventDimFormDesignUpdate::Init);
        element.selectMode(inventItemBarcode_itemBarCode);
    }

]]></Source>
			</Method>
			<Method>
				<Name>lookupControlHasWildcard</Name>
				<Source><![CDATA[
    private boolean lookupControlHasWildcard()
    {
        return lookupControl && strFind(lookupControl.text(), '*', 1, strLen(lookupControl.text()));
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    void updateDesign(InventDimFormDesignUpdate mode)
    {
        switch (mode)
        {
            case InventDimFormDesignUpdate::Init           :
                if (!inventDimFormSetup)
                {
                    inventDimFormSetup  = InventDimCtrl_Frm_ItemBarcode::newFromForm(element);
                }
            // do continue
            case InventDimFormDesignUpdate::Active         :
                inventDimFormSetup.formSetControls(true);
                break;

            default : throw error(strFmt("@SYS54195",funcName()));
        }
    }

]]></Source>
			</Method>
