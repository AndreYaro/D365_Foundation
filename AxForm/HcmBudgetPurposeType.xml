						<Name>validateWrite</Name>
						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean ret;
            HcmBudgetPurposeTypeDetail purposeTypeDetail;

            purposeTypeDetail = HcmBudgetPurposeTypeDetail::findByBudgetPurposeTypeLegalEntityOnly(
                                                            hcmBudgetPurposeEarningBasis.EarningBudgetPurposeType,
                                                            hcmBudgetPurposeTypeDetail.LegalEntity);

            if (hcmBudgetPurposeType.CostElementType == HcmBudgetCostElementType::Earning
                    && purposeTypeDetail.IsPercentageBased == NoYes::Yes)
            {
                ret = checkFailed("@SYS4005574");
            }
            else
            {
                ret = super();
            }

            return ret;
        }

]]></Source>
