						<Name>written</Name>
						<Source><![CDATA[
        public void written()
        {
            super();

            if (isAmountOrPercentModified)
            {
                // call this method only if amount or percent is changed.
                element.isEarningBasisAdditionAllowed();
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;

            ret = super();

            if (hcmBudgetPurposeTypeDetail.LegalEntity != selectedCompany)
            {
                selectedCompany = hcmBudgetPurposeTypeDetail.LegalEntity;
                element.setLDDAC_COAContext();
            }

            element.setBudgetPurposeTypeDetailEnabled();

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>create</Name>
						<Source><![CDATA[
        public void create(boolean _append = false)
        {
            HcmBudgetPurposeTypeDetail  budgetPurposeTypeDetail;
            super(_append);

            // Upon creating a new record in HcmBudgetPurposeTypeDetail the selected company should be set to current company context
            // instead of last selected row in the HcmBudgetPurposeTypeDetail. Also set the ledger dimension controller context
            selectedCompany = CompanyInfo::findDataArea(curext()).RecId;
            element.setLDDAC_COAContext();

            select count(RecId) from budgetPurposeTypeDetail
                where budgetPurposeTypeDetail.BudgetPurposeType == hcmBudgetPurposeType.RecId;

            if (budgetPurposeTypeDetail.RecId == 0)
            {
                // Default the LE only if there are no detail records associated with this budget purpose type
                hcmBudgetPurposeTypeDetail.LegalEntity = selectedCompany;
            }
        }

]]></Source>
