				<Source><![CDATA[
[Form]
public class HcmBudgetPurposeType extends FormRun
{
    CompanyInfoRecId selectedCompany;
    FormDataObject costElement;

    boolean isAmountOrPercentModified;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        costElement = hcmBudgetPurposeType_ds.object(fieldNum(HcmBudgetPurposeType, CostElementType));

        element.setLDDAC_COAContext();
    }

]]></Source>
			</Method>
			<Method>
				<Name>isEarningBasisAdditionAllowed</Name>
				<Source><![CDATA[
    public void isEarningBasisAdditionAllowed()
    {
        boolean isValueAmount;

        isValueAmount = HcmBudgetPurposeTypeDetail::isEarningBasisAdditionAllowed(hcmBudgetPurposeType.RecId);

        hcmBudgetPurposeEarningBasis_ds.allowCreate(!isValueAmount);
        hcmBudgetPurposeEarningBasis_ds.allowEdit(!isValueAmount);

        isAmountOrPercentModified = false;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setBudgetPurposeTypeDetailEnabled</Name>
				<Source><![CDATA[
    public void setBudgetPurposeTypeDetailEnabled()
    {
        boolean allowEnabled;
        allowEnabled = !HcmPositionForecastBudgetAcctLine::checkExistByBudgetPurposeTypeDetail(hcmBudgetPurposeTypeDetail.RecId);

        hcmBudgetPurposeTypeDetail_ds.object(fieldNum(HcmBudgetPurposeTypeDetail,EffectiveDate)).allowEdit(allowEnabled);
        hcmBudgetPurposeTypeDetail_ds.object(fieldNum(HcmBudgetPurposeTypeDetail,ExpirationDate)).allowEdit(allowEnabled);

        hcmBudgetPurposeTypeDetail_ds.allowDelete(allowEnabled);

        hcmBudgetPurposeTypeDetail_ds.object(fieldNum(HcmBudgetPurposeTypeDetail, AnnualLimit)).allowEdit(hcmBudgetPurposeTypeDetail.IsPercentageBased == NoYes::Yes);
    }

]]></Source>
			</Method>
			<Method>
				<Name>setLDDAC_COAContext</Name>
				<Source><![CDATA[
    public void setLDDAC_COAContext()
    {
        if (!selectedCompany)
        {
            selectedCompany = CompanyInfo::findDataArea(curext()).RecId;
        }

        // Set Ledger Dimension segmented entry control DataAreaId
        hcmBudgetPurposeTypeDetail_LedgerDimension.parmDataAreaId(CompanyInfo::findRecId(selectedCompany).DataArea);
    }

]]></Source>
