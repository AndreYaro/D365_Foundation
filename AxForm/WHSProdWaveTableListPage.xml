						<Source><![CDATA[
        private void addLinkWHSProdWaveTableWithNoWorkOrOpenWorkView()
        {
            qbdsProdWaveTable = qbdsWave.addDataSource(tableStr(WHSProdWaveTableWithNoWorkOrOpenWorkView));
            qbdsProdWaveTable.enabled(true);
            qbdsProdWaveTable.joinMode(JoinMode::ExistsJoin);
            qbdsProdWaveTable.addLink(fieldNum(WHSWaveTable, WaveId), fieldNum(WHSProdWaveTableWithNoWorkOrOpenWorkView, WaveId));
        }

]]></Source>
