					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();
            this.query().dataSourceTable(tablenum(KanbanRule)).addRange(fieldnum(KanbanRule,LegalEntity)).value(queryValue(CompanyInfo::current()));
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            QueryBuildRange     range;

            range = SysQuery::findOrCreateRange(this.query().dataSourceTable(tablenum(KanbanRule)),
                                                                             fieldnum(KanbanRule, ReplenishmentStrategy));
            if (leanReplenishmentStrategy)
            {
                range.value(queryValue(leanReplenishmentStrategy));
            }
            else
            {
                range.value(SysQuery::valueUnlimited());
            }

            super();
        }

]]></Source>
