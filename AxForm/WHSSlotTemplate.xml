				<Type>ReferenceGroup</Type>
				<Methods>
					<Method>
						<Name>lookupReference</Name>
						<Source><![CDATA[
        public Common lookupReference()
        {
            Query lookupQuery = new Query();

            QueryBuildDataSource lookupQueryDataSource = lookupQuery.addDataSource(tableNum(WMSLocation));

            lookupQueryDataSource.addRange(fieldNum(WMSLocation, InventLocationId)).value(SysQueryRangeUtil::value(WHSSlotTemplate.InventLocationId));

            SysReferenceTableLookup sysRefTableLookup = SysReferenceTableLookup::newParameters(tableNum(WMSLocation), this);

            sysRefTableLookup.addLookupfield(fieldNum(WMSLocation, WMSLocationId));
            sysRefTableLookup.addLookupfield(fieldNum(WMSLocation, InventLocationId));
 
            sysRefTableLookup.parmQuery(lookupQuery);
  
            return sysRefTableLookup.performFormLookup();
        }

]]></Source>
					</Method>
				</Methods>
