{
    private const str WHSSlotTemplateQueryPart_Range = menuItemDisplayStr(WHSSlotTemplateQueryPart_Range);
    private const str WHSSlotTemplateLineQueryPart_Range = menuItemDisplayStr(WHSSlotTemplateLineQueryPart_Range);
    private WHSIQueryPart headerRangePart;
    private WHSIQueryPart lineRangePart;

    private WHSSlotTemplateForm formClass;
   
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        formClass = new WHSSlotTemplateForm();

        WHSSequenceNumberMoverForm::newFromDataSource(
                WHSSlotTemplateLine_ds,
                fieldStr(WHSSlotTemplateLine, SequenceNumber),
                [fieldStr(WHSSlotTemplateLine, SlotTemplate)])
            .initNewButtons(ButtonGroupLine);

        this.initWarehouseSelectionFields();
        this.initQueryFormParts();
        
    }

]]></Source>
			</Method>
			<Method>
				<Name>initWarehouseSelectionFields</Name>
				<Source><![CDATA[
    private void initWarehouseSelectionFields()
    {
        WHSSlotTemplate_ds.object(fieldNum(WHSSlotTemplate, WarehouseGroupId)).mandatory(WHSSlotTemplate.WarehouseGroupAll == WhsWarehouseGroupAll::Group);
        WHSSlotTemplate_ds.object(fieldNum(WHSSlotTemplate, InventLocationId)).mandatory(WHSSlotTemplate.WarehouseGroupAll == WhsWarehouseGroupAll::Warehouse);

        WHSSlotTemplate_ds.object(fieldNum(WHSSlotTemplate, WarehouseGroupId)).visible(WHSSlotTemplate.WarehouseGroupAll == WhsWarehouseGroupAll::Group);
        WHSSlotTemplate_ds.object(fieldNum(WHSSlotTemplate, InventLocationId)).visible(WHSSlotTemplate.WarehouseGroupAll == WhsWarehouseGroupAll::Warehouse);
    }

]]></Source>
			</Method>
			<Method>
				<Name>initQueryFormParts</Name>
				<Source><![CDATA[
    private void initQueryFormParts()
    {
        str fieldNameQuery = fieldId2Name(tableNum(WHSSlotTemplate), fieldNum(WHSSlotTemplate, PackedQueryRun));
        str fieldNameQueryName = fieldId2Name(tableNum(WHSSlotTemplate), fieldNum(WHSSlotTemplate, RecId));
        headerRangePart = WHSQueryHelper::initializeQueryFormPart(element, WHSSlotTemplateQueryPart_Range, fieldNameQuery, '%1', fieldNameQueryName, WHSSlotTemplate.defaultPackedQueryRun(), true, false);
   
        str fieldNameQuerLine = fieldId2Name(tableNum(WHSSlotTemplateLine), fieldNum(WHSSlotTemplateLine, PackedQueryRun));
        str fieldNameQueryNameLine = fieldId2Name(tableNum(WHSSlotTemplateLine), fieldNum(WHSSlotTemplateLine, SlotTemplate));
        str fieldNameQueryNameLine2 = fieldId2Name(tableNum(WHSSlotTemplateLine), fieldNum(WHSSlotTemplateLine, RecId));
        lineRangePart = WHSQueryHelper::initializeQueryFormPart(element, WHSSlotTemplateLineQueryPart_Range, fieldNameQuery, '%1|%2', fieldNameQueryName, WHSSlotTemplateLine.defaultPackedQueryRun(), true, false, fieldNameQueryNameLine2);
    }

]]></Source>
			</Method>
			<Method>
				<Name>queryUpdated</Name>
				<Source><![CDATA[
    public void queryUpdated(FormRun _sender, container _packedQuery)
    {
        str menuItemName = _sender.args().menuItemName();

        if(menuItemName == WHSSlotTemplateQueryPart_Range)
        {
            WHSSlotTemplate.PackedQueryRun = _packedQuery;
            WHSSlotTemplate.write();
        }
        else if(menuItemName == WHSSlotTemplateLineQueryPart_Range)
        {
            WHSSlotTemplateLine.PackedQueryRun = _packedQuery;
            WHSSlotTemplateLine.write();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>toggleButtons</Name>
				<Source><![CDATA[
    public void toggleButtons()
    {
        WHSSlotDemand slotDemand;

        select firstOnly RecId from slotDemand
            where slotDemand.SlotTemplate == WHSSlotTemplate.RecId;

        WHSSlotDemandLocated slotDemandLocated;

        select firstOnly RecId from slotDemandLocated
            where slotDemandLocated.SlotTemplate == WHSSlotTemplate.RecId;

        WHSSlotDemand.enabled(slotDemand.RecId != 0);
        ButtonLocateSlots.enabled(slotDemand.RecId != 0);
        WHSSlotDemandLocated.enabled(slotDemandLocated.RecId != 0);
        BtnRunReplen.enabled(slotDemandLocated.RecId != 0);

        this.toggleMoveUpDownButtons();
    }

]]></Source>
			</Method>
			<Method>
				<Name>toggleMoveUpDownButtons</Name>
				<Source><![CDATA[
    private void toggleMoveUpDownButtons()
    {
        WHSSlotTemplateLine lineAbove;

        select firstonly RecId from lineAbove
            order by SequenceNumber
            where lineAbove.SlotTemplate == WHSSlotTemplateLine.SlotTemplate
                && lineAbove.SequenceNumber < WHSSlotTemplateLine.SequenceNumber;

        WHSSlotTemplateLine lineBelow;

        select firstonly RecId from lineBelow
            order by SequenceNumber desc
            where lineBelow.SlotTemplate == WHSSlotTemplateLine.SlotTemplate
                && lineBelow.SequenceNumber > WHSSlotTemplateLine.SequenceNumber;
    }

]]></Source>
