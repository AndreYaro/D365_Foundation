            if (ret)
            {
                WHSSlotTemplate currentRecord = WHSSlotTemplate_ds.cursor();
                if (WHSSlotTemplate.SlotDemandType == WHSSlotTemplate.orig().SlotDemandType)
                {
                    WHSSlotTemplate.PackedQueryRun = currentRecord.orig().PackedQueryRun;
