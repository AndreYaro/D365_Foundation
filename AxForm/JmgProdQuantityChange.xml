}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        FormRun caller;

        super();

        caller = element.args().caller();
        if (caller &&
            (caller.name() == formstr(JmgCalcApproveDateView) ||
            caller.name() == formstr(JmgCalcApproveWeekView)))
        {
            jmgStampJournalTrans_JobFinished.visible(true);
        }
        else
        {
            jmgStampJournalTrans_JobFinished.visible(false);
        }
    }

]]></Source>
