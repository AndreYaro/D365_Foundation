				<Methods>
					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        public void write()
        {
            JmgPostStandardSystem       jmgPostStandardSystem;
            JmgProdMultiStartUp         jmgProdMultiStartUp;
            JmgProdMultiReportFinished  jmgProdMultiReportFinished;

            try
            {
                using(var activityContext = JmgInstrumentationLogger::createLogger(formStr(JmgProdQuantityChange)).registration().correctFeedback())
                {
                    // Post changes in quantities
                    jmgPostStandardSystem   = JmgPostStandardSystem::constructJmgStampJournalTrans(jmgStampJournalTrans);

                    if (jmgStampJournalTrans.QtyStartup != jmgStampJournalTrans.SysQtyStartup
                        || jmgStampJournalTrans.PdsCWQtyStartup != jmgStampJournalTrans.PdsCWSysQtyStartup
                        )
                    {
                        jmgProdMultiStartUp     = jmgPostStandardSystem.createProductionStartUpJournal();
                    }

                    if (jmgStampJournalTrans.QtyGood            != jmgStampJournalTrans.SysQtyGood              ||
                        jmgStampJournalTrans.PdsCWQtyGood       != jmgStampJournalTrans.PdsCWSysQtyGood         ||
                        jmgStampJournalTrans.ErrorSpecification != jmgStampJournalTrans.SysErrorSpecification
                        )
                    {
                        jmgProdMultiReportFinished = jmgPostStandardSystem.createReportFinishedJournal();
                    }

                    if ((jmgProdMultiStartUp && jmgProdMultiStartUp.parmUserCanceled())  ||
                        (jmgProdMultiReportFinished && jmgProdMultiReportFinished.parmUserCanceled()))
                    {
                        throw error("@SYS18447");
                    }

                    if (jmgProdMultiStartUp)
                    {
                        jmgProdMultiStartUp.runOperation();
                    }
                    if (jmgProdMultiReportFinished)
                    {
                        jmgProdMultiReportFinished.runOperation();
                    }

                    jmgPostStandardSystem.postQty(jmgStampJournalTrans);

                    super();
                }
            }
            catch
            {
                this.reread();
            }
            this.reread();
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;
            ProdTable prodTable;
            JmgTermJob_Prod  prodJob;

            // Check that production status allow edit of feedback quantities.
            prodTable = ProdTable::find(jmgStampJournalTrans.JobRef);

            itemId = prodTable.ItemId;
            isCWItem = PdsGlobal::pdsIsCWItem(itemId);

            if (prodTable && prodTable.status().isBefore(ProdStatus::ReportedFinished))
            {
                prodJob = JmgTermJob::construct(jmgStampJournalTrans.JobId);
                // Overview tab
                jmgStampJournalTrans_QtyStartup.allowEdit(JmgProdParametersDim::find(prodJob.siteInventDimId()).StartUpStatus == JmgProdStatusUpdate::Qty);
                jmgStampJournalTrans_QtyGood.allowEdit(true);
                qtyError.allowEdit(true);
                jmgStampJournalTrans_JobFinished.allowEdit(true);

                // General tab
                jmgStampJournalTrans_QtyStartup1.allowEdit(true);
                jmgStampJournalTrans_QtyGood1.allowEdit(true);
                qtyError1.allowEdit(true);

                jmgStampJournalTrans_PdsCWQtyStartup1.allowEdit(isCWItem && jmgStampJournalTrans_QtyStartup1.allowEdit());
                jmgStampJournalTrans_PdsCWQtyGood1.allowEdit(isCWItem && jmgStampJournalTrans_QtyGood1.allowEdit());
                pdsCWQtyError1.allowEdit(isCWItem && qtyError1.allowEdit());
            }
            else
            {
                // Overview tab
                jmgStampJournalTrans_QtyStartup.allowEdit(false);
                jmgStampJournalTrans_QtyGood.allowEdit(false);
                qtyError.allowEdit(false);
                jmgStampJournalTrans_JobFinished.allowEdit(false);

                // General tab
                jmgStampJournalTrans_QtyStartup1.allowEdit(false);
                jmgStampJournalTrans_QtyGood1.allowEdit(false);
                qtyError1.allowEdit(false);

                jmgStampJournalTrans_PdsCWQtyStartup1.allowEdit(false);
                jmgStampJournalTrans_PdsCWQtyGood1.allowEdit(false);
                pdsCWQtyError1.allowEdit(false);
            }

            ret = super();

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            JmgTmpJobStatus         jmgTmpJobStatus;
            JmgStampJournalTrans    stampJournalTrans;
            Object                  caller;
            super();

            if (element.args() && element.args().dataset() == tablenum(JmgTmpJobStatus))
            {
                jmgTmpJobStatus = element.args().record();
                this.query().dataSourceTable(tablenum(JmgStampJournalTrans)).addRange(fieldnum(JmgStampJournalTrans, JobId)).value(queryValue(jmgTmpJobStatus.JobId));
                this.query().dataSourceTable(tablenum(JmgStampJournalTrans)).addRange(fieldnum(JmgStampJournalTrans, JourRegType)).value(queryValue(JmgJourRegTypeEnum::MrpProcess));
            }
            else if (element.args() && element.args().dataset() == tablenum(JmgStampJournalTrans))
            {
                stampJournalTrans = element.args().record();
                caller = element.args().caller();
                this.query().dataSourceTable(tablenum(JmgStampJournalTrans)).addRange(fieldnum(JmgStampJournalTrans, RecId)).value(queryValue(stampJournalTrans.RecId));
            }
        }

]]></Source>
