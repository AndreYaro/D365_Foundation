				<Name>PayrollWorkCycle</Name>
				<Methods>
					<Method>
						<Name>linkActive</Name>
						<Source><![CDATA[
        public void linkActive()
        {
            TransDate nextStartDate;

            super();

            // Set the start date value if periods have been generated previously for this cycle
            nextStartDate = payrollWorkCycle.calculateNextStartDate();

            if (nextStartDate != dateNull())
            {
                dialogContent_StartDate.dateValue(nextStartDate);
                dialogContent_StartDate.enabled(false);
            }
        }

]]></Source>
					</Method>
				</Methods>
