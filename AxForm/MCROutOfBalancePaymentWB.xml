			<Control>
				<Name>Underpaid_Bill</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Submits the order and inserts a letter
        ///     into the event table.
        /// </summary>
        void clicked()
        {
            MCROrderEventTable mcrOrderEventTable;

            if (element.submitOrder(salesTable.data(), MCROutOfBalanceReleaseType::Bill))
            {
                salesTable_ds.refresh();
                salesTable_ds.reread();

                RetailEventNotificationAction::InsertRetailOENAction(
                            RetailEventNotificationType::MCRSalesOrderBill,
                            salesTable.RecId,
                            salesTable);

                mcrOrderEventTable.insertOOBEvent(MCROrderEventType::UnderpaymentRelease, salesTable, "@MCR25189");
            }

            salesTable_ds.executeQuery();
        }

]]></Source>
