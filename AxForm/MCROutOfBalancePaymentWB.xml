			<Control>
				<Name>Underpaid_WriteOff</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Submits the order and at invoicing
        ///     the under paid amount is written off.
        /// </summary>
        void clicked()
        {
            MCROrderEventTable mcrOrderEventTable;

            if (element.submitOrder(salesTable, MCROutOfBalanceReleaseType::WriteOff))
            {
                salesTable.reread();
                mcrOrderEventTable.insertOOBEvent(MCROrderEventType::UnderpaymentRelease, salesTable, "@MCR25187");
            }

            salesTable_ds.executeQuery();
        }

]]></Source>
					</Method>
