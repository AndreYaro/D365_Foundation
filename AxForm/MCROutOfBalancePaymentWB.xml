					<Method>
						<Name>displayDaysOnHold</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Determines the days on hold for the passed sales table record.
        /// </summary>
        /// <param name="_salesTable">
        /// The sales order record to find the number of dayes on hold.
        /// </param>
        /// <returns>
        /// The days on hold for the passed sales table record.
        /// </returns>
        display Integer displayDaysOnHold(SalesTable _salesTable)
        {
            date createdDate;

            createdDate = DateTimeUtil::date(MCRCustPaymTable::findByRefRecIDRefTableID(_salesTable.RecId, _salesTable.TableId).CreatedDateTime);

            return (DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone()) - createdDate);
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Forces the query to only retreive sales
        ///     orders that have under paid.
        /// </summary>
        public void init()
        {
            QueryBuildDataSource queryBuildDataSourceSalesTable;
            QueryBuildDataSource queryBuildDataSourceMCRSalesTable;

            super();

            queryBuildDataSourceSalesTable = this.query().dataSourceTable(tableNum(SalesTable));

            queryBuildDataSourceSalesTable.addSortField(fieldNum(SalesTable, CreatedDateTime), SortOrder::Ascending);
            queryBuildDataSourceSalesTable.addSortField(fieldNum(SalesTable, SalesId), SortOrder::Ascending);

            queryBuildDataSourceSalesTable.addRange(fieldNum(SalesTable, SalesStatus))
                            .value(enum2str(SalesStatus::Backorder));

            queryBuildDataSourceMCRSalesTable = queryBuildDataSourceSalesTable.addDataSource(tableNum(MCRSalesTable));
            queryBuildDataSourceMCRSalesTable.relations(true);
            queryBuildDataSourceMCRSalesTable.joinMode(JoinMode::InnerJoin);

            paymOutOfBalanceQueryRange = queryBuildDataSourceMCRSalesTable.addRange(fieldNum(MCRSalesTable, PaymOutOfBalance));

            this.cacheAddMethod(identifierStr(displayDaysOnHold));
            this.cacheAddMethod(identifierStr(displayAmount));
        }

]]></Source>
					</Method>
					<Method>
						<Name>displayAmount</Name>
						<Source><![CDATA[
        display AmountCur displayAmount(SalesTable _salesTable)
        {
            MCRCustPaymTable mcrCustPaymTable;

            select sum(Amount) from mcrCustPaymTable
                where mcrCustPaymTable.RefTableId == _salesTable.TableId
                   && mcrCustPaymTable.RefRecId == _salesTable.RecId;

            return mcrCustPaymTable.Amount;
        }

]]></Source>
