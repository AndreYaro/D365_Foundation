    /// <summary>
    ///     Loads the customer transaction form with the selected
    ///     sales order.
    /// </summary>
    /// <param name="_salesTable">
    /// The sales order to load.
    /// </param>
    void loadCustTransForm(SalesTable _salesTable)
    {
        Args args;
        FormRun loadTransTable;
        CustTable custTable;

        if (_salesTable)
        {
            custTable = CustTable::find(_salesTable.InvoiceAccount);
            args = new Args();
            args.record(custTable);

            loadTransTable = new MenuFunction(menuitemDisplayStr(CustTrans), MenuItemType::Display).create(args);
            loadTransTable.run();
            loadTransTable.wait();
        }
    }

