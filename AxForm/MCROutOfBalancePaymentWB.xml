			<Control>
				<Name>Overpaid_Release</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///     Submits the order and creates a refund.
        /// </summary>
        void clicked()
        {
            MCRPaymentAdjustments mcrPaymentAdjustments;
            MCROrderEventTable mcrOrderEventTable;
            MCRCustPaymTotals mcrCustPaymTotals;

            // Create refund checks
            mcrCustPaymTotals = MCRCustPaymTotals::construct(salesTable.TableId, salesTable.RecId);

            mcrPaymentAdjustments = MCRPaymentAdjustments::construct(mcrCustPaymTotals.getTotalPaymAmount(), salesTable);
            mcrPaymentAdjustments.processPaymentAdjustments();

            // Must process refund checks before running end order
            salesTable.reread();
            // End the order
            if (element.submitOrder(salesTable.data(), MCROutOfBalanceReleaseType::Refund))
            {
                mcrOrderEventTable.insertOOBEvent(MCROrderEventType::OverpaymentRelease, salesTable, "@MCR25213");
            }

            salesTable_ds.executeQuery();
        }

]]></Source>
					</Method>
