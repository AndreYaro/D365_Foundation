			<Method>
				<Name>submitOrder</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Runs the order submit process for the passed sales order.
    /// </summary>
    /// <param name="_salesTable">
    /// The sales order to submit.
    /// </param>
    /// <param name="_releaseType">
    ///  The release type to set on the order.
    /// </param>
    /// <returns>
    /// true when a sales order is submitted; otherwise, false.
    /// </returns>
    boolean submitOrder(SalesTable _salesTable, MCROutOfBalanceReleaseType _releaseType)
    {
        MCRSalesOrderTotals mcrSalesOrderTotals;
        MCRSalesTable       mcrSalesTable;

        if (_salesTable)
        {
            ttsbegin;
            mcrSalesTable = MCRSalesTable::find(_salesTable.RecId, true);
            mcrSalesTable.SalesTable = _salesTable.RecId;
            mcrSalesTable.PaymOutOfBalance = MCRPaymOutOfBalance::None;
            mcrSalesTable.OutOfBalanceReleased = _releaseType;
            mcrSalesTable.write();

            mcrSalesOrderTotals = new MCRSalesOrderTotals(_salesTable);

            MCREndOrder::endOrder(_salesTable.SalesId, false, mcrSalesOrderTotals);
            ttscommit;

            return true;
        }
        else
        {
            info("@MCR24645");
            return false;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>getOverallTotalOrders</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Returns the number of orders in the sales table
    ///     that have an out of balance status of the
    ///     passed value.
    /// </summary>
    /// <param name="_status">
    /// The status of the orders to count.
    /// </param>
    /// <returns>
    /// The number of orders in the sales table
    ///     that have an out of balance status of the
    ///     passed value.
    /// </returns>
    int64 getOverallTotalOrders(MCRPaymOutOfBalance _status)
    {
        SalesTable      salesTableLocal;
        MCRSalesTable   mcrSalesTable;

        select count(RecId) from salesTableLocal
            where salesTableLocal.SalesStatus == SalesStatus::Backorder
            exists join mcrSalesTable
            where mcrSalesTable.SalesTable == salesTableLocal.RecId
                && mcrSalesTable.PaymOutOfBalance == _status;

        return salesTableLocal.RecId;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setTotalDisplays</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Sets the over paid overall values
    ///     that are displayed on the form.
    /// </summary>
    void setTotalDisplays()
    {
        int64 totalNumOverpaidOrders    = element.getOverallTotalOrders(MCRPaymOutOfBalance::Overpaid);
        date oldestOverpaidDate         = SalesTable::mcrGetOldestOutOfBalanceDate(MCRPaymOutOfBalance::Overpaid);
        int64 totalNumUnderpaidOrders   = element.getOverallTotalOrders(MCRPaymOutOfBalance::Underpaid);
        date oldestUnderpaidDate        = SalesTable::mcrGetOldestOutOfBalanceDate(MCRPaymOutOfBalance::Underpaid);

        Overall_TotalOverpaidOrders.value(totalNumOverpaidOrders);
        Overall_TotalUnderpaidOrders.value(totalNumUnderpaidOrders);
        Overall_OldestOverpaidDate.dateValue(oldestOverpaidDate);
        Overall_OldestUnderpaidDate.dateValue(oldestUnderpaidDate);
    }

]]></Source>
