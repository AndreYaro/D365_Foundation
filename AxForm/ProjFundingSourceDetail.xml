			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        super();
        if (callerForm && formHasMethod(callerForm, identifierStr(setTmpProjGrantFunding)))
        {
            callerForm.setTmpProjGrantFunding(projFundingSource.DefaultDimension, tmpProjGrantFundingSource.FundingSourceId);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>getTmpProjGrantFundingSource</Name>
				<Source><![CDATA[
    public TmpProjGrantFundingSource getTmpProjGrantFundingSource()
    {
        return tmpProjGrantFundingSource;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {     
		// callerForm is a global variable used by other part of the form.
		callerForm = element.args().caller();
        
        str callerName;
        if (callerForm)
        {
            callerName = callerForm.name();
        }

        if (callerName == formStr(ProjInvoiceProposalDetail)
            || callerName == formStr(ProjInvoiceProposalDetailV2)
            || callerName == formStr(ProjTransPosting))
        {
            element.args().copyCallerQuery(CopyCallerQuery::No);
        }

        super();

        if (callerName == formStr(ProjGrantFundingDialog))
        {
            tmpProjGrantFundingSource = element.args().record();
        }
    }

]]></Source>
