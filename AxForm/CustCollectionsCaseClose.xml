			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        CustCollectionsLinkHandler handler = CustCollectionsLinkHandler::construct();

        context.parmHandleDisputedTransactions(resolveDisputes.checked());
        handler.parmContext(context);
        handler.parmCaller(caller);

        if (caller is CustCollectionsICloseCaseParmCompany)
        {
            CustCollectionsICloseCaseParmCompany callerForm = caller as CustCollectionsICloseCaseParmCompany;

            changecompany(callerForm.parmCompany())
            {
                handler.run();
            }
        }
        else
        {
            handler.run();
        }

        super();

        caller.refreshCases();
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        caller = element.args().caller();

        if (caller && caller is FormRun && formHasMethod(caller, identifierStr(getLinkContext)))
        {
            context = caller.getLinkContext(CustCollectionsLinkAction::CloseCase);
        }
        else
        {
            throw error(Error::wrongUseOfFunction(funcname()));
        }

        closeDate.dateValue(DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone()));
        resolveDisputes.checked(true);
    }

]]></Source>
