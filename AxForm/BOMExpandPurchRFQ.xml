					<Method>
						<Name>findBOMLines</Name>
						<Source><![CDATA[
        void findBOMLines()
        {
            container   conBOM;
            int         numBOMLines,i;

            tmpFrmVirtual = null;

            tmpBOM = null;
            tmpBOM.setTmp();

            if (callerDataSource.allowEdit() && ! purchRFQCaseLine.receivedInventInTotal() && ! purchRFQCaseLine.registered())
            {
                conBOM = BOMSearch::bomTreePackLevelConfig(callerPurchRFQCaseLine.inventTable().bomId(callerPurchRFQCaseLine.deliveryDate(),
                                                                                                      callerPurchRFQCaseLine.QtyOrdered,
                                                                                                      callerPurchRFQCaseLine.inventDim()),
                                                           callerPurchRFQCaseLine.deliveryDate(),
                                                           callerPurchRFQCaseLine.ItemId,
                                                           callerPurchRFQCaseLine.inventDim().ConfigId,
                                                           true,
                                                           true);

                numBOMLines = conlen(conBOM);

                for (i=1; i<=numBOMLines; i++)
                {
                    tmpBOM.data(conpeek(conBOM,i));
                    tmpBOM.doInsert();
                }
            }

            bom.setTmpData(tmpBOM);
        }

]]></Source>
