					<Method>
						<Name>linkActive</Name>
						<Source><![CDATA[
        public void linkActive()
        {
            element.setCaller();
            queryRecId.value(strfmt('%1',callerPurchRFQCaseLine.RecId));

            element.updateDesign(InventDimFormDesignUpdate::LinkActive);

            super();

            bom_ds.allowEdit(callerDataSource.allowEdit());
            bom_ds.findBOMLines();
            bom_ds.executeQuery();
        }

]]></Source>
