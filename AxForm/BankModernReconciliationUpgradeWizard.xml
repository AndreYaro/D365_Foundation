		<DataSources xmlns="">
			<DataSource>
				<Name>CompanyInfo</Name>
				<Methods>
					<Method>
						<Name>dispDataAreaWizardStatus</Name>
						<Source><![CDATA[
        internal display str dispDataAreaWizardStatus(CompanyInfo _companyInfo)
        {
            BankParameters bankParameters;
            select firstonly crosscompany BankModernReconciliationUpgradeWizardExecutionStatus from bankParameters
                where bankParameters.DataAreaId == _companyInfo.DataArea;
            return enum2Str(bankParameters.BankModernReconciliationUpgradeWizardExecutionStatus);
        }

]]></Source>
					</Method>
					<Method>
						<Name>editCheckboxCheck</Name>
						<Source><![CDATA[
        internal edit boolean editCheckboxCheck(boolean _set, CompanyInfo _companyInfo, boolean _checked)
        {
            if (_set)
            {
                if (_checked)
                {
                    dataAreaSet.add(_companyInfo.DataArea);
                }
                else
                {
                    dataAreaSet.remove(_companyInfo.DataArea);
                }
            }
            return dataAreaSet.in(_companyInfo.DataArea);
        }

]]></Source>
					</Method>
					<Method>
						<Name>dispExecutionStatus</Name>
						<Source><![CDATA[
        internal display str dispExecutionStatus(CompanyInfo _companyInfo)
        {
            str statusDescription;
            if (dataAreaSet.in(_companyInfo.DataArea))
            {
                statusDescription = "@Bank:WillExecuteDataUpgrade";
            }
            else
            {
                statusDescription = "@Bank:NoNeedToExecuteDataUpgrade";
            }
            return statusDescription;
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            Query q = this.query();
            QueryBuildDataSource qbds = q.dataSourceTable(tableNum(CompanyInfo));
            QueryBuildDataSource qbdsBankParameters = qbds.addDataSource(tableNum(BankParameters));
            qbdsBankParameters.joinMode(JoinMode::InnerJoin);
            qbdsBankParameters.relations(true);
            qbdsBankParameters.addLink(fieldNum(CompanyInfo, DataArea), fieldNum(BankParameters, DataAreaId));
            qbdsBankParameters.addOrderByField(fieldNum(BankParameters, BankModernReconciliationUpgradeWizardExecutionStatus));
            super();
        }

]]></Source>
					</Method>
