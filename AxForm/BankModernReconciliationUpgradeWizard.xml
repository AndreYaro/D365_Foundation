				<Methods>
					<Method>
						<Name>jumpRef</Name>
						<Source><![CDATA[
        public void jumpRef()
        {
            super();
            changecompany(InfluencedbankStatementTmp.RefLegalEntity)
            {
                BankAccountTable bankAccountTable = BankAccountTable::findByRecId(InfluencedbankStatementTmp.RefRecId);
                if (bankAccountTable)
                {
                    Args args = new Args();
                    args.record(bankAccountTable);
                    BankAutomationStatementFormMenuItemController::main(args);
                }
            }
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>MatchingRuleID</Name>
				<Type>String</Type>
				<Methods>
					<Method>
						<Name>jumpRef</Name>
						<Source><![CDATA[
        public void jumpRef()
        {
            super();
            BankReconciliationMatchRule matchingRule = BankReconciliationMatchRule::find(InfluencedMatchingRuleAndRuleSet.RefRecId);
            if (matchingRule)
            {
                Args args = new Args();
                args.record(matchingRule);
                args.lookupRecord(matchingRule);
                new MenuFunction(menuitemDisplayStr(BankReconciliationMatchRule), MenuItemType::Display).run(args);
            }
        }

]]></Source>
					</Method>
