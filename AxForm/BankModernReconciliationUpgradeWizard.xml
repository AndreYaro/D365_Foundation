				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class BankModernReconciliationUpgradeWizard extends FormRun
{
    private BankModernReconciliationUpgradeWizard sysWizard;
    private BankModernReconciliationUpgradeWizardGridTmp bankStatementInfoCollectTmp;
    private BankModernReconciliationUpgradeWizardGridTmp matchingRuleInfoCollectTmp;
    private BankModernReconciliationUpgradeWizardController wizardController;
    private BankModernReconciliationUpgradeWizardContract wizardContract;
    private BankModernReconciliationUpgradeWizardDataSourceInitializer dataSourceInitializer;
    private Set dataAreaSet;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (element.args().caller())
        {
            sysWizard = element.args().caller() as BankModernReconciliationUpgradeWizard;
        }
        else
        {
            throw error(Error::wrongUseOfFunction(funcName()));
        }

        if (BankModernReconciliationUpgradeWizardTrigger::isClassBatchRunning(classNum(BankReconciliationMatchRuleAutoBatch))
            || BankModernReconciliationUpgradeWizardTrigger::isClassBatchRunning(classNum(BankReconciliationHeaderReconcileInBatch)))
        {
            warning(strFmt("@Bank:BankModernReconciliationBatchIsExecutingWhenWizardInit", "@Bank:BankReconciliationMatchingRuleCaption", "@CashManagement:MarkAsReconciledInBatch"));
        }

        dataAreaSet = new Set(Types::String);
        dataSourceInitializer = BankModernReconciliationUpgradeWizardDataSourceInitializer::construct(this);
        dataSourceInitializer.loadDataSources();
        this.linkTmpDataSources();
        this.setVisibilityOfNoDataText();
        wizardController = BankModernReconciliationUpgradeWizardController::construct();
        wizardContract = wizardController.getContract();
    }

]]></Source>
			</Method>
			<Method>
				<Name>wizard</Name>
				<Source><![CDATA[
    private SysWizard wizard()
    {
        return sysWizard;
    }

]]></Source>
			</Method>
			<Method>
				<Name>getDataAreaSet</Name>
				<Source><![CDATA[
    internal Set getDataAreaSet()
    {
        return dataAreaSet;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateFinish</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates the wizard data upgrade when about to finish.
    /// </summary>
    /// <returns>true if the wizard data upgrade is valid; otherwise, false.</returns>
    internal boolean validateFinish()
    {
        boolean ret = true;
        if (!this.validateUpgradeAfterDate())
        {
            warning(strFmt("@SYS116347", "@SYS24050"));
            ret = false;
        }
        if (ret && dataAreaSet.empty())
        {
            warning("@Bank:BankModernReconciliationUpgradeWizardNoDataAreaSelectedForUpgrade");
            ret = false;
        }
        if (ret && BankModernReconciliationUpgradeWizardTrigger::isClassBatchRunning(classNum(BankModernReconciliationUpgradeWizardController)))
        {
            warning("@Bank:BankModernReconciliationUpgradeWizardIsExecutingInBatchInfo");
            ret = false;
        }
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateUpgradeAfterDate</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates the upgrade after date field.
    /// </summary>
    /// <returns>true if the upgrade after date is valid; otherwise, false.</returns>
    internal boolean validateUpgradeAfterDate()
   {
        boolean ret;
        if (GenerateMatchedTransFromDate.dateValue() != dateNull())
        {
            ret = true;
        }
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>processUpgrade</Name>
				<Source><![CDATA[
    /// <summary>
    /// Processes the wizard data upgrade.
    /// </summary>
    internal void processUpgrade()
    {
        wizardContract.parmDataAreaToExecuteSet(dataAreaSet);
        wizardContract.parmUpgradeAfterDate(GenerateMatchedTransFromDate.dateValue());
        wizardContract.parmNeedsToBeSplitIntoTasks(true);
        wizardController.run();
    }

]]></Source>
			</Method>
			<Method>
				<Name>addDataAreaForUpgradeMatchedTransactions</Name>
				<Source><![CDATA[
    /// <summary>
    /// Adds data area for upgrade matched transactions.
    /// </summary>
    internal void addDataAreaForUpgradeMatchedTransactions()
    {
        dataSourceInitializer.loadDataSourcesForUpgradeMatchedTransactions(GenerateMatchedTransFromDate.dateValue());
    }

]]></Source>
			</Method>
			<Method>
				<Name>linkTmpDataSources</Name>
				<Source><![CDATA[
    private void linkTmpDataSources()
    {
        bankStatementInfoCollectTmp = dataSourceInitializer.parmBankStatementInfoCollectTmp();
        matchingRuleInfoCollectTmp = dataSourceInitializer.parmMatchingRuleInfoCollectTmp();
        InfluencedbankStatementTmp.linkPhysicalTableInstance(bankStatementInfoCollectTmp);
        InfluencedMatchingRuleAndRuleSet.linkPhysicalTableInstance(matchingRuleInfoCollectTmp);
    }

]]></Source>
			</Method>
			<Method>
				<Name>setVisibilityOfNoDataText</Name>
				<Source><![CDATA[
    private void setVisibilityOfNoDataText()
    {
        select count(RefRecId) from bankStatementInfoCollectTmp;
        if (bankStatementInfoCollectTmp.RefRecId == 0)
        {
            UpgradeMarkAsNewGrid.heightMode(FormHeight::SizeToContent);
            UpgradeMarkAsNewNoDataText.visible(true);
        }
        select count(RefRecId) from matchingRuleInfoCollectTmp;
        if (matchingRuleInfoCollectTmp.RefRecId == 0)
        {
            UpgradeMatchingRuleGrid.heightMode(FormHeight::SizeToContent);
            UpgradeMatchingRuleNoDataText.visible(true);
        }
    }

]]></Source>
			</Method>
