			<Method>
				<Name>setChartsOfAccounts</Name>
				<Source><![CDATA[
    public void setChartsOfAccounts()
    {
        RefRecId chartOfAccounts = Ledger::chartOfAccounts();
        RefRecId ledgerTransRuleChartOfAccounts = Ledger::chartOfAccounts(CompanyInfo::findDataArea(ledgerTransRule.ToCompanyId).RecId);

        DefaultAccount.parmCurrentLedgerCOA(chartOfAccounts);
        DefaultAccountOffset.parmCurrentLedgerCOA(chartOfAccounts);
        DefaultAccount_general.parmCurrentLedgerCOA(chartOfAccounts);
        DefaultAccountOffset_general.parmCurrentLedgerCOA(chartOfAccounts);

        ToDefaultAccount.parmCurrentLedgerCOA(ledgerTransRuleChartOfAccounts);
        ToDefaultAccount_general.parmCurrentLedgerCOA(ledgerTransRuleChartOfAccounts);
    }

]]></Source>
