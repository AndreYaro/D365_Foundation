				<Source><![CDATA[
    public void close()
    {
        super();

        if (ruleForm)
        {
            ruleForm.ruleGroupIdValue(stringRuleGroupId.text());
            xSysLastValue::saveLast(ruleForm);
        }
        super();
    }

]]></Source>
