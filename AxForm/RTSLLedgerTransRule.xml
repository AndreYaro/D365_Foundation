			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (! element.args().caller())
        {
            ruleForm = new RTSLRuleForm(this);
            stringRuleGroupId.text(ruleForm.ruleGroupIdValue());
        }
        else
        {
            stringRuleGroupId.visible(false);
        }
    }

]]></Source>
