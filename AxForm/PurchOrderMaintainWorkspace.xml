				<Source><![CDATA[
[Form]
public class PurchOrderMaintainWorkspace extends FormRun implements SysIFilterProvider
{
    SysFilterChangeEvent changeEvent;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        SysFilterLastValueHelper::getLastValue(BuyerGroupIdFilter);
        changeEvent = SysFilterChangeEvent::newFromFormControl(BuyerGroupIdFilter);

        PurchParameters purchParameters = PurchParameters::find();
        boolean changeManagementEnabled = (purchParameters.ChangeRequestEnabled || purchParameters.ChangeRequestAllowOverride);

        AssignedToMeTab.visible(changeManagementEnabled);
        AssignedToMeTile.visible(changeManagementEnabled);
        InDraftTab.visible(changeManagementEnabled);
        InDraftTile.visible(changeManagementEnabled);
        InReviewTab.visible(changeManagementEnabled);
        InReviewTile.visible(changeManagementEnabled);

        if (PurchTableChangeReviewWorkspaceWhatsNewFlight::instance().isEnabled())
        {
            SystemNotificationsWhatsNewManager::AddWhatsNewWithActionLink(
                    '{98A66E64-DE72-43CB-886D-656FC088C30F}',
                    "@ProcurementAndSourcing:PurchTableChangeReviewWorkspaceWhatsNewTitle",
                    "@ProcurementAndSourcing:PurchTableChangeReviewWorkspaceWhatsNewDescription",
                    WorkspaceActions.name(),
                    'https://go.microsoft.com/fwlink/?linkid=2238906');
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        super();

        SysFilterLastValueHelper::saveLastValue(BuyerGroupIdFilter);
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmFilter</Name>
				<Source><![CDATA[
    public SysIFilter parmFilter()
    {
        SysIFilter filter       = SysFilterFactory::createEmptyFilter();
        str        buyerGroupId = BuyerGroupIdFilter.valueStr();
        UserInfo   currentUser;

        if (buyerGroupId != '')
        {
            SysIFilterValue filterValue = SysFilterValueFactory::createFilterValueForString(buyerGroupId, extendedTypeNum(ItemBuyerGroupId));
            filter.addItem(filterValue, SysFilterOperator::Equal, SysFilterValueResolutionMethod::None);
        }

        SysIFilterValue currentUserFilter = SysFilterValueFactory::createFilterValueForString(curUserId(), extendedTypeNum(WorkflowUser));
        filter.addItem(currentUserFilter, SysFilterOperator::Equal, SysFilterValueResolutionMethod::None);
        return filter;
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmChangeEvent</Name>
				<Source><![CDATA[
    public SysFilterChangeEvent parmChangeEvent()
    {
        return changeEvent;
    }

]]></Source>
