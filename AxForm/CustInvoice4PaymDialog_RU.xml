						<Source><![CDATA[
        /// <summary>
        /// Mark journal from CustInvoice4PaymJour_RU.
        /// </summary>
        /// <param name = "_set"></param>
        /// A Boolean value that indicates whether to set.
        /// <param name = "_invoice4PaymJour"></param>
        /// An instance of CustInvoice4PaymJour_RU.
        /// <param name = "_markTrans"></param>
        /// A Boolean value that indicates whether to mark journal.
        /// <returns>
        /// The _invoice4PaymJour have been marked or not.
        /// </returns>
        edit NoYes markJour(boolean                 _set,
                            CustInvoice4PaymJour_RU _invoice4PaymJour,
                            NoYes                   _markTrans)
        {
            NoYes res;
            if (_set)
            {
                if (_markTrans == NoYes::Yes)
                {
                    markedJour.add(_invoice4PaymJour.RecId);
                    res = true;
                }
                else
                {
                    if (markedJour.in(_invoice4PaymJour.RecId))
                    {
                        markedJour.remove(_invoice4PaymJour.RecId);
                    }
                    res = false;
                }
            }
            else
            {
                res = markedJour.in(_invoice4PaymJour.RecId);
            }

            return res;
        }

]]></Source>
