    /// <summary>
    /// Performs the mapping of form controls to the <c>budgetReservationYearEndClose_PSN</c> class controls variables.
    /// </summary>
    private void mapFormControlsOnFormHelperClass()
    {
        budgetReservationYearEndClose.parmBudgetReservYearEndCloseForm(element);

        // Group GroupYearEndOptions controls
        budgetReservationYearEndClose.parmYearEndOptionControl(budgetReservationParmTmpPeriodClose_PSN_YearEndOption);
        budgetReservationYearEndClose.parmOriginalBudgetTransactionCode(budgetReservationParmTmpPeriodClose_PSN_OriginalBudgetTransactionCode);
        budgetReservationYearEndClose.parmCarryForwardBudgetTransactionCode(budgetReservationParmTmpPeriodClose_PSN_CarryForwardBudgetTransactionCode);

        // Group GroupPeriodOptions controls
        // Sub group GroupCalendar controls
        budgetReservationYearEndClose.parmFiscalCalendarIdControl(fiscalCalendar_CalendarId);
        budgetReservationYearEndClose.parmSelectFiscalCalendarYear(selectFiscalCalendarYear);

        // Sub GroupClosingParameters controls
        budgetReservationYearEndClose.parmClosingPostingDate(closingPostingDate);
        budgetReservationYearEndClose.parmPeriodCode(PeriodCode);
        budgetReservationYearEndClose.parmClosingFiscalCalendarPeriod(closingFiscalCalendarPeriod);

        // Sub GroupOpenParameters controls
        budgetReservationYearEndClose.parmOpeningPostingDate(openingPostingDate);
        budgetReservationYearEndClose.parmOpeningFiscalCalendarPeriod(openingFiscalCalendarPeriod);

        // Group GroupRetrieveBudgetRes controls
        // ActionPaneButtonGroup controls
        budgetReservationYearEndClose.parmButtonRetrieveBudgetReservation(btnRetrieveBudgetReservation);
        budgetReservationYearEndClose.parmButtonIncludeAll(btnIncludeAll);
        budgetReservationYearEndClose.parmButtonExcludeAll(btnExcludeAll);
        budgetReservationYearEndClose.parmButtonViewSubledgerJournal(btnViewSubledgerJournal);
        budgetReservationYearEndClose.parmButtonViewBudgetReservation(btnViewBudgetReservations);

        // Grid control
        budgetReservationYearEndClose.parmGridRetrieveBudgetReservations(grdRetrieveBudgetReservation);

        // ButtonGroup controls
        budgetReservationYearEndClose.parmButtonProcess(btnProcess);
    }

