			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    public boolean canClose()
    {
        boolean canClose;
        str message = budgetReservationYearEndClose.parmIsBudgetOnly() ? "@SYS321696" : "@Budgeting_BudgetReservationPS:BudgetReservationYECreateClosingOpeningEntriesMessage";

        canClose = super();

        if (canClose && dialog.closedOk())
        {
            budgetReservationYearEndClose.setContractParmFromFormControl();

            budgetReservationParmTmpPeriodClose_PSN_ds.research(true);
            budgetReservationTmpPeriodSelection_PSN_ds.research(true);

            canClose = budgetReservationYearEndClose.validateProcessParameters()
                && (Box::okCancel(message, DialogButton::Ok, "@SYS304470") == DialogButton::Ok);
        }

        return canClose;
    }

]]></Source>
