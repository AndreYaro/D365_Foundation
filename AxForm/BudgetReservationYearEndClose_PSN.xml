    /// <summary>
    /// Initializes the <c>budgetReservationYearEndClose_PSN</c> class.
    /// </summary>
    private void initFormHelper()
    {
        // Initializing form helper class
        budgetReservationYearEndClose = BudgetReservationYearEndClose_PSN::construct();
        budgetReservationYearEndClose.parmbudgetReservationYearEndCloseContract(budgetReservationYearEndCloseContract);

        // Mapping form controls with form helper class controls
        element.mapFormControlsOnFormHelperClass();

        // Show hide controls
        budgetReservationYearEndClose.visibleControls();

        // Setting fiscal calendar
        element.setFiscalCalendarRecId();

        // Set temp tables in form helper class
        budgetReservationYearEndClose.parmBudgetReservParmTmpPeriodClose(budgetReservationParmTmpPeriodClose_PSN);
        budgetReservationYearEndClose.parmBudgetReservTmpPeriodSelection(budgetReservationTmpPeriodSelection_PSN);

        // Initialize temp tables
        budgetReservationYearEndClose.initBudgetReservationParmTmpPeriodClose();
        budgetReservationYearEndClose.deleteBudgetReservationTmpPeriodSel();
    }

]]></Source>
