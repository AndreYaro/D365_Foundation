				<Source><![CDATA[
    public void init()
    {
        if (!element.args().caller())
        {
            throw error(Error::missingParameter(this));
        }

        super();

        dialog = element.args().caller();
        budgetReservationYearEndCloseController = dialog.controller();
        budgetReservationYearEndCloseContract = budgetReservationYearEndCloseController.getDataContractObject();

        element.initFormHelper();
    }

]]></Source>
