			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        isCurrentUserProcurementAgent = xUserInfo::doesCurrentUserHaveProcurementAgentRights();

        super();

        if (isCurrentUserProcurementAgent)
        {
            if (this.args().record().RecId == 0)
            {
                FormComboBoxControl categoryRequestFilter = element.control(element.controlId("VendCategoryRequestsFilter")) as FormComboBoxControl;
                categoryRequestFilter.text(enum2Str(VendVendorRequestStatus::PendingApproval));

                QueryBuildRange qbr = SysQuery::findOrCreateRange(VendVendorRequestNewCategory_ds.query().dataSourceTable(tableNum(VendVendorRequestNewCategory)), fieldNum(VendVendorRequestNewCategory, RequestStatus));
                qbr.value(SysQuery::value(VendVendorRequestStatus::PendingApproval));
            }

            CategoryRequestDropDialogButtonWorkflow.text("@SYS103643");
        }

        FormControl workflowButton = element.control(element.controlId(identifierStr(WorkflowActionBarButtonGroup)));
        workflowButton.visible(false);

        vendorRequestNewCategoryManager = VendVendorRequestNewCategoryManager::construct();
        vendorRequestNewCategoryManager.init(isCurrentUserProcurementAgent, element, VendVendorRequestNewCategory, VendRequestNewCategory, callerIsWorkspace);

    }

]]></Source>
