	<Name>EximEPCGCreateExtension_IN</Name>
	<SourceCode>
		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
    [Form]
    public class EximEPCGCreateExtension_IN extends FormRun
    {
        EximEPCGUpd_Extension_IN epcgExtension;
}
]]></Source>
			</Method>
			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    public boolean canClose()
    {
        boolean        ret;
        FormDataSource callerDS;

        ret = super();

        if (ret && this.closedOk())
        {
            ret = element.validateFields();

            if (ret)
            {
                epcgExtension.parmCertificateNumber(certificateNum.valueStr());
                epcgExtension.parmTransDate(certificateDate.dateTimeValue());
                epcgExtension.parmExtension(extension.selection());
                epcgExtension.parmPeriodInterval(periodInterval.selection());
                epcgExtension.parmValue(editValue.value());
                epcgExtension.parmPostingStatus(true);

                epcgExtension.blockExtension();

                callerDS = epcgExtension.parmFormDataSource();

                if (callerDS)
                {
                    callerDS.executeQuery();
                }
            }
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        Object caller;
        ;

        super();

        caller = element.args().caller();

        if (!element.args()
            || !caller
            || !(caller is EximEPCGUpd_Extension_IN))
        {
            throw error("@SYS22539");
        }

        epcgExtension = caller;

        editValue.value(1);
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateFields</Name>
				<Source><![CDATA[
    public boolean validateFields()
    {
        boolean ret = true;
        ;
        if (!certificateNum.valueStr())
        {
            ret = checkFailed(strFmt("@SYS26332", "@GLS5355"));
        }

        if (!certificateDate.dateTimeValue())
        {
            ret = checkFailed(strFmt("@SYS26332", "@GLS62283"));
        }

        return ret;
    }

]]></Source>
			</Method>
		</Methods>
