						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            boolean ret;

            ret = super();

            if (ret && editValue.value() == 0)
            {
                editValue.value(1);
            }

            return ret;
        }

]]></Source>
