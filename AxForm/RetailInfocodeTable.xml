			<Method>
				<Name>enableRetailUseInfocode</Name>
				<Source><![CDATA[
    /// <summary>
    /// Enables or disables <c>RetailUseInfocode</c> field.
    /// When <c>inputType</c> is <c>RetailInfocodeInputType::SubCode</c>, then <c>RetailUseInfocode</c> is enabled; otherwise is disabled.
    /// </summary>
    public void enableRetailUseInfocode()
    {
        boolean enabled;

        enabled = (RetailInfocodeTable.inputType == RetailInfocodeInputType::SubCode);

        RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, RetailUseInfocode)).enabled(enabled);
    }

]]></Source>
