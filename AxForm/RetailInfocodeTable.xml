			<Method>
				<Name>enableTransactionActivityTypeInputFields</Name>
				<Source><![CDATA[
    /// <summary>
    /// Enables or disabled values fields based on input type.
    /// </summary>
    /// <param name = "_infocodeInputType">
    /// The info code input type.
    /// </param>
    private void enableTransactionActivityTypeInputFields(RetailInfocodeInputType _infocodeInputType)
    {
        RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, isMultiLineText)).enabled(false);
        RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, isMultiLineText)).visible(false);

        switch (_infocodeInputType)
        {
            case RetailInfocodeInputType::Text:
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, minimumLength)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, maximumLength)).enabled(true);

                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, isMultiLineText)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, isMultiLineText)).visible(true);
                break;
            case RetailInfocodeInputType::AgeLimit:
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, minimumValue)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, maximumValue)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, minimumLength)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, maximumLength)).enabled(false);
                break;
            case RetailInfocodeInputType::Numeric:
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, minimumValue)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, maximumValue)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, minimumLength)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, maximumLength)).enabled(false);
                break;
            default:
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, minimumValue)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, maximumValue)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, minimumLength)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, maximumLength)).enabled(false);
                break;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>setupDefaultValuesFields</Name>
				<Source><![CDATA[
    /// <summary>
    /// Setup default values based on input type.
    /// </summary>
    private void setupDefaultValuesFields()
    {
        switch (RetailInfocodeTable.inputType)
        {
			// Set default values for numeric type
            case RetailInfocodeInputType::Numeric:
                RetailInfocodeTable.minimumLength = 1;
                RetailInfocodeTable.maximumLength = 10;
                RetailInfocodeTable.minimumValue = 0;
                RetailInfocodeTable.maximumValue = intMax();
                break;
            // Set default values for AgeLimit type
            case RetailInfocodeInputType::AgeLimit:
                RetailInfocodeTable.minimumLength = 0;
                RetailInfocodeTable.maximumLength = 0;
                RetailInfocodeTable.minimumValue = 0;
                RetailInfocodeTable.maximumValue = intMax();
                break;
            default:
                RetailInfocodeTable.minimumLength = 0;
                RetailInfocodeTable.maximumLength = 0;
                RetailInfocodeTable.minimumValue = 0;
                RetailInfocodeTable.maximumValue = 0;
                break;
        }

        RetailInfocodeTable_DS.refresh();
    }

]]></Source>
