			<Method>
				<Name>enableValuesFields</Name>
				<Source><![CDATA[
    /// <summary>
    /// Enables or disabled values fields based on input type.
    /// </summary>
    private void enableValuesFields()
    {
        switch (RetailInfocode.Activity)        
        {
            case RetailInfocodeActivityType::Transaction:
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, inputRequired)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, oncePerTransaction)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, linkedInfocodeId)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, randomFactor)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, RetailUseInfocode)).enabled(true);
                
                this.enableTransactionActivityTypeInputFields(RetailInfocodeTable.inputType);

                // Printing
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, printPromptOnReceipt)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, printInputOnReceipt)).enabled(true);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, printInputNameOnReceipt)).enabled(true);
                break;
            case RetailInfocodeActivityType::OrderFulfillment:
                this.setupDefaultValuesFields();

                RetailInfocodeTable.inputRequired = false;
                RetailInfocodeTable.oncePerTransaction = false;
                RetailInfocodeTable.linkedInfocodeId = null;
                RetailInfocodeTable.randomFactor = 100;
                RetailInfocodeTable.RetailUseInfocode = false;
                RetailInfocodeTable.minimumLength = 0;
                RetailInfocodeTable.maximumLength = 0;
                RetailInfocodeTable.minimumValue = 0;
                RetailInfocodeTable.maximumValue = 0;

                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, inputRequired)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, oncePerTransaction)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, linkedInfocodeId)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, randomFactor)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, RetailUseInfocode)).enabled(false);

                // Numeric type
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, minimumLength)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, maximumLength)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, minimumValue)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, maximumValue)).enabled(false);

                // Printing
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, printPromptOnReceipt)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, printInputOnReceipt)).enabled(false);
                RetailInfocodeTable_ds.object(fieldNum(RetailInfocodeTable, printInputNameOnReceipt)).enabled(false);
                break;
        }
    }

]]></Source>
