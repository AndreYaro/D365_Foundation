			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (element.args() &&
                element.args().parm())
        {
            // A filter to a specific infocode record when the infocodeId is  provided...
            str infocodeid = element.args().parm();

            RetailInfocode_DS.query()
                .dataSourceTable(tableNum(RetailInfocode))
                .addRange(fieldNum(RetailInfocode, infocodeid))
                .value(queryValue(infocodeid));

            this.enableValuesFields();
        }
        if (RetailInfocodeTableUseInfocodeFieldVisibleFlight::instance().isEnabled())
        {
            Handheld_RetailUseInfocode.visible(true);
        }
    }

]]></Source>
