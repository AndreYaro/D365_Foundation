					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            Query                   query;
            QueryBuildDataSource    qbs_Origin;
            QueryBuildDataSource    qbs_Trans;

            super();

            query   = new Query();
            qbs_Origin     = query.addDataSource(tableNum(InventTransOriginHistory));
            qbs_Origin.name(InventTransOriginHistory_ds.name());

            qbs_Trans = qbs_Origin.addDataSource(tableNum(InventTrans));
            qbs_Trans.name(inventTrans_ds.name());
            qbs_Trans.joinMode(JoinMode::InnerJoin);
            qbs_Trans.fetchMode(QueryFetchMode::One2One);
            qbs_Trans.relations(true);

            qbs_Origin.addGroupByField(fieldNum(InventTransOriginHistory,InventTransId));
            qbs_Origin.addGroupByField(fieldNum(InventTransOriginHistory,ReferenceCategory));
            qbs_Origin.addGroupByField(fieldNum(InventTransOriginHistory,ReferenceId));
            qbs_Trans.addGroupByField(fieldNum(InventTrans,StatusReceipt));
            qbs_Trans.addGroupByField(fieldNum(InventTrans,StatusIssue));

            this.query(query);
        }

]]></Source>
