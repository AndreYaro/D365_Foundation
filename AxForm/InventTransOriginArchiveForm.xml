			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        super();

        if (  (element.args().callerName() == formstr(InventTransOriginArchiveTransFormPart)
            && element.args().record() is InventTransOriginArchiveTrans))
        {
            InventTransOriginArchiveTrans archiveTrans = element.args().record();
            var archiveJobSourceLink = ArchiveServiceJobDetailsView::findForSourceLink(archiveTrans);

            QueryBuildDataSource qbds = inventTransOriginHistory_ds.query().dataSourceTable(tableNum(InventTransOriginHistory));
            qbds.addRange(fieldNum(InventJournalTableHistory, ArchiveServiceJobId)).value(SysQuery::value(archiveJobSourceLink.ArchiveServiceJobId));
        }

        element.selectMode(inventTransOrigin_InventTransId);
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
