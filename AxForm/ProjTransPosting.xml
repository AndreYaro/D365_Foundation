            return projTransPosting.AmountMst;
        }

]]></Source>
					</Method>
					<Method>
						<Name>linkActive</Name>
						<Source><![CDATA[
        public void linkActive()
        {
            if (formHasMethod(element.args().caller(), identifierStr(projStatistic)))
            {
                projStatistic   = element.args().caller().projStatistic();
                if (projStatistic)
                {
                    included.visible(true);
                }
            }

            super();
        }

]]></Source>
					</Method>
					<Method>
						<Name>included</Name>
						<Source><![CDATA[
        display boolean included(ProjTransPosting   _projTransPosting)
        {
            if (! projStatistic)
            {
                return true;
            }

            if (_projTransPosting.LedgerTransDate < projStatistic.parmLedgerFromDate())
            {
                return false;
            }

            if (_projTransPosting.LedgerTransDate > projStatistic.parmLedgerToDate())
            {
                return false;
            }

            return true;
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            GeneralJournalAccountEntry              generalJournalAccountEntry;
            Voucher                                 voucher;
            TmpProjControlActual                    tmpProjControlActual;

            super();

            if (element.args().dataset() == tableNum(GeneralJournalAccountEntry))
            {
                generalJournalAccountEntry = element.args().record();
                voucher = SubledgerVoucherGeneralJournalEntry::findByGeneralJournalEntryRecId(generalJournalAccountEntry.GeneralJournalEntry).Voucher;
                this.query().dataSourceNo(1).addRange(fieldnum(ProjTransPosting,Voucher)).value(voucher);
            }

            this.query().dataSourceNo(1).addRange(fieldNum(ProjTransPosting,PostingType)).value(strFmt('!%1,!%2',LedgerPostingType::SalesIssue,LedgerPostingType::InventIssue));
            this.query().dataSourceNo(1).addSortField(fieldNum(ProjTransPosting, LedgerTransDate));
            this.query().dataSourceNo(1).addSortField(fieldNum(ProjTransPosting, Voucher));

            if (element.args().dataset() == tableNum(TmpProjControlActual))
            {
                tmpProjControlActual = element.args().record();
                this.query().dataSourceNo(1).addRange(fieldnum(ProjTransPosting,TransId)).value(tmpProjControlActual.TransId);
            }
        }

]]></Source>
