			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (element.args().dataset() == tableNum(GeneralJournalAccountEntry))
        {
            projTransPosting_Voucher.visible(false);
            projTransPosting_LedgerTransDate.visible(false);
            projTransPosting_TransId.visible(true);
            projTransPosting_ProjId.visible(true);
        }
        else
        {
            projTransPosting_Voucher.visible(true);
            projTransPosting_LedgerTransDate.visible(true);
            projTransPosting_TransId.visible(false);
            projTransPosting_ProjId.visible(false);
        }
        
        ProjMutilContractLinesFeatureFormController::updateFormControlVisibity(element, [Transaction_ProjType.id()]);
    }

]]></Source>
			</Method>
			<Method>
				<Name>PSAButtonIndirectComponentTrans</Name>
				<Source><![CDATA[
    void PSAButtonIndirectComponentTrans()
    {
        indirectComponentTrans.enabled(indirectComponentGroupCost.valueStr() != '');
    }

]]></Source>
			</Method>
			<Method>
				<Name>setFieldsVisibility</Name>
				<Source><![CDATA[
    /// <summary>
    /// Set the visibility of <c>Resource</c>, <c>ResourceCategory</c>, <c>EmplItemId</c>, <c>SubscriptionId</c> and <c>WorkerLegalEntity</c> fields
    /// based on the value of <c>ProjTransType</c>
    /// </summary>
    public void setFieldsVisibility()
    {
        switch (projTransPosting.ProjTransType)
        {
            case ProjTransType::Hour:
            case ProjTransType::Cost:
                projTransPosting_ds.object(fieldNum(ProjTransPosting, Resource)).visible(true);
                projTransPosting_ds.object(fieldNum(ProjTransPosting, ResourceCategory)).visible(false);
                projTransPosting_ds.object(fieldNum(ProjTransPosting, EmplItemId)).visible(false);
                projTransPosting_ds.object(fieldNum(ProjTransPosting, SubscriptionId)).visible(false);
                break;

            case ProjTransType::Revenue:
                if (projTransPosting.LedgerOrigin == ProjOrigin::Subscription)
                {
                    projTransPosting_ds.object(fieldNum(ProjTransPosting, Resource)).visible(false);
                    projTransPosting_ds.object(fieldNum(ProjTransPosting, ResourceCategory)).visible(false);
                    projTransPosting_ds.object(fieldNum(ProjTransPosting, EmplItemId)).visible(false);
                    projTransPosting_ds.object(fieldNum(ProjTransPosting, SubscriptionId)).visible(true);
                }
                else
                {
                    projTransPosting_ds.object(fieldNum(ProjTransPosting, Resource)).visible(true);
                    projTransPosting_ds.object(fieldNum(ProjTransPosting, ResourceCategory)).visible(false);
                    projTransPosting_ds.object(fieldNum(ProjTransPosting, EmplItemId)).visible(false);
                    projTransPosting_ds.object(fieldNum(ProjTransPosting, SubscriptionId)).visible(false);
                }
                break;

            case ProjTransType::Item:
                projTransPosting_ds.object(fieldNum(ProjTransPosting, Resource)).visible(false);
                projTransPosting_ds.object(fieldNum(ProjTransPosting, ResourceCategory)).visible(false);
                projTransPosting_ds.object(fieldNum(ProjTransPosting, EmplItemId)).visible(true);
                projTransPosting_ds.object(fieldNum(ProjTransPosting, SubscriptionId)).visible(false);
                break;
        }
    }

]]></Source>
			</Method>
