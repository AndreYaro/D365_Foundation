            Object mainForm = args.caller();

            if (formHasMethod(mainForm, identifierStr(createTemplate)))
            {
                WHSShipConsolidationTemplateName        templateName        = shipConsolidationTemplateName.text();
                WHSShipConsolidationTemplateDescription templateDescription = shipConsolidationTemplateDescription.text();

                if (mainForm.createTemplate(templateName, templateDescription))
                {
                    super();
                }
            }
        }
    }

