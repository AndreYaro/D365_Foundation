        /// <summary>
        /// Display method for the line state image field.
        /// </summary>
        /// <param name = "_catCartLineState">The <c>CatCartLineState</c> datasource.</param>
        /// <returns>An error image if a line is invalid, an empty image otherwise.</returns>
        [SysClientCacheDataMethod]
        public display container cartLineStateImage(CatCartLineState _catCartLineState)
        {
            ImageReference imageReference;

            if(!_catCartLineState.IsValidProduct)
            {
                imageReference = ImageReference::constructForSymbol(ImageReferenceSymbol::Error);
            }
            else
            {
                imageReference = ImageReference::constructForSymbol('');
            }
            
            return imageReference.pack();
        }

