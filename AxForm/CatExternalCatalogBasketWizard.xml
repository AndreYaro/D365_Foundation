[Form]
public class CatExternalCatalogBasketWizard extends FormRun
{
    private TmpVendLookup                   tmpVendLookup;
    private RefRecId                        currUser;
    private CurrencyCode                    currencyCode;
    private CatExternalCatalogBasketWizard  basketWizard;
    private boolean                         multipleRequisitions;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        CatExternalCatalogBasketWizardHelper::showWarningIfLinesContainIncorrectInformation();

        if (element.Args().caller() && element.Args().caller() is CatExternalCatalogBasketWizard)
        {
            basketWizard = element.Args().caller();
            
            setprefix("@SCM:CatExternalCatalogBasketWizardMessagesPrefix");
                        
            CatPunchoutRequest::processShoppingBaskets();

            catPunchoutRequest_ds.allowCreate(false);
            catCartLine_ds.allowCreate(false);

            element.customizeWizardButtons();
        }
        else
        {
            CatExternalCatalogBasketWizard::main(new Args());
            element.close();
        }        
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        super();

        CatPunchoutRequest::deleteAllActivePunchoutRequestsWithoutLinesForUser(currUser);        
        catPunchoutRequest_ds.research();

        element.enableDisableOkButton(true);

        element.checkMultipleValidSessionsStarted();        

        element.tryDefaultProcurementCategories();
    }

]]></Source>
			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        if(element.validateBeforeClosing())
        {
            CatPunchoutRequestAddToPurchRequisition::createPurchReqLines();

            super();

            PurchReqTable purchReqTable = PurchReqTable::find(catPunchoutRequest.PurchReqTable);
                
            if (purchReqTable)
            {
                var args = new Args();
                args.record(purchReqTable);
                args.caller(element);
                new MenuFunction(menuitemDisplayStr(PurchReqTable), MenuItemType::Display).run(Args);
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>closeCancel</Name>
				<Source><![CDATA[
    public void closeCancel()
    {
        if (catPunchoutRequest.RecId && Box::yesNo("@SCM:CatExternalCatalogPunchoutCancelPrompt", DialogButton::No) == DialogButton::Yes)
        {
            CatPunchoutRequest::deleteAllActivePunchoutRequestsForUser(currUser);
        }     

        super();
    }

]]></Source>
