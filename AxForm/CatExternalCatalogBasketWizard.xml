			<Method>
				<Name>enableDisableOkButton</Name>
				<Source><![CDATA[
    /// <summary>
    /// Disables the Ok button if a punchout is invalid, and enables the Ok button if all pucnhouts are valid.
    /// </summary>
    /// <param name = "_showWarning">A <c>boolean</c> indicating whether or not a warning should be shown if the originator is or is not allowed.</param>
    /// <remarks>
    /// A punchout is invalid if the originator is not allowed to purchase on their behalf, the requester is not allowed in the legal entity/operating unit, or if the catalog is expired.
    /// </remarks>
    private void enableDisableOkButton(boolean _showWarning)
    {
        boolean allCatalogsActive = CatPunchoutRequest::checkCatalogsAreActiveForPunchouts(currUser, _showWarning);
        boolean allowedOnBehalfAuthorization = CatPunchoutRequest::checkAuthorizationForPunchouts(currUser, _showWarning);

        if (element.getButtonControl('Finish'))
        {
            element.getButtonControl('Finish').enabled(allowedOnBehalfAuthorization && allCatalogsActive);
        }
    }

]]></Source>
