            public void modified()
            {
                super();

                CatExternalCatalogBasketWizardHelper basketHelper = CatExternalCatalogBasketWizardHelper::construct(tmpVendLookup);
                basketHelper.populateTmpVendLookupTable(catPunchoutRequest);

                if (!basketHelper.isCategoryValid(catExternalQuoteProductReference.ProcurementCategory))
                {
                    EcoResCategoryName invalidCategoryName = EcoResCategory::find(catExternalQuoteProductReference.ProcurementCategory).Name;
                    warning(strFmt("@SCM:CategoryNotFoundOrUnavailable", invalidCategoryName));

                    catExternalQuoteProductReference.ProcurementCategory = 0;
                }
            }

