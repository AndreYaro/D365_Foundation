			<Method>
				<Name>customizeWizardButtons</Name>
				<Source><![CDATA[
    /// <summary>
    /// Customizes the wizard's buttons. Hides the "Back" button and changes the text of the "Finish" button to "Ok".
    /// </summary>
    public void customizeWizardButtons()
    {
        // The wizard buttons are created at runtime, thus it is required that we use the id's that are put on the sysWizard verbatim.
        element.getButtonControl('Back').visible(false);
        element.getButtonControl('Finish').text("@SCM:AddCartLinesToPurchReq");
    }

]]></Source>
