			<Method>
				<Name>shouldProcessMultipleRequisitions</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates that the user is aware that if there are multiple punchouts, all will be processed.
    /// </summary>
    /// <param name = "_allLinesValid">A <c>boolean</c> specifying whether all lines are valid.</param>
    /// <returns>True if the user agrees, otherwise, false.</returns>
    public boolean shouldProcessMultipleRequisitions(boolean _allLinesValid)
    {
        boolean ret = Box::yesNo("@SCM:CatExternalCatalogPunchoutMultiplePrompt", DialogButton::No,"@SYS12529");
        
        if(!_allLinesValid && ret)
        {
            ret = element.deleteInvalidLines();
        }
         
        return ret;        
    }

]]></Source>
			</Method>
			<Method>
				<Name>deleteInvalidLines</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Validates that the user is aware that invalid lines will be deleted, and then deletes the invalid lines.
    /// </summary>
    /// <returns>True if user agrees, false otherwise.</returns>
    public boolean deleteInvalidLines()
    {
        boolean ret = Box::yesNo("@SCM:CatExternalCatalogPunchoutInvalidLinesPrompt", DialogButton::No,"@SYS12529");
        
        if(ret)
        {
            CatPunchoutRequest::deleteAllActiveInvalidRowsInPunchoutRequestsForUser(currUser);
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateBeforeClosing</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates the closing of the form through prompts shown to the user.
    /// </summary>
    /// <returns>true if okay to close; otherwise false.</returns>
    /// <remarks>
    /// - Validates that all procurement categories are filled out.
    /// - Validates that the user is aware that if there are multiple punchouts, all will be processed.
    /// - Validates that the user is aware that invalid lines will be deleted.
    /// </remarks>
    public boolean validateBeforeClosing()
    {
        boolean ret = catCartLine.RecId;

        if (ret)
        {
            boolean allLinesValid = CatPunchoutRequest::checkAllLinesValid(currUser);

            if (!CatPunchoutRequest::checkAllProcurementCategoriesSpecified(currUser))
            {
                ret = checkFailed("@SCM:CatExternalCatalogPunchoutMissingCategories");
            }
            else if (multipleRequisitions)
            {
                ret = element.shouldProcessMultipleRequisitions(allLinesValid);
            }
            else if (!allLinesValid)
            {
                ret = element.deleteInvalidLines();
            }
            else
            {
                CatPunchoutRequest::deleteAllActivePunchoutRequestsWithoutLinesForUser(currUser);
            }
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>checkMultipleValidSessionsStarted</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if there are multiple valid punchout sessions started. If so, the punchout grid will be shown. Otherwise, the punchout grid will be hidden.
    /// </summary>
    private void checkMultipleValidSessionsStarted()
    {
        multipleRequisitions = CatPunchoutRequest::multipleValidSessionsStarted(currUser);

        str newCaption = multipleRequisitions ? "@SCM:CatExternalCatalogValidateBaskets" : "@SCM:CatExternalCatalogValidateBasket";

        element.getTabControl('Basket').caption(newCaption);

        PunchoutSessionsGroup.visible(multipleRequisitions);
        PurchReqId.visible(!multipleRequisitions);     
    }

]]></Source>
