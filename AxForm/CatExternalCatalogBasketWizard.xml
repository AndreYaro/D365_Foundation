			<Method>
				<Name>updateSelectedProcurementCategories</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates selected procurement categories for a basket.
    /// </summary>
    /// <param name = "_category">New category to be updated.</param>
    public void updateSelectedProcurementCategories(EcoResCategory _category)
    {
        MultiSelectionHelper selectionHelper = MultiSelectionHelper::construct();
        selectionHelper.parmDataSource(catExternalQuoteProductReference_ds);

        CatExternalQuoteProductReference catExternalQuoteProductReferenceBuffer = selectionHelper.getFirst();

        while(catExternalQuoteProductReferenceBuffer)
        {
            CatExternalQuoteProductReference::updateProcurementCategory(_category, catExternalQuoteProductReferenceBuffer);
            catExternalQuoteProductReferenceBuffer = selectionHelper.getNext();
        }

        catCartLine_ds.research(false);
    }

]]></Source>
