			<Method>
				<Name>updateAllProcurementCategories</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates all procurement categories for a basket.
    /// </summary>
    /// <param name = "_category">New category to be updated.</param>
    public void updateAllProcurementCategories(EcoResCategory _category)
    {
        CatPunchoutRequest::updateAllProcurementCategories(_category, catCartLine.PunchoutRequest);
        catCartLine_ds.research();
    }

]]></Source>
