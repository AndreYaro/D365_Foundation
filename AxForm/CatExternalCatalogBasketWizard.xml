			<Control>
				<Name>RemovePunchout</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {            
            super();     
                        
            if (Box::okCancel("@SCM:RemoveShoppingCart", DialogButton::Cancel) == DialogButton::Cancel)
            {
                return;
            }

            catPunchoutRequest.delete();
            catPunchoutRequest_ds.research(true);

            element.enableDisableOkButton(false);

            element.checkMultipleValidSessionsStarted();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>Remove</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            super();     
        
            if (catCartLine.RecId)
            {
                if (Box::okCancel("@SCM:RemoveShoppingCartLine", DialogButton::Cancel) == DialogButton::Ok)
                {
                    MultiSelectionHelper selectionHelper = MultiSelectionHelper::construct();
                    selectionHelper.parmDataSource(catCartLine_ds);

                    CatCartLine catCartLineBuffer = selectionHelper.getFirst();

                    while(catCartLineBuffer)
                    {
                        catCartLineBuffer.delete();
                        catCartLineBuffer = selectionHelper.getNext();
                    }
        
                    catCartLine_ds.research(true);
                }

                // Check if there are any cart lines left after the selected ones have been removed. If none remain, then delete the pucnhout requests without lines.
                if (!catCartLine.RecId)
                {
                    CatPunchoutRequest::deleteAllActivePunchoutRequestsWithoutLinesForUser(currUser);
                    catPunchoutRequest_ds.research();

                    element.checkMultipleValidSessionsStarted();
                }                
            }
        }

]]></Source>
