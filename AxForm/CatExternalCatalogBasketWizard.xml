						<Source><![CDATA[
        public void init()
        {
            super();

            currUser = HcmWorkerLookup::currentWorker();

            var queryBuildDataSource = this.query().dataSourceTable(this.table());

            // Filter to only show punchouts that are ready for consumption
            queryBuildDataSource.addRange(fieldNum(CatPunchoutRequest, RequestStatus))
                .value(queryValue(CatPunchoutRequestStatus::BasketConsumed));

            // Filter to only show punchouts where the originator is the current user
            queryBuildDataSource.addRange(fieldNum(CatPunchoutRequest, Originator))
                .value(queryValue(currUser));
        }

]]></Source>
