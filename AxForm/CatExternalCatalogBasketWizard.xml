				<Methods>
					<Method>
						<Name>calculateUnitPrice</Name>
						<Source><![CDATA[
        /// <summary>
        /// Calculates the unit price for the cart line by dividing the line amount by the quantity ordered.
        /// </summary>
        /// <param name = "_catCartLine">The <c>CatCartLine</c> whose unit price will be calculated.</param>
        /// <returns>The unit price <c>real</c> for the line.</returns>
        [SysClientCacheDataMethod]
        public display real calculateUnitPrice(CatCartLine _catCartLine)
        {
            return _catCartLine.Quantity == 0 ? 0 : _catCartLine.LineAmount / _catCartLine.Quantity;
        }

]]></Source>
