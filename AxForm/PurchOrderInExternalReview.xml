						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            if (element.args().menuItemName() == menuItemDisplayStr(PurchOrderAwaitingVendorResponseFormPart))
            {
                this.queryBuildDataSource().joinMode(JoinMode::InnerJoin);
                this.queryBuildDataSource().addRange(fieldNum(PurchaseOrderResponseHeaderAllVersions, ResponseState)).value(queryValue(PurchaseOrderResponseState::Pending) + ',' + queryValue(PurchaseOrderResponseState::Editing));
            }

            else if (element.args().menuItemName() == menuItemDisplayStr(PurchOrderRequiresActionFormPart))
            {
                this.queryBuildDataSource().joinMode(JoinMode::InnerJoin);
                this.queryBuildDataSource().addRange(fieldNum(PurchaseOrderResponseHeaderAllVersions, ResponseState)).value(SysQuery::valueNot(PurchaseOrderResponseState::Pending) + ',' + SysQuery::valueNot(PurchaseOrderResponseState::Editing));
                element.setControlsVisiblityForPurchOrderRequiresActionFormPart();
            }

            else
            {
                this.queryBuildDataSource().joinMode(JoinMode::NoExistsJoin);
                PurchaseOrderResponseHeaderAllVersions_ResponseState.visible(false);
            }
        }

]]></Source>
					</Method>
				</Methods>
