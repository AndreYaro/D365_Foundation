				<Source><![CDATA[
    [Form]
    public class SalesTaxComponentTotals_IN extends FormRun
    {
        SalesTaxInquiryTrans_IN         salesTaxInquiry;
        TaxTotalsArguments_IN           taxTotalsArguments;
        FormDataSource                  callerFormDatasource;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        taxTotalsArguments      = element.args().parmObject();
        salesTaxInquiry         = taxTotalsArguments.getSalesTaxInquiryObject();
        callerFormDatasource    = taxTotalsArguments.getFormDataSource();
        element.insertIntoTmpTable();
    }

]]></Source>
			</Method>
			<Method>
				<Name>insertIntoTmpTable</Name>
				<Source><![CDATA[
    void insertIntoTmpTable()
    {
        TmpSalesTaxInquiryTable_IN          tmpSalesTaxInquiryTable;
        TmpSalesTaxInquiryTable_IN          tmpSalesTaxInquiryTableLoc;
        TmpSalesTaxComponentTotals_IN       tmpSalesTaxComponentTotals;
        SalesTaxTotal_IN                    salesTaxTotal;

        tmpSalesTaxInquiryTable  = salesTaxInquiry.tmpSalesTaxInquiryTable();

        switch (callerFormDatasource.name())
        {
            case tableid2name(tablenum(SalesTaxTotal_IN))           :
                salesTaxTotal                   = callerFormDatasource.cursor();
                break;
            case tableid2name(tablenum(TmpSalesTaxInquiryTable_IN)) :
                tmpSalesTaxInquiryTableLoc      = callerFormDatasource.cursor();
                break;
            default :
                break;
        }

        if (salesTaxTotal.RecId)
        {
            tmpSalesTaxComponentTotals = tmpSalesTaxComponentTotals.salesTaxComponentTotals(tmpSalesTaxInquiryTable, salesTaxTotal);
        }

        if (tmpSalesTaxInquiryTableLoc.RecId)
        {
            tmpSalesTaxComponentTotals = tmpSalesTaxComponentTotals.salesTaxComponentTotals(tmpSalesTaxInquiryTable, tmpSalesTaxInquiryTableLoc);
        }

        tmpSalesTaxComponentTotals_IN.setTmpData(tmpSalesTaxComponentTotals);
        tmpSalesTaxComponentTotals_IN_ds.research();
    }

]]></Source>
