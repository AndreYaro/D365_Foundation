					<Method>
						<Name>validateWrite</Name>
						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean ret = true;

            if (!mcrScriptTranslation.MCRScript || !mcrScriptTranslation.LanguageId)
            {
                mcrScriptTranslation.MCRScript = mcrScript.RecId;   // Initialize in order to avoid validateWrite to fail.
                mcrScriptTranslation.setDefaultLanguage();          // Initialize in order to avoid validateWrite to fail.
            }

            if (mcrScriptTranslation.canMaintainTranslation())
            {
                ret = super();
            }

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            // Set default language for script translations
            MCRScriptTranslation::queryAddDefaultLanguage(this.query());
        }

]]></Source>
