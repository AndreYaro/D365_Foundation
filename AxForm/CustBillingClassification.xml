				<Source><![CDATA[
    /// <summary>
    /// Sets the accessibility  of buttons based on selected record.
    /// </summary>
    public void setAllowEditBillingCode()
    {
        // if lowest and highest priorities are equal.
        if (lowestBillingCodePriority == highestBillingCodePriority)
        {
            moveUpBillingCode.enabled(false);
            moveDownBillingCode.enabled(false);
        }
        // if selected billing code priority is higest.
        else if (custBillingClassificationCode.SettlementPriority == highestBillingCodePriority)
        {
            moveUpBillingCode.enabled(false);
            moveDownBillingCode.enabled(true);
        }
        // if selected billing code priority is lowest.
        else if (custBillingClassificationCode.SettlementPriority == lowestBillingCodePriority)
        {
            moveUpBillingCode.enabled(true);
            moveDownBillingCode.enabled(false);
        }
        else
        {
            moveUpBillingCode.enabled(true);
            moveDownBillingCode.enabled(true);
        }
        if (!custbillingClassificationCode.RecId)
        {
            moveUpBillingCode.enabled(false);
            moveDownBillingCode.enabled(false);
        }
            removeBillingCode.enabled(true);
    }

]]></Source>
