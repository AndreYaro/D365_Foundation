        /// <summary>
        /// Sets the accessibility  of <c>InterestCode</c> field.
        /// </summary>
        public void setFieldAccess()
        {
            // enable or disable the Interest code field.
            custBillingClassification_ds.object(fieldNum(CustBillingClassification, CustInterest)).allowEdit(!custBillingClassification.UseFromPostingProfile);

            // if the Interest code is used from posting profile.
            if (custBillingClassification.UseFromPostingProfile)
            {
                custBillingClassification_ds.object(fieldNum(CustBillingClassification, CustInterest)).setValue(0);
                custBillingClassification_ds.rereadReferenceDataSources();
            }

            if (custBillingClassification.UseBillingClassCollectionLetter)
            {
                custBillingClassification.CollectionLetterCourse = "";
            }

            custBillingClassification_ds.object(fieldNum(CustBillingClassification, CollectionLetterCourse)).allowEdit(!custBillingClassification.UseBillingClassCollectionLetter);
        }

