						<Name>delete</Name>
						<Source><![CDATA[
        /// <summary>
        /// Arrange billing classification priority wise after deletion of billing classification.
        /// </summary>
        public void delete()
        {
            SettlementPriority settlementPriority = custBillingClassification.SettlementPriority;

            super();

            if (settlementPriority)
            {
                lowestBillingClassificationPriority -= 1;

                if (highestBillingClassificationPriority != settlementPriority)
                {
                    element.args().lookupRecord(CustBillingClassification::findBySettlementPriority(settlementPriority));
                    custBillingClassification_ds.research(false);
                }
            }
        }

]]></Source>
