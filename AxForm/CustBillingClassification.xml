						<Name>write</Name>
						<Source><![CDATA[
        /// <summary>
        /// Saves lowest billing classification priority.
        /// </summary>
        public void write()
        {
            boolean isRecordInserted = false;
            if (custBillingClassification.RecId)
            {
                isRecordInserted = true;
            }

            super();

            if (!isRecordInserted)
            {
                lowestBillingClassificationPriority = custBillingClassification.SettlementPriority;
            }
        }

]]></Source>
