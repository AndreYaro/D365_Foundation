			<Method>
				<Name>setAllowEditCustBillingClassification</Name>
				<Source><![CDATA[
    /// <summary>
    /// Sets the accessibility  of <c>InterestCode</c> field and buttons based on selected record.
    /// </summary>
    /// <remarks>
    /// Saves lowest and highest priority of billing codes associated to billing classification.
    /// </remarks>
    public void setAllowEditCustBillingClassification()
    {
        int numberOfRows;

        // if lowest and highest priorities are equal.
        if (highestBillingClassificationPriority == lowestBillingClassificationPriority)
        {
            moveUpBillingClass.enabled(false);
            moveDownBillingClass.enabled(false);
        }
        // if selected billing classification priority is higest.
        else if (custBillingClassification.SettlementPriority == highestBillingClassificationPriority)
        {
            moveUpBillingClass.enabled(false);
            moveDownBillingClass.enabled(true);
        }
        // if selected billing classification priority is lowest.
        else if (custBillingClassification.SettlementPriority == lowestBillingClassificationPriority)
        {
            moveUpBillingClass.enabled(true);
            moveDownBillingClass.enabled(false);
        }
        else
        {
            moveUpBillingClass.enabled(true);
            moveDownBillingClass.enabled(true);
        }

        // if billing classification is not saved.
        if (!custbillingClassification.RecId)
        {
            addBillingCode.enabled(false);
            removeBillingCode.enabled(false);
            moveUpBillingClass.enabled(false);
            moveDownBillingClass.enabled(false);
        }
        else
        {
             addBillingCode.enabled(true);
        }

        // Gets total # of billing codes associated to this Billing Classification.
        numberOfRows = custBillingClassificationCode.getLowestSettlementPriority(custBillingClassification.RecId);
        // if more than one billing code is associated
        if (numberOfRows > 1)
        {
            lowestBillingCodePriority = numberOfRows;
            highestBillingCodePriority = 1;
            removeBillingCode.enabled(true);
        }
        // if no billing code is associated
        else if (numberOfRows == 0)
        {
            lowestBillingCodePriority = 0;
            highestBillingCodePriority = 1;
            moveUpBillingCode.enabled(false);
            moveDownBillingCode.enabled(false);
            removeBillingCode.enabled(false);
        }
        else
        {
            removeBillingCode.enabled(true);
            lowestBillingCodePriority = 1;
            highestBillingCodePriority = 1;
        }
    }

]]></Source>
