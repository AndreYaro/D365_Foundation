						<Name>init</Name>
						<Source><![CDATA[
        /// <summary>
        /// Saves lowest and highest priority of billing classification
        /// </summary>
        /// <remarks>
        /// calls getLowestSettlementPriority method to get the total # of records.
        /// </remarks>
        public void init()
        {
            int numberOfRows;
            super();

            // Gets total # of records in CustBillingClassification.
            numberOfRows = custBillingClassification.getLowestSettlementPriority();
            // if more than one billing classification exists
            if (numberOfRows > 1)
            {
                // Sets lowest priority.
                lowestBillingClassificationPriority = numberOfRows;
                highestBillingClassificationPriority = 1;
            }
            // if no billing classification exists
            else if (numberOfRows == 1)
            {
                lowestBillingClassificationPriority = 1;
                highestBillingClassificationPriority = 1;
            }
            else
            {
                lowestBillingClassificationPriority = 0;
                highestBillingClassificationPriority = 1;
                moveUpBillingClass.enabled(false);
                moveDownBillingClass.enabled(false);
                addBillingCode.enabled(false);
                removeBillingCode.enabled(false);
                moveUpBillingCode.enabled(false);
                moveDownBillingCode.enabled(false);
            }
        }

]]></Source>
