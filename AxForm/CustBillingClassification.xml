            public void modified()
            {
                boolean ret = true;

                if (custBillingClassificationCode.RecId)
                {
                    if (CustBillingCode::isUsedByInvoice(custBillingClassificationCode.orig().CustBillingCode))
                    {
                        custBillingClassificationCode.CustBillingCode = custBillingClassificationCode.orig().CustBillingCode;
                        custBillingCodeClass_CustBillingCode.value(custBillingClassificationCode.CustBillingCode);
                        ret = false;
                        throw error("@SPS368");
                    }
                }

                if (ret)
                {
                    super();
                }
            }

