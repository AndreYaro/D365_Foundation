        /// <summary>
        /// Arrange billing codes associated to billing classification in priority wise after removing the billing code.
        /// </summary>
        public void delete()
        {
            SettlementPriority settlementPriority = custBillingClassificationCode.SettlementPriority;

            super();

            if (settlementPriority)
            {
                lowestBillingCodePriority -= 1;
                this.research();
                if (highestBillingCodePriority != settlementPriority)
                {
                    element.args().lookupRecord(CustBillingClassificationCode::findBySettlementPriority(settlementPriority));
                    custBillingClassificationCode_ds.research(false);
                }
            }
            if (lowestBillingCodePriority <= 0)
            {
                removeBillingCode.enabled(false);
            }
        }

