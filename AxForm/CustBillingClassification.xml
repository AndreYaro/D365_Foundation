								<Name>lookupReference</Name>
								<Source><![CDATA[
            /// <summary>
            /// Creates the lookup for the <c>BillingCode</c> and <c>Description</c> field.
            /// </summary>
            /// <param name="_formReferenceControl">
            /// The form reference control to perform the lookup for.
            /// </param>
            /// <returns>
            /// A lookup for for the <c>BillingCode</c> and <c>Description</c> field.
            /// </returns>
            public Common lookupReference(FormReferenceControl _formReferenceControl)
            {
                Query                   query = new Query();
                QueryBuildDataSource    queryBuildDataSourceBillingCode;
                QueryBuildDataSource    queryBuildDataSourceBillingClassificationCode;

                // Instantiate SysReferenceTableLookup object using table.
                SysReferenceTableLookup custBillingCodelookup = SysReferenceTableLookup::newParameters(tableNum(CustBillingCode),
                                                                                                _formReferenceControl, true);
                // Create the query. Select Billing Codes that are not associated with a billing classification.
                queryBuildDataSourceBillingCode = query.addDataSource(tableNum(CustBillingCode));
                queryBuildDataSourceBillingClassificationCode = queryBuildDataSourceBillingCode.addDataSource(
                                                                            tableNum(CustBillingClassificationCode));

                queryBuildDataSourceBillingClassificationCode.relations(true);
                queryBuildDataSourceBillingClassificationCode.joinMode(JoinMode::NoExistsJoin);
                // Set the query to be used by the lookup form
                custBillingCodelookup.parmQuery(query);

                // Specify the fields to show in the form.We have chosen Billing Code and Description.

                custBillingCodelookup.addLookupfield(fieldNum(CustBillingCode, BillingCode));
                custBillingCodelookup.addLookupfield(fieldNum(CustBillingCode, Description));
                // Perform the lookup
                return custBillingCodelookup.performFormLookup();
            }

]]></Source>
