				<Source><![CDATA[
[Form]
public class VendVendorPortalWorkspace extends FormRun implements SysIFilterProvider, VendIVendorFilterProvider
{
    SysFilterChangeEvent changeEvent;
    boolean              formInitError;
    int                  purchVendorPortalResponsesPosition;
    boolean              retainPrchVendorPortalResponsesPosition;

}
]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        super();

        if (!formInitError)
        {
            SysFilterLastValueHelper::saveLastValue(VendAccountFilter);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        VendTable vendTable;
        VendAccount vendAccount;
        
        select firstonly AccountNum from vendTable;

        if (!vendTable)
        {
            formInitError = true;
            error("@SCM:VendorPortalInsufficientRights");
            this.close();
        }

        super();

        if (element.args() && element.args().caller() && element.args().caller().name() == formStr(VendVendorProfileManagementWorkspace))
        {
            vendTable = element.args().record();
            VendAccountFilter.text(VendTable.AccountNum);
            vendAccount = VendTable.AccountNum;
        }
        else
        {
            SysFilterLastValueHelper::getLastValue(VendAccountFilter);
            vendAccount = VendAccountFilter.valueStr();
        }

        if (!vendAccount || !VendTable::find(vendAccount))
        {
            VendAccountFilter.text(vendTable.AccountNum);
        }

        changeEvent = SysFilterChangeEvent::newFromFormControl(VendAccountFilter);
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmFilter</Name>
				<Source><![CDATA[
    public SysIFilter parmFilter()
    {
        SysIFilter filter       = SysFilterFactory::createEmptyFilter();

        SysIFilterValue filterValue = SysFilterValueFactory::createFilterValueForString(VendAccountFilter.valueStr(), extendedTypeNum(VendAccount));
        filter.addItem(filterValue, SysFilterOperator::Equal, SysFilterValueResolutionMethod::None);
        return filter;
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmChangeEvent</Name>
				<Source><![CDATA[
    public SysFilterChangeEvent parmChangeEvent()
    {
        return changeEvent;
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmVendorFilter</Name>
				<Source><![CDATA[
    public VendAccount parmVendorFilter()
    {
        return SysFilterValueHelper::getFirstValueRecordFromFilterForType(this.parmFilter(), SysFilterValueTypeEnum::String, extendedTypeNum(VendAccount));
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmPurchVendorPortalResponsesPosition</Name>
				<Source><![CDATA[
    public int parmPurchVendorPortalResponsesPosition(int _purchVendorPortalResponsesPosition = purchVendorPortalResponsesPosition)
    {
        purchVendorPortalResponsesPosition = _purchVendorPortalResponsesPosition;
        return purchVendorPortalResponsesPosition;
    }

]]></Source>
			</Method>
			<Method>
