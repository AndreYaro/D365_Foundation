        void clicked()
        {
            Args args = new Args(element);

            if (DateTimeUtil::date(transDateTime.dateTimeValue()))
            {
                super();

                args.record(eximDBKSchemesLine);

                EximDBKUpdate_IN::newPostDutyDrawbackRealization(args);

                element.updateTotals();

                eximDBKSchemesLine_ds.executeQuery();
            }
            else
            {
                checkFailed(strFmt("@SYS116347", "@SYS67"));
            }
        }

