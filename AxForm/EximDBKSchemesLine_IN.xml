			<Method>
				<Name>enableDimensionButtonsActive</Name>
				<Source><![CDATA[
    /// <summary>
    /// Indicates whether the financial dimension menu items on the lines form are enabled.
    /// </summary>
    public void enableDimensionButtonsActive()
    {
        boolean enable  = true;

        enable = (dbkSchemesTable.CustAccount)? true : false;

        accountDimension.enabled(enable);
    }

]]></Source>
