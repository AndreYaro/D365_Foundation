						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            boolean   appliedStatus;
            boolean   closedStatus;
            int       ret;

            ret = super();

            if (ret)
            {
                appliedStatus = eximDBKSchemesLine.Status == EximDBKTransStatus_IN::Applied;
                closedStatus  = eximDBKSchemesLine.Status != EximDBKTransStatus_IN::Closed;

                postButton.enabled(amountToSettle != 0);

                eximDBKSchemesLine_IN_MarkSelect.enabled(appliedStatus);
                eximDBKSchemesLine_IN_AmountToSettle.enabled(appliedStatus);
                eximDBKSchemesLine_IN_Status.enabled(closedStatus);
            }

            return ret;
        }

]]></Source>
