        ecoResProductTranslationStringControls.addEnd(element.controlId(formControlStr(EcoResCategoryAddProduct, EcoResProductTranslationSelected_Name)));
        fallbackStringControls.addEnd(element.controlId(formControlStr(EcoResCategoryAddProduct, EcoResProductTranslationFallbackViewSelected_Name)));

        EcoResProductTranslationFallbackViewModel::setupFallbackTranslationForm(element,
