			<Method>
				<Name>updateCategoryProduct</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Adds all the records from the temporary table to the <c>EcoResProductCategory</c> table.
    /// </summary>
    void updateCategoryProduct()
    {
        int                     nAdded = 0;

        EcoResProductCategory   tmpEcoResProductCategory;
        DialogButton response;
        Set productIds;
        RetailCategoryProductUpdateParm parameters;
        Args args;
        MenuFunction menuFunction;
        EcoResCategoryHierarchy currentHierarchy;

        tmpEcoResProductCategory.setTmp();
        tmpEcoResProductCategory.setTmpData(tmpEcoResProductCategorySelected);

        if (isConfigurationkeyEnabled(configurationKeyNum(Retail)))
        {
            productIds = new Set(Types::Int64);
        }

        ttsbegin;
        while select * from tmpEcoResProductCategory
        {
            // This logic is moved at EcoResProductCategory::insertEcoResProductCategory(..,..)
            // It's required to just remove the duplicate code for button 'Add' and retail button 'Add products' functionality.

            if (EcoResProductCategory::insertEcoResProductCategory(
                        currentCategory,
                        EcoResProduct::find(tmpEcoResProductCategory.Product)))
            {
                if (isConfigurationkeyEnabled(configurationKeyNum(Retail)))
                {
                    productIds.add(tmpEcoResProductCategory.Product);
                }
                nAdded++;
            }
   
            if (element.args().caller()
                    && element.args().caller().name() == formStr(RetailCategory))
            {
                using(RetailDimensionDefaultAttributeValuesImportContext context = RetailDimensionDefaultAttributeValuesImportContext::construct())
                {
                    context.parmEcoResProductRecId(tmpEcoResProductCategory.Product);
                    RetailDimensionDefaultAttributevaluesImport::assignDefaultAttribute(currentCategory.RecId, currentCategory.CategoryHierarchy);
                }
            }
        }
        ttscommit;

        // The update functionality should work only if it has any product to update.
        if (isConfigurationkeyEnabled(configurationKeyNum(Retail)) &&
            !productIds.empty())
        {
            currentHierarchy = EcoResCategoryHierarchy::find(currentCategory.CategoryHierarchy);

            if (EcoResCategoryHierarchyRole::existRoleHierarchyPair(
                    EcoResCategoryNamedHierarchyRole::Retail,
                    currentHierarchy.RecId))
            {
                response = Box::yesNo("@RET260050", DialogButton::No, "@RET260051", "@RET260052");

                if (DialogButton::Yes == response)
                {
                    args = new Args(formStr(RetailProductUpdateForm));

                    parameters = RetailCategoryProductUpdateParm::construct();
                    //parameters.parmUserSessionId(mSessionId);
                    parameters.parmEntityName(currentCategory.Name);
                    parameters.parmEntityType(RetailProductUpdateMode::ProductSet);
                    parameters.parmEntityId(currentCategory.RecId);
                    parameters.parmCategoryId(currentCategory.RecId);

                    parameters.parmProductIds(productIds);
                    args.parmObject(parameters);

                    menuFunction = new MenuFunction(menuitemDisplayStr(RetailProductUpdate), MenuItemType::Display);
                    menuFunction.run(args);
                }
            }

            if (EcoResCategoryHierarchyRole::existRoleHierarchyPair(
                EcoResCategoryNamedHierarchyRole::RetailSpecialGroup,
                currentHierarchy.RecId))
            {
                while select * from tmpEcoResProductCategory
                {
                    if (EcoResProductCategory::findByProductIdAndCategoryId(tmpEcoResProductCategory.Product, tmpEcoResProductCategory.Category))
                    {
                        RetailSpecialCategoryMember categoryMember = RetailSpecialCategoryMember::findByMemberLine(
                                tmpEcoResProductCategory.orig().Category,
                                0,
                                tmpEcoResProductCategory.Product,
                                0,
                                true);

                        if (categoryMember)
                        {
                            categoryMember.Category = tmpEcoResProductCategory.Category;
                            categoryMember.update();
                        }
                        else
                        {
                            RetailSpecialCategoryMember::addProductLine(tmpEcoResProductCategory.Category, tmpEcoResProductCategory.Product);
                        }
                    }
                }
            }
        }

        // notify parent FORM to refresh
        if (nAdded && formHasMethod(caller, identifierStr(refreshProductCategoryGrid)))
        {
            caller.refreshProductCategoryGrid();
        }

        // notify parent FORM to refresh
        if (nAdded && formHasMethod(caller, identifierStr(refreshCategoryProducts)))
        {
            caller.refreshCategoryProducts();
        }
    }

]]></Source>
