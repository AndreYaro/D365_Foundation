			<Method>
				<Name>selectProduct</Name>
				<Source><![CDATA[
    /// <summary>
    ///  Adds the selected <c>EcoResProduct</c> record to the temporary table.
    /// </summary>
    void selectProduct()
    {
        EcoResProduct           selectedProduct;
        EcoResProductCategory   tmpEcoResProductCategory;
        EcoResProductCategory   existingEcoResProductCategory;

        tmpEcoResProductCategory.setTmp();

        // Are there any marked records?
        for ( selectedProduct = ecoResProduct_ds.getFirst(1)?ecoResProduct_ds.getFirst(1):ecoResProduct_ds.cursor();
        selectedProduct;
        selectedProduct = ecoResProduct_ds.getNext())
        {
            tmpEcoResProductCategory.setTmpData(tmpEcoResProductCategorySelected);
            select firstonly tmpEcoResProductCategory
                where tmpEcoResProductCategory.Category == currentCategory.RecId
                    && tmpEcoResProductCategory.CategoryHierarchy == currentCategory.CategoryHierarchy
                    && tmpEcoResProductCategory.Product == selectedProduct.RecId;

            if (!tmpEcoResProductCategory.RecId)
            {
                tmpEcoResProductCategory.clear();
                tmpEcoResProductCategory.initValue();
                tmpEcoResProductCategory.Category           = currentCategory.RecId;
                tmpEcoResProductCategory.CategoryHierarchy  = currentCategory.CategoryHierarchy;
                tmpEcoResProductCategory.Product            = selectedProduct.RecId;

                // Find existing record with same Product and CategoryHierarchy, and set the DisplayOrder to keep consistent if found.
                // This logic should alwasy work even when the feature is turn off, or it will cause data inconsistent issue.
                existingEcoResProductCategory = EcoResProductCategory::findByProductAndCategoryHierarchy(selectedProduct.RecId, currentCategory.CategoryHierarchy);
                
                if (existingEcoResProductCategory)
                {
                    tmpEcoResProductCategory.DisplayOrder = existingEcoResProductCategory.DisplayOrder;
                }

                tmpEcoResProductCategory.doInsert();
            }
        }
    }

]]></Source>
