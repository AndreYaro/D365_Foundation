			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        Common                  commonForEmptyTitleDS;
        QueryBuildDataSource    qbds;

        if (!element.args() ||
            !element.args().caller() ||
            !element.args().record() ||
            !element.args().dataset() ||
            element.args().dataset() != tablenum(EcoResCategory))
        {
            throw error(strfmt("@SYS22678",element.name()));
        }

        caller                  = element.args().caller();
        currentCategory.data(element.args().record());

        super();

        this.setupFallbackProductTranslation();
        //set category filter, based on inheritance.
        qbds = ecoResProductCategory_ds.query().dataSourceTable(tablenum(EcoResProductCategory));
        qbds.clearRange(fieldnum(EcoResProductCategory, CategoryHierarchy));
        qbds.addRange(fieldnum(EcoResProductCategory, CategoryHierarchy))
            .value(queryValue(currentCategory.CategoryHierarchy));

        // To clear dynamic Title dataSource, need to pass either null or empty table buffer.
        element.args().record(commonForEmptyTitleDS);
    }

]]></Source>
