				<Source><![CDATA[
    /// <summary>
    ///  Deletes the selected record from the temporary table.
    /// </summary>
    void deselectProduct()
    {
        ttsbegin;

        if (tmpEcoResProductCategorySelected_ds.anyMarked())
        {
            tmpEcoResProductCategorySelected_ds.deleteMarked();
        }
        else if (tmpEcoResProductCategorySelected_ds.getFirst())
        {
            tmpEcoResProductCategorySelected_ds.delete();
        }

        ttscommit;
    }

]]></Source>
