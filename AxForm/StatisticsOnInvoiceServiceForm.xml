			<Method>
				<Name>getVendInvoiceJour</Name>
				<Source><![CDATA[
    private VendInvoiceJour getVendInvoiceJour()
    {
        VendInvoiceJour vendInvoiceJour;
        select firstonly vendInvoiceJour
            where vendInvoiceJour.RecId == StatisticsOnInvoiceJour.InvoiceJourRecId
                && vendInvoiceJour.TableId == StatisticsOnInvoiceJour.InvoiceJourTableId;

        return vendInvoiceJour;
    }

]]></Source>
