			<Method>
				<Name>updatePaymentControls</Name>
				<Source><![CDATA[
    public void updatePaymentControls()
    {
        switch (FBGeneralAdjustmentCodeIPI_BR.getTaxAssessmentClassificationIPI())
        {
            case FBTaxAssessmentClassification_BR::GeneralDebitAdjustment:
                CreatePaymentCheckbox.enabled(true);
                break;

            default:
                CreatePaymentCheckbox.enabled(false);
                FBGeneralAdjustmentCodeIPI_BR.CreatePayment = NoYes::No;
                break;
        }

        RevenueCode.enabled(CreatePaymentCheckbox.checked());
    }

]]></Source>
