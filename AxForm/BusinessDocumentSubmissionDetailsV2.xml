						<Source><![CDATA[
        public void clicked()
        {
            super();

            if (isSubmission && submission)
            {
                Args args = new Args();
                args.record(submission);
                args.parmEnumType(enumNum(NoYes));
                args.parmEnum(NoYes::No);
                args.caller(this);

                new MenuFunction(menuItemActionStr(BusinessDocumentResubmit), MenuItemType::Action).run(args);

                submission.reread();
                element.updateDesign();
            }
            else if (isExport)
            {
                BusinessDocumentExportController controller = BusinessDocumentExportController::newDocument(BusinessDocument);
                controller.parmResubmitType(BusinessDocumentResubmitType::Resubmit);
                controller.resubmit();

                BusinessDocument.reread();
                element.updateDesign();
            }
            else if (isExternal)
            {
                BusinessDocumentExternalController controller = BusinessDocumentExternalController::newSubmission(submissionExternal);
                controller.resubmit();

                submissionExternal.reread();
                element.updateDesign();
            }
        }

]]></Source>
