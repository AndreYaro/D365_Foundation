						<Source><![CDATA[
        public void clicked()
        {
            super();

            if (isSubmission && submission)
            {
                Args args = new Args();
                args.record(submission);
                args.parmEnumType(enumNum(BusinessDocumentResubmitType));
                args.parmEnum(BusinessDocumentResubmitType::ResubmitFromFailedAction);
                args.caller(this);

                new MenuFunction(menuItemActionStr(BusinessDocumentResubmitFromFailedAction), MenuItemType::Action).run(args);

                submission.reread();
                element.updateDesign();
            }
            else if (isExport)
            {
                BusinessDocumentExportController controller = BusinessDocumentExportController::newDocument(BusinessDocument);
                controller.parmResubmitType(BusinessDocumentResubmitType::ResubmitFromFailedAction);
                controller.resubmit();

                BusinessDocument.reread();
                element.updateDesign();
            }
        }

]]></Source>
