				<Source><![CDATA[
[Form]
public class BusinessDocumentSubmissionDetailsV2 extends FormRun
{
    BusinessDocumentInfo documentInfo;
    private boolean isExternal;
    private boolean isExport;
    private boolean isSubmission;
    private boolean resubmitFromFailedActionFeature;
    private BusinessDocumentSubmission submission;
    private BusinessDocumentSubmissionExternal submissionExternal;
    private BusinessDocument BusinessDocument;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        str callerName = element.args().callerName();

        boolean isExternalAgent = callerName == formStr(BusinessDocumentExternalAgentsV2);
        isExternal = callerName == formStr(BusinessDocumentSubmissionExternalV2);

        isExport = callerName == formStr(BusinessDocumentSubmissionExport);
        resubmitFromFailedActionFeature = FeatureStateProvider::isFeatureEnabled(BusinessDocumentResubmitFromFailedActionFeature::instance());

        guid submissionId;

        if (isExternal)
        {
            submissionExternal =  BusinessDocumentSubmissionExternal::findByCommon(element.args().record());
            submissionId = submissionExternal.UID;

            BusinessDocument = BusinessDocument::findByUID(submissionId);
        }
        else if (isExternalAgent)
        {
            BusinessDocument = element.args().record();

            if (BusinessDocument.Direction == BusinessDocumentDirection::Incoming)
            {
                submissionExternal =  BusinessDocumentSubmissionExternal::findByUID(BusinessDocument.UID);
                isExternal = submissionExternal.RecId != 0;
            }
        }
        else if (isExport)
        {
            BusinessDocument = element.args().record();
        }
        else
        {
            submission = BusinessDocumentSubmission::findByCommon(element.args().record());
            submissionId = submission.UID;

            BusinessDocument = BusinessDocument::findByUID(submissionId);
            isSubmission = true;
        }

        if (!submission && !submissionExternal && !BusinessDocument)
        {
            throw Error(Error::wrongUseOfFunction(funcName()));
        }
       
        this.fillData();
        this.updateDesign();
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    private void updateDesign()
    {
        ResubmitButton.visible(isSubmission || isExport || isExternal);
        ResubmitButton.enabled(BusinessDocument.RecId && 
            conFind([
                BusinessDocumentState::Canceled,
                BusinessDocumentState::ValidationFailed,
                BusinessDocumentState::Failed], BusinessDocument.State));

        ResubmitFromFailedActionButton.visible(resubmitFromFailedActionFeature && (isSubmission || isExport));
        ResubmitFromFailedActionButton.enabled(resubmitFromFailedActionFeature && BusinessDocument.RecId && BusinessDocument.State == BusinessDocumentState::Failed);
    }

]]></Source>
			</Method>
			<Method>
				<Name>fillData</Name>
				<Source><![CDATA[
    private void fillData()
    {
        boolean isSuccess;
        ERForInvoicingActivities activities = ERForInvoicingActivities::newFromCategory(formStr(BusinessDocumentSubmissionDetails));
        using (var activityContext = activities.updateDocumentStatus())
        {
            try
            {
                if (isExternal)
                {
                    this.updateBusinessDocumentExternalState();
                }
                else if (isSubmission)
                {
                    this.updateBusinessDocumentState();
                }
  
                isSuccess = true;
            }
            catch (Exception::CLRError)
            {
                str message = SRSProxy::getLastClrExceptionMessage();
                activityContext.addCustomProperty(ERForInvoicingInstrumentationConstants::CustomPropertyError, message);
                error(message);
            }
            finally
            {
                activityContext.addCustomProperty(ERForInvoicingInstrumentationConstants::CustomPropertyIsSuccess, any2Str(isSuccess));
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateBusinessDocumentState</Name>
				<Source><![CDATA[
    private void updateBusinessDocumentState()
    {
        BusinessDocumentSubmissionService::updateDocumentStatusV2(submission.getDocument(), submission.Stage);
        this.updateData(BusinessDocumentExecutionRequest_DS, submission);
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateBusinessDocumentExternalState</Name>
				<Source><![CDATA[
    private void updateBusinessDocumentExternalState()
    {
        BusinessDocumentVendorImportService::updateDocumentStateV2(submissionExternal, BusinessDocument);
        this.updateData(BusinessDocumentExecutionRequest_DS, submissionExternal);
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateData</Name>
				<Source><![CDATA[
    private void updateData(FormDataSource dataSource, Common document)
    {
        BusinessDocument.reread();
        dataSource.reread();
        dataSource.refresh();
        dataSource.active();
        document.reread();
        this.updateDesign();
    }

]]></Source>
			</Method>
			<Method>
				<Name>task</Name>
				<Source><![CDATA[
    public int task(int _p1)
    {
        #Task
    
        switch (_p1)
        {
            case #taskRefresh:
                this.fillData();
                break;
        }
        
        return super(_p1);
    }

]]></Source>
			</Method>
			<Method>
				<Name>outputFile</Name>
				<Source><![CDATA[
    private void outputFile(str _fileContents, str _fileName = '')
    {
        #File
        const str jsonExt = '.json';
        str fileName = _fileName ? _fileName : 'output';

        str fileExt;
        switch (true)
        {
            case strStartsWith(_fileContents, '<') : fileExt = #xml; break;
            case strStartsWith(_fileContents, '{') : fileExt = jsonExt; break;
            default: fileExt = #txt; break;
        }

        File::SendStringAsFileToUser(_fileContents, fileName + fileExt);
    }

]]></Source>
