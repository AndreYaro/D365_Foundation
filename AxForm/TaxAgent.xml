						<Source><![CDATA[
        public void updateButtons()
        {
            boolean exist = (taxAgent.RecId != 0);

            dirPartyPostalAddressView.updateButtonsMenuItem(exist, newAddress, editAddress, detailAddress, deactivateAddress, mapButton, logisticsLocationDefault);
            newAddress.enabled(dirPartyPostalAddressView.RecId == 0);
            deleteAddress.enabled(hasTableAccess(tableNum(LogisticsLocation), AccessType::Delete) && exist && dirPartyPostalAddressView.IsPrimary == false);
        }

]]></Source>
