				<Methods>
					<Method>
						<Name>getPINPadIP</Name>
						<Source><![CDATA[
        display str getPINPadIP(RetailTerminalTable _record)
        {
            RetailHardwareConfiguration configuration;
            select firstonly PinpadIP 
                from configuration
                where configuration.TerminalId == _record.terminalId;

            if (configuration.PinpadIP)
            {
                return configuration.PinpadIP;
            }
            
            RetailStoreHardwareStationTable storeHardwareProfile;
            select firstonly PinpadIP
                from configuration
                join storeHardwareProfile
                where storeHardwareProfile.HardwareProfileId == _record.hardwareProfile &&
                storeHardwareProfile.Store == _record.StoreRecId &&
                configuration.StoreHardwareStation == storeHardwareProfile.RecId;

            if (configuration.PinpadIP)
            {
                return configuration.PinpadIP;
            }

            return "";
        }

]]></Source>
