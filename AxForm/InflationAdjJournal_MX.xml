				<Methods>
					<Method>
						<Name>initValue</Name>
						<Source><![CDATA[
        public void initValue()
        {
            boolean enable;

            super();

            enable = InflationAdjJournal_MX::existsRecord();
            inflationAdjJournal_MX_ds.object(fieldNum(InflationAdjJournal_MX, FromDate)).allowEdit(!enable);
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;

            ret = super();

            var inflationJournalOpen = inflationAdjJournal_MX.Status == InflationAdjJournalStatus_MX::Open;

            inflationAdjJournal_MX_ds.allowDelete(InflationAdjJournal_MX::isLastOpenRecord(inflationAdjJournal_MX.RecId));
            inflationAdjJournal_MX_ds.object(fieldNum(InflationAdjJournal_MX, PostingLayer)).allowEdit(inflationJournalOpen);
            inflationAdjJournal_MX_ds.object(fieldNum(InflationAdjJournal_MX, Notes)).allowEdit(inflationJournalOpen);
            inflationAdjJournal.enabled(inflationJournalOpen);
            voucher.enabled(inflationJournalOpen == false);
            simulation.enabled(inflationJournalOpen);
            reversal.enabled(InflationAdjJournal_MX::isLastPostedRecord(inflationAdjJournal_MX.RecId));
            printReport.enabled(inflationAdjJournal_MX.Status == InflationAdjJournalStatus_MX::Posted);

            return ret;
        }

]]></Source>
					</Method>
				</Methods>
