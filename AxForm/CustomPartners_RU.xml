			<Method>
				<Name>enableControls</Name>
				<Source><![CDATA[
        void enableControls()
    {
        boolean isBroker = (customPartners.PartnerType == CustomPartnerType_RU::Broker) ? true : false;

        customService.enabled(isBroker);

        customPartners_ds.object(fieldNum(CustomPartners_RU, CustomServiceProvider)).enabled(isBroker);

        CustomPosts_RU.enabled(! isBroker);

        feaccIndicativePrice.enabled(! isBroker);
        feaccInventTable.enabled(! isBroker);
    }

]]></Source>
