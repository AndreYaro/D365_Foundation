				<Source><![CDATA[
    /// <summary>
    /// Validates access to the form when the form is initially opened.
    /// </summary>
    /// <exception cref="Exception::Error">
    /// 'Use invoice matching' must be enabled in AP setup in order to open this form.
    /// </exception>
    public void init()
    {
        // Form is designed to be launched directly from a setup menu, not as a child to another window/table buffer
        Debug::assert(element.args().record() == null);

        // Prevent the window from opening if the setup option is not checked
        VendParameters vendParameters = VendParameters::find();
        if (vendParameters.editUseInvoiceMatching() == NoYes::No)
        {
            throw error("@SYS114452");
        }

        super();
    }

