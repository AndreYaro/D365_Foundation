					<Method>
						<Name>findCaller</Name>
						<Source><![CDATA[
        void findCaller()
        {
            ProdRoute        tmpProdRoute;
            ProdRouteJob     tmpProdRouteJob;

            switch (element.args().dataset())
            {
                case (tablenum(ProdRoute)):

                    tmpProdRoute = element.args().record();

                    if (tmpProdRoute)
                        prodRoute_ds.positionToRecord(tmpProdRoute);

                    break;

                case (tablenum(ProdRouteJob)):

                    tmpProdRouteJob = element.args().record();
                    tmpProdRoute = tmpProdRouteJob.prodRoute();

                    if (tmpProdRoute)
                        prodRoute_ds.positionToRecord(tmpProdRoute);

                    break;
            }
        }

]]></Source>
