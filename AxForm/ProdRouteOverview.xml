				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        void init()
        {
            Query       query;
            fieldId     id;
            QueryBuildDataSource qbs;
            JmgJobTable jobTable;

            if (element.args().caller())
            {
                id = fieldname2id(element.args().dataset(),fieldstr(ProdTable,ProdId));

                if (id)
                {
                    query = new Query();
                    query.addDataSource(tablenum(ProdRoute));
                    query.dataSourceNo(1).addDynalink(fieldnum(ProdRoute,ProdId),element.args().record(),id);

                    prodRoute_DS.query(query);
                }
            }
            super();
            if (element.args().dataset() == tableNum(JmgJobTable))
            {
                jobTable = element.args().record();
                query = ProdRoute_DS.query();
                qbs = query.dataSourceNo(1);
                qbs.clearDynalinks();
                qbs.addRange(fieldNum(ProdRoute, RecId)).value(queryValue(jobTable.prodRoute().RecId));
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
