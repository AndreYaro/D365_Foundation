			<Method>
				<Name>editType</Name>
				<Source><![CDATA[
    edit PCTableConstraintType editType(
        boolean       _set,
        PCTableConstraintType _type)
    {
        Dialog dialog = new Dialog("@SYS6998");
        dialog.addImage(931);// big warning image
        dialog.addText("@SYS309142");

        if (_set)
        {
            if (_type != wizardClass.parmTCType())
            {
                if (wizardClass.openWarningDialog() == NoYes::Yes)
                {
                    if (dialog.run())
                    {
                        element.setType(_type);
                    }
                }
                else
                {
                    // set selected type
                    wizardClass.setTCType(_type);

                    if (wizardClass.parmTCType() == PCTableConstraintType::System)
                    {
                        systemTableName.enabled(true);
                        systemTableName.mandatory(true);
                    }
                    else
                    {
                        // clear and disable system table name stringEdit control
                        systemTableName.enabled(false);
                        systemTableName.mandatory(false);
                    }
                }
            }
        }

        return wizardClass.parmTCType();
    }

]]></Source>
