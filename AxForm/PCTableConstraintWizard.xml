			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    public boolean canClose()
    {
        Args args = new Args();

        boolean ret;

        ret = super();

        if (ret && !this.closedOk())
        {
            if (wizardClass)
            {
                wizardClass.deleteTableConstraint();
            }
        }

        if (ret && this.closedOk() && wizardClass)
        {
            if (wizardClass.parmTCType() == PCTableConstraintType::UserDefined)
            {
                args.record(wizardClass.getTableConstraint());

                new MenuFunction(menuitemActionStr(PCTableConstraintUserDefinedContent), MenuItemType::Action).run(args);
            }
        }

        return ret;
    }

]]></Source>
