			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        super();

        metadataLookup = PCMetadataLookup::construct();

        if (element.args().caller())
        {
            wizardClass = element.args().caller();
            wizardClass.parmSystemColumnDS(systemColumn_ds);
            wizardClass.parmUserColumnDS(userDefinedColumn_ds);
        }
        else
        {
            PCTableConstraintWizard::main(new Args());
            element.closeCancel();
        }
    }

]]></Source>
