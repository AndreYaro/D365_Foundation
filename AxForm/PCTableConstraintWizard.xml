        if (wizardClass.parmTCType() == PCTableConstraintType::System)
        {
            systemTableName.enabled(true);
            systemTableName.mandatory(true);
        }
        else
        {
            // clear and disable system table name stringEdit control
            systemTableName.enabled(false);
            systemTableName.mandatory(false);
        }
    }

]]></Source>
