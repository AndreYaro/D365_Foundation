					<Method>
						<Name>lastJournals</Name>
						<Source><![CDATA[
        void  lastJournals()
        {
            PurchConfirmationRequestJour    lastConfirmationRequestJournal;
            PurchConfirmationRequestJour    lastBindingConfirmationRequestJournal;
            container                       lastJournals = purchTable.lastJournals();

            [lastPurchOrder,     lastPurchOrderDate]     = conpeek(lastJournals, PurchTableType::posPurchaseOrder());
            [lastReceiptsList,   lastReceiptsListDate]   = conpeek(lastJournals, PurchTableType::posReceiptsList());
            [lastProductReceipt, lastProductReceiptDate] = conpeek(lastJournals, PurchTableType::posPackingSlip());
            [lastInvoice,        lastInvoiceDate]        = conpeek(lastJournals, PurchTableType::posInvoice());

            lastConfirmationRequestJournal = purchTable.lastConfirmationRequestJournal();
            lastConfirmationRequest        = lastConfirmationRequestJournal.DocumentNumber;
            lastConfirmationRequestDate    = lastConfirmationRequestJournal.DocumentDate;

            lastBindingConfirmationRequestJournal = purchTable.lastBindingConfirmationRequestJournal();
            lastBindingConfirmationRequest        = lastBindingConfirmationRequestJournal.DocumentNumber;
            lastBindingConfirmationRequestDate    = lastBindingConfirmationRequestJournal.DocumentDate;

            // <GEERU>
            if (SysCountryRegionCode::isLegalEntityInCountryRegion([#isoRU]))
            {
                [lastInvoice4Paym,  lastInvoice4PaymDate] = conPeek(lastJournals, PurchTableType::posInvoice4Paym_RU());
                [lastFacture,       lastFactureDate]      = conPeek(lastJournals, PurchTableType::posFacture_RU());
            }
            // </GEERU>
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;

            this.lastJournals();

            ret = super();

            return ret;
        }

]]></Source>
