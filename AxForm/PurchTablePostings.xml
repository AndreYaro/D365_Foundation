			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (!element.args().dataset())
        {
            throw error(strfmt("@SYS22678",element.name()));
        }

        purchtable_ds.linkActive();

        this.updateDesignConsInvoice_JP();
    }

]]></Source>
