			<Method>
				<Name>validateInventLocation</Name>
				<Source><![CDATA[
    private boolean validateInventLocation(InventLocationId locationId)
    {
        boolean ret = true;

        if (InventLocation::find(locationId).InventLocationType == InventLocationType::Quarantine)
        {
            warning(strFmt("@SYS76498", "@SYS58417", "@GLS108111"));
            ret = false;
        }

        return ret;
    }

]]></Source>
