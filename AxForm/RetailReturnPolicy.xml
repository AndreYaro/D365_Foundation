    /// <summary>
    /// Creates an instance of the <c>NumberSeqFormHandler</c> class for the retail policy number identifier
    /// number sequence.
    /// </summary>
    /// <returns>
    /// An instance of the <c>NumberSeqFormHandler</c> class for the return policy number identifier
    /// sequence.
    /// </returns>
    public NumberSeqFormHandler numberSeqFormHandler()
    {
        if (!numberSeqFormHandler)
        {
            numberSeqFormHandler = NumberSeqFormHandler::newForm(
                    RetailParameters::numRefRetailReturnPolicyNumber().NumberSequenceId,
                    element,
                    RetailReturnPolicy_DS,
                    fieldnum(RetailReturnPolicy, PolicyNumber));
        }

        return numberSeqFormHandler;
    }

]]></Source>
