			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        this.parmActiveConfigurationType(VendFieldMetadataGroupType::VendProspectiveVendorRegistration);
        VendRequestProfileHelper::ensureHasDefaultProfile(this.parmActiveConfigurationType());

        countriesRegionsListPanel = SysListPanel_VendRequestProfileCountry::newForm(element, element.controlId(formControlStr(VendProspectiveVendorRegistrationConfig, CountriesRegions)));
        
        super();
     
        requestType.text("@SYS133943");

        countriesRegionsListPanel.init();
        
        vendRequestProfile_Q.dataSourceTable(tableNum(VendRequestProfile))
			.addRange(fieldNum(VendRequestProfile, ProfileType))
            .status(RangeStatus::Hidden);

        this.setControlState();

        tmpSysTableField_Q.dataSourceTable(tableNum(VendFieldMetadata)).addLink(fieldNum(TmpSysTableField, FieldId), fieldNum(VendFieldMetadata, Field));
        tmpSysTableField_Q.dataSourceTable(tableNum(VendFieldMetadata)).addLink(fieldNum(TmpSysTableField, DictTableId), fieldNum(VendFieldMetadata, FieldTable));
        tmpSysTableField_Q.dataSourceTable(tableNum(VendFieldMetadata)).addRange(fieldNum(VendFieldMetadata, MetadataGroup));

        VendRequestProfileHelper::allowedFieldsForAllTypes(vendFieldMetaDataTmp);
        VendRequestProfileHelper::addNewProfileFields(vendFieldMetaDataTmp);
    }

]]></Source>
			</Method>
			<Method>
				<Name>setControlState</Name>
				<Source><![CDATA[
    private void setControlState()
    {
        vendRequestProfile_DS.query().dataSourceTable(tableNum(VendRequestProfile)).findRange(fieldNum(VendRequestProfile, ProfileType)).value(queryValue(configType));
        vendRequestProfile_DS.executeQuery();

        tabContent.visible(VendRequestProfileHelper::supportsContent(this.parmActiveConfigurationType()));

        vendRequestProfile_SubmittedText.visible(VendRequestProfileHelper::supportsSubmittedText(this.parmActiveConfigurationType()));

        tabFields.visible(VendRequestProfileHelper::supportsFields(this.parmActiveConfigurationType()));

        requireCategory.visible(VendRequestProfileHelper::supportsRequireCategory(this.parmActiveConfigurationType()));

        showBusinessInformation.visible(VendRequestProfileHelper::supportsShowBusinessInformation(this.parmActiveConfigurationType()));

        tabQuestionnaire.visible(VendRequestProfileHelper::supportsQuestionnaire(this.parmActiveConfigurationType()));
    }

]]></Source>
