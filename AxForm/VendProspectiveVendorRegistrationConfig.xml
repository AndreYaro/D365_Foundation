					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret = super();

            QueryBuildRange range = tmpSysTableField_Q.dataSourceTable(tableNum(VendFieldMetadata)).findRange(fieldNum(VendFieldMetadata, MetadataGroup));
            range.value(queryValue(vendRequestProfile.MetadataGroup));

            TmpSysTableField tempData;

            if (vendRequestProfile.RecId)
            {
                insert_recordset tempData(
                    DictTableId,
                    FieldId,
                    FieldName,
                    FieldLabel)
                select
                    FieldTable,
                    Field,
                    FieldName,
                    FieldLabel
                from vendFieldMetaDataTmp
                    where vendFieldMetaDataTmp.profileType == vendRequestProfile.ProfileType;
            }

            tmpSysTableField.setTmpData(tempData);
            tmpSysTableField_DS.research();

            countriesRegionsListPanel.parmVendRequestProfileRecId(vendRequestProfile.RecId);
            countriesRegionsListPanel.fill();

            disabledCountriesRegionsMessage.visible(vendRequestProfile.IsDefault);

            element.setEnabled_AgreementText();
            element.setEnabled_AcceptAgreementText();

            copyConfigurationButton.enabled(vendRequestProfile.RecId);

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        public void write()
        {
            boolean isNew = vendRequestProfile.RecId == 0;

            if (isNew)
            {
                vendRequestProfile.MetadataGroup = vendFieldMetadataGroup.RecId;
            }

            super();

            if (isNew)
            {
                ttsbegin;
                VendRequestProfileHelper::createProfileFields(
                    vendRequestProfile.RecId,
                    vendFieldMetaDataTmp,
                    vendRequestProfile.ProfileType,
                    vendRequestProfile.MetadataGroup);
                ttscommit;
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>validateDelete</Name>
						<Source><![CDATA[
        public boolean validateDelete()
        {
            boolean ret;

            if (vendRequestProfile.IsDefault)
            {
                ret = checkFailed("@SCM:ProspectiveVendorRegistrationDeleteDefaultProfileNotAllowed");
            }
            else
            {
                ret = super();
            }

            return ret;
        }

]]></Source>
