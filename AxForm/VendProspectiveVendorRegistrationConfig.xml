			<Method>
				<Name>setSelectedFieldStatus</Name>
				<Source><![CDATA[
    private void setSelectedFieldStatus(VendFieldMetadataStatus _status)
    {
        TmpSysTableField current = tmpSysTableField.data();
        TmpSysTableField selected = (tmpSysTableField_DS.getFirst(true) ? tmpSysTableField_DS.getFirst(true) : tmpSysTableField_DS.cursor()) as TmpSysTableField;

        while (selected)
        {
            VendFieldMetadata::setFieldStatus(vendFieldMetadataGroup.RecId, selected.DictTableId, selected.FieldId, _status);
            selected = tmpSysTableField_DS.getNext() as TmpSysTableField;
        }

        tmpSysTableField_DS.research();
        tmpSysTableField_DS.positionToRecord(current);
    }

]]></Source>
