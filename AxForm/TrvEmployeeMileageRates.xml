			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        HcmWorker   hcmWorker;
        super();

        if ( element.args().record().TableId == hcmWorker.TableId)
        {
            if (element.args().dataset() != tablenum(HcmWorker) || !element.args().record())
            {
                throw error ("@SYS122503");
            }
            else
            {
                hcmWorker = element.args().record();
                worker = hcmWorker.RecId;
                element.design().caption(strfmt("@Expense:Setup_EmployeeMileageRates_Caption", hcmWorker.name()));
            }
        }
    }

]]></Source>
