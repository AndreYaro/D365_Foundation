                /// <summary>
        /// The errorExist method displays the image for an error or for a correct transaction
        /// depending on the results of checkLine method of the TradeBLWILines table.
        /// </summary>
        /// <param name="_lines">
        /// The _lines parameter contains a record of the TradeBLWILines tables for which a image needs to be displayed.
        /// </param>
        /// <returns>
        /// A image is returned.
        /// </returns>
        display ImageRes errorExist(TradeBLWILines _lines)
        {
            ImageRes           res = -1;
            #resAppl

            if (! tradeBLWILines.checkLine(_lines))
            {
                res = imageListAppl.image(#ImageError);
            }
            else
            {
                res = imageListAppl.image(#Image_OK);
            }

            return res;
        }

]]></Source>
