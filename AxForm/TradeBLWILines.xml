					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        /// <summary>
        /// If the record currently selected exists, its data should be cached so it can be reused when splitting
        /// the transaction.
        /// </summary>
        /// <returns>
        /// The active method returns an integer.
        /// </returns>
        public int active()
        {
            int ret;

            ret = super();

            if (tradeBLWILines.RecId)
            {
              recordSelected.data(tradeBLWILines);
            }
            else
            {
              recordSelected.clear();
            }

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>initValue</Name>
						<Source><![CDATA[
        /// <summary>
        /// For each newly created record, the newly created record needs to be initialised.
        /// </summary>
        public void initValue()
        {
            super();

            if (recordSelected)
            {
              tradeBLWILines.data(recordSelected);
              tradeBLWILines.AutoGenerated = NoYes::No;
              tradeBLWILines.AmountCur = 0;
              tradeBLWILines.AmountMst = 0;
              tradeBLWILines.RecId = 0;
            }
        }

]]></Source>
