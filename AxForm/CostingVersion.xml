					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            QueryBuildRange range;

            super();

            if (openedFromPeriodic)
            {
                range = this.query().dataSourceTable(tableNum(CostingVersion)).addRange(fieldNum(CostingVersion, Blocked));
                range.value(queryValue(NoYes::No));
                range.status(RangeStatus::Hidden);

                range = this.query().dataSourceTable(tableNum(CostingVersion)).addRange(fieldNum(CostingVersion, CostingType));
                range.value(queryRange(CostingVersionCostingType::Normal, CostingVersionCostingType::Standard));
                range.status(RangeStatus::Hidden);
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int                     ret;
            InventStdCostConv       inventStdCostConv;

            ret = super();

            element.enableButtons();
            element.enableFields();

            if (!openedFromPeriodic)
            {
                costingVersion_ds.allowEdit(true);

                if (costingVersion.RecId)
                {
                    costingVersion_CostingType.allowEdit(false);
                }
                else
                {
                    if (costingVersion_CostingType.allowEdit() == false)
                    {
                        costingVersion_CostingType.allowEdit(true);
                    }
                }

                if (costingVersion.CostingType == CostingVersionCostingType::Conversion)
                {
                    if (element.args().dataset() == tableNum(InventStdCostConv))
                    {
                        inventStdCostConv = element.args().record();
                        if (inventStdCostConv.Status != InventStdCostConvStatus::Planned)
                        {
                            costingVersion_ds.allowEdit(false);
                            costingVersion_CostingType.allowEdit(false);
                        }
                    }
                    else
                    {
                        bomCalc.enabled(false);
                        price.enabled(false);
                        costingVersion_ds.allowEdit(false);
                        costingVersion_CostingType.allowEdit(false);
                    }
                }
            }

            return ret;
        }

]]></Source>
