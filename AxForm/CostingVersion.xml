				<Source><![CDATA[
    public void init()
    {
        openedFromPeriodic = element.args().menuItemName() == menuitemDisplayStr(CostingVersionPeriodic);

        super();

        costingVersion_ds.allowCreate(!openedFromPeriodic && element.args().dataset() != tableNum(InventStdCostConv));
        costingVersion_ds.allowEdit(!openedFromPeriodic);
        costingVersion_ds.allowDelete(!openedFromPeriodic && element.args().dataset() != tableNum(InventStdCostConv));

        recordingTabPage.visible(!openedFromPeriodic);
        calculationTabPage.visible(!openedFromPeriodic);

        element.design().caption(openedFromPeriodic ? "@SYS121353" : "@SYS121352");

        element.enableButtons();
    }

]]></Source>
