    void enableFields()
    {
        boolean versionIsNotStandard = costingVersion.CostingType != CostingVersionCostingType::Standard;

        costingVersion_ds.object(fieldNum(CostingVersion, RecordRestrict)).allowEdit(versionIsNotStandard);
        costingVersion_ds.object(fieldNum(CostingVersion, PriceAllocateMarkup)).allowEdit(versionIsNotStandard);
        costingVersion_ds.object(fieldNum(CostingVersion, bomCalcRestrict)).allowEdit(versionIsNotStandard);
        costingVersion_ds.object(fieldNum(CostingVersion, bomCostPriceModel)).allowEdit(versionIsNotStandard);
        costingVersion_ds.object(fieldNum(CostingVersion, bomCalcExplosionMode)).allowEdit(versionIsNotStandard);
        costingVersion_ds.object(fieldNum(CostingVersion, PurchPriceModel)).allowEdit(versionIsNotStandard);

        if (!versionIsNotStandard)
        {
            costingVersion.initFromCostingType();
        }

        this.enableFallBackFields();
    }

]]></Source>
