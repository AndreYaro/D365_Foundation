							<Method>
								<Name>performFormLookup</Name>
								<Source><![CDATA[
            public void performFormLookup(xFormRun _form, FormControl _formControl)
            {
                FormDataSource  formDataSource;
                Query           formQuery;
                str             queryValue;

                formDataSource  = _form.objectSet();
                formQuery       = formDataSource.query();

                SysQuery::findOrCreateRange(formQuery.dataSourceTable(tableNum(CostingVersion)),
                                                                      fieldNum(CostingVersion, Blocked)).value(queryValue(NoYes::No));

                switch (costingVersion.CostingType)
                {
                    case CostingVersionCostingType::Standard,
                        CostingVersionCostingType::Conversion:
                        queryValue = queryValue(CostingVersionCostingType::Standard);
                        break;

                    default:
                        queryValue = queryRange(CostingVersionCostingType::Normal, CostingVersionCostingType::Standard);
                }

                SysQuery::findOrCreateRange(formQuery.dataSourceTable(tableNum(CostingVersion)),
                                                                      fieldNum(CostingVersion, CostingType)).value(queryValue);

                super(_form, _formControl);
            }

]]></Source>
