						<Source><![CDATA[
        void clicked()
        {
            FormRun formRun;
            Args    args;

            if (strlen(smmEmailGroups.GroupId) == 0 || strlen(smmEmailGroups.CategoryId) == 0)
            {
                // Fill in email group and category prior to open the email members form
                info("@SYS80717");
                return;
            }

            super();

            args = new Args();
            args.name(formstr(smmEmailMembers));
            args.caller(element);
            args.record(smmEmailGroups);

            formRun = classfactory.formRunClass(args);

            if (formRun)
            {
                formRun.init();
                formRun.run();
                formRun.detach();
            }
        }

]]></Source>
