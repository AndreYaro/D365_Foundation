			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        FormStringControl callingControl;
        boolean haveFilter = false;
        str searchText;
        int length;
        const str wildCard = '*';

        callingControl = element.args().caller();
        searchText = element.args().lookupValue();
        length = strLen(searchText);

        if ((length > 0) && (strFind(searchText, wildCard, 1, length) != 0) && callingControl.hasChanged())
        {
            // only filter if there is a wildcard
            vendLedger_ds.autoSearch(false);

            haveFilter = true;
        }

        super();

        if (haveFilter)
        {
            vendLedger_ds.queryRun(new SysQueryRun(vendLedger_q));

            vendLedger_PostingProfile.filter(searchText);
        }
    }

]]></Source>
