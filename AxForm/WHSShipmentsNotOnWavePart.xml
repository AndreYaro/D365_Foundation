			<Control>
				<Name>AddSelectedShipmentsToNewWave</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            FormRun createWaveDlg = new MenuFunction(menuItemDisplayStr(WHSWaveTableCreateNew), MenuItemType::Display).create(new Args(element)) as FormRun;
            createWaveDlg.run();
            createWaveDlg.wait();

            if (createWaveDlg.closedOk())
            {
                var waveTableProvider = createWaveDlg as WHSIWaveTableProvider;
                var newWaveTable = waveTableProvider.getWaveTable();

                ttsbegin;
                for (var shipmentTable = WHSShipmentTable_DS.getFirst(true) ? WHSShipmentTable_DS.getFirst(true) : WHSShipmentTable_DS.cursor();
                    shipmentTable;
                    shipmentTable = WHSShipmentTable_DS.getNext())
                {
                    WHSWaveLine::createFromWaveIdAndShipment(newWaveTable.WaveId, shipmentTable, true);
                }
                ttscommit;

                // Force refresh 2 tiles that are impacted by the above actions, so we immediately see the updated counts.
                TileDataService::forceRefresh(identifierStr(WHSShipmentsNotOnWave), SysDataServiceFormHelper::newFromCaller(element.args().caller()));
                TileDataService::forceRefresh(identifierStr(WHSUnprocessedWaves), SysDataServiceFormHelper::newFromCaller(element.args().caller()));

                new MenuFunction(menuItemDisplayStr(WHSWaveTable), MenuItemType::Display).run(new Args(newWaveTable));

                super();
            }
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
