					<Method>
						<Name>relationStringLookup</Name>
						<Source><![CDATA[
        public void relationStringLookup(FormStringControl _callingControl)
        {
            Query                       query;
            QueryBuildDataSource        mainQueryBuildDataSource;
            SysTableLookup              tableLookup;

            switch (FSHReplenishmentLines.Type)
            {
                case RetailReplenishmentRuleType::GroupType:
                    RetailReplenishmentTreeLookup::lookup(_callingControl, FSHReplenishmentLines.RelationId);
                    break;
                case RetailReplenishmentRuleType::LocationType:
                    query = new Query();
                    tableLookup = SysTableLookup::newParameters(tableNum(RetailStoreTable), _callingControl);
                    tableLookup.addLookupfield(fieldNum(RetailStoreTable, StoreNumber));
                    tableLookup.addLookupMethod(tableMethodStr(RetailStoreTable, UnitName));

                    mainQueryBuildDataSource = query.addDataSource(tableNum(RetailStoreTable));
                    tableLookup.parmQuery(query);
                    tableLookup.performFormLookup();
                    break;
                default:
                    // in case we have some newly added replenishment rule types
                    error("@RET260033");
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>referenceName</Name>
						<Source><![CDATA[
        edit Name referenceName(boolean set, RetailReplenishmentRuleLines _fshReplenishmentLines, Name _relationId)
        {
            RetailReplenishmentRuleType type = _fshReplenishmentLines.Type;
            RetailStoreTable            storeTable;
            OMHierarchyRelationship     relationship;
            OMInternalOrganization  org;

            if (set)
            {
                switch (type)
                {
                    case RetailReplenishmentRuleType::GroupType:
                        FSHReplenishmentLines.RelationId = str2int64(_relationId);
                        break;
                    case RetailReplenishmentRuleType::LocationType:
                        select firstonly storeTable
                            where storeTable.StoreNumber == _relationId;

                        FSHReplenishmentLines.RelationId = storeTable.RecId;
                        break;
                    default:
                        error("@RET260033");
                        break;
                }

                FSHReplenishmentLines.modifiedField(fieldNum(RetailReplenishmentRuleLines, RelationId));

                return _relationId;
            }
            else
            {
                switch (type)
                {
                    case RetailReplenishmentRuleType::GroupType:
                        // return name of the organization from the selected relationship
                        select firstonly RecId, ChildOrganization from relationship
                        join org
                            where relationship.RecId == _fshReplenishmentLines.RelationId &&
                            org.RecId == relationship.ChildOrganization;

                        return org.Name;

                    case RetailReplenishmentRuleType::LocationType:
                        select firstonly RecId, storeNumber from storeTable
                            where storeTable.RecId == _fshReplenishmentLines.RelationId;

                        return storeTable.StoreNumber;

                    default:
                        return '';
                }
            }
        }

]]></Source>
