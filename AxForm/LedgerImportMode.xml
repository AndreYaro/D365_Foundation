			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        List availableFormats = new DictClass(classnum(LedgerInAccountStatement)).extendedBy();
        ListEnumerator availableFormatsEnumerator = availableFormats.getEnumerator();
        DictClass curClass;
        LedgerInAccountStatement ledgerInAccountStatement;

        while (availableFormatsEnumerator.moveNext())
        {
            curClass = new DictClass(availableFormatsEnumerator.current());

            if (curClass)
            {
                ledgerInAccountStatement = curClass.makeObject();

                if (ledgerInAccountStatement)
                {
                    tmpIdRef.Name = curClass.name();
                    tmpIdRef.HelpTxt = ledgerInAccountStatement.interfaceName();
                    tmpIdRef.insert();
                }
            }
        }

        Posting_InterCoAccount.parmPostingType(LedgerPostingType::None);
    }

]]></Source>
			</Method>
			<Method>
				<Name>lookUpActionClassName</Name>
				<Source><![CDATA[
    public void lookUpActionClassName()
    {
        SysTableLookup sysTableLookup = SysTableLookup::newParameters(tableNum(TmpIdRef), ActionClassName);
        Query query = new Query();
        QueryBuildDataSource qbds;

        sysTableLookup.addLookupfield(fieldNum(TmpIdRef, HelpTxt));
        sysTableLookup.setLabel("@SYS7576");
        sysTableLookup.addLookupfield(fieldNum(TmpIdRef, Name), true);
        sysTableLookup.setLabel("@SYS24724");
        sysTableLookup.parmTmpBuffer(tmpIdRef);
        qbds = query.addDataSource(tableNum(TmpIdRef));
        qbds.addSortField(fieldNum(TmpIdRef, HelpTxt));
        sysTableLookup.parmQuery(query);
        sysTableLookup.performFormLookup();
        LedgerImportMode_ds.reread();
    }

]]></Source>
