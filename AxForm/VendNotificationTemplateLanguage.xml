        /// <summary>
        /// FIlters the query to return records that are associated with the current notification template.
        /// </summary>
        public void init()
        {
            QueryBuildRange range;

            super();

            if (_vendNotificationTemplate.RecId)
            {
                range = this.query().dataSourceTable(tableNum(VendNotificationTemplateTranslation)).addRange(fieldNum(VendNotificationTemplateTranslation, VendNotificationTemplate));
                range.status(RangeStatus::Hidden);
                range.value(SysQuery::value(_vendNotificationTemplate.RecId));
            }
        }

