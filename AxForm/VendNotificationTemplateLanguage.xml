    /// <summary>
    /// Initializes the <c>VendNotificationTemplate</c> record with the dataset of the calling form.
    /// </summary>
    /// <exception cref="Exception::Error">
    /// Throws an error if the calling dataset is empty.
    /// </exception>
    public void init()
    {
        if (!element.args().dataset())
        {
            throw error(Error::missingRecord(this.name()));
        }

        if (tableNum(VendNotificationTemplate) == element.args().dataset())
        {
            _vendNotificationTemplate = element.args().record();
        }

        super();

        if (element.args().menuItemName() == menuitemDisplayStr(VendNotificationTemplateLanguageView))
        {
            vendNotificationTemplateTranslation_ds.allowEdit(false);
            vendNotificationTemplateTranslation_ds.allowCreate(false);
            vendNotificationTemplateTranslation_ds.allowDelete(false);
        }
    }

