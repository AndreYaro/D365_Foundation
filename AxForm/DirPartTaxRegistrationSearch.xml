			<Method>
				<Name>setMenuFunctionOptionsByRoleType</Name>
				<Source><![CDATA[
    /// <summary>
    /// Sets <c>MenuItemName</c> and accountNumRecord for different values of emum <c>DirPartyRoleType</c>.
    /// </summary>
    protected void setMenuFunctionOptionsByRoleType()
    {
        switch (DirPartyView.RoleType)
        {
            case DirPartyRoleType::Customer:
                accountNumMenuItemName = menuitemdisplaystr(CustTable);
                accountNumRecord = CustTable::findByCompany(DirPartyView.DataArea, DirPartyView.Accountnum);
                break;
            case DirPartyRoleType::Vendor:
                accountNumMenuItemName = menuitemdisplaystr(VendTable);
                accountNumRecord = VendTable::findByCompany(DirPartyView.DataArea, DirPartyView.Accountnum);
                break;
            case DirPartyRoleType::Worker:
                accountNumMenuItemName = menuitemdisplaystr(HcmWorker);
                accountNumRecord = HCMWorker::findByPerson(DirPartyView.Party);
                break;
            case DirPartyRoleType::Applicant:
                accountNumMenuItemName = menuitemdisplaystr(HcmApplicant);
                accountNumRecord = HcmApplicant::findByPerson(DirPartyView.Party);
                break;
            case DirPartyRoleType::BusRel:
                accountNumMenuItemName = menuitemdisplaystr(smmBusRelTable);
                accountNumRecord = smmBusRelTable::findByParty(DirPartyView.Party);
                break;
            case DirPartyRoleType::Competitor:
                accountNumMenuItemName = menuitemdisplaystr(smmQuotationCompetitorGroup);
                accountNumRecord = smmQuotationCompetitorGroup::find(DirPartyView.Party);
                break;
            case DirPartyRoleType::ContactPerson:
                accountNumMenuItemName = menuitemdisplaystr(ContactPerson);
                accountNumRecord = ContactPerson::findByPartyRecId(DirPartyView.Party);
                break;
            case DirPartyRoleType::DisallowedVendor:
                accountNumMenuItemName = menuitemdisplaystr(VendRequestDisallowedVendor);
                accountNumRecord = VendRequestDisallowedVendor::findByParty(DirPartyView.Party);
                break;
            case DirPartyRoleType::ProspectiveVendor:
                accountNumMenuItemName = menuitemdisplaystr(VendRequestNVAForm);
                changeCompany(DirPartyView.DataArea)
                {
                    accountNumRecord = VendRequest::findByVendorParty(DirPartyView.Party);
                }
                break;
            default:
                accountNumMenuItemName = '';
        }
    }

]]></Source>
