        public void create(boolean _append = false)
        {
            TmpTaxRegistrationSearchCriteria locTmpTaxRegistrationSearchCriteria;

            locTmpTaxRegistrationSearchCriteria.setTmpData(TmpTaxRegistrationSearchCriteria_ds.cursor());

            select firstOnly count(RecId) from locTmpTaxRegistrationSearchCriteria;

            if (locTmpTaxRegistrationSearchCriteria.RecId < MaxCriteriaQty)
            {
                super(_append);
            }
            else
            {
                warning(strFmt("@SYS4081671", MaxCriteriaQty));
            }
        }

