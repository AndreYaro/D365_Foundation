			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (element.args().parmEnumType() == enumNum(DirPartyRoleType))
        {
            RoleType.selection(element.args().parmEnum());
        }
       
        if (RoleType.selection() != enum2Int(DirPartyRoleType::None))
        {
            qbrRoleType.value(queryValue(RoleType.selection()));
        }
        else
        {
            qbrRoleType.value(SysQuery::valueUnlimited());
        }
       
        this.setFilter();
    }

]]></Source>
			</Method>
			<Method>
				<Name>setFilter</Name>
				<Source><![CDATA[
    public void setFilter()
    {
        TmpTaxRegistrationSearchCriteria locTmpTaxRegistrationSearchCriteria;

        Query query = new Query();

        QueryBuildDataSource qbdsDirPartyTable = query.addDataSource(tableNum(DirPartyTable));

        locTmpTaxRegistrationSearchCriteria.setTmpData(TmpTaxRegistrationSearchCriteria_ds.cursor());

        select firstOnly RecId from locTmpTaxRegistrationSearchCriteria;

        if (locTmpTaxRegistrationSearchCriteria.RecId)
        {
            while select locTmpTaxRegistrationSearchCriteria
                where locTmpTaxRegistrationSearchCriteria.RegistrationNumber != ''
            {
                QueryBuildDataSource qbdsDirPartyLocation = qbdsDirPartyTable.addDataSource(tableNum(DirPartyLocation));
                qbdsDirPartyLocation.joinMode(JoinMode::ExistsJoin);
                qbdsDirPartyLocation.addLink(fieldNum(DirPartyTable, RecId), fieldNum(DirPartyLocation, Party));

                QueryBuildDataSource qbds = qbdsDirPartyLocation.addDataSource(tableNum(TaxRegistration));
                qbds.addLink(fieldNum(DirPartyLocation, RecId), fieldNum(TaxRegistration, DirPartyLocation));
                qbds.joinMode(JoinMode::ExistsJoin);
                qbds.addRange(fieldNum(TaxRegistration, RegistrationNumber)).value(locTmpTaxRegistrationSearchCriteria.RegistrationNumber);

                qbds = qbds.addDataSource(tableNum(TaxRegistrationTypeApplicabilityRule));
                qbds.relations(true);
                qbds.joinMode(JoinMode::ExistsJoin);
                qbds.addRange(fieldNum(TaxRegistrationTypeApplicabilityRule, TaxRegistrationType)).value(queryValue(locTmpTaxRegistrationSearchCriteria.TaxRegistrationType));

                if (locTmpTaxRegistrationSearchCriteria.CountryRegionId)
                {
                    qbds.addRange(fieldNum(TaxRegistrationTypeApplicabilityRule, CountryRegionId)).value(queryValue(locTmpTaxRegistrationSearchCriteria.CountryRegionId));
                }
            }
        }
        else
        {
            qbdsDirPartyTable.addRange(fieldNum(DirPartyTable, RecId)).value(queryValue(0));
        }

        if (SearchCurrentCompany.value() || RoleType.selection() != enum2int(DirPartyRoleType::None))
        {
            qbdsDirPartyTable = query.dataSourceTable(tableNum(DirPartyTable));
            QueryBuildDataSource qbds = qbdsDirPartyTable.addDataSource(tableNum(DirPartyView));
            qbds.addLink(fieldNum(DirPartyTable, RecId), fieldNum(DirPartyView, Party));
            qbds.joinMode(JoinMode::ExistsJoin);
            qbds.addRange(fieldNum(DirPartyView, DataArea)).value(qbrDataArea.value());
            qbds.addRange(fieldNum(DirPartyView, RoleType)).value(qbrRoleType.value());
        }

        DirPartyTable_ds.query(query);

        DirPartyTable_ds.executeQuery();
    }

]]></Source>
			</Method>
