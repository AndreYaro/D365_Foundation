			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates the form design
    /// </summary>
    public void updateDesign()
    {
        FiscalOrganization_BR fiscalOrganization = FiscalOrganization_BR::findByFiscalEstablishmentId(FBBookingPeriod_BR.FiscalEstablishmentId, FBBookingPeriod_BR.FiscalEstablishmentDataArea);
        boolean isRootFE = fiscalOrganization ? true : FiscalOrganizationChangeHistory_BR::isRootByBookingPeriod(FBBookingPeriod_BR);
        boolean isCPRB = fiscalOrganization.IsCPRB;

        FBTaxAssessmentPISCOFINSListPage_BR.enabled(isRootFE);
        FBTaxAssessmentINSSCPRBListPage_BR.enabled(isRootFE && isCPRB); // PIS and COFINS tax assessment are only enabled in case of root fiscal establishment.
        FBSpedReinfEvent_R2010_BR.enabled(isRootFE);
        FBSpedReinfEvent_R2020_BR.enabled(isRootFE);
        FBSpedReinfEvent_R2055_BR.enabled(isRootFE);
        FBSpedReinfEvent_R4010_BR.enabled(isRootFE);
        FBSpedReinfEvent_R4020_BR.enabled(isRootFE);
        FBSpedReinfEvent_R4040_BR.enabled(isRootFE);
        FBSpedReinfEvent_R4080_BR.enabled(isRootFE);
        FBSpedReinfEvent_R4098_BR.enabled(isRootFE);
        FBSpedReinfEvent_R4099_BR.enabled(isRootFE);

        boolean syncProductionOrder = FBSpedFiscalFileParameters_BR::find(FBBookingPeriod_BR.FiscalEstablishmentId, FBBookingPeriod_BR.FiscalEstablishmentDataArea).SyncProductionOrder;
        ProductionOrder.visible(syncProductionOrder);
    }

]]></Source>
			</Method>
