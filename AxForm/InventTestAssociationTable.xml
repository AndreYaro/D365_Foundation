			<Method>
				<Name>shouldTestBeExecutedBefore</Name>
				<Source><![CDATA[
    /// <summary>
    /// Checks if the test execution should be done before.
    /// </summary>
    /// <returns>
    /// true, if the test execution should be done before; otherwise, false.
    /// </returns>
    protected boolean shouldTestBeExecutedBefore()
    {
        return ((inventTestAssociationTable.DocumentType == InventTestDocumentStatus::End
                 && inventTestAssociationTable.OrderType    == InventTestReferenceType::Quarantine)
                || (inventTestAssociationTable.OrderType    == InventTestReferenceType::Sales));
    }

]]></Source>
