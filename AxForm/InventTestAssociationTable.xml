			<Method>
				<Name>initRecord</Name>
				<Source><![CDATA[
    public void initRecord()
    {
        FormDataObject  itemRelationObject      = inventTestAssociationTable_ds.object(fieldNum(InventTestAssociationTable, ItemRelation));
        FormDataObject  accountCodeObject       = inventTestAssociationTable_ds.object(fieldNum(InventTestAssociationTable, AccountCode));
        FormDataObject  accountRelationObject   = inventTestAssociationTable_ds.object(fieldNum(InventTestAssociationTable, AccountRelation));
        FormDataObject  wrkCtrRelationObject    = inventTestAssociationTable_ds.object(fieldNum(InventTestAssociationTable, WrkCtrRelation));
        FormDataObject  wrkCtrCodeObject        = inventTestAssociationTable_ds.object(fieldNum(InventTestAssociationTable, WrkCtrCode));        
        FormControl     activeControl           = element.selectedControl(); // keep focus on same control

        if (inventTestAssociationTable.ItemCode == TableGroupAll::All)
        {
            itemRelationObject.skip(true);
            itemRelationObject.enabled(false);
            inventTestAssociationTable.ItemRelation = '';
        }
        else
        {
            itemRelationObject.skip(false);
            itemRelationObject.enabled(true);
        }

        if (this.isAccountCodeSupported())
        {
            accountCodeObject.skip(false);
            accountCodeObject.enabled(true);
        }
        else
        {
            accountCodeObject.skip(true);
            accountCodeObject.enabled(false);
            inventTestAssociationTable.AccountCode = TableGroupAll::All;
        }

        if (inventTestAssociationTable.AccountCode == TableGroupAll::All)
        {
            accountRelationObject.skip(true);
            accountRelationObject.enabled(false);
            inventTestAssociationTable.AccountRelation = '';
        }
        else
        {
            accountRelationObject.skip(false);
            accountRelationObject.enabled(true);
        }

        if (this.isWrkCtrCodeSupported())
        {
            wrkCtrCodeObject.skip(false);
            wrkCtrCodeObject.enabled(true);
        }
        else
        {
            wrkCtrCodeObject.skip(true);
            wrkCtrCodeObject.enabled(false);
            inventTestAssociationTable.WrkCtrCode = TableGroupAll::All;
        }

        if (inventTestAssociationTable.WrkCtrCode == TableGroupAll::All)
        {
            wrkCtrRelationObject.skip(true);
            wrkCtrRelationObject.enabled(false);
            inventTestAssociationTable.WrkCtrRelation = '';
        }
        else
        {
            wrkCtrRelationObject.skip(false);
            wrkCtrRelationObject.enabled(true);
        }

        element.initializeDocumentTypeComboBox();

        if (inventTestAssociationTable.OrderType == InventTestReferenceType::Inventory)
        {
            qualityOrderGeneration_TestExecution.enabled(false);
        }
        else
        {
            qualityOrderGeneration_TestExecution.enabled(true);
        }

        this.setTestExecution();

        element.allowEditShowInfoOnCreate();
        inventTestAssociationTable_ds.object(fieldNum(InventTestAssociationTable, QuarantineOnFailure)).allowEdit(inventTestAssociationTable.allowEditQuarantineOnFailure());

        if (activeControl)
        {
            element.selectControl(activeControl);
        }
    }

]]></Source>
