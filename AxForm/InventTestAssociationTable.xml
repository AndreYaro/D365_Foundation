				<Name>initializeDocumentTypeComboBox</Name>
				<Source><![CDATA[
    /// <summary>
    /// Populates the DocumentType combo box with elements appropriate for current test association.
    /// </summary>
    public void initializeDocumentTypeComboBox()
    {
        int         selected;

        //Check if user has security access
        if (!hasFieldAccess(tableNum(InventTestAssociationTable),
                            fieldNum(InventTestAssociationTable, DocumentType),
                            AccessType::View))
        {
            return;
        }

        documentType.clear();

        int numberAdded = 0;
        DictEnum dictEnum = DictEnum::newFromName(enumStr(InventTestDocumentStatus));
        Enumerator enum = dictEnum.getEnumerator();

        while (enum.moveNext())
        {
            InventTestDocumentStatus value = enum.current();

            if (InventTestAssociationTable::isOrderTypeAndDocumentStatusConsistent(
                    inventTestAssociationTable.OrderType,
                    value))
            {
                documentType.add(dictEnum.value2Label(any2Int(value)));

                if (value == inventTestAssociationTable.DocumentType)
                {
                    selected = numberAdded;
                }

                ++numberAdded;
            }
        }

        documentType.enabled(numberAdded > 1);
        documentType.selection(selected);
        // if the current value of the dropdown is no longer available, we will default to the first value
        if (!selected)
        {
            inventTestAssociationTable.setDocumentType(DocumentType.getText(selected));
        }
    }

]]></Source>
