            public void jumpRef()
            {
                if (!inventTestAssociationTable.AccountRelation
                 &&  inventTestAssociationTable.AccountCode != TableGroupAll::All)
                {
                    Args args = new Args();

                    if (inventTestAssociationTable.AccountCode == TableGroupAll::Table
                     && inventTestAssociationTable.OrderType   != InventTestReferenceType::Purch)
                    {
                        args.name(formStr(CustTable));
                    }
                    else if (inventTestAssociationTable.AccountCode == TableGroupAll::Table
                          && inventTestAssociationTable.OrderType   == InventTestReferenceType::Purch)
                    {
                        args.name(formStr(VendTable));
                    }

                    else if (inventTestAssociationTable.AccountCode == TableGroupAll::GroupId
                          && inventTestAssociationTable.OrderType   != InventTestReferenceType::Purch)
                    {
                        args.name(formStr(CustGroup));
                    }
                    else
                    {
                        args.name(formStr(VendGroup));
                    }

                    FormName name = element.getAccountRelationFormName(inventTestAssociationTable.OrderType, inventTestAssociationTable.AccountCode);
                    if (name)
                    {
                        args.name(name);
                    }

                    FormRun formRun = classfactory.formRunClass(args);
                    formRun.init();
                    formRun.run();
                    formRun.wait();
                }
                else
                {
                    super();
                }
            }

