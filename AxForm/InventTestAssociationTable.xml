			<Method>
				<Name>setTestExecution</Name>
				<Source><![CDATA[
    private void setTestExecution()
    {
        FormDataObject testExecutionObject = inventTestAssociationTable_ds.object(fieldNum(InventTestAssociationTable, TestExecution));

        if (inventTestAssociationTable.DocumentType == InventTestDocumentStatus::Registration)
        {
            inventTestAssociationTable.TestExecution = InventTestExecution::None;
            testExecutionObject.enabled(false);
        }
        else if (this.shouldTestBeExecutedBefore())
        {
            inventTestAssociationTable.TestExecution = InventTestExecution::Before;
            testExecutionObject.enabled(false);
        }
        else
        {
            testExecutionObject.enabled(true);
        }
    }

]]></Source>
