			<Method>
				<Name>editPurchAgreementId</Name>
				<Source><![CDATA[
    edit PurchAgreementId editPurchAgreementId(boolean _set, PurchAgreementId _purchAgreementId)
    {
        if (_set)
        {
            if (_purchAgreementId != purchAgreementHeader.PurchNumberSequence)
            {
                purchAgreementHeader = PurchAgreementHeader::findAgreementId(_purchAgreementId);
            }
        }

        return purchAgreementHeader.PurchNumberSequence;
    }

]]></Source>
