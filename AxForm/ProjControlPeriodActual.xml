				<Source><![CDATA[
[Form]
public final class ProjControlPeriodActual extends FormRun
{
    ProjControlActual               projControlActual;
    ProjControlPeriodTable          projControlPeriodTable;
    boolean isCEIntegEnabled = ProjProjectOperationsIntegrationFeatureHelper::isFeatureEnabled();
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        ProjControlPeriodCostGroup  projControlPeriodCostGroup;
        
        if (! element.args().dataset() == tableNum(ProjControlPeriodCostGroup))
        {
            throw error("@SYS55194");
        }

        projControlPeriodCostGroup  = element.args().record();
        projControlPeriodTable      = projControlPeriodCostGroup.projControlPeriod();

        super();
		
		if (isCEIntegEnabled)
        {
			Transactino_TransId.enableFormRef(false);
		}
        projControlActual = new  ProjControlActual();
        projControlActual.parmProjControlId(projControlPeriodTable.ControlId);
        projControlActual.parmIncludeCurrentPeriod(true);
        if (element.args().parmEnum() == NoYes::Yes)
        {
            projControlActual.parmProjPeriodFrom(dateNull());
        }
        else
        {
            projControlActual.parmUseFromDate(true);
            projControlActual.parmProjPeriodFrom(projControlPeriodTable.PeriodFrom);
        }
        projControlActual.parmProjPeriodTo(projControlPeriodTable.PeriodTo);
        projControlActual.parmProjControlVersion(projControlPeriodTable.Version);
        projControlActual.parmWIPProjId(projControlPeriodTable.ProjId);
        projControlActual.parmIsContractLineUsed(ProjWIPTable::find(projControlPeriodTable.ProjId).IsContractLineUsed);
        projControlActual.parmGetEliminated(true);
        if (element.args().parm() == '1')
        {
            projControlActual.parmIncludeActual(true);
            voucher.enabled(true);
            projTransPosting.enabled(true);
        }
        else if (element.args().parm() == '2')
        {
            projControlActual.parmIncludeCommitted(true);
            voucher.enabled(false);
            projTransPosting.enabled(false);
        }
        projControlActual.run();

        tmpProjControlActual.linkPhysicalTableInstance(projControlActual.tmpProjControlActual());
        tmpProjControlActualCost.linkPhysicalTableInstance(projControlActual.tmpProjControlActualCost());
    }

]]></Source>
			</Method>
