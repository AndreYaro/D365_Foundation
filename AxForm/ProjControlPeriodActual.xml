					<Method>
						<Name>linkActive</Name>
						<Source><![CDATA[
        void linkActive()
        {
            ProjControlPeriodCostGroup  projControlPeriodCostGroup;;

            super();

            if (! element.args().dataset() == tableNum(ProjControlPeriodCostGroup))
            {
                throw error("@SYS55194");
            }

            projControlPeriodCostGroup  = element.args().record();
            if (projControlPeriodTable.RecId != projControlPeriodCostGroup.projControlPeriod().RecId)
            {
                element.close();
            }

            switch (projControlPeriodCostGroup.costType())
            {
                case ProjCostType::Hour:
                    items.visible(false);
                    timeSheet.visible(true);
                    break;

                case ProjCostType::Cost:
                    items.visible(false);
                    timeSheet.visible(false);
                    break;

                case ProjCostType::Item :
                    items.visible(true);
                    timeSheet.visible(false);
                    break;
            }
        }

]]></Source>
