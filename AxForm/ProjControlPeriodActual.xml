        void clicked()
        {
            MenuFunction    menuFunction;
            Args            args = new Args();
            ProjEmplTrans   projEmplTrans;
            ProjCostTrans   projCostTrans;
            ProjItemTrans   projItemTrans;

            super();

            switch (tmpProjControlActual.CostType)
            {
                case 0:
                    projEmplTrans = ProjEmplTrans::find(tmpProjControlActual.TransId);
                    args.record(projEmplTrans);
                    break;

                case 1:
                    projCostTrans = ProjCostTrans::find(tmpProjControlActual.TransId);
                    args.record(projCostTrans);
                    break;

                case 2:
                    projItemTrans = ProjItemTrans::find(tmpProjControlActual.TransId);
                    args.record(projItemTrans);
                    break;
            }
            args.parmEnumType(enumNum(NoYes));
            args.parmEnum(NoYes::Yes);
            menuFunction = new MenuFunction(menuitemDisplayStr(ProjLedgerTransVoucher), MenuItemType::Display);
            menuFunction.run(args);
        }

