								<Name>jumpRef</Name>
								<Source><![CDATA[
            public void jumpRef()
            {
                if (tmpProjControlActual.Type == ProjControlActualCostType::Committed)
                {
                    return;
                }

                Args            args = new Args();
                MenuFunction    menuFunction;

                switch (tmpProjControlActual.CostType)
                {
                    case ProjCostType::Cost:
                        menuFunction = new MenuFunction(menuItemDisplayStr(ProjTransCost), MenuItemType::Display);
                        args.record(ProjCostTrans::find(tmpProjControlActual.TransId));
                        break;

                    case ProjCostType::Hour:
						menuFunction = new MenuFunction(menuItemDisplayStr(ProjTransEmpl), MenuItemType::Display);
                        args.record(ProjEmplTrans::find(tmpProjControlActual.TransId));
                        break;

                    case ProjCostType::Item:
						menuFunction = new MenuFunction(menuItemDisplayStr(ProjTransItem), MenuItemType::Display);
                        args.record(ProjItemTrans::find(tmpProjControlActual.TransId));
                        break;
                }

                args.caller(element);
                menuFunction.run(args);
            }

]]></Source>
