						<Source><![CDATA[
        public void executeQuery()
        {
            QueryBuildDataSource    qbds;
            Query                   query = this.query();

            qbds = query.dataSourceTable(tableNum(ProdTable));

            qbds.clearRanges();
            qbds.clearDynalinks();
            qbds.clearLinks();

            qbds.addRange(fieldNum(ProdTable,ProdId)).value(queryValue(prodBOM.ProdId));

            super();
        }

]]></Source>
