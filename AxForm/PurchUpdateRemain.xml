						<Source><![CDATA[
        /// <summary>
        /// Validates the purchase quantity which is given in delivery remainder.
        /// </summary>
        /// <returns>false if Purchase quantity has different sign with source quantity, otherwise true.</returns>
        public boolean validate()
        {
            boolean ret = super();

            if (ret && this.realValue() && sign(this.realValue()) != sign(callerPurchLine.PurchQty))
            {
                ret = checkFailed("@SCM:PurchQuantityMustHaveSameSignAsOrderQuantity");
            }

            ret = !element.callerPurchLine.isInterCompanyOrderInvoiced();

            return ret;
        }

