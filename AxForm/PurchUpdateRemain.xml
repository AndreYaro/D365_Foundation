        int active()
        {
            int ret = super();
            callerPurchLine     = element.args().record();

            if (!callerPurchLine.RecId)
            {
                throw error(Error::missingRecord(element.design().caption()));
            }

            callerDataSource    = FormDataUtil::getFormDataSource(callerPurchLine);

            remainPurchPhysical.allowEdit(callerDataSource.allowEdit());
            remainInventPhysical.allowEdit(callerDataSource.allowEdit() && callerPurchLine.isStocked());
            buttonCancelLine.enabled(callerDataSource.allowEdit());

            remainPurchPhysical.realValue(callerPurchLine.RemainPurchPhysical);
            remainInventPhysical.realValue(callerPurchLine.RemainInventPhysical);

            if (formCtrl)
            {
                formCtrl.dataSourceActivePost();
            }

            return ret;
        }

