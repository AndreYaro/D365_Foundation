			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        Object          purchLineDataSource = callerDataSource;
        FormDataSource  purchTableDataSource;

        try
        {
            ttsbegin;

            if (formCtrl)
            {
                formCtrl.closeOk();
            }

            PurchQty RemainPurchPhys = callerPurchLine.RemainPurchPhysical;

            callerPurchLine.RemainPurchPhysical  = remainPurchPhysical.realValue();
            callerPurchLine.RemainInventPhysical = remainInventPhysical.realValue();

            if (callerPurchLine.RemainInventPhysical)
            {
                if (!callerPurchLine.RemainPurchPhysical)
                {
                    throw error("@SCM:InventoryQuantityCannotBe0WhenOrderQuantityIs0");
                }
                if (sign(callerPurchLine.RemainInventPhysical) != sign(callerPurchLine.RemainPurchPhysical))
                {
                    throw error("@SCM:InventoryQuantityMustHaveSameSignAsOrderQuantity");
                }
            }

            if (callerPurchLine.validateWrite() && callerPurchLine.purchTable().checkUpdate())
            {
                this.updateRemainPhysical(callerPurchLine);

                // Update project commitments when deliver remainder is updated or quantity is canceled.
                this.updateProjCommitment(callerPurchLine, RemainPurchPhys);
                
                if (callerPurchLine.PurchStatus  == PurchStatus::Canceled &&
                    callerPurchLine.PurchaseType == PurchaseType::ReturnItem &&
                    callerPurchLine.isInterCompanyOrder())
                {
                    if (callerPurchLine.InterCompanyInventTransId)
                    {
                        changecompany(callerPurchLine.purchTable().InterCompanyCompanyId)
                        {
                            var interCompanySalesLine = SalesLine::findInventTransId(callerPurchLine.InterCompanyInventTransId);
                            var interCompanySalesTable = SalesTable::find(interCompanySalesLine.SalesId);
    
                            if (interCompanySalesTable.ReturnStatus == ReturnStatusHeader::Created &&
                                !interCompanySalesTable.existRegisteredReceivedInvoicedLines())
                            {
                                interCompanySalesLine.SalesStatus = SalesStatus::Canceled;
                                interCompanySalesLine.ReturnStatus = ReturnStatusLine::Canceled;
                                interCompanySalesLine.doUpdate();
                            }
                            
                            interCompanySalesTable.SalesStatus = SalesTableType::construct(interCompanySalesTable).lowestSalesStatus();
                            if (interCompanySalesTable.SalesStatus == SalesStatus::Canceled)
                            {
                                interCompanySalesTable.ReturnStatus = ReturnStatusHeader::Canceled;
                                interCompanySalesTable.doUpdate();
                            }
                        }
                    }
                }

                if (callerPurchLine.purchTable().PurchStatus == PurchStatus::Canceled)
                {
                    PurchPrepayRemove::reversePrepaymentInvoice(callerPurchLine.PurchId);
                }

                if (formDataSourceHasMethod(purchLineDataSource, identifierStr(purchTable_ds)))
                {
                    purchTableDataSource = purchLineDataSource.purchTable_ds();
                    purchTableDataSource.reread();
                }

                callerDataSource.reread();
                callerDataSource.refresh();
            }

            ttscommit;

            super();
        }
        catch
        {
            callerPurchLine.reread();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateRemainPhysical</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates the remaining physical quantities in the <c>PurchLine</c> record.
    /// </summary>
    /// <param name = "_callerPurchLine">The caller <c>PurchLine</c> record to update the remaining physical quantities for.</param>
    protected void updateRemainPhysical(PurchLine _callerPurchLine)
    {
        PurchLine       purchLineLocal = PurchLine::findRecId(_callerPurchLine.RecId);

        InventQty       diffRemainPurchPhysical       = purchLineLocal.RemainPurchPhysical       - remainPurchPhysical.realValue();
        InventQty       diffRemainInventPhysical      = purchLineLocal.RemainInventPhysical      - remainInventPhysical.realValue();
        PdsCWInventQty  diffPdsCWRemainInventPhysical = purchLineLocal.PdsCWRemainInventPhysical - pdsCWRemainInventPhysical.realValue();
        MCRSalesOrderCancellation mcrSalesOrderCancellation;
        PurchSalesOrderCancellation purchSalesOrderCancellation = new PurchSalesOrderCancellation();
        mcrSalesOrderCancellation = purchSalesOrderCancellation.mcrSalesOrderCancelInit(mcrSalesOrderCancellation, purchLineLocal.InventRefTransId);

        InterCompanyUpdateRemPhys::synchronize(_callerPurchLine, 
                                                diffRemainInventPhysical,
                                                diffRemainPurchPhysical,
                                                this.retrieveInterCompanySkipUpdateFlag(_callerPurchLine),
                                                diffPdsCWRemainInventPhysical);

        if (_callerPurchLine.ItemRefType  == InventRefType::Sales
                    && _callerPurchLine.InventRefTransId
                    && _callerPurchLine.isDropShipment())
        {
            _callerPurchLine.reread();
            _callerPurchLine.RemainPurchPhysical         = remainPurchPhysical.realValue();
            _callerPurchLine.RemainInventPhysical        = remainInventPhysical.realValue();
            _callerPurchLine.PdsCWRemainInventPhysical   = pdsCWRemainInventPhysical.realValue();
        }
        _callerPurchLine.write();

        purchSalesOrderCancellation.mcrSalesOrderCancel(mcrSalesOrderCancellation, purchLineLocal.InventRefTransId);
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        callerPurchLine = element.args().record();
        if (!callerPurchLine.RecId)
        {
            throw error(Error::missingRecord(element.design().caption()));
        }

        if (#PdsCatchWeightEnabled && PdsCatchWeightItem::exist(callerPurchLine.ItemId))
        {
            formCtrl = PdsFormCtrl_UpdateRemain::newFromForm(this);
        }
        else
        {
            CWQuantityRow.visible(false);
        }

        if (!callerPurchLine.purchTable().type().canUpdateRemainingQty())
        {
            throw error(Error::missingRecord(element.design().caption()));
        }

        if (formCtrl)
        {
            formCtrl.initPost();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>retrieveInterCompanySkipUpdateFlag</Name>
				<Source><![CDATA[
    /// <summary>
    /// Retrieves the flag controlling which parts of the intercompany update process are skipped.
    /// </summary>
    /// <param name = "_callerPurchLine">
    /// The caller purchase order line record.
    /// </param>
    /// <returns>
    /// The intercompany skip update flag.
    /// </returns>
    [SuppressBpWarning("BPParameterNotUsed", "Parameter _callerPurchLine is used by extensions that wrap this method.")]
    protected InterCompanySkipUpdate retrieveInterCompanySkipUpdateFlag(PurchLine _callerPurchLine)
    {
        return InterCompanySkipUpdate::No;
    }

]]></Source>
