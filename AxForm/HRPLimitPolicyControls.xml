								<Source><![CDATA[
            public void modified()
            {
                if (hrpSigningLimitParameters.IsEmployeeOnlyGetSigningLimits == NoYes::Yes)
                {
                    if (Box::yesNo("@SYS302271", DialogButton::No, "@SYS302270") == DialogButton::Yes)
                    {
                        super();
                    }
                    else
                    {
                        hrpSigningLimitParameters.IsEmployeeOnlyGetSigningLimits = NoYes::No;
                    }
                }
            }

]]></Source>
