					<Field>
						<DataField>ReasonCode</DataField>
						<Methods>
							<Method>
								<Name>validate</Name>
								<Source><![CDATA[
            public boolean validate()
            {
                boolean ret;

                ret = super();

                select firstonly ReasonCode from limitRevocationReasonCode
                    where limitRevocationReasonCode.ReasonCode == hrpSigningLimitRevocationReasonCode.ReasonCode;
                if (limitRevocationReasonCode.ReasonCode && (strCmp(strUpr(limitRevocationReasonCode.ReasonCode), strUpr(hrpSigningLimitRevocationReasonCode.ReasonCode)) == 0) && (hrpSigningLimitRevocationReasonCode.RecId == 0))
                {
                    ret = checkFailed("@SYS302126");
                }

                return ret;
            }

]]></Source>
