								<Name>modified</Name>
								<Source><![CDATA[
            public void modified()
            {
                int base;
                base = hrpSigningLimitParameters.orig().SigningLimitBasedOn;

                if (hrpSigningLimitParameters.orig().SigningLimitBasedOn != hrpSigningLimitParameters.SigningLimitBasedOn)
                {
                    if (Box::yesNo("@SYS302132", DialogButton::No, "@SYS302133") == DialogButton::Yes)
                    {
                        super();
                    }
                    else
                    {
                        hrpSigningLimitParameters.SigningLimitBasedOn = hrpSigningLimitParameters.orig().SigningLimitBasedOn;
                    }
                }
            }

]]></Source>
