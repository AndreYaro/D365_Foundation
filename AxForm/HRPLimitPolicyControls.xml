								<Source><![CDATA[
            public boolean validate()
            {
                boolean ret;

                ret = super();

                select firstonly AgreementName from limitAgreement
                    where limitAgreement.AgreementName == hrpSigningLimitAgreement.AgreementName;
                if (limitAgreement.AgreementName && (strCmp(strUpr(limitAgreement.AgreementName), strUpr(hrpSigningLimitAgreement.AgreementName)) == 0) && (hrpSigningLimitAgreement.RecId == 0))
                {
                    ret = checkFailed("@SYS302135");
                }
                return ret;
            }

]]></Source>
