		<DataControls xmlns="">
			<Control>
				<Name>Refresh</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            super();
            custStatisticsContainer = CustStatisticsUS::calculateStatistics(linkedCustTable);
            s_HighestBalance.realValue(conpeek(custStatisticsContainer,5));
            s_AverageInvoice.realValue(conpeek(custStatisticsContainer,6));
            OpenBalanceMst.realValue(linkedCustTable.openBalanceMST());
            OpenBalanceReportingCurrency.realValue(linkedCustTable.openBalanceReportingCurrency());
        }

]]></Source>
					</Method>
