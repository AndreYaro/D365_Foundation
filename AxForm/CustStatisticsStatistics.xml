						<Source><![CDATA[
        public void linkActive()
        {
            Object callerForm = element.args().caller();
            CustInvoiceTable linkedCustInvoiceTable;

            super();

            if (callerForm && callerForm.name() == formstr(CustFreeInvoice))
            {
                linkedCustInvoiceTable = element.args().record();
                linkedCustTable = CustTable::find(linkedCustInvoiceTable.OrderAccount);
            }
            else
            {
                linkedCustTable = element.args().record();
            }

            s_HighestBalance.realValue(0);
            s_AverageInvoice.realValue(0);
            openBalanceMst.realValue(0);
            OpenBalanceReportingCurrency.realValue(0);
        }

]]></Source>
					</Method>
				</Methods>
				<Fields />
			</DataSource>
