				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            PMFSequenceTable    seqTable;
            PMFSequenceGroupTrans   preSeqGroupTrans;

            if (element.args().caller()
            && element.args().dataset() == tableNum(PMFSequenceTable))
            {
                mapGroupId  = new Map(Types::Int64,Types::String);
                seqTable    = element.args().record();
                preSeqId    = seqTable.SequenceId;

                while select preSeqGroupTrans
                where preSeqGroupTrans.SequenceId == preSeqId
                {
                    if (!mapGroupId.exists(preSeqGroupTrans.SequenceGroupId))
                    {
                        mapGroupId.insert(preSeqGroupTrans.RecId,preSeqGroupTrans.SequenceGroupId);
                    }
                }
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            str                     groupIds;
            MapIterator             mi;

            if (preSeqId)
            {
                QueryBuildRange qbrSeqGroupId = this.query().dataSourceTable(tableNum(PMFSequenceGroup)).addRange(fieldNum(PMFSequenceGroup,SequenceGroupId));

                mi = new MapIterator(mapGroupId);
                mi.begin();

                while (mi.more())
                {
                    if (groupIds == '')
                    {
                        groupIds = strFmt('%1',mi.value());
                    }
                    else
                    {
                        groupIds += strFmt(',%1',mi.value());
                    }
                    mi.next();
                }
                qbrSeqGroupId.value(groupIds);
            }

            super();
        }

]]></Source>
					</Method>
				</Methods>
