				<Methods>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;

            ret = super();

            element.enableFieldsActive();

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        void init()
        {
            super();

            criteriaOpen    = this.query().dataSourceNo(1).addRange(fieldNum(CustTrans, Closed));
            paymMethod      = this.query().dataSourceNo(1).addRange(fieldNum(CustTrans, PaymMethod));
            invoice         = this.query().dataSourceNo(1).addRange(fieldNum(CustTrans, Invoice));
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        void executeQuery()
        {
            paymMethod.value(enum2Value(NetCurrent::COD));
            invoice.value('!\"\"');

            switch (checkBoxSelect.value())
            {
                case NoYes::Yes:
                    criteriaOpen.value(strFmt('%1..',dateNull()));
                    break;

                case NoYes::No:
                    criteriaOpen.value('');
                    break;
            }

            super();
        }

]]></Source>
					</Method>
