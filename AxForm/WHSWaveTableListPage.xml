				<Source><![CDATA[
[Form]
public class WHSWaveTableListPage extends FormRun implements SysIFilterConsumerForm
{
    WHSWaveListPageFilter   filter;
    str                     waveIdFilter;
    QueryBuildRange         qbrWave;
    boolean                 showClosedWaves;
    private boolean         isWHSWaveTableListPageCustomViewFilterFixFlightEnabed;
    private boolean         executeQueryWasRun;

}
]]></Source>
			</Method>
			<Method>
				<Name>registerDatasourceOnQueryingEvent</Name>
				<Source><![CDATA[
    public void registerDatasourceOnQueryingEvent()
    {
        WHSWaveTable_DS.OnQueryExecuting += eventhandler(this.parmFilterFormQueryEventHandler().applyFilter);
    }

]]></Source>
			</Method>
			<Method>
				<Name>doRefresh</Name>
				<Source><![CDATA[
    void doRefresh()
    {
        whsWaveTable_ds.executeQuery();
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        isWHSWaveTableListPageCustomViewFilterFixFlightEnabed = WHSWaveTableListPageCustomViewFilterFlight::instance().isEnabled();
        executeQueryWasRun = false;

        if (element.filterProvider && element.args().parm())
        {
            waveIdFilter = element.args().parm();
        }

        super();
        
        if (element.filterProvider && (element.args().record() || element.args().parm()))
        {
            showClosed.value(true);
            showClosedWaves = true;
            whsWaveTable_ds.executeQuery();
        }
        
        if (element.args().parmEnumType() == enumNum(WHSWaveListPageFilter))
        {
            filter = element.args().parmEnum();
            switch (filter)
            {
                case WHSWaveListPageFilter::Pending:
                case WHSWaveListPageFilter::Open:
                    showClosed.visible(false);
                    break;

                case WHSWaveListPageFilter::Closed:
                    showClosed.visible(false);
                    NewGroup.enabled(false);
                    WHSWaveTable_DS.allowCreate(false);
                    break;
            }
        }
    }

]]></Source>
			</Method>
