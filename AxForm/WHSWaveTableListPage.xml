					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            qbrWave = whsWaveTable_q.dataSourceTable(tableNum(WHSWaveTable)).addRange(fieldNum(WHSWaveTable, WaveStatus));

            if (waveIdFilter)
            {
                whsWaveTable_q.dataSourceTable(tableNum(WHSWaveTable)).addRange(fieldNum(WHSWaveTable, WaveId)).value(waveIdFilter);
            }

            this.cacheAddMethod(tableMethodStr(WHSWaveTable, displayPercentComplete));
        }

]]></Source>
					</Method>
					<Method>
						<Name>reReadLines</Name>
						<Source><![CDATA[
        void reReadLines()
        {
            whsWaveTable_ds.executeQuery();
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            if (element.args() && element.args().parmEnumType() == enumNum(WHSWaveListPageFilter))
            {
                switch (true)
                {
                    case filter == WHSWaveListPageFilter::Pending && showClosedWaves == true:
                        qbrWave.value(strFmt('(WaveStatus < %1) || (WaveStatus == %2)', any2int(WHSWaveStatus::Released), any2int(WHSWaveStatus::Shipped)));
                        break;

                    case filter == WHSWaveListPageFilter::Pending && showClosedWaves == false:
                        qbrWave.value(strFmt('(WaveStatus < %1)', any2int(WHSWaveStatus::Released)));
                        break;

                    case filter == WHSWaveListPageFilter::Open && showClosedWaves == true:
                        qbrWave.value(strFmt('(WaveStatus >= %1)', any2int(WHSWaveStatus::Released)));
                        break;

                    case filter == WHSWaveListPageFilter::Open && showClosedWaves == false:
                        qbrWave.value(strFmt('(WaveStatus >= %1) && (WaveStatus < %2)', any2int(WHSWaveStatus::Released), any2int(WHSWaveStatus::Shipped)));
                        break;

                    case filter == WHSWaveListPageFilter::Closed:
                        qbrWave.value(queryValue(WHSWaveStatus::Shipped));
                        break;

                    case filter == WHSWaveListPageFilter::Unprocessed:
                        qbrWave.value(queryValue(WHSWaveStatus::Created));
                        break;

                    case filter == WHSWaveListPageFilter::Unreleased:
                        qbrWave.value(queryValue(WHSWaveStatus::Held));
                        break;
                }
            }
            else 
            {
                //Don't set hardcoded value for qbrWave for the first run of executeQuery to protect against overwriting initially loaded saved view range.
                if (this.shouldAddRanges())
                {
                    if (showClosedWaves)
                    {
                        qbrWave.value('');
                    }
                    else
                    {
                        qbrWave.value(strFmt('(WaveStatus < %1)', any2int(WHSWaveStatus::Shipped)));
                    }
                }
            }

            super();

            executeQueryWasRun = true;
        }

]]></Source>
					</Method>
					<Method>
						<Name>shouldAddRanges</Name>
						<Source><![CDATA[
        private boolean shouldAddRanges()
        {
            return !isWHSWaveTableListPageCustomViewFilterFixFlightEnabed
                || executeQueryWasRun
                || !qbrWave.value();
        }

]]></Source>
