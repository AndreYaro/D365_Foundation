				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class WHSMovementCreate extends FormRun
{
    WMSLocation             location;
    WHSMovementCreateForm   whsMovementCreateForm;
    InventDimCtrl_Frm_WHS   inventDimFormSetup;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        whsMovementCreateForm = new WHSMovementCreateForm();

        if (element.args().dataset() != tableNum(WMSLocation))
        {
            throw error("@WAX3840");
        }

        WMSLocation initialLocation = element.args().record();
        location = WMSLocation::find(initialLocation.wMSLocationId, initialLocation.inventLocationId, true);

        super();

        element.populateInfoFields();
        whsMovementCreate.buildFromWMSLocation(location);
        element.populateMovementFromContext();

        element.updateDesign(InventDimFormDesignUpdate::Init);
    }

]]></Source>
			</Method>
			<Method>
				<Name>inventDimSetupObject</Name>
				<Source><![CDATA[
    /// <summary>
    /// Returns the dimensions display object.
    /// </summary>
    /// <returns>
    /// The dimensions display object
    /// </returns>
    public InventDimCtrl_Frm_WHS inventDimSetupObject()
    {
        return inventDimFormSetup;
    }

]]></Source>
			</Method>
			<Method>
				<Name>populateInfoFields</Name>
				<Source><![CDATA[
    void populateInfoFields()
    {
        inventSiteId.text(location.inventSiteId());
        inventLocationId.text(location.InventLocationId);
        wmsLocationId.text(location.wmsLocationId);
    }

]]></Source>
			</Method>
			<Method>
				<Name>populateMovementFromContext</Name>
				<Source><![CDATA[
    /// <summary>
    /// Set the location to move to, the qty to move, as well as filter the records to the one location to move from.
    /// </summary>
    void populateMovementFromContext()
    {
        if (element.args().caller() is WHSICreateMovementContextProvider)
        {
            var movementContext = element.args().caller() as WHSICreateMovementContextProvider;

            var qbdsMoveCreate = WHSMovementCreate_Q.dataSourceTable(tableNum(WHSMovementCreate));
            qbdsMoveCreate.addRange(fieldNum(WHSMovementCreate, ItemId)).value(queryValue(movementContext.itemIdToMove()));
            qbdsMoveCreate.addRange(fieldNum(WHSMovementCreate, InventDimId)).value(queryValue(movementContext.inventDimIdMoveFrom()));

            select firstonly forupdate whsMovementCreate
                where whsMovementCreate.ItemId == movementContext.itemIdToMove()
                    && whsMovementCreate.InventDimId == movementContext.inventDimIdMoveFrom();

            whsMovementCreate.ToLoc = movementContext.wmsLocationIdMoveTo();
            whsMovementCreate.QtyToMove = min(movementContext.qtyToMove(), whsMovementCreate.InventQty);
            whsMovementCreate.update();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates the dimensions display object.
    /// </summary>
    /// <param name="_mode">
    /// The state of the dimensions display form.
    /// </param>
    /// <exception cref="Exception::Error">
    /// Incorrect parameter for the function.
    /// </exception>
    public void updateDesign(InventDimFormDesignUpdate _mode)
    {
        InventDimParm inventDimParmLocal;

        switch (_mode)
        {
            case InventDimFormDesignUpdate::Init:
                if (!inventDimFormSetup)
                {
                    inventDimFormSetup  = InventDimCtrl_Frm_WHS::newFromForm(element);
                }

                inventDimParmLocal.setAllProductDimensions();
                inventDimParmLocal.setAllTrackingDimensions();
                inventDimFormSetup.parmDimParmVisibleGrid(inventDimParmLocal);
            // do continue
            case InventDimFormDesignUpdate::Active:
                inventDimFormSetup.formActiveSetup();
                inventDimFormSetup.formSetControls(true);
                break;

            default:
                throw error(strFmt("@SYS54195",funcName()));
        }
    }

]]></Source>
			</Method>
