				<Name>classDeclaration</Name>
				<Source><![CDATA[
    [Form]
    public class TaxWithholdTmpWorkTrans_IN extends FormRun implements IFormRefreshable
    {
         TmpTaxWithholdWorkTrans_IN         tmpTaxWithholdWorkTransLoc;
         [FormObservable]
         AmountCur                          amountOrignTotal;
         TaxWithhold_IN                     taxWithhold;
         TaxWithholdRegulation_IN           taxWithholdRegulation;
         TmpTaxWithholdRegulationTrans_IN   taxWithholdRegulationTrans;
         FormRun                            callerForm;
         AmountCur                          transactionCurrencyAmount;
}
]]></Source>
			</Method>
			<Method>
				<Name>callerRefresh</Name>
				<Source><![CDATA[
    public void callerRefresh()
    {
        if (callerForm  && formHasMethod(callerForm, identifierstr(callerRefresh)))
        {
            callerForm.callerRefresh();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initTax</Name>
				<Source><![CDATA[
    public void initTax(TaxWithhold_IN    _taxWithhold)
    {
        LedgerJournalTrans  ledgerJournalTrans;

        if (_taxWithhold)
        {
            taxWithhold =_taxWithhold;
            
            amountOrignTotal = taxWithhold.taxAmountCurTotal();
            transactionCurrencyAmount = taxWithhold.getTransactionAmount(tmpTaxWithholdWorkTrans.SourceBaseAmountCur);
            tmpTaxWithholdWorkTrans.setTmpData(_taxWithhold.tmpTaxWithholdWorkTrans());

            taxWithholdRegulation = TaxWithholdRegulation_IN::newTaxWithholdRegulation(_taxWithhold);
            tmpTaxWithholdRegulationTrans.setTmpData(taxWithholdRegulation.tmpTaxWithhodRegulation());
        }

        callerForm  = element.args().parmObject();

        select firstonly TaxWithholdCode from tmpTaxWithholdWorkTrans;

        if ( !tmpTaxWithholdWorkTrans.TaxWithholdCode)
        {
            thresholdButton.enabled(false);
            designerButton.enabled(false);
        }

        if (element.args().dataset() == tableNum(PurchLine))
        {
            tabAdjustment.visible(false);
        }
        if (element.args().dataset() == tableNum(SalesLine))
        {
            TabAdjustment.visible(false);
        }
        if (element.args().dataset() == tableNum(LedgerJournalTrans))
        {
            ledgerJournalTrans = element.args().record();
            if (ledgerJournalTrans.ledgerJournalTransTaxExtensionIN().TaxWithholdCode != '')
            {
                tabAdjustment.visible(false);
                TDSAmountCurTotal.visible(false);
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        #ISOCountryRegionCodes
        if (SysCountryRegionCode::isLegalEntityInCountryRegion([#isoIN]))
        {
            TmpTaxWithholdWorkTrans_TaxWithholdValue.noOfDecimals(5);
            RateOfDeduction.noOfDecimals(5);
            Calculation_TaxWithholdValue.noOfDecimals(5);
        }
    }

]]></Source>
