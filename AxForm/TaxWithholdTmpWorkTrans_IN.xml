						<Name>displayTaxAmount</Name>
						<Source><![CDATA[
        display TaxAmount displayTaxAmount(TmpTaxWithholdWorkTrans_IN _tmpTaxWithholdWorkTrans)
        {
            int sign = 1;

            if (_tmpTaxWithholdWorkTrans.SourceBaseAmountCur)
            {
                sign = real2int(_tmpTaxWithholdWorkTrans.SourceBaseAmountCur / abs(_tmpTaxWithholdWorkTrans.SourceBaseAmountCur));
            }

            return (abs(_tmpTaxWithholdWorkTrans.TaxAmount) * sign);
        }

