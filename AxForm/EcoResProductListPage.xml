			<Method>
				<Name>parmProductVariantRecIdList</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Provides the variants of the hovered product master or of all the marked ones.
    /// </summary>
    /// <returns>
    ///    A list containing the product variant record IDs.
    /// </returns>
    [Hookable(false)]
    public List parmProductVariantRecIdList()
    {
        List                productVariantRecIdList = new List(Types::Int64);
        EcoResProductMaster productMaster;

        if (EcoResProduct_ds.anyMarked())
        {
            EcoResProduct product = EcoResProduct_ds.getFirst(true);

            while (product)
            {
                productMaster = this.getProductMasterToGenerateVariantNames(product);

                if (productMaster.RecId)
                {
                    productVariantRecIdList.appendList(productMaster.getProductVariants());
                }
                product = EcoResProduct_ds.getNext();
            }
        }
        else if (EcoResProduct_ds.cursor())
        {
            productMaster = this.getProductMasterToGenerateVariantNames(ecoResProduct);

            if (productMaster.RecId)
            {
                productVariantRecIdList.appendList(productMaster.getProductVariants());
            }
        }

        return productVariantRecIdList;
    }

]]></Source>
			</Method>
