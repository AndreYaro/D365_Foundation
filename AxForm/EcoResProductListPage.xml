				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        /// Opens product variants form for the product master.
        /// </summary>
        void clicked()
        {
            // Overridden to run product variants form in the context of the Retail assortments or Retail stores.
            if (isConfigurationkeyEnabled(configurationKeyNum(Retail)))
            {
                Args retailAssortedProductVariantArgs = new Args();
                retailAssortedProductVariantArgs.record(element.args().record());

                MenuFunction menuFunction;
                Args args = new Args();
                args.caller(element);
                args.record(EcoResProduct_EcoResProductMaster);
                args.parmObject(retailAssortedProductVariantArgs);

                menuFunction = new MenuFunction(menuitemDisplayStr(EcoResProductVariantsAction), MenuItemType::Display);

                menuFunction.run(args);
            }
        }

]]></Source>
