			<Method>
				<Name>enableGenerateVariantNamesForMarkedProductMasters</Name>
				<Source><![CDATA[
    /// <summary>
    ///   Checks if hovered product master or at least one of the marked ones satisfy 
    ///   the requirements to have the names of its variants regenerated.
    /// </summary>
    /// <returns>
    ///   True if check succeeds; otherwise false.
    /// </returns>
    [Hookable(false)]   
    public boolean enableGenerateVariantNamesForMarkedProductMasters()
    {
        if (EcoResProduct_ds.anyMarked())
        {
            EcoResProduct product = EcoResProduct_ds.getFirst(true);

            while (product)
            {
                if (this.getProductMasterToGenerateVariantNames(product).RecId)
                {
                    return true;
                }
                product = EcoResProduct_ds.getNext();
            }

            return false;
        }

        if (EcoResProduct_ds.cursor())
        {   
            return this.getProductMasterToGenerateVariantNames(ecoResProduct).RecId;
        }

        return false;
    }

]]></Source>
			</Method>
			<Method>
				<Name>getProductMasterToGenerateVariantNames</Name>
				<Source><![CDATA[
    private EcoResProductMaster getProductMasterToGenerateVariantNames(EcoResProduct _product)
    {
        EcoResProductMaster returnedProductMaster;

        if (_product.isProductMaster())
        {
            EcoResProductMaster inputProductMaster = EcoResProductMaster::find(_product.RecId);

            if (inputProductMaster.enableGenerateVariantNames())
            {
                returnedProductMaster = inputProductMaster;
            }
        }

        return returnedProductMaster;
    }

]]></Source>
