			<Method>
				<Name>createTableListView</Name>
				<Source><![CDATA[
    /// <summary>
    /// Creates the list view for the label layout query tables.
    /// </summary>
    private void createTableListView()
    {        
        Query query = WHSLabelLayout.getLayoutQuery();

        int dsCount = query.dataSourceCount();

        for (int i = 1; i <= dsCount; ++i)
        {
            if (query.dataSourceNo(i).enabled())
            {
                QueryBuildDataSource qbds = query.dataSourceNo(i);

                var dictTable = new DictTable(qbds.table());

                if (dictTable)
                {
                    var formListItem = new FormListItem(this.getTableListViewTableName(dictTable.label(), qbds.name()), qbds.table(), con2Str([qbds.name(), dictTable.name()]));
                    TableListView.addItem(formListItem);
                }
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>getTableListViewTableName</Name>
				<Source><![CDATA[
    private str getTableListViewTableName(str _tableName, str _dataSourceName)
    {
        return strFmt('%1 (%2)', _tableName, _dataSourceName);
    }

]]></Source>
