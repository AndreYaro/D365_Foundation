			<Method>
				<Name>createListViewsBasedOnDataSource</Name>
				<Source><![CDATA[
    private void createListViewsBasedOnDataSource()
    {
        int prevTableIndex = TableListView.getNextItem(FormListNext::Focused);

        if (prevTableIndex == -1)
        {
            prevTableIndex = 0;
        }

        labelLayoutRecId = WHSLabelLayout.RecId;
        FieldListViewOnTab.deleteAll();
        MethodListView.deleteAll();
        TableListView.deleteAll();

        WHSLabelLayoutDataProvider labelLayoutDataProvider = WHSLabelLayout.getLabelLayoutDataProvider();
        if (labelLayoutDataProvider.hasNoDefaultDataSource() && !WHSLabelLayout.LabelLayoutDataSource)
        {
            return;
        }

        Query dataSourceQuery = WHSLabelLayout.getLayoutQuery();
        if (dataSourceQuery)
        {
            TableListGroup.visible(true);

            this.createTableListView();
            int tmpTableIndex;

            tmpTableIndex = TableListView.getCount() <= prevTableIndex ? 0 : prevTableIndex;

            FormListItem tmpItem = TableListView.getItem(tmpTableIndex);
            tmpItem.stateSelected(true);
            tmpItem.stateFocus(true);
            TableListView.setItem(tmpItem);

            this.createFieldListView();
            this.createMethodListView();
        }
        else
        {
            TableId namelessSingleTableDataSource = labelLayoutDataProvider.getNamelessSingleTableDataSource();
            if (!namelessSingleTableDataSource)
            {
                throw Error("@WAX:NoQueryNorSingleTableForLabelLayoutError");
            }

            TableListGroup.visible(false);
            
            this.createFieldListView(namelessSingleTableDataSource);
            this.createMethodListView(namelessSingleTableDataSource);
        }
    }

]]></Source>
