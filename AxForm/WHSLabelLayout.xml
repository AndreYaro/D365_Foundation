			<Method>
				<Name>createFieldListView</Name>
				<Source><![CDATA[
    /// <summary>
    /// Creates the list view for the label layout query fields.
    /// </summary>
    private void createFieldListView(TableId _singleNamelessDataSourceTableId = 0)
    {        
        FieldListViewOnTab.deleteAll();

        SysDictTable    dictTable;
        if (!_singleNamelessDataSourceTableId)
        {
            int focused = TableListView.getNextItem(FormListNext::Focused);
            TableName layoutTableName = conPeek(str2con(TableListView.getItem(focused).data()), 2);

            dictTable = SysDictTable::newName(layoutTableName);
        }
        else
        {
            dictTable = SysDictTable::newTableId(_singleNamelessDataSourceTableId);
        }

        if (dictTable)
        {
            Set fieldsSet = dictTable.fields();
            SetEnumerator fieldsSetEnumerator = fieldsSet.getEnumerator();
            while (fieldsSetEnumerator.moveNext())
            {
                SysDictField dictField = fieldsSetEnumerator.current();

                if (!dictField.isSystem())
                {
                    var formListItem = new FormListItem(dictField.label(), dictField.id(), dictField.name());
                    FieldListViewOnTab.addItem(formListItem);
                }
            }            
        }

        if (FieldListViewOnTab.getCount() > 0)
        {
            FormListItem tmpItem = FieldListViewOnTab.getItem(0);
            tmpItem.stateSelected(true);
            tmpItem.stateFocus(true);
            FieldListViewOnTab.setItem(tmpItem);
        }
    }

]]></Source>
