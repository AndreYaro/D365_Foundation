            public void lookup(FormControl _formControl, str _filterStr)
            {
                SysTableLookup sysTableLookup = SysTableLookup::newParameters(tableNum(WHSLabelLayoutDataSource), _formControl);

                Query query = new Query();
                QueryBuildDataSource queryBuildDataSource;
                QueryBuildRange queryBuildRange;
 
                queryBuildDataSource = query.addDataSource(tableNum(WHSLabelLayoutDataSource));
                queryBuildRange = queryBuildDataSource.addRange(fieldNum(WHSLabelLayoutDataSource, LayoutType));

                queryBuildRange.value(queryValue(WHSLabelLayout.LayoutType));

                sysTableLookup.addLookupfield(fieldNum(WHSLabelLayoutDataSource, LabelLayoutDataSourceId));
 
                sysTableLookup.parmQuery(query);
                sysTableLookup.performFormLookup();
            }

