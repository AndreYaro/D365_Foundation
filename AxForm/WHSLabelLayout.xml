				<Name>insertSelectedFieldIntoStringControl</Name>
				<Source><![CDATA[
    private void insertSelectedFieldIntoStringControl(FormStringControl _formStringControl)
    {
        if (FieldsTabPage.isActivePage())
        {
            if (TableListGroup.visible())
            {
                _formStringControl.pasteText(strFmt('$%1.%2$',
                                                            conPeek(str2con(TableListView.getItem(TableListView.getNextItem(FormListNext::Focused)).data()), 1),
                                                            FieldListViewOnTab.getItem(FieldListViewOnTab.getNextItem(FormListNext::Focused)).data()));
            }
            else
            {
                _formStringControl.pasteText(strFmt('$%1$', FieldListViewOnTab.getItem(FieldListViewOnTab.getNextItem(FormListNext::Focused)).data()));

            }
        }
        else if (MethodsTabPage.isActivePage())
        {
            if (MethodListView.getCount() > 0)
            {
                if (TableListGroup.visible())
                {
                    _formStringControl.pasteText(strFmt('$%1.%2$',
                                                                conPeek(str2con(TableListView.getItem(TableListView.getNextItem(FormListNext::Focused)).data()), 1),
                                                                MethodListView.getItem(MethodListView.getNextItem(FormListNext::Focused)).data()));
                }
                else
                {
                    _formStringControl.pasteText(strFmt('$%1$', MethodListView.getItem(MethodListView.getNextItem(FormListNext::Focused)).data()));
                }
            }
        }
    }

