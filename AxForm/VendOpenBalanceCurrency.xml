						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret = super();

            if (vendTransBalanceCurrency.CurrencyCode != '')
            {
                detailsVendTrans.enabled(true);
            }
            else
            {
                detailsVendTrans.enabled(false);
            }

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>linkActive</Name>
						<Source><![CDATA[
        void linkActive()
        {
            if (vendTable.AccountNum)
            {
                // Only recalculate if the account number is changed
                if (vendTable.AccountNum != balanceCalculatedForAccount)
                {
                    // Calculate vendor balances
                    vendTransBalanceCurrency.setTmpData(TmpCustVendTrans::vendTransBalanceCurrency(vendTable.AccountNum, true));

                    balanceCalculatedForAccount = vendTable.AccountNum;
                }
            }
            else
            {
                element.close();
            }

            super();
        }

]]></Source>
