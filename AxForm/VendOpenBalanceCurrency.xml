			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        switch (element.args().dataset())
        {
            case tablenum(VendTable) :
                vendTable = element.args().record();
                break;
        }

        if (!vendTable.RecId)
        {
            throw error("@SYS22904");
        }

        super();

        // Save the account number the balance is calculated for to make sure to don't calculate again in linkActive
        balanceCalculatedForAccount = vendTable.AccountNum;

        element.initBankLCTransHelper();
        // Calculate vendor balances
        element.updateBalance();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initBankLCTransHelper</Name>
				<Source><![CDATA[
    /// <summary>
    /// Inits the instance of <c>BankLCCustVendTransHelper</c>.
    /// </summary>
    private void initBankLCTransHelper()
    {
        bankLCTransHelper = BankLCVendTransHelper::construct();
        bankLCTransHelper.parmBankLCVendTransDS(bankLCVendTrans_DS);
        bankLCTransHelper.parmBankDocumentTypeFilterControl(bankDocumentTypeFilter);
        bankLCTransHelper.configBankLCControls(vendTrans_DS.query());
    }

]]></Source>
			</Method>
			<Method>
				<Name>refreshData</Name>
				<Source><![CDATA[
    /// <summary>
    /// Executes the query to refresh the data.
    /// </summary>
    public void refreshData()
    {
        vendTrans_DS.executeQuery();

        element.updateBalance();

        vendTransBalanceCurrency_DS.executeQuery();
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateBalance</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates the balance.
    /// </summary>
    public void updateBalance()
    {
        vendTransBalanceCurrency.setTmpData(
                TmpCustVendTrans::vendTransBalanceCurrency(
                balanceCalculatedForAccount,
                true,
                bankLCTransHelper.parmBankDocumentTypeFilter()));
    }

]]></Source>
