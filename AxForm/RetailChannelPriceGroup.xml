    public void init()
    {
        QueryBuildDataSource qbds;
        QueryBuildRange rangeChannelId;
        RetailOnlineChannelTable onlineChannel;
        super();

        if (element.args().dataset() != tableNum(RetailChannelTable)
            && element.args().dataset() != tableNum(RetailStoreTable)
            && element.args().dataset() != tableNum(RetailOnlineChannelTable)
            && element.args().dataset() != tableNum(RetailMCRChannelTable)
        )
        {
            throw(error("@RET6044"));
        }

        callerChannel = element.args().record();

        /// Add channel filter to RetailChannelPriceGroup data source.
        qbds = RetailChannelPriceGroup_ds.query().dataSourceTable(tableNum(RetailChannelPriceGroup));
        rangeChannelId = qbds.addRange(fieldnum(RetailChannelPriceGroup, RetailChannel));
        rangeChannelId.value(queryValue(callerChannel.RecId));
    }

]]></Source>
