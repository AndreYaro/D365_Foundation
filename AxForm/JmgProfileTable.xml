					<Method>
						<Name>typeChanged</Name>
						<Source><![CDATA[
        void typeChanged()
        {
            boolean isNotSignInOut;
            boolean isNotABreak;
            boolean isNotAnEvent;

            JmgProfileSpecType type = JmgProfileTypeTable::find(jmgProfileSpec.ProfileType).ProfileSpecType;

            isNotSignInOut  = ((type != JmgProfileSpecTypeEnum::SignIn) && (type != JmgProfileSpecTypeEnum::SignOut));
            isNotABreak     = ((type != JmgProfileSpecTypeEnum::Break) && (type != JmgProfileSpecTypeEnum::BreakPay));
            isNotAnEvent    = (! jmgProfileSpec.EventCode);

            toleranceBefore.enabled(!isNotSignInOut);
            hourGrid_ToleranceAfterStart.enabled(!isNotSignInOut);
            hourGrid_ToleranceBeforeEnd.enabled(!isNotSignInOut);
            toleranceAfter.enabled(!isNotSignInOut);
            endTime.enabled(isNotSignInOut);
            endTime.skip(!isNotSignInOut);
            toleranceBefore.skip(isNotSignInOut);
            toleranceAfter.skip(isNotSignInOut);
            hourGrid_ToleranceAfterStart.skip(isNotSignInOut);
            hourGrid_ToleranceBeforeEnd.skip(isNotSignInOut);
            hourGrid_SecondaryProfileType.enabled(!(isNotABreak && isNotAnEvent));
            hourGrid_SecondaryProfileType.skip((isNotABreak && isNotAnEvent));
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
