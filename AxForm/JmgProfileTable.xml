				<Source><![CDATA[
[Form]
public class JmgProfileTable extends FormRun
{
    QueryBuildRange    rangeDayId;
    QueryBuildRange    rangeDayProfile;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        super();

        rangeDayId = jmgProfileDay_ds.query().dataSourceNo(1).addRange(fieldNum(JmgProfileDay,DayId));
        rangeDayId.value(enum2Value(JmgDayIdEnum::Monday));

        rangeDayProfile = jmgProfileSpec_ds.query().dataSourceNo(1).addRange(fieldNum(JmgProfileSpec,DayId));
        rangeDayProfile.value(enum2Value(JmgDayIdEnum::Monday));

        this.initDayOfWeekMenuButton();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initDayOfWeekMenuButton</Name>
				<Source><![CDATA[
    private void initDayOfWeekMenuButton()
    {
        ProdEnumToMenuButtonsController::init(
            DayOfWeekMenuButton,
            enumNum(JmgPickDayId),
            formmethodstr(JmgProfileTable, dayOfWeekSelectionChangedButtonHandler),
            this);

        this.dayOfWeekSelectionChanged(JmgPickDayId::Monday);
    }

]]></Source>
			</Method>
			<Method>
				<Name>dayOfWeekSelectionChangedButtonHandler</Name>
				<Source><![CDATA[
    private void dayOfWeekSelectionChangedButtonHandler(FormCommandButtonControl _selectedControl)
    {
        JmgPickDayId newSelection = str2int(_selectedControl.parameters());
        this.dayOfWeekSelectionChanged(newSelection);
    }

]]></Source>
			</Method>
			<Method>
				<Name>dayOfWeekSelectionChanged</Name>
				<Source><![CDATA[
    private void dayOfWeekSelectionChanged(JmgPickDayId _newSelection)
    {
        DayOfWeekMenuButton.text(enum2str(_newSelection));

        rangeDayId.value(enum2Value(_newSelection));
        rangeDayProfile.value(enum2Value(_newSelection));
        jmgProfileDay.setDayCode();
        jmgProfileDay_ds.executeQuery();
        jmgProfileSpec_ds.executeQuery();
    }

]]></Source>
			</Method>
