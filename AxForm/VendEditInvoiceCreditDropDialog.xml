				<Source><![CDATA[
[Form]
public class VendEditInvoiceCreditDropDialog extends FormRun
{
    VendIInvoicePostPrint callerForm;
    PurchFormLetter purchFormLetter;

}
]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        super();

        if (PurchParmUpdate.RecId == 0)
        {
            // Try getting PurchParmUpdate from purchFormLetter of calling form
            callerForm = null;

            if (element.args())
            {
                callerForm = element.args().caller();

                purchFormLetter = callerForm.runBasePostPrint();
                purchParmUpdate.data(purchFormLetter.purchParmUpdate());
            }
        }

        checkCreditMax.selection(purchParmUpdate.CheckCreditMax);
        storno.value(purchParmUpdate.Storno);
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="" />
		<DataControls xmlns="">
			<Control>
				<Name>OKButton</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            purchParmUpdate.CheckCreditMax = checkCreditMax.selection();
            purchParmUpdate.Storno = storno.value();

            if (purchFormLetter != null)
            {
                purchFormLetter.purchParmUpdate(purchParmUpdate.data());
            }

            super();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
