			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        RetailPeriodicDiscount retailPeriodicDiscount;
        QueryBuildDataSource qbds;
        QueryBuildRange rangeOfferId;
        boolean allowEdit;

        super();

        if (element.args().caller().name() == formStr(RetailPeriodicDiscount))
        {
            callerPeriodicDiscount = element.args().record();
        }
        else if (element.args().caller().name() == formStr(RetailCoupon))
        {
            callerRetailCoupon = element.args().record();
        }
        
        if (!callerPeriodicDiscount && element.args().caller().name() == formStr(RetailPeriodicDiscount))
        {
            throw error(strFmt("@SYS19096", formStr(RetailDiscountPriceGroup), tableStr(RetailPeriodicDiscount)));
        }
        else if (!callerRetailCoupon && element.args().caller().name() == formStr(RetailCoupon))
        {
            throw error(strFmt("@SYS19096", formStr(RetailDiscountPriceGroup), tableStr(RetailCoupon)));
        }

        qbds = RetailDiscountPriceGroup_ds.query().dataSourceTable(tableNum(RetailDiscountPriceGroup));
        rangeOfferId = qbds.addRange(fieldnum(RetailDiscountPriceGroup, OfferId));

        if (callerPeriodicDiscount)
        {
            rangeOfferId.value(callerPeriodicDiscount.OfferId);
            allowEdit = callerPeriodicDiscount.Status == RetailDisabledEnabled::Disabled;
        }
        else if (callerRetailCoupon)
        {
            rangeOfferId.value(callerRetailCoupon.DiscountOfferId);      
            retailPeriodicDiscount = RetailPeriodicDiscount::findByOfferId(callerRetailCoupon.DiscountOfferId);
            allowEdit = retailPeriodicDiscount.Status == RetailDisabledEnabled::Disabled;
        }
        
        retailDiscountPriceGroup_ds.allowCreate(allowEdit);
        retailDiscountPriceGroup_ds.allowEdit(allowEdit);
        retailDiscountPriceGroup_ds.allowDelete(allowEdit);
    }

]]></Source>
