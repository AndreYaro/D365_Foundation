							<Method>
								<Name>lookup</Name>
								<Source><![CDATA[
            public void lookup(FormControl _dimensionValueControl, str _filterStr)
            {
                Name            dimensionAttributeName;
                RefRecId        dimensionAttributeRecId;
                FormStringControl   dimensionValueControl = _dimensionValueControl;

                dimensionAttributeRecId = DimensionMatrixTable_CN.DimensionAttribute;

                dimensionAttributeName = DimensionAttribute::find(dimensionAttributeRecId).Name;

                if (_dimensionValueControl != null)
                {
                    DimensionValueLookupHelper::lookupDimensionValues(DimensionAttribute::findByLocalizedName(dimensionAttributeName, false, SystemParameters::systemLanguageId()), dimensionValueControl);
                }
            }

]]></Source>
