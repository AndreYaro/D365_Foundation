								<Name>resolveReference</Name>
								<Source><![CDATA[
            public Common resolveReference(FormReferenceControl _formReferenceControl)
            {
                DimensionAttribute  resolvedRecord;
                Name                dimensionAttributeName;

                dimensionAttributeName = _formReferenceControl.filterValue(AbsoluteFieldBinding::construct(fieldStr(DimensionAttribute, Name), tableStr(DimensionAttribute))).value();
                resolvedRecord = DimensionAttribute::findByName(dimensionAttributeName);
                return resolvedRecord;
            }

]]></Source>
