                mf = new MenuFunction(menuitemDisplayStr(DimensionDetails), MenuItemType::Display);

                args = new args();
                args.lookupRecord(DimensionAttribute::find(DimensionMatrixTable_CN.DimensionAttribute));

                mf.run(args);
            }

]]></Source>
