			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        FormDataSource      formDataSource;
        TStimesheetTable    listTimesheet;
        TSTimesheetTable    timesheet;

        if (element.args().dataset() && element.args().dataset() != tableNum(TSTimesheetTable))
        {
            throw error(strFmt("@SYS22828", funcName()));
        }

        // Delete the header
        ttsBegin;
        formDataSource = FormDataUtil::getFormDataSource(element.args().record());
        listTimesheet = formDataSource.getFirst(true) ? formDataSource.getFirst(true) : formDataSource.cursor();

        if ((ProjParameters::find().TimesheetChangeReasonMandatory && ChangeReason.text()) ||
            (!ProjParameters::find().TimesheetChangeReasonMandatory) ||
            (!listTimesheet.hasLines()))
        {
            while (listTimesheet)
            {
                // select a fresh record from the database as the list may not have all fields selected for logging
                timesheet = TSTimesheetTable::find(listTimesheet.TimesheetNbr, true);
                TSTimesheetTableLog::createTableLog(
                    timesheet,
                    ChangeReason.text(),
                    TsTimesheetChangeType::Delete);
                if (listTimesheet.validateDelete())
                {
                    listTimesheet.delete();
                }

                listTimesheet = formDataSource.getNext();
            }
        }
        ttsCommit;

        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        ChangeReason.mandatory(ProjParameters::find().TimesheetChangeReasonMandatory);
    }

]]></Source>
