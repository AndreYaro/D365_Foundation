			<Method>
				<Name>updatePlanVersionRangesOnSavedViewChange</Name>
				<Source><![CDATA[
    /// <summary>
    /// If the saved view is changed, we need to adjust the PlanId and PlanVersion filters to the one selected in the view.
    /// </summary>
    private void updatePlanVersionRangesOnSavedViewChange()
    {
        QueryBuildDataSource qbds = ReqPlanVersion_ds.queryRunQueryBuildDataSource();

        if (ReqTransPoFormRunConfigurationToggle::instance().isEnabled() 
            && ReqTransPoFormRunConfigurationContext::instanceExists() 
            && qbds)
        {
            var planRange = ReqTransPoFormPlanVersionFilterManager::findEnabledRange(qbds, fieldNum(ReqPlanVersion, ReqPlanId));
            ReqPlanId currentPlanId;
            if (planRange)
            {
                currentPlanId = planRange.value();
            }
            
            if (currentPlanId)
            {
                PlanIdFilter.text(currentPlanId);
                PlanIdFilter.modified();

                RefRecId activePlanVersionRecId = reqTransPlanIdFilter.planVersion().RecId;
                ReqTransPoFormPlanVersionFilterManager::updatePlanVersionQueryRanges(ReqPo_ds.queryRun().query(), activePlanVersionRecId);
            }
        }
    }

]]></Source>
