			<Method>
				<Name>initPlanIdFilter</Name>
				<Source><![CDATA[
    public void initPlanIdFilter()
    {
        if (!openedWithRecord)
        {
            if (planId == '') // The planIdFilter was not set by the xSysLastValue::getLast(this); call.
            {
                if (element.args().menuItemName() == menuitemDisplayStr(ReqPoGridViewPurchaseTodayOrOverdue))
                {
                    planId = ReqPlanSched::defaultStaticId();
                }
                else
                {
                    planId = ReqPlanSched::defaultDynamicId();
                }
            }
        }
        else
        {
            Args args = element.args();
            Common argsRecord = args.record() ? args.record() : args.lookupRecord();

            if (argsRecord && argsRecord.TableId == tablenum(ReqPO))
            {
                ReqPO argsReqPo = argsRecord;
                planId = ReqPlanVersion::find(argsReqPo.PlanVersion).ReqPlanId;
            }
            else if (argsRecord && argsRecord.TableId == tablenum(ReqTrans))
            {
                ReqTrans argsReqTrans = argsRecord;
                planId = ReqPlanVersion::find(argsReqTrans.PlanVersion).ReqPlanId;
            }
        }
    }

]]></Source>
