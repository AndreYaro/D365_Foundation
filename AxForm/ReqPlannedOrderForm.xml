			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        ReqParameters::checkPlanningProcessesAreEnabled();

        openedWithRecord = element.args() && (element.args().record() || element.args().lookupRecord());

        controller = ReqPlannedOrderFormController::newFormRun(element);

        if (!openedWithRecord)
        {
            xSysLastValue::getLast(this);
        }

        super();
        
        controller.formInitPost();
        this.initPlanIdFilter();        
        reqTransPlanIdFilter = ReqTransPlanIdFilter::newStandard(formControlStr(ReqPlannedOrderForm, PlanIdFilter), this, planId);
    }

]]></Source>
