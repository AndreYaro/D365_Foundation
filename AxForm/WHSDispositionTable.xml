				<Methods>
					<Method>
						<Name>lookup</Name>
						<Source><![CDATA[
        public void lookup()
        {
            Set transTypes;
            
            if (WHSDispositionTableUseSupportedTransTypesFlight::instance().isEnabled())
            {
                transTypes = WHSDispositionTable::supportedTransTypesSet();
            }
            else
            {
                transTypes = new Set(Types::Enum);

                transTypes.add(WHSWorkTransType::ReturnOrder);
                transTypes.add(WHSWorkTransType::Purch);
                transTypes.add(WHSWorkTransType::ProdPut);
            }

            WHSDispositionTable::lookupWHSWorkTemplateCode(this, transTypes);
        }

]]></Source>
