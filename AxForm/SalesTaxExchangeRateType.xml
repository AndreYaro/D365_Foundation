    [Form]
    public class SalesTaxExchangeRateType extends FormRun
    {
        QueryBuildRange     qbrLedger;
        QueryBuildRange     qbrCurrency;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        LedgerRecId currentLedger;

        super();

        // Find the ledger that is primary for the current logged in company
        currentLedger = Ledger::current();
        ledgerId.value(currentLedger);
        qbrLedger.value(SysQuery::value(currentLedger));
    }

]]></Source>
