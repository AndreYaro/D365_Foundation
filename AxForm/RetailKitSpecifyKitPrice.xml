			<Method>
				<Name>InitDefaultKitPrice</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initialize the default kit price from the components.
    /// </summary>
    private void InitDefaultKitPrice()
    {
        specifyKitPrice.realValue(RetailPricingKitCalculator::getCurrentKitPrice(currentRetailKit));
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (element.args().record() is RetailKit)
        {
            currentRetailKit = element.args().record();
        }

        super();

        // Default the UI using the current price.
        element.InitDefaultKitPrice();
    }

]]></Source>
