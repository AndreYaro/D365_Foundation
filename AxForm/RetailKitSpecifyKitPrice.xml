			<Method>
				<Name>distributeKitPrice</Name>
				<Source><![CDATA[
    public void distributeKitPrice()
    {
        RetailPricingKitCalculator::updateKitPrice(currentRetailKit, specifykitPrice.realValue());
    }

]]></Source>
