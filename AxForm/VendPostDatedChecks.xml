			<Control>
				<Name>SettleClearingTransaction</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            super();

            if (FeatureStateProvider::isFeatureEnabled(BankBridgedTransAutoClearingFeature::instance()))
            {
                BankBridgedTransClearingLock bankBridgedTransClearingLock = new BankBridgedTransClearingLock(custVendPDC.LedgerJournalTrans);

                try
                {
                    bankBridgedTransClearingLock.enterBridgeClearingLock();

                    if (bankBridgedTransClearingLock.lockAcquired)
                    {
                        CustVendPDCManager::settleClearingTransactionsV2(custVendPDC);
                    }
                }
                finally
                {
                    bankBridgedTransClearingLock.exitBridgeClearingLock();
                }
            }
            else
            {
                CustVendPDCManager::settleClearingTransactionsV2(custVendPDC);
            }

            custVendPDC_DS.executeQuery();
            custVendPDC_DS.refresh();
        }

]]></Source>
