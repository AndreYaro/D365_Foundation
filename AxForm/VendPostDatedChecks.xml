        void clicked()
        {
            super();
            QueryBuildDataSource qbs = custVendPDC_DS.query().dataSourceTable(tableNum(CustVendPDCRegister));
            custVendPDC_DS.query().addQueryFilter(qbs, fieldStr(CustVendPDCRegister, MaturityDate)).value('..' + SysQuery::value(DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone())));
            custVendPDC_DS.query().addQueryFilter(qbs, fieldStr(CustVendPDCRegister, pdcStatus)).value(SysQuery::value(PostDatedCheckStatus::Posted));
            custVendPDC_DS.executeQuery();
            custVendPDC_DS.refresh();
        }

]]></Source>
