			<Method>
				<Name>getDataEntityContext</Name>
				<Source><![CDATA[
    /// <summary>
    /// Gets the <see cref="OfficeGeneratedExportMenuItem"/> that specifies what should be exported for the export option with the specified id.
    /// </summary>
    /// <param name="_exportOption">The export option.</param>
    /// <returns>
    /// Returns the <see cref="ExportToExcelDataEntityContext"/> that specifies what should be exported for the export option with the specified id;
    /// otherwise, null.
    /// </returns>
    /// <remarks>
    /// This method is only called for export options of type ExportToExcelExportType::CustomGenerated.
    /// </remarks>
    public ExportToExcelDataEntityContext getDataEntityContext(OfficeGeneratedExportMenuItem _menuItem)
    {
        if (_menuItem.id() != GeneratedId)
        {
            return null;
        }

        ExportToExcelDataEntityContext context = ExportToExcelDataEntityContext::construct();

        var entity = new ExportToExcelDataEntityInfo();
        entity.entityName(tablestr(BankPostDatedCheckEntity));
        context.entities().addEnd(entity);

        var binding = new ExportToExcelDataEntityBindingInfo();
        binding.entityName(entity.entityName());
        binding.useFieldGroup(true);
        binding.fieldGroupName(AutoReportFieldGroupName);
        context.bindings().addEnd(binding);

        // Add a filter to show data only for vendor postdated checks
        ExportToExcelFilterTreeBuilder exportFilterTreeBuilder = new ExportToExcelFilterTreeBuilder(tablestr(BankPostDatedCheckEntity));
        entity.filter(exportFilterTreeBuilder.areEqual(fieldstr(BankPostDatedCheckEntity, AccountType), CustVendACType::Vend));

        return context;
    }

]]></Source>
			</Method>
			<Method>
				<Name>customizeMenuOptions</Name>
				<Source><![CDATA[
    /// <summary>
    /// Customizes the options used to populate the Office Menu.
    /// </summary>
    /// <param name = "_menuOptions">The menu options to be customized.</param>
    public void customizeMenuOptions(OfficeMenuOptions _menuOptions)
    {
        // Clear the menu options which are driven by the root datasource of an entity
        ListIterator dataEntityIterator = new ListIterator(_menuOptions.dataEntityOptions());

        while (dataEntityIterator.more())
        {
            dataEntityIterator.delete();
        }

        OfficeGeneratedExportMenuItem generatedMenuItem = OfficeGeneratedExportMenuItem::construct(tableStr(BankPostDatedCheckEntity), GeneratedId);
        generatedMenuItem.displayName("@SYS322510");
        _menuOptions.customMenuItems().addEnd(generatedMenuItem);
    }

]]></Source>
