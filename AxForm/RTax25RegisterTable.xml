				<Name>addSelected</Name>
				<Source><![CDATA[
    void addSelected()
    {
        RTax25RegisterHideSetup hideSetup;

        hideSetup.RegisterId    = rTax25RegisterTable.RegisterId;
        hideSetup.TableTrans    = NoneTableTrans::Table;
        hideSetup.FieldId       = AllFieldsTableControl.cell(2, AllFieldsTableControl.row()).data();

        if (hideSetup.FieldId != 0 && !RTax25RegisterHideSetup::findTable(hideSetup.RegisterId, hideSetup.FieldId, false))
        {
            hideSetup.insert();
        }

        element.buildTableControls(true);
    }

