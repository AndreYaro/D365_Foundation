			<Method>
				<Name>updateDesignForRegType</Name>
				<Source><![CDATA[
    /// <summary>
    /// Update form design depending on register type
    /// </summary>
    public void updateDesignForRegType()
    {
        regType = RTax25RegType::construct(rTax25RegisterTable.RegisterType);

        rTax25RegisterProfit.enabled(regType.supportLedger());

        tableBuilt = false;

        if (tabHide.isExpanded())
        {
            element.buildTableControls(true);
        }
        
	rTax25RegisterTable_ds.object(fieldNum(RTax25RegisterTable, Disabled)).allowEdit(RTax25RegCalcController::isRegisterObsolete(rTax25RegisterTable.RegisterType));
        rTax25RegisterTable_ds.object(fieldNum(RTax25RegisterTable, Expand)).allowEdit(rTax25RegisterTable.RegisterType != RTax25RegisterType::TaxRemainGoods);
    }

]]></Source>
			</Method>
			<Method>
				<Name>buildTableControls</Name>
				<Source><![CDATA[
    private void buildTableControls(boolean _rebuild = false)
    {
        container               fieldLabels = regType.fieldLabel();

        if (tableBuilt && ! _rebuild)
            return;

        SelFieldsTableControl.clear();
        AllFieldsTableControl.clear();

        RTax25RegisterHideSetup hideSetup;
        select count(RecId) from hideSetup
            where hideSetup.RegisterId == rTax25RegisterTable.RegisterId
                && hideSetup.TableTrans == NoneTableTrans::Table;

        SelFieldsTableControl.rows(any2Int(hideSetup.RecId));

        container selLabels;
        int curSelIdx = 0;

        while select FieldId, Hided
            from hideSetup order by FieldId
            where hideSetup.RegisterId == rTax25RegisterTable.RegisterId
                && hideSetup.TableTrans == NoneTableTrans::Table
        {
            curSelIdx++;
            RTax25FieldLabel fieldLabel = conpeek(fieldLabels, hideSetup.FieldId);
            selLabels += fieldLabel;
            SelFieldsTableControl.cell(1, curSelIdx).data(hideSetup.Hided);
            SelFieldsTableControl.cell(2, curSelIdx).data(fieldLabel);
            SelFieldsTableControl.cell(3, curSelIdx).data(hideSetup.FieldId);
        }

        AllFieldsTableControl.rows(conLen(fieldLabels) - conLen(selLabels));

        int curAllIdx = 0;
        for (int i = 1; i <= conlen(fieldLabels); i++)
        {
            RTax25FieldLabel fieldLabel = conpeek(fieldLabels, i);

            if (confind(selLabels, fieldLabel))
            {
                continue;
            }

            curAllIdx ++;
            AllFieldsTableControl.cell(1, curAllIdx).data(fieldLabel);
            AllFieldsTableControl.cell(2, curAllIdx).data(i);
        }

        tableBuilt = true;
    }

]]></Source>
