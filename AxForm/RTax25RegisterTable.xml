				<Source><![CDATA[
    void addAll()
    {
        RTax25RegisterHideSetup hideSetup;

        hideSetup.RegisterId    = rTax25RegisterTable.RegisterId;
        hideSetup.TableTrans    = NoneTableTrans::Table;
        
        int totalRows = AllFieldsTableControl.rows();

        for (int i = 1; i <= totalRows; i++)
        {
            hideSetup.FieldId = AllFieldsTableControl.cell(2, i).data();
            hideSetup.insert();
        }

        element.buildTableControls(true);
    }

]]></Source>
