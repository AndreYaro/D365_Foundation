					<Method>
						<Name>qtyCalc</Name>
						<Source><![CDATA[
        public display Qty qtyCalc(ProdTableJour _prodTableJour)
        {
            PmfProdCoBy pmfProdCoBy = PmfProdCoBy::findByProdItemDim(_prodTableJour.ProdId, _prodTableJour.ItemId);

            if (pmfProdCoBy)
            {
                return pmfProdCoBy.QtyCoByCalc;
            }
            else
            {
                return ProdTable.QtyCalc;
            }
        }

]]></Source>
