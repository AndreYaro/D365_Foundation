[Form]
public class JmgJobsWithDeviationReportedQty extends FormRun
{
    QueryBuildRange       jobIdRange;
    QueryBuildRange       jobTypeRange;
    QueryBuildRange       oprNumRange;
    QueryBuildRange       oprPriorityRange;
    QueryBuildRange       transRefIdRange;

    JmgJobReportDeviation jmgJobReportDeviation;
    ProdRouteJob          prodRouteJob;
    ProdRoute             prodRoute;

}
]]></Source>
			</Method>
			<Method>
				<Name>setSumTab</Name>
				<Source><![CDATA[
    public void setSumTab(Qty _qtyGood, Qty _qtyError)
    {
        if (prodRoute.OprPriority == RouteOprPriority::Primary)
        {
            CalcQty.realValue(JmgJobTable.CalcQty);
            SumGoodQty.realValue(_qtyGood);
            DeviationQty.realValue(_qtyGood - JmgJobTable.CalcQty);
            SumErrorQty.realValue(_qtyError);
            PostDifference.enabled(DeviationQty.realValue() != 0);
        }
        else
        {
            CalcQty.realValue(0);
            SumGoodQty.realValue(0);
            DeviationQty.realValue(0);
            SumErrorQty.realValue(0);
            PostDifference.enabled(false);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>postDifference</Name>
				<Source><![CDATA[
    public void postDifference()
    {
        if (prodRouteJob)
        {
            JmgJobReportDeviationPosting::openProdJournalTableJobCard(this, prodRouteJob, ProdRouteTrans, 0, -DeviationQty.realValue());
        }
        else
        {
            JmgJobReportDeviationPosting::openProdJournalTableRouteCard(this, prodRoute, ProdRouteTrans, 0, -DeviationQty.realValue());
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        jmgJobReportDeviation jmgJobReportDeviationToUpdate = JmgJobReportDeviation::find(jmgJobReportDeviation.UserId, jmgJobReportDeviation.JobId, true);

        if (jmgJobReportDeviationToUpdate)
        {
            JmgJobReportDeviation::updateReportedQtyDeviation(jmgJobReportDeviationToUpdate);
        }

        super();
    }

]]></Source>
