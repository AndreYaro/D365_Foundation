    void init()
    {
        // <GEERU>
        #ISOCountryRegionCodes
        // </GEERU>
        super();

        // <GEERU>
        if (SysCountryRegionCode::isLegalEntityInCountryRegion([#isoRU]))
        {
            purchSummaryParametersForm_Invoice4Paym = PurchSummaryParametersForm::construct(DocumentStatus::Invoice4Paym_RU, selectedInvoice4Paym, notSelectedInvoice4Paym);
            purchSummaryParametersForm_Invoice4Paym.fillViews();
        }
        // </GEERU>
        purchSummaryParametersForm_ReceiptsList = PurchSummaryParametersForm::construct(DocumentStatus::ReceiptsList, selectedReceiptsList, notSelectedReceiptsList);
        purchSummaryParametersForm_ReceiptsList.fillViews();
        purchSummaryParametersForm_PackingSlip = PurchSummaryParametersForm::construct(DocumentStatus::PackingSlip, selectedPackingSlip, notSelectedPackingSlip);
        purchSummaryParametersForm_PackingSlip.fillViews();
        purchSummaryParametersForm_Invoice = PurchSummaryParametersForm::construct(DocumentStatus::Invoice, selectedInvoice, notSelectedInvoice);
        purchSummaryParametersForm_Invoice.fillViews();

        sumByCtrl = SalesPurchSummarySumByCtrl::newSumByControl(summaryupdate_SummaryUpdateBy);
        sumByCtrl.remove(AccountOrder::Journal);
        sumByCtrl.build();
    }

]]></Source>
