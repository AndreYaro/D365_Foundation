			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        prodId = element.args().parm();

        pmfFormCtrl_ProdJournalBOMLookup
            = PmfFormCtrl_ProdJournalBOMLookup::newFromForm(element);
        super();

        element.design().caption(strfmt("@SYS26199",prodId));
        element.selectMode(ctrlTransId);

        element.updateDesign(InventDimFormDesignUpdate::Init);

        pmfFormCtrl_ProdJournalBOMLookup.setDatasources(
            prodBOM);
        pmfFormCtrl_ProdJournalBOMLookup.initPost();
    }

]]></Source>
			</Method>
			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        FormStringControl   callerControl   = SysTableLookup::getCallerStringControl(element.args());
        boolean             filterLookup    = false;

        filterLookup = SysTableLookup::filterLookupPreRun(
            callerControl,
            ctrlTransId,
            prodBOM_ds);

        super();

        SysTableLookup::filterLookupPostRun(
            filterLookup,
            callerControl.text(),
            ctrlTransId,
            prodBOM_ds);
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    void updateDesign(InventDimFormDesignUpdate mode)
    {
        InventDimCtrl_Frm   inventDimFormSetup;
        InventDimParm       inventDimParm;

        switch (mode)
        {
            case InventDimFormDesignUpdate::Init :
                inventDimFormSetup  = InventDimCtrl::newFromForm(element);
                inventDimParm.initProductDimensionsAllGroups();
                inventDimFormSetup.parmDimParmVisibleGrid(inventDimParm);
                inventDimFormSetup.formSetControls(true);
                break;

            default :
                throw error(strfmt("@SYS54195",funcname()));
        }
    }

]]></Source>
