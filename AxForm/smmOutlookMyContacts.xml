				<Source><![CDATA[
    [Form]
    public class smmOutlookMyContacts extends FormRun
    {
        CrmWorkerRecId                      currentEmployee;
        QueryBuildRange                     deletedRange;
        QueryBuildRange                     userRange;
        smmOutlookSyncPrinciple             outlookSyncPrinciple;
        smmOutlookSyncType                  maxSynchronizationType;
        container                           addressLocationTypes;

        LogisticsElectronicAddressLookup contactInfoLookup;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (smmOutlookSyncDeprecationHandler::isOutlookSyncDisabled())
        {
            smmOutlookSyncDeprecationHandler::logSyncSkipMessageToTelemetry();
            throw error(strFmt("@SYS19306",funcName()));
        }

        smmOutlookSyncDeprecationHandler::showDeprecationMessage();

        addressLocationTypes = [smmOutlookContactFields::BusinessAddress, smmOutlookContactFields::HomeAddress, smmOutlookContactFields::OtherAddress];

        super();

        // Find the current employee based on the logged on user
        if (!DirPersonUser::current())
        {
            // No employee is mapped to the current user. Set mapping in Employee option form.
            error("@SYS80637");
            element.close();
        }

        if (!smmAxaptaOutlookMapping::isOutlookMappingSetupCompleted())
        {
            smmAxaptaOutlookMapping::createDefaultSetup();
        }

                // Find Outlook sync principe from the CRM parameters
        outlookSyncPrinciple = OutlookSyncParameters::find().OutlookSyncPrinciple;

        // Find current employee
        currentEmployee = HcmWorker::userId2Worker(curUserId());

        // Find allowed syncrhonization type from parameters
        maxSynchronizationType = OutlookSyncParameters::find().OutlookMaxSynchronizationType;

        // Disable the synchronization type selection field if maximum allowed type is view
        smmContactPersonSynchronize_ds.object(fieldNum(smmContactPersonSynchronize, OutlookSyncType)).allowEdit(maxSynchronizationType != smmOutlookSyncType::ViewContact);

        // Set status of buttons
        element.setActiveFields();
    }

]]></Source>
			</Method>
			<Method>
				<Name>refreshDatasource</Name>
				<Source><![CDATA[
    void refreshDatasource()
    {
        smmContactPersonSynchronize_ds.executeQuery();
    }

]]></Source>
			</Method>
			<Method>
				<Name>setActiveFields</Name>
				<Source><![CDATA[
    void setActiveFields()
    {
        // Can only main responsible can synchronize with edit rights
        if (outlookSyncPrinciple == smmOutlookSyncPrinciple::MainReponsibleMaster)
        {
            // If allowed synchronization type is view, the syncrhonization type cannot be changed
            if (maxSynchronizationType != smmOutlookSyncType::ViewContact)
            {
                // If current employee is not the main responsible for the contact, only view rights are allowed
                smmContactPersonSynchronize_ds.object(fieldNum(smmContactPersonSynchronize, OutlookSyncType)).allowEdit(ContactPerson::find(smmContactPersonSynchronize.ContactPersonId).MainResponsibleWorker == currentEmployee);
            }
        }

        // Contact person and Check in buttons should only be enabled if contact person are selected
        btnContactPerson.enabled(smmContactPersonSynchronize.ContactPersonId != '');
    }

]]></Source>
