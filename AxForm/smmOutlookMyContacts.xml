            public boolean validate()
            {
                boolean ret;

                ret = super();

                if (ret && contactInfoLookup)
                {
                    ContactPersonSyncFieldMapping_Contact.Location = contactInfoLookup.parmElectronicAddress().RecId;
                    ret = ContactPersonSyncFieldMapping_Contact.validateField(fieldNum(smmContactPersonSynchronizeFieldMapping, Location));
                    if (!ret)
                    {
                        ContactPersonSyncFieldMapping_Contact.Location = 0;
                    }
                }

                return ret;
            }

