					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            super();

            // Finds the RetailDevice record in the data source and make it current record for first time only.
            if (firstTime &&
                element.args() &&
                element.args().record() &&
                element.args().record() is RetailDevice
                )
            {
                firstTime = false;
                RetailDevice deviceLocal = element.args().record();
                RetailDevice_DS.positionToRecord(deviceLocal);
            }
        }

]]></Source>
