			<Method>
				<Name>setDeviceFieldsAccess</Name>
				<Source><![CDATA[
    /// <summary>
    /// Enable or disable fields based on the current selected record values.
    /// </summary>
    public void setDeviceFieldsAccess()
    {
        RetailDeviceTypes deviceType;
        select firstonly1 type from deviceType where deviceType.RecId == RetailDevice.Type;

        // Sideloading key and download button are visible/ enabled only when device type is windows8
        RetailSharedParameters_SideloadingKey.visible(deviceType.Type == RetailDeviceTypeBase::ModernPOS);

        // Special conditions to allow editing UseInMemoryDeviceDataStorage switch and package name drop down.
        RetailDevice_DS.object(fieldNum(RetailDevice, UseInMemoryDeviceDataStorage)).allowEdit(deviceType.Type == RetailDeviceTypeBase::CloudPOS);

        // The CloudPOSUrl control is only visible when the device type is CloudPOS, ModernPOSiOS, ModernPOSAndroid or StoreCommerce.
        CloudPOSUrl.visible(deviceType.Type == RetailDeviceTypeBase::CloudPOS
                            || deviceType.Type == RetailDeviceTypeBase::ModernPOSiOS
                            || deviceType.Type == RetailDeviceTypeBase::ModernPOSAndroid
                            || deviceType.Type == RetailDeviceTypeBase::StoreCommerce);

        // The StoreCommerceDownloadUrl control is only visible when the device type is Store Commerce
        StoreCommerceDownloadUrl.visible(false);

        if (deviceType.Type == RetailDeviceTypeBase::ModernPOS)
        {
            // Only expose the UI for "AllowMassActivation" on Windows Modern Pos client (online and offline)
            // Note: For now it has been decided to keep this UI disabled, so we ensure it is set to false.
            RetailDevice_AllowMassActivation.visible(false);
        }
        else
        {
            RetailDevice_AllowMassActivation.visible(false);
        }

        // Set POSSelfServiceReferenceTab visibility
        if (deviceType.Type == RetailDeviceTypeBase::ModernPOS
            || deviceType.Type == RetailDeviceTypeBase::ModernPOSWindowsPhone)
        {
            POSSelfServiceReferenceTab.visible(true);
            DownloadButtonGroup.enabled(true);
            RetailDevice_DS.object(fieldNum(RetailDevice, MergedSelfServicePackageReference)).allowEdit(true);
        }
        else
        {
            POSSelfServiceReferenceTab.visible(false);
            DownloadButtonGroup.enabled(false);
            RetailDevice_DS.object(fieldNum(RetailDevice, MergedSelfServicePackageReference)).allowEdit(false);
        }

        this.hideControlsForRetailTrial();
    }

]]></Source>
			</Method>
			<Method>
				<Name>hideControlsForRetailTrial</Name>
				<Source><![CDATA[
    /// <summary>
    /// Hides form controls for retail trial.
    /// </summary>
    public void hideControlsForRetailTrial()
    {
        if (RetailTrialUserManagement::isRetailTrialUser())
        {
            DownloAdActionPaneTab.visible(false);
            CloudPOSUrl.visible(false);
            StoreCommerceDownloadUrl.visible(false);
        }
    }

]]></Source>
