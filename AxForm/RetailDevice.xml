				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    /// Initialize the form.
    /// </summary>
    public void init()
    {
        super();

        RetailSelfServiceMigrationHelper::populateDefaultPackageReferencesFromRetailSharedParameters();
        RetailSelfServiceMigrationHelper::populateDefaultPackageReferencesFromRetailDefaultChannelPackages();
        selfServicePackageManager = new RetailSelfServiceMergedPackageManager();
    }

]]></Source>
			</Method>
			<Method>
				<Name>lookupMergedPackageReference</Name>
				<Source><![CDATA[
    void lookupMergedPackageReference(Common selectedRecord, FormReferenceControl referenceControl, boolean usingMergedPackages)
    {
        PackageType packageTypeToFilterBy;

        // Check if offline is configured for device
        if (selectedRecord.TableId == tableNum(RetailDevice))
        {
            RetailDevice devicesTable = selectedRecord;
            packageTypeToFilterBy = devicesTable.computePackageTypeFilter();
        }

        // Perform the lookup with provided PackageType filter.
        if (usingMergedPackages)
        {
            RetailSelfServiceMergedPackages::performLookup(packageTypeToFilterBy, referenceControl);
        }
        else
        {
            RetailSelfServicePackageInfo::PerformLookup(packageTypeToFilterBy, RetailDevice_SelfServicePackage);
        }
    }

]]></Source>
