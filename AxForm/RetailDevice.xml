						<Source><![CDATA[
        /// <summary>
        /// Download Modern POS setup file.
        /// </summary>
        public void clicked()
        {
            super();

            if (RetailSelfServiceMergedPackageManager::validateDownloadPackageReference(RetailDevice, fieldNum(RetailDevice, MergedSelfServicePackageReference)))
            {
                str packageDownloadUrl = selfServicePackageManager.generatePackageDownloadLink(RetailDevice.MergedSelfServicePackageReference, PackageType::ModernPosWithOffline, true);

                // Opens new tab and navigate to specified url
                RetailSelfServicePackageManager::OpenLinkInNewBrowserTab(packageDownloadUrl, "@RETAIL:SelfServicePkgRetrievalError");
            }
        }

]]></Source>
