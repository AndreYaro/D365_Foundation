			<Method>
				<Name>closeFinalReport</Name>
				<Source><![CDATA[
    void closeFinalReport()
    {
        InventFiscalLIFOJournalTransAdj inventFiscalLIFOJournalTransAdj;

        if (!inventFiscalLIFOJournalTable.checkAllowCloseFinalReport())
        {
            throw error("@SYS98350");
        }

        if (Box::okCancel("@SYS93010",DialogButton::Cancel,"@SYS98355") == DialogButton::Ok)
        {
            inventFiscalLIFOJournalTable.FinalReportClosed = NoYes::Yes;
            inventFiscalLIFOJournalTable.Posted            = NoYes::Yes;
            inventFiscalLIFOJournalTable.PostedDateTime    = DateTimeUtil::getSystemDateTime();
            inventFiscalLIFOJournalTable.PostedUserId      = curUserId();
            inventFiscalLIFOJournalTable.write();

            update_recordset inventFiscalLIFOJournalTransAdj
                setting FinalReportClosed = NoYes::Yes
                where inventFiscalLIFOJournalTransAdj.JournalId == inventFiscalLIFOJournalTable.JournalId;

            inventFiscalLIFOJournalTable_ds.reread();
            inventFiscalLIFOJournalTable_ds.refresh();
        }
    }

]]></Source>
