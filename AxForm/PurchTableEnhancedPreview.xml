			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class PurchTableEnhancedPreview extends FormRun
{
    private const int maxRecordsInPurchTableEnhancedPreview = 20;
}
]]></Source>
			</Method>
			<Method>
				<Name>overridePurchLineQuery</Name>
				<Source><![CDATA[
    private void overridePurchLineQuery()
    {
        RecIdCount countPurchaseOrders = this.countPurchOrdersInVendInvoiceInfoSubTable();

        if (countPurchaseOrders > 1)
        {
            VendInvoiceInfoTable localVendInvoiceInfoTable = element.args().record();
            VendInvoiceInfoSubTable vendInvoiceInfoSubTable;
            QueryBuildDataSource qbds = PurchLine_ds.query().dataSourceTable(tableNum(PurchLine));
            int countLoop = 1;

            qbds.clearDynalinks();
            qbds.clearRanges();

            while select OrigPurchId from vendInvoiceInfoSubTable
                where vendInvoiceInfoSubTable.ParmId == localVendInvoiceInfoTable.ParmId
                    && vendInvoiceInfoSubTable.TableRefId == localVendInvoiceInfoTable.TableRefId
                    && vendInvoiceInfoSubTable.OrigPurchId != ''
            {
                qbds.addRange(fieldNum(PurchLine, PurchId)).value(queryValue(vendInvoiceInfoSubTable.OrigPurchId));
                if (countLoop >= maxRecordsInPurchTableEnhancedPreview)
                {
                    break;
                }
                countLoop ++;
            }

            // Remove datasource in order to set text and label in PurchTable_purchId.
            PurchTable_purchId.dataSource('');
            PurchTable_purchId.text(strFmt("@AccountsPayable:CountOfPurchOrders", countPurchaseOrders));

            if (countPurchaseOrders > maxRecordsInPurchTableEnhancedPreview)
            {
                PurchTable_purchId.label(strFmt("@AccountsPayable:PreviewCountOfPurchOrders", maxRecordsInPurchTableEnhancedPreview));
            }
            else 
            {
                PurchTable_purchId.label("@SYS319737");
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>countPurchOrdersInVendInvoiceInfoSubTable</Name>
				<Source><![CDATA[
    private RecIdCount countPurchOrdersInVendInvoiceInfoSubTable()
    {
        VendInvoiceInfoTable localVendInvoiceInfoTable = element.args().record();
        VendInvoiceInfoSubTable vendInvoiceInfoSubTable;

        select count(RecId) from vendInvoiceInfoSubTable
            where vendInvoiceInfoSubTable.ParmId == localVendInvoiceInfoTable.ParmId
                && vendInvoiceInfoSubTable.TableRefId == localVendInvoiceInfoTable.TableRefId
                && vendInvoiceInfoSubTable.OrigPurchId != '';

        return vendInvoiceInfoSubTable.RecId;
    }

]]></Source>
			</Method>
