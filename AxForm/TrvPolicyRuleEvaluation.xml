[Form]
public class TrvPolicyRuleEvaluation extends FormRun
{
    TableName policyDocumentTableName;
    Microsoft.Dynamics.AX.Framework.Expression.Model.ExpressionModel model;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (element.args() && element.args().caller() && element.args().parm())
        {
            policyDocumentTableName = element.args().parm();
            model = element.args().caller().getModel();

            switch (policyDocumentTableName)
            {
                case tableStr(TrvExpTable):
                    // Select an expense report to evaluate the condition
                    mainInstruction.text("@SYS340862");
                    trvRequisitionTableGroup.visible(false);
                    trvRequisitionLineGroup.visible(false);
                    break;
                case tableStr(TrvExpTrans):
                    // Select an expense report line to evaluate the condition
                    mainInstruction.text("@SYS340861");
                    trvRequisitionTableGroup.visible(false);
                    trvRequisitionLineGroup.visible(false);
                    break;
                case tableStr(TrvRequisitionTable):
                    // Select an travel requisition to evaluate the condition
                    mainInstruction.text("@SYS340863");
                    trvExpTableGroup.visible(false);
                    trvExpTransGroup.visible(false);
                    break;
                case tableStr(TrvRequisitionLine):
                    // Select an travel requisition line to evaluate the condition
                    mainInstruction.text("@SYS340864");
                    trvExpTableGroup.visible(false);
                    trvExpTransGroup.visible(false);
                    break;
            }
        }
        else
        {
            throw error("@SYS22996"); // Form was called incorrectly.
        }
    }

]]></Source>
			</Method>
		</Methods>
