						<Name>clicked</Name>
						<Source><![CDATA[
        // Evaluate the expression and show the results.
        void clicked()
        {
            Common expressionRecord;

            switch (policyDocumentTableName)
            {
                case tablestr(TrvExpTable):
                    expressionRecord = trvExpTable;
                    break;
                case tablestr(TrvExpTrans):
                    expressionRecord = trvExpTrans;
                    break;
                case tableStr(TrvRequisitionTable):
                    expressionRecord = trvRequisitionTable;
                    break;
                case tableStr(TrvRequisitionLine):
                    expressionRecord = trvRequisitionLine;
                    break;
            }

            // The condition evaluated to: %1
            warning(strFmt("@SYS340860", SysExpression::evaluateTestExpression(
                expressionRecord.DataAreaId,
                expressionRecord.TableId,
                expressionRecord.RecId,
                model)));

            // Do not close yet. The user can test/evaluate other records.
            //super();
        }

]]></Source>
