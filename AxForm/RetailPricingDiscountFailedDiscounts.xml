					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        /// Create a batch job to re-run those failed discounts.
        /// </summary>
        public void clicked()
        {
            container offerIds;

            MultiSelectionHelper multiSelectionHelper = MultiSelectionHelper::construct();
            multiSelectionHelper.parmDatasource(RetailPeriodicDiscount_ds);

            RetailPeriodicDiscount selectedDiscounts = multiSelectionHelper.getFirst();

            while (selectedDiscounts.RecId != 0)
            {
                offerIds += selectedDiscounts.OfferId;
                selectedDiscounts = multiSelectionHelper.getNext();
            }

            RetailPeriodicDiscountPublishController controller = new RetailPeriodicDiscountPublishController();
            RetailPeriodicDiscountPublishContract dataContract = controller.getDataContractObject();
            dataContract.parmOfferIds(offerIds);
            controller.runOperation();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
