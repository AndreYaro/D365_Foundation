				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            if (!jmgTermReg)
            {
                jmgTermReg.Worker = jmgEmployee.Worker;
                jmgTermReg.ProfileDate = viewDate;
            }
            super();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>FindOpen</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            JmgStampJournalTrans    jmgStampJournalTrans;
            boolean                 found;

            super();

            while select ProfileDate
                from  jmgStampJournalTrans
                where jmgStampJournalTrans.Worker == jmgEmployee.Worker &&
                      jmgStampJournalTrans.Active
            {
                info(strfmt("@SYS39480", date2StrUsr(jmgStampJournalTrans.ProfileDate, DateFlags::FormatAll)));
                found = true;
            }

            if (!found)
                info("@SYS39481");
        }

]]></Source>
					</Method>
				</Methods>
