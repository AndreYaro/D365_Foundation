	<SourceCode>
		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
    [Form]
    public class JmgTermRegTrans extends FormRun
    {
         TransDate              viewDate;
         QueryBuildRange        rangeProfileDate;

}
]]></Source>
			</Method>
			<Method>
				<Name>dateSelect</Name>
				<Source><![CDATA[
    public edit TransDate dateSelect(boolean set, TransDate _date)
    {
        if (set)
        {
            viewDate = _date;
            rangeProfileDate.value(queryValue(viewDate));
            jmgTermReg_ds.executeQuery();
        }

        return viewDate;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (element.args() && element.args().dataset() == tablenum(JmgStampJournalTable))
        {
            JmgStampJournalTable jmgStampJournalTable = element.args().record();
            element.dateSelect(true, jmgStampJournalTable.ProfileDate);
        }
        else
        {
            element.dateSelect(true,DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone()));
        }
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>JmgTermReg</Name>
				<Methods>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;

            ret = super();

            if (ret)
            {
                jmgTermCloseRegTransButton.enabled((jmgTermReg.RegType ==JmgTermRegType::JobStart) && (jmgTermReg.JobActive == NoYes::Yes));
                ShowErrorCausesBtn.enabled(jmgTermReg.hasJobRegistrationError());
            }

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();
            rangeProfileDate = jmgTermReg_ds.query().dataSourceNo(1).addRange(fieldnum(JmgTermReg,ProfileDate));
        }

]]></Source>
					</Method>
				</Methods>
