        public void write()
        {
            boolean isInsert = !leanScheduleGroupItem.RecId;

            ttsbegin;

            super();

            if (isInsert)
            {
                switch (leanScheduleGroupItem.EntryType)
                {
                    case LeanScheduleGroupItemType::Item:
                        leanScheduleGroupEntrySingle.LeanScheduleGroupItem = leanScheduleGroupItem.RecId;
                        leanScheduleGroupEntrySingle.write();
                        leanScheduleGroupEntrySingle_ds.reread();
                        break;

                    case LeanScheduleGroupItemType::Group:
                        leanScheduleGroupEntryGroup.LeanScheduleGroupItem  = leanScheduleGroupItem.RecId;
                        leanScheduleGroupEntryGroup.write();
                        leanScheduleGroupEntryGroup_ds.reread();
                        break;
                }
            }

            ttscommit;
        }

