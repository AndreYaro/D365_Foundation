						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean ret = super();

            if (!leanScheduleGroupItem.RecId)
            {
                switch (leanScheduleGroupItem.EntryType)
                {
                    case LeanScheduleGroupItemType::Item:
                        leanScheduleGroupEntrySingle.LeanScheduleGroupItem = fakeRecId;
                        leanScheduleGroupEntrySingle.InventDimId           = InventDim::findOrCreate(inventDim).InventDimId;
                        ret = ret && leanScheduleGroupEntrySingle.validateWrite();
                        ret = ret && leanScheduleGroupEntrySingle.validateUniqueness(leanScheduleGroup);
                        leanScheduleGroupEntrySingle.LeanScheduleGroupItem = 0;
                        break;

                    case LeanScheduleGroupItemType::Group:
                        leanScheduleGroupEntryGroup.LeanScheduleGroupItem = fakeRecId;
                        ret = ret && leanScheduleGroupEntryGroup.validateAllocationId(leanScheduleGroup.WorkCell);
                        ret = ret && leanScheduleGroupEntryGroup.validateWrite();
                        leanScheduleGroupEntryGroup.LeanScheduleGroupItem = 0;
                        break;
                }
            }

            return ret;
        }

]]></Source>
