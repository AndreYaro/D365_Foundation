			<Method>
				<Name>clear</Name>
				<Source><![CDATA[
    public void clear(boolean _cleanInventDimId = true, boolean _clearFKs = false)
    {
        const str emptyValue = '';
        InventDim inventDimEmpty;

        if (_clearFKs)
        {
            leanScheduleGroupEntrySingle.ItemId         = emptyValue;
            leanScheduleGroupEntrySingle.InventDimId    = emptyValue;
            leanScheduleGroupEntryGroup.AllocationId    = emptyValue;
        }

        if (_cleanInventDimId ||
            _clearFKs)
        {
            inventDim.InventDimFieldsMap::initProductDimensionsFrom(inventDimEmpty);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    public void updateDesign(InventDimFormDesignUpdate _mode)
    {
        InventDimParm   inventDimParmShow;
        InventDimParm   inventDimParmEnabled;

        leanScheduleGroupEntryGroup_ds.allowEdit(leanScheduleGroupItem.EntryType == LeanScheduleGroupItemType::Group);
        leanScheduleGroupEntrySingle_ds.allowEdit(leanScheduleGroupItem.EntryType == LeanScheduleGroupItemType::Item);

        switch (_mode)
        {
            case InventDimFormDesignUpdate::Init :
                if (!inventDimFormSetup)
                {
                    inventDimFormSetup = InventDimCtrl::newFromForm(element);
                }
                inventDimParmShow.initProductDimensionsAllGroups();
                inventDimFormSetup.parmDimParmVisibleGrid(inventDimParmShow);

                GanttColorEdt.colorScheme(FormColorScheme::RGB);
            // No break

            case InventDimFormDesignUpdate::Active, InventDimFormDesignUpdate::FieldChange :
                inventDimParmEnabled = EcoResProductDimGroupSetup::newItemId(leanScheduleGroupEntrySingle.ItemId).inventDimParmActiveDimensions();
                inventDimFormSetup.parmDimParmEnabled(inventDimParmEnabled);
                inventDimFormSetup.formSetControls(true);
                break;

            case InventDimFormDesignUpdate::SetupChange:
                break;

            default :
                // Incorrect parameter for the function '%1'
                throw error(strfmt("@SYS54195", funcname()));
        }
    }

]]></Source>
