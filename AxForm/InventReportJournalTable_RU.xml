			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
    [Form]
    public class InventReportJournalTable_RU extends FormRun
    {
        InventJournalReportType_RU  inventReportType;
        InventDimCtrl_Frm_Mov       inventDimFormSetup;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        if ( ! element.args() || ! element.args().parmEnumType() || ! (element.args().parmEnumType() == enumnum(InventJournalReportType_RU)))
        {
            throw error(Error::wrongUseOfFunction(funcname()));
        }

        inventReportType = element.args().parmEnum();

        if (inventReportType == InventJournalReportType_RU::Transfer)
        {
            throw error(Error::wrongUseOfFunction(funcname()));
        }

        super();

        this.setCaption();

        element.updateDesign(InventDimFormDesignUpdate::Init);
    }

]]></Source>
			</Method>
			<Method>
				<Name>inventDimSetupObject</Name>
				<Source><![CDATA[
    Object inventDimSetupObject()
    {
        return inventDimFormSetup;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setCaption</Name>
				<Source><![CDATA[
    void setCaption()
    {
        const str Caption = '\%1 - \%2 \%3';
        switch (inventReportType)
        {
            case InventJournalReportType_RU::Loss:
                this.design().caption(strfmt(Caption, "@GLS102180", "@GLS102191", inventJournalReportTable.JournalId));
                break;

            case InventJournalReportType_RU::BOMReq:
                this.design().caption(strfmt(Caption, "@GLS102181", "@GLS102191", inventJournalReportTable.JournalId));
                break;

            case InventJournalReportType_RU::CountColl:
                this.design().caption(strfmt(Caption, "@GLS102182", "@GLS102191", inventJournalReportTable.JournalId));
                break;

            case InventJournalReportType_RU::CountingList:
                this.design().caption(strfmt(Caption, "@GLS113027", "@GLS102191", inventJournalReportTable.JournalId));
                break;

            case InventJournalReportType_RU::Transfer:
                this.design().caption(strfmt(Caption, "@GLS102183", "@GLS102191", inventJournalReportTable.JournalId));
                break;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    void updateDesign(InventDimFormDesignUpdate _mode)
    {
        InventMovement tmpmovement = InventMovement::construct(inventJournalTrans);

        switch (_mode)
        {
            case InventDimFormDesignUpdate::Init:
                if (!inventDimFormSetup)
                {
                    inventDimFormSetup = InventDimCtrl_Frm_Mov::newFromFormMovement(element, tmpmovement);
                }
            case InventDimFormDesignUpdate::Active:
                inventDimFormSetup.formActiveSetup(tmpmovement.inventDimGroupSetup(), tmpmovement);
                inventDimFormSetup.formSetControls(true);
                break;
            case InventDimFormDesignUpdate::FieldChange :
                inventDimFormSetup.formActiveSetup(tmpmovement.inventDimGroupSetup(), tmpmovement);
                inventDimFormSetup.formSetControls(false);
                break;

            default : throw error(strfmt("@SYS54195",funcname()));
        }
    }

]]></Source>
			</Method>
