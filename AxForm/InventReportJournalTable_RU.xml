				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        void init()
        {
            super();

            inventJournalReportTable_ds.query().dataSourceNo(1).addRange(fieldnum(InventJournalReportTable_RU, ReportTypeInvent)).value(SysQuery::value(inventReportType));
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        int active()
        {
            const str NotZero = '! 0';
            const str LessZero = '<0';
            int ret = super();

            element.setCaption();

            inventJournalTrans_ds.query().dataSourceTable(tablenum(InventJournalTrans)).clearRanges();

            findOrCreateRange_W(inventJournalTrans_ds.query().dataSourceTable(tablenum(InventJournalTrans)), fieldnum(InventJournalTrans, Qty)).value(NotZero);

            switch (inventReportType)
            {
                case InventJournalReportType_RU::Loss:
                    findOrCreateRange_W(inventJournalTrans_ds.query().dataSourceTable(tablenum(InventJournalTrans)), fieldnum(InventJournalTrans, TransDate)).value(queryValue(inventJournalReportTable.TransDate));
                    findOrCreateRange_W(inventJournalTrans_ds.query().dataSourceTable(tablenum(InventJournalTrans)), fieldnum(InventJournalTrans, Qty)).value(LessZero);
                break;
                case InventJournalReportType_RU::BOMReq:
                    findOrCreateRange_W(inventJournalTrans_ds.query().dataSourceTable(tablenum(InventJournalTrans)), fieldnum(InventJournalTrans, TransDate)).value(queryValue(inventJournalReportTable.TransDate));
                    if (inventJournalTrans.JournalType == InventJournalType::BOM)
                    {
                        findOrCreateRange_W(inventJournalTrans_ds.query().dataSourceTable(tablenum(InventJournalTrans)), fieldnum(InventJournalTrans, bomLine)).value(queryValue(NoYes::Yes));
                    }
                    else
                    {
                        findOrCreateRange_W(inventJournalTrans_ds.query().dataSourceTable(tablenum(InventJournalTrans)), fieldnum(InventJournalTrans, Qty)).value(LessZero);
                    }
                break;
            }

            return ret;
        }

]]></Source>
