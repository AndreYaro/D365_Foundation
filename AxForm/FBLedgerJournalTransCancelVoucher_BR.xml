			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    public boolean canClose()
    {
        FBLedgerJournalTransCancelVoucher_BR cancelVoucher;
        boolean canClose;

        canClose = super();

        try
        {
            if (element.closedOk() && canClose)
            {
                cancelVoucher = FBLedgerJournalTransCancelVoucher_BR::newLedgerJournalTable(ledgerJournalTable);

                cancelVoucher.parmTmpFrmVirtualLines(tmpFrmVirtualLines);
                cancelVoucher.updateNow();

                element.args().caller().research();
            }
        }
        catch
        {
            canClose = false;
        }

        return canClose;
    }

]]></Source>
