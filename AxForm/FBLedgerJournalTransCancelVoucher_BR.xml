					<Method>
						<Name>marTrans</Name>
						<Source><![CDATA[
        edit NoYes marTrans(boolean            _set,
                            LedgerJournalTrans _ledgerJournalTrans,
                            NoYes              _mark)
        {
            NoYes       marTrans;

            if (_set)
            {
                if (_mark)
                {
                    tmpFrmVirtualLines.RecordNo     = _ledgerJournalTrans.RecId;
                    tmpFrmVirtualLines.write();
                    marTrans = true;
                }
                else
                {
                    delete_from tmpFrmVirtualLines
                        where tmpFrmVirtualLines.RecordNo == _ledgerJournalTrans.RecId;
                }
            }
            else
            {
                select tmpFrmVirtualLines
                    where tmpFrmVirtualLines.RecordNo   == _ledgerJournalTrans.RecId;

                marTrans = tmpFrmVirtualLines.RecId != 0;
            }

            return marTrans;
        }

]]></Source>
