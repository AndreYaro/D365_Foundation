			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (element.args().dataset() != tableNum(WHSContainerTable))
        {
            throw error("@SYS22539");
        }

        WHSContainerTable containerTable = element.args().record();
        WHSShipmentTable shipmentTable = WHSShipmentTable::find(containerTable.ShipmentId);

        super();

        if (containerTable || shipmentTable)
        {
            TMSShipmentAccessorialTable_CarrierCode.text(shipmentTable.CarrierCode);
            TMSShipmentAccessorialTable_CarrierServiceCode.text(shipmentTable.CarrierServiceCode);
            TMSShipmentAccessorialTable_ShipmentId.text(containerTable.ShipmentId);
            TMSShipmentAccessorialTable_ContainerId.text(containerTable.ContainerId);

            level = TMSRouteAccessorialLevel::Container;
        }
    }

]]></Source>
			</Method>
