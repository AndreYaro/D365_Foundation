						<Source><![CDATA[
        public void initValue()
        {
            TMSShipmentAccessorialTable.CarrierCode = TMSShipmentAccessorialTable_CarrierCode.text();
            TMSShipmentAccessorialTable.CarrierServiceCode = TMSShipmentAccessorialTable_CarrierServiceCode.text();
            TMSShipmentAccessorialTable.ShipmentId = TMSShipmentAccessorialTable_ShipmentId.text();
            TMSShipmentAccessorialTable.ContainerId = TMSShipmentAccessorialTable_ContainerId.text();
            TMSShipmentAccessorialTable.RouteAccessorialLevel = level;
            TMSShipmentAccessorialTable.AccessorialAssignmentLevel = TMSAccessorialAssignmentLevel::Package;

            super();
        }

]]></Source>
