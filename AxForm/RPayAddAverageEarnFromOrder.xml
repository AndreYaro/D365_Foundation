			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        addAEFromOrder = element.args().caller();

        super();

        this.initFormDesign();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initFormDesign</Name>
				<Source><![CDATA[
    private void initFormDesign()
    {
        element.design().caption(addAEFromOrder.formCaption());

        switch (addAEFromOrder.journalType())
        {
            case RHRMJournalType::Vacation:
                rhrmOrderTrans_DS.object(fieldNum(RHRMOrderTrans, BusinessTripTypeId)).visible(false);
                rhrmOrderTrans_DS.object(fieldNum(RHRMOrderTrans, TripObject)).visible(false);
                rhrmOrderTrans_DS.object(fieldNum(RHRMOrderTrans, TripAtCost)).visible(false);
                businessTripTypeGroup.visible(false);
                tripPurpose.visible(false);
                tripDueToMeans.visible(false);
                break;

            case RHRMJournalType::Trip:
                rhrmOrderTrans_DS.object(fieldNum(RHRMOrderTrans, VacationTypeId)).visible(false);
                rhrmOrderTrans_DS.object(fieldNum(RHRMOrderTrans, PayVacationDuration)).visible(false);
                typeOfVacationGroup.visible(false);
                vacationPeriod.visible(false);
                break;
        }
    }

]]></Source>
			</Method>
