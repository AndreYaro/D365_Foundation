			<Method>
				<Name>updateMonthlyAverageCost_CN</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates form controls based on invent model group selection.
    /// </summary>

    void updateMonthlyAverageCost_CN()
    {
        if (inventModelGroup.InventModel == InventModel::MonthlyAverageCost)
        {
            inventModelGroup_ds.object(fieldnum(InventModelGroup, StandardCost)).allowEdit(false);
            inventModelGroup_ds.object(fieldnum(InventModelGroup, StandardCost)).skip(true);
        }
    }

]]></Source>
