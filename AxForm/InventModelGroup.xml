				<Source><![CDATA[
    protected void setLookupRecordFromArgs()
    {
        InventModelGroupItem    inventModelGroupItem;

        if (this.args().record() is InventModelGroupItem)
        {
            inventModelGroupItem = this.args().record() as InventModelGroupItem;

            this.args().record(null);
            this.args().lookupTable(tableNum(InventModelGroup));
            this.args().lookupField(fieldNum(InventModelGroup, ModelGroupId));
            this.args().lookupValue(inventModelGroupItem.ModelGroupId);
        }
    }

]]></Source>
