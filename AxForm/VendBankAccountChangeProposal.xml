			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (!this.args() || !this.args().record() || this.args().record().TableId != tableNum(VendBankAccount))
        {
            throw error(strFmt("@SYS22678", element.name()));
        }
        else
        {
            callerRecord = this.args().record() as VendBankAccount;
        }

        if (!callerRecord.canModifyChangeProposal())
        {
            element.disableDiscardButtons();
            if (this.args().parm())
            {
                warning("@AccountsPayable:VendTableChangeProposal_ChangesNotAllowed");
                this.args().parm('');
            }
        }

        if (this.args().parm())
        {
            info(this.args().parm());
        }
    }

]]></Source>
			</Method>
