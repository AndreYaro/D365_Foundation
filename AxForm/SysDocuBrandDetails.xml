				<Source><![CDATA[
    /// <summary>
    /// Show images in form controls if images have been set up.
    /// </summary>
    private void showLogo()
    {
        try
        {
            if (SysDocuBrandDetails.Image1Name)
            {
                element.showImage(SysDocuBrandImages::find(SysDocuBrandDetails.Image1Name).Image, Image1);
            }
            else
            {
                element.showImage(conNull(), Image1);
            }
            if (SysDocuBrandDetails.Image2Name)
            {
                element.showImage(SysDocuBrandImages::find(SysDocuBrandDetails.Image2Name).Image, Image2);
            }
            else
            {
                element.showImage(conNull(), Image2);
            }
            if (SysDocuBrandDetails.Image3Name)
            {
                element.showImage(SysDocuBrandImages::find(SysDocuBrandDetails.Image3Name).Image, Image3);
            }
            else
            {
                element.showImage(conNull(), Image3);
    }
        }
        catch (Exception::Warning)
        {
            throw error("@SYS60147");
        }
    }

]]></Source>
