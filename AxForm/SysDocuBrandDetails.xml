				<Source><![CDATA[
    /// <summary>
    /// Display image in form image control
    /// </summary>
    /// <param name = "imageData">The container of image data.</param>
    /// <param name = "image">The form image control</param>
    private void showImage(Container imageData, FormWindowControl image)
    {
        Image   logoImage;

        if (imageData)
        {
            logoImage = new Image();
            logoImage.setData(imageData);
            image.image(logoImage);
        }
        else
        {
            image.image(null);
        }
    }

]]></Source>
