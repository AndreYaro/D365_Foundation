			<Method>
				<Name>validateReportName</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validate whether report name can be found in metadata collection.
    /// </summary>
    /// <param name = "reportName">The name of report.</param>
    /// <returns><c>True</c> if the report name is valid, otherwise, <c>False</c>.</returns>
    public boolean validateReportName(str reportName)
    {
        boolean found = false;

        if (reportName)
        {
            System.Collections.Specialized.StringEnumerator reportNamesCollection = Microsoft.Dynamics.Ax.Xpp.MetadataSupport::ReportNames();
            while (reportNamesCollection.MoveNext())
            {
                if (reportName == reportNamesCollection.Current)
                {
                    found = true;
                    break;
                }
            }
        }

        return found;
    }

]]></Source>
			</Method>
			<Method>
				<Name>validateDesignName</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validate whether design name can be found in report.
    /// </summary>
    /// <param name = "designName">The name of design.</param>
    /// <returns><c>True</c> if the design name is valid, otherwise, <c>False</c></returns>
    public boolean validateDesignName(str designName)
    {
        boolean found = false;

        if (designName)
        {
            str reportName = Overview_ReportName.text();
            if (reportName)
            {
                Microsoft.Dynamics.AX.Metadata.MetaModel.AxReport reportMetadata = Microsoft.Dynamics.Ax.Xpp.MetadataSupport::GetReport(reportName);
                if (reportMetadata)
                {
                    var designCollection = reportMetadata.get_Designs();
                    if (designCollection)
                    {
                        var reportDesignEnum = designCollection.GetEnumerator();
                        while (reportDesignEnum.MoveNext())
                        {
                            if (designName == reportDesignEnum.get_Current().Name)
                            {
                                found = true;
                                break;
                            }
                        }
                    }
                }
            }
        }

        return found;
    }

]]></Source>
