			<Method>
				<Name>getReportNameTmpTable</Name>
				<Source><![CDATA[
    /// <summary>
    /// Get temp table for report name look-up.
    /// </summary>
    /// <returns>The temp table with all report names.</returns>
    public TmpSrsReportName getReportNameTmpTable()
    {
        System.Collections.Specialized.StringEnumerator reportNamesCollection = Microsoft.Dynamics.Ax.Xpp.MetadataSupport::ReportNames();
        while (reportNamesCollection.MoveNext())
        {
            reportNameTable.ReportName = reportNamesCollection.Current;
            reportNameTable.insert();
        }

        return reportNameTable;
    }

]]></Source>
