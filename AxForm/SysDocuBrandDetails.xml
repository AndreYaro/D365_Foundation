						<Name>validateWrite</Name>
						<Source><![CDATA[
        /// <summary>
        /// Validate whether the brand document has been specified in the system.
        /// </summary>
        /// <returns><c>True</c> if the new brand info. is valid, otherwise, <c>False</c></returns>
        public boolean validateWrite()
        {
            boolean ret;
            str brandId = Overview_BrandId.text();
            str reportName = Overview_ReportName.text();
            str designName = Overview_DesignName.text();

            ret = super();

            SysDocuBrandDetails brandDetails = SysDocuBrandDetails::findBrandDocument(brandId, reportName, designName);
            if (brandDetails.RecId != 0 && this.cursor().RecId != brandDetails.RecId)
            {
                ret =  checkFailed(strFmt("@sys328054", brandId));
            }

            return ret;
        }

]]></Source>
