			<Method>
				<Name>setColor</Name>
				<Source><![CDATA[
    /// <summary>
    /// Set color for form string control.
    /// </summary>
    /// <param name = "control">The form string control.</param>
    /// <returns>The color value from the control.</returns>
    public int setColor(FormStringControl control)
    {
        int color = 0;
        if (control)
        {
            str valueStr = control.valueStr();
            if (valueStr)
            {
                str colorHex = subStr(valueStr, 2, strLen(valueStr) - 1);
                color = hex2Int(colorHex);
            }
        }
        color = ColorSelection::selectColorStringControl(control, color);

        return color;
    }

]]></Source>
