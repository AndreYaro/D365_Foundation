			<Method>
				<Name>adjustLookupDataSource</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Replaces default lookup datasource in case it does not match main datasource on the form.
    /// </summary>
    /// <remarks>
    ///     In some cases when this form is opened via View Details function <c>ReqPlanForecast</c> is passed
    ///     as lookup datasource. However this form uses ReqPlan as it's main datasource - this should be adjusted in order for the
    ///     View Details to disambiguate the lookup argumets.
    /// </remarks>
    private void adjustLookupDataSource()
    {
        Args args = element.args();

        if ( args.lookupTable() == tableNum(ReqPlanForecast) && args.lookupField() == fieldNum(ReqPlanForecast,ReqPlanIdForecast) )
        {
            args.lookupTable(tableNum(ReqPlan));
            args.lookupField(fieldNum(ReqPlan,ReqPlanId));
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>enableFields</Name>
				<Source><![CDATA[
    void enableFields()
    {
        reqPlan_ds.object(fieldnum(ReqPlan, ForecastReduceType)).enabled(!reqPlanForecast.SkipCovCalc);
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        ReqParameters::checkPlanningProcessesAreEnabled();

        super();

        this.adjustLookupDataSource();
    }

]]></Source>
			</Method>
