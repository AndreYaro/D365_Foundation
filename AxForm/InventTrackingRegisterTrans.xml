				<Name>updateScanModeButtons</Name>
				<Source><![CDATA[
    public void updateScanModeButtons()
    {
        if (formController.isReadonly())
        {
            return;
        }

        if (registerMode)
        {
            registerModeButton.toggleValue(1);
            unregisterModeButton.toggleValue(0);
        }
        else
        {
            registerModeButton.toggleValue(0);
            UnregisterModeButton.toggleValue(1);
        }

        formController.postRegisterModeChange();
        element.refresh();
    }

]]></Source>
