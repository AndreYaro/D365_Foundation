			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        formController = InventTrackingRegisterTransForm::construct(this.args());
        sysDictEnum = new SysDictEnum(enumNum(FormStringControlExtendedStyle));

        super();

        requiredQuantity.value(real2int(formController.requiredQty()));
        registerMode = true;
        this.updateDesign();
        blankInventDimId = InventDim::findOrCreateBlank().InventDimId;
    }

]]></Source>
			</Method>
			<Method>
				<Name>performRegistrationAction</Name>
				<Source><![CDATA[
    public boolean performRegistrationAction()
    {
        if (serialIdInput.valueStr() != #EmptyString && !element.inViewMode())
        {
            if (registerMode)
            {
                formController.insertSerialNumber(serialIdInput.valueStr());
            }
            else
            {
                formController.unregisterSerialNumber(serialIdInput.valueStr());
            }

            this.refresh();
            serialIdInput.text(#EmptyString);
            serialIdInput.setFocus();

            return true;
        }

        return false;
    }

]]></Source>
			</Method>
			<Method>
				<Name>recalculateTotal</Name>
				<Source><![CDATA[
    public void recalculateTotal()
    {
        boolean canAddMore;

        if (formController.isReadonly())
        {
            return;
        }

        registeredQuantity.value(any2int(formController.getTotalLinesCount()));
        remainingQuantity.value(any2int(formController.remainingQty()));

        canAddMore = remainingQuantity.value() > 0;
        inventTrackingRegisterTransAddSerial.enabled(canAddMore);
        inventTrackingRegisterTransAddUnknown.enabled(canAddMore);
        inventTrackingRegisterTransAddUnknownMaintain.enabled(canAddMore);

        removeAll.enabled(registeredQuantity.value() > 0);
    }

]]></Source>
			</Method>
			<Method>
				<Name>refresh</Name>
				<Source><![CDATA[
    /// <summary>
    /// Refreshes the status text field.
    /// </summary>
    public void refresh()
    {
        inventTrackingRegisterTrans_1_DS.executeQuery();

        if (formController.statusMessageIsError())
        {
            scanStatus.extendedStyle(sysDictEnum.index2Symbol(FormStringControlExtendedStyle::entityStatus_warning));
        }
        else
        {
            scanStatus.extendedStyle(sysDictEnum.index2Symbol(FormStringControlExtendedStyle::entityStatus_info));
        }

        scanStatus.text(formController.parmStatusMessage());
        scanStatusSerialNumber.text(serialIdInput.text());
        serialIdInput.setFocus();
    }

]]></Source>
			</Method>
			<Method>
				<Name>refreshManualStatus</Name>
				<Source><![CDATA[
    /// <summary>
    /// Refreshes the status text field when manuel entering serial numbers.
    /// </summary>
    public void refreshManualStatus()
    {
        if (formController.statusMessageIsError())
        {
            warning(formController.parmStatusMessage());
        }

        scanStatus.text(#EmptyString);
        scanStatus.extendedStyle(sysDictEnum.index2Symbol(FormStringControlExtendedStyle::entityStatus_info));

        scanStatusSerialNumber.text(#EmptyString);

        inventTrackingRegisterTrans_1_DS.executeQuery();

        if (ScanTab.isExpanded())
        {
            serialIdInput.setFocus();
        }
    }

]]></Source>
