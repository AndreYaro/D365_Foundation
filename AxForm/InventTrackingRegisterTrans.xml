			<Method>
				<Name>updateControlsVisibility</Name>
				<Source><![CDATA[
    private void updateControlsVisibility()
    {
        boolean isNotReadOnly = !formController.isReadonly();

        infoGroup.visible(isNotReadOnly);
        inputAndScanMode.visible(isNotReadOnly);
        scanMode.visible(isNotReadOnly);
        scanStatusGroup.visible(isNotReadOnly);
        actionPane.visible(isNotReadOnly);

        inventTrackingRegisterTrans_1_DS.allowEdit(isNotReadOnly);
        inventTrackingRegisterTrans_1_DS.allowDelete(isNotReadOnly);
        inventTrackingRegisterTrans_1_DS.allowCreate(isNotReadOnly);

        maintain.enabled(formController.parmCanAddRemove());
        inventTrackingRegisterTransAddSerial.enabled(formController.parmCanAddRemove());
        inventTrackingRegisterTransAddUnknown.enabled(formController.parmCanAddRemove());
        inventTrackingRegisterTransAddUnknownMaintain.enabled(formController.parmCanAddRemove());

        inventTrackingRegisterTrans_1_DS.allowDelete(formController.parmCanAddRemove());
        inventTrackingRegisterTrans_1_DS.allowCreate(formController.parmCanAddRemove());
        serialStatus.visible(isNotReadOnly && formController.parmShowStatus());

        if (infoGroup.visible())
        {
            registeredQuantity.label(formController.lineCountLabel());
        }
    }

]]></Source>
