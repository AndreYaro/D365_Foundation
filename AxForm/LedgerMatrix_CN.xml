			<Method>
				<Name>lookupSubAccountControl</Name>
				<Source><![CDATA[
    MainAccount lookupSubAccountControl(FormReferenceControl _formReferenceControl)
    {
        MainAccount                 mainAccount;
        MainAccountNum              subLevelAccountNum;

        Query                       lookupQuery = new Query();
        QueryBuildDataSource        qbds;
        SysReferenceTableLookup     sysTableLookup = SysReferenceTableLookup::newParameters(tableNum(MainAccount), _formReferenceControl, true);

        subLevelAccountNum = element.getRangeValueForSubAccount(LedgerMatrixTable);

        qbds = lookupQuery.addDataSource(tableNum(MainAccount));
        qbds.addRange(fieldNum(MainAccount, MainAccountId)).value(subLevelAccountNum);
        MainAccount::addSecureRangeForChartOfAccounts(qbds);

        sysTableLookup.addLookupfield(fieldNum(MainAccount, mainAccountId));
        sysTableLookup.addLookupMethod(tableMethodStr(MainAccount, localizedName));

        sysTableLookup.parmQuery(lookupQuery);
        mainAccount = SysTableLookup.performFormLookup();

        return mainAccount;
    }

]]></Source>
			</Method>
			<Method>
				<Name>resolveSubAccountControl</Name>
				<Source><![CDATA[
    MainAccount resolveSubAccountControl(FormReferenceControl _formReferenceControl)
    {
        MainAccount             mainAccount;
        MainAccountNum          mainAccountId;
        Query                   query = new Query();
        QueryBuildDataSource    qbds;
        QueryRun                queryRun;

        mainAccountId = _formReferenceControl.filterValue(AbsoluteFieldBinding::construct(fieldStr(MainAccount, MainAccountId), tableStr(MainAccount))).value();

        // If no MainAccountId, short circuit and return the empty buffer
        if (mainAccountId)
        {
            qbds = query.addDataSource(tableNum(MainAccount));
            qbds.addRange(fieldNum(MainAccount, MainAccountId)).value(mainAccountId);
            mainAccount::addSecureRangeForChartOfAccounts(qbds);
            queryRun = new QueryRun(query);
            queryRun.next();
            mainAccount = queryRun.get(tableNum(MainAccount));
        }

        return mainAccount;
    }

]]></Source>
			</Method>
