			<Method>
				<Name>updateDesignMain</Name>
				<Source><![CDATA[
    public void updateDesignMain(InventDimFormDesignUpdate _mode,
                                 InventTable               _inventTable = null)
    {
        InventDimParm   inventDimParmEnabled;
        Set             validDataSources;

        if (_mode == InventDimFormDesignUpdate::Init    ||
            _mode == InventDimFormDesignUpdate::Active  ||
            _mode == InventDimFormDesignUpdate::FieldChange)
        {
            if (! inventDimFormSetupMain)
            {
                validDataSources = new Set(Types::String);
                validDataSources.add(inventByProductTable_DS.name());
                validDataSources.add(inventDimMain_DS.name());

                inventDimFormSetupMain = InventDimCtrl::newFromFormValidDataSources(element, validDataSources);
            }

            if (_inventTable)
            {
                inventDimParmEnabled = EcoResProductDimGroupSetup::newInventTable(_inventTable).inventDimParmActiveDimensions();
                inventDimFormSetupMain.parmDimParmEnabled(inventDimParmEnabled);
            }

            inventDimFormSetupMain.formSetControls(true);
        }
        else
        {
            throw error(strfmt("@SYS54195",funcname()));
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesignProd</Name>
				<Source><![CDATA[
    public void updateDesignProd(InventDimFormDesignUpdate  _mode,
                                InventTable                 _inventTable = null)
    {
        InventDimParm           inventDimParmEnabled;
        Set                     validDataSources;
        ProdJournalRelease_RU   prodJournalRelease;

        if (_mode == InventDimFormDesignUpdate::Init    ||
            _mode == InventDimFormDesignUpdate::Active  ||
            _mode == InventDimFormDesignUpdate::FieldChange)
        {
            prodJournalRelease.initFromInventByProduct(inventByProductTable);

            if (!inventDimFormSetupProd)
            {
                validDataSources = new Set(Types::String);
                validDataSources.add(inventByProductTable_DS.name());
                validDataSources.add(inventDimByProd_DS.name());

                inventDimFormSetupProd = InventDimCtrl_Frm_Mov_ProdRelease_RU::newFromFormMovementDataSources(element, prodJournalRelease.inventMovement(), validDataSources);
            }

            if (_inventTable)
            {
                inventDimParmEnabled = EcoResProductDimGroupSetup::newInventTable(_inventTable).inventDimParmActiveDimensions();
                inventDimFormSetupProd.parmDimParmEnabled(inventDimParmEnabled);
            }

            inventDimFormSetupProd.formSetControls(true);
        }
        else
        {
            throw error(strfmt("@SYS54195",funcname()));
        }
    }

]]></Source>
