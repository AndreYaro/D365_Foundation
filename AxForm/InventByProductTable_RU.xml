						<Name>write</Name>
						<Source><![CDATA[
        void write()
        {
            try
            {
                ttsbegin;
                inventByProductTable.InventDimIdMain    = InventDim::findOrCreate(inventDimMain).InventDimId;
                inventByProductTable.InventDimIdByProd  = InventDim::findOrCreate(inventDimByProd).InventDimId;
                ttscommit;

                ttsbegin;

                super();

                if (inventByProductTable.InventDimIdMain != inventDimMain.InventDimId)
                {
                    inventDimMain.data(InventDim::find(inventByProductTable.InventDimIdMain));
                    inventDimMain_ds.setCurrent();
                }

                if (inventByProductTable.InventDimIdByProd != inventDimByProd.InventDimId)
                {
                    inventDimByProd.data(InventDim::find(inventByProductTable.InventDimIdByProd));
                    inventDimByProd_ds.setCurrent();
                }

                ttscommit;

                inventByProductTable_ds.active();
            }
            catch (Exception::Error)
            {
                throw Exception::Error;
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret = super();

            element.enableFields();

            element.updateDesignMain(InventDimFormDesignUpdate::Active, inventByProductTable.inventTableByMain());
            element.updateDesignProd(InventDimFormDesignUpdate::Active, inventByProductTable.inventTableByProd());

            inventDimMain_ds.allowEdit(!inventByProductTable.RecId);
            inventDimByProd_ds.allowEdit(!inventByProductTable.RecId);

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>validateWrite</Name>
						<Source><![CDATA[
        public boolean validateWrite()
        {
            ttsbegin;
            inventByProductTable.InventDimIdMain    = InventDim::findOrCreate(inventDimMain).InventDimId;
            inventByProductTable.InventDimIdByProd  = InventDim::findOrCreate(inventDimByProd).InventDimId;
            ttscommit;

            return super();
        }

]]></Source>
					</Method>
					<Method>
						<Name>percentage</Name>
						<Source><![CDATA[
        edit Percent percentage(boolean                 _set,
                                InventByProductTable_RU _inventByProductTable,
                                Percent                 _percentage)
        {
            if (_inventByProductTable.ByProdCosting == ProdByProdCosting_RU::Calculated)
            {
                if (_set)
                {
                    _inventByProductTable.Percentage = _percentage;
                    if (!_inventByProductTable.validateField(fieldnum(InventByProductTable_RU, Percentage)))
                    {
                        throw Exception::Error;
                    }
                }

                return _inventByProductTable.Percentage;
            }

            return _inventByProductTable.inventTableByProd().inventTableModuleInvent().price();
        }

]]></Source>
