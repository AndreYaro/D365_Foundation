			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        element.updateDesignMain(InventDimFormDesignUpdate::Init);
        element.updateDesignProd(InventDimFormDesignUpdate::Init);
    }

]]></Source>
			</Method>
			<Method>
				<Name>itemId</Name>
				<Source><![CDATA[
    ItemId itemId()
    {
        if (isItemIdByProd)
        {
            return inventByProductTable.ItemIdByProd;
        }

        return inventByProductTable.ItemIdMain;
    }

]]></Source>
