				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            QueryBuildRange generationSourceRange;
            const str rangeFormatterWithOr = "fieldStr(PayrollEarningStatementLine, GenerationSource) == enum2int(PayrollGenerationSource::Retroactive) || fieldStr(PayrollEarningStatementLine, GenerationSource) == enum2int(PayrollGenerationSource::Salary)";

            super();

            // Show only Salary and Retroactive lines
            generationSourceRange = this.query().dataSourceTable(tableNum(PayrollEarningStatementLine)).addRange(fieldNum(PayrollEarningStatementLine, GenerationSource));
            generationSourceRange.value(rangeFormatterWithOr);
            generationSourceRange.status(RangeStatus::Locked);
            generationSourceRange.status(RangeStatus::Hidden);
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            QueryBuildDataSource queryBuildDataSourceEarningStatementLine;
            QueryBuildRange noneRange;
            QueryBuildRange onHoldRange;
            QueryBuildRange releasedRange;

            queryBuildDataSourceEarningStatementLine = this.queryBuildDataSource();

            // Clear all ranges
            queryBuildDataSourceEarningStatementLine.clearRanges();

            if (PayrollPaymentProcessingStatus.selection() == PayrollPaymentProcessingStatusFilter::NoneOrOnHold)
            {
                // Add a range for the payment status of None
                noneRange = queryBuildDataSourceEarningStatementLine.addRange(fieldNum(PayrollEarningStatementLine, PaymentStatus));
                noneRange.value(queryValue(PayrollPaymentProcessingStatus::None));
                noneRange.status(RangeStatus::Locked);

                // Add a range for the payment status of OnHold
                onHoldRange = queryBuildDataSourceEarningStatementLine.addRange(fieldNum(PayrollEarningStatementLine, PaymentStatus));
                onHoldRange.value(queryValue(PayrollPaymentProcessingStatus::OnHold));
                onHoldRange.status(RangeStatus::Locked);
            }
            else if (PayrollPaymentProcessingStatus.selection() == PayrollPaymentProcessingStatusFilter::Released)
            {
                // Add a range for the payment status of Released
                releasedRange = queryBuildDataSourceEarningStatementLine.addRange(fieldNum(PayrollEarningStatementLine, PaymentStatus));
                releasedRange.value(queryValue(PayrollPaymentProcessingStatus::Released));
                releasedRange.status(RangeStatus::Locked);
            }

            super();
        }

]]></Source>
					</Method>
				</Methods>
				<Fields />
			</DataSource>
		</DataSources>
		<DataControls xmlns="">
			<Control>
				<Name>ReleaseEarningStatementLines</Name>
				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            PayrollEarningStatementLine earningStatementLine;
            boolean                     lineDistributed = true;

            PayrollAccrualProcess payrollAccrualProcess = new PayrollAccrualProcess();

            for (earningStatementLine = payrollEarningStatementLine_DS.getFirst(1) ? payrollEarningStatementLine_DS.getFirst(1) : payrollEarningStatementLine_DS.cursor(); earningStatementLine; earningStatementLine = payrollEarningStatementLine_DS.getNext())
            {
                if ((earningStatementLine.GenerationSource == PayrollGenerationSource::Salary
                        || earningStatementLine.GenerationSource == PayrollGenerationSource::Retroactive)
                    && earningStatementLine.PaymentStatus != PayrollPaymentProcessingStatus::Released
                    && earningStatementLine.PaymentStatus != PayrollPaymentProcessingStatus::Processed
                    && payrollAccrualProcess.validateUsage(earningStatementLine.data()))
                {
                    lineDistributed = PayrollEarningStatementLine::releaseLine(earningStatementLine.data()) && lineDistributed;
                }
            }

            if (!lineDistributed)
            {
                warning("@Payroll:OneOrMoreEarningsWereNotFullyDistributedAndCouldNotBeRelease");
            }

            payrollEarningStatementLine_DS.research();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>HoldEarningStatementLines</Name>
				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            PayrollEarningStatementLine earningStatementLine;
            boolean                     lineUpdated;

            for (earningStatementLine = payrollEarningStatementLine_DS.getFirst(1) ? payrollEarningStatementLine_DS.getFirst(1) : payrollEarningStatementLine_DS.cursor(); earningStatementLine; earningStatementLine = payrollEarningStatementLine_DS.getNext())
            {
                if (earningStatementLine.PaymentStatus == PayrollPaymentProcessingStatus::None ||
                    earningStatementLine.PaymentStatus == PayrollPaymentProcessingStatus::Released)
                {
                    earningStatementLine.PaymentStatus = PayrollPaymentProcessingStatus::OnHold;

                    // Skip adjusting salary with this update to improve performance
                    earningStatementLine.update(true);

                    lineUpdated = true;
                }
            }

            if (lineUpdated)
            {
                payrollEarningStatementLine_DS.executeQuery();
            }
        }

]]></Source>
					</Method>
