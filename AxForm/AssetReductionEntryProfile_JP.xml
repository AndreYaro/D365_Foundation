        public int active()
        {
            int ret;

            ret = super();

            if (AssetReductionEntryProfile_JP_Method.selection() == AssetReductionEntryMethod_JP::Reserve)
            {
                AssetReductionEntryProfile_JP_AllocationConvention.enabled(true);
            }
            else if (AssetReductionEntryProfile_JP_Method.selection() == AssetReductionEntryMethod_JP::DirectOff)
            {
                AssetReductionEntryProfile_JP_AllocationConvention.enabled(false);
            }

            return ret;
        }

