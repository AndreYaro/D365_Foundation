						<Source><![CDATA[
        void clicked()
        {
            Args                args = new Args();
            MenuFunction        menuFunction;
            Common              commonForecastArg;

            commonForecastArg = ProjForecastUnion::getForecast(
                projForecastUnion);

            args.record(commonForecastArg);
            menuFunction = new MenuFunction(
                menuitemDisplayStr(ProjTransBudget), MenuItemType::Display
            );

            menuFunction.run(args);
        }

]]></Source>
