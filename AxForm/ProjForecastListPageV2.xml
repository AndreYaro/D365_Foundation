			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        
        boolean isProjBudgetManagementFeatureEnabled = ProjBudgetManagementFeatureHelper::isFeatureEnabled();

        if (ProjProjectOperationsIntegrationFeatureHelper::isFeatureEnabled())
        {
            this.form().design().showNewButton(AutoNoYes::No);
            this.form().design().showDeleteButton(AutoNoYes::No);
            this.form().design().viewEditMode(ViewEditMode::View);

            deleteForecast.visible(false);
            deleteForecast.enabled(false);
            NewGroup.visible(false);
            NewGroup.enabled(false);
        }

        ImportBudgetLinesFromDataverse.visible(isProjBudgetManagementFeatureEnabled);
        ImportBudgetLinesFromDataverse.enabled(isProjBudgetManagementFeatureEnabled);
        
    }

]]></Source>
			</Method>
