			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class CFMPaymentRequestCopy extends FormRun
{
    CFMPaymentRequestTable paymentRequestTable;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (!element.args().caller() || element.args().dataset() != tableNum(CFMPaymentRequestTable))
        {
            throw error("@SYS22539");
        }

        super();

        paymentRequestTable = element.args().record();

        dueDateCtrl.dateValue(paymentRequestTable.DueDate);
    }

]]></Source>
			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        CFMPaymentRequestId paymentRequestId = CFMPaymentRequestTable::paymentRequestCopy(paymentRequestTable.RecId, dueDateCtrl.dateValue());

        if (paymentRequestId)
        {
            info(strFmt("@CashFlowManagement:PaymentRequestCreated", paymentRequestId));
        }

        super();
    }

]]></Source>
