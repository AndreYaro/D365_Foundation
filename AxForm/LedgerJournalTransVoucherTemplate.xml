		<DataControls xmlns="">
			<Control>
				<Name>CommandOk</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            Dialog                              dialog;
            DialogField                         dfAmount;
            boolean                             dResultOk;

            // bring up the Amount dialog when OK is pressed if the VoucherTemplateType is Percent
            if (ledgerJournalTransVoucherTemplate.VoucherTemplateType == PercentAmount::Percent)
            {
                dialog = new Dialog("@SYS116167");
                dfAmount = dialog.addField(extendedtypestr(Amount), "@SYS98362", "@SYS116168");
                dResultOk = dialog.run();

                if (dResultOk)
                {
                    // OK was selected, verify the amount is <> 0
                    newVoucherAmount = dfAmount.value();

                    if (!newVoucherAmount)
                    {
                        // Amount = 0, give error and return to caller
                        Box::info("@SYS116169");
                        return;
                    }
                }
                else
                {
                    // Cancel or close box selected, return to caller
                    return;
                }
            }

            // VoucherTemplateType = Amount, or VoucherTemplateType = Percent and non-zero Amount was entered
            super();
        }

]]></Source>
					</Method>
