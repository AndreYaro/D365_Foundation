			<Method>
				<Name>setLookupButton</Name>
				<Source><![CDATA[
    void setLookupButton()
    {
        num.enabled(true);
        num.lookupButton(0);

        if (rCashLedgerAccounts.AccountCode == RCashTableAll::All)
        {
            num.enabled(false);
            num.lookupButton(1);
            rCashLedgerAccounts.Num = '';
        }
    }

]]></Source>
