				<Source><![CDATA[
    [Form]
    public class RDeferralsTablePreview extends FormRun
    {
        RecordSortedList              deferralsTableList;
        RecordSortedList              deferralsBookList;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        if (this.args() && this.args().caller() && (classIdGet(this.args().caller()) == classNum(RTax25StdChannel_DeferralPreview)))
        {
            this.initData();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initData</Name>
				<Source><![CDATA[
    void initData()
    {
        RTax25StdChannel_DeferralPreview channel = this.args().caller();
        boolean                          loop;
        RDeferralsTable                  deferralsTable;
        RDeferralsBook                   deferralsBook;

        rDeferralsTable.setTmp();
        rDeferralsBook.setTmp();
        deferralsTableList = channel.deferralsTableList();
        loop = deferralsTableList.first(deferralsTable);
        while (loop)
        {
            rDeferralsTable.data(deferralsTable);
            rDeferralsTable.insert();
            loop = deferralsTableList.next(deferralsTable);
        }
        deferralsBookList = channel.deferralsBookList();
        loop = deferralsBookList.first(deferralsBook);
        while (loop)
        {
            rDeferralsBook.data(deferralsBook);
            rDeferralsBook.insert();
            loop = deferralsBookList.next(deferralsBook);
        }
    }

]]></Source>
			</Method>
