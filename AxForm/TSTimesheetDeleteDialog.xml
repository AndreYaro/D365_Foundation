			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        this.deleteSelectedTimesheets();

        TSIParentForm tsIParentForm = element.args().caller() as TSIParentForm;

        if (tsIParentForm)
        {
            tsIParentForm.setChildDialogClosedOk(true);
        }

        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        Args    args = element.args();

        if (   !args
           || !args.caller()
           || args.dataset() != tableNum(TSTimesheetTable)
           || !args.record())
        {
            throw error(Error::missingFormActiveBuffer(this.name(),tablestr(tsTimesheetTable)));
        }

        super();

        ChangeReason.mandatory(ProjParameters::find().TimesheetChangeReasonMandatory);
    }

]]></Source>
			</Method>
			<Method>
				<Name>deleteSelectedTimesheets</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Deletes timesheets that are selected in the caller datasource.
    /// </summary>
    public void deleteSelectedTimesheets()
    {
        FormDataSource      formDataSource;
        TStimesheetTable    tsTimesheetTableSelected;
        TSTimesheetTable    tsTimesheetTableDelete;
        ProjParameters      projParameters;

        ttsBegin;
        projParameters           = ProjParameters::find();
        formDataSource           = FormDataUtil::getFormDataSource(element.args().record());
        tsTimesheetTableSelected = formDataSource.getFirst(true);
        tsTimesheetTableSelected = tsTimesheetTableSelected ? tsTimesheetTableSelected : formDataSource.cursor();

        if (   (projParameters.TimesheetChangeReasonMandatory && ChangeReason.text())
            || !projParameters.TimesheetChangeReasonMandatory
            || !tsTimesheetTableSelected.hasLines())
        {
            while (tsTimesheetTableSelected)
            {
                // select a fresh record from the database as the list may not have all fields selected for logging
                tsTimesheetTableDelete = TSTimesheetTable::find(tsTimesheetTableSelected.TimesheetNbr,true);

                TSTimesheetTableLog::createTableLog(
                    tsTimesheetTableDelete,
                    ChangeReason.text(),
                    TsTimesheetChangeType::Delete);

                if (tsTimesheetTableDelete.validateDelete())
                {
                    tsTimesheetTableDelete.delete();
                }

                tsTimesheetTableSelected = formDataSource.getNext();
            }
        }
        ttsCommit;

        formDataSource.research(true);
    }

]]></Source>
			</Method>
