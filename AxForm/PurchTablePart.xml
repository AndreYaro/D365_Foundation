						<Source><![CDATA[
        public void linkActive()
        {
            QueryBuildDataSource    purchTableDS;
            ProjTable               projTable;
            QueryBuildDataSource    queryBuildDataSource;

            purchTableDS = purchTable_ds.query().dataSourceTable(tableNum(PurchTable));

            switch (element.args().dataset())
            {
                case tableNum(ProjTable):
                    purchTableDS.clearDynalinks();
                    projTable  = element.args().record() as ProjTable;

                    queryBuildDataSource = purchTableDS.addDataSource(tableNum(PurchLine));
                    queryBuildDataSource.relations(true);
                    queryBuildDataSource.joinMode(JoinMode::ExistsJoin);
                    queryBuildDataSource.addRange(fieldNum(PurchLine, ProjId)).value(projTable.ProjId);
                    break;
            }

            super();
        }

]]></Source>
