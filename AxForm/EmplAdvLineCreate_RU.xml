					<Method>
						<Name>emplTransMark</Name>
						<Source><![CDATA[
        /// <summary>
        /// Mark the EmplTrans from <c>TmpFrmVirtual</c>.
        /// </summary>
        /// <param name = "_set">Set or not.</param>
        /// <param name = "_emplTrans">The record in EmplTrans_RU.</param>
        /// <param name = "_mark">Check write or delete record in the <c>TmpFrmVirtual</c>.</param>
        /// <returns>EmplTrans have been marked or not.</returns>
        edit NoYes emplTransMark(boolean       _set,
                                 EmplTrans_RU  _emplTrans,
                                 NoYes         _mark)
        {
            NoYes   mark;

            select firstonly tmpFrmVirtualMarker
                where tmpFrmVirtualMarker.TableNum  == _emplTrans.TableId &&
                      tmpFrmVirtualMarker.RecordNo  == _emplTrans.RecId;
            if (tmpFrmVirtualMarker)
            {
                mark = NoYes::Yes;
            }

            if (_set)
            {
                if (_mark)
                {
                    tmpFrmVirtualMarker.TableNum = _emplTrans.TableId;
                    tmpFrmVirtualMarker.RecordNo = _emplTrans.RecId;

                    tmpFrmVirtualMarker.write();
                    mark = NoYes::Yes;
                }
                else
                {
                    tmpFrmVirtualMarker.delete();
                    mark = NoYes::No;
                }

            }
            return mark;
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            TmpFrmVirtual   buffer;
            int             ret = super();

            if (emplTrans)
            {
                transSource = EmplTransSource_RU::newEmplTrans(emplTrans);
                if (transSource)
                {
                    buffer = transSource.tmpFrmVirtual();
                    tmpFrmVirtual.setTmpData(buffer);
                    emplTransMark.enabled(buffer.RecId);
                }
            }
            return ret;
        }

]]></Source>
