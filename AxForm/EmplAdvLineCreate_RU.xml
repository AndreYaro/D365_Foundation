				<Name>classDeclaration</Name>
				<Source><![CDATA[
    [Form]
    public class EmplAdvLineCreate_RU extends FormRun
    {
        EmplTransSource_RU      transSource;
        TmpFrmVirtual           tmpFrmVirtualMarker;
        EmplAdvTable_RU         advTable;
}
]]></Source>
			</Method>
			<Method>
				<Name>doApply</Name>
				<Source><![CDATA[
    public void doApply()
    {
        boolean         updated = false;
        FormDataSource  formDS;
        EmplAdvTable_RU emplAdvTable_RU;

        super();

        if (! advTable)
            return;

        ttsbegin;
        while select tmpFrmVirtualMarker
        {
            updated = true;
            EmplAdvanceLineType_Fixed_RU::createFromEmplTransSource(
                        EmplTransSource_RU::newEmplTrans(EmplTrans_RU::findRecId(tmpFrmVirtualMarker.RecordNo)),
                        advTable.AdvanceId);
        }
        ttscommit;

        if (! updated)
        {
            throw warning("@SYS52714");
        }
        if (updated && FormDataUtil::getFormDataSource(element.args().record()))
        {
            emplAdvTable_RU.data(element.args().record());
            formDS = FormDataUtil::getFormDataSource(element.args().record());
            formDS.research();
            formDS.positionToRecord(emplAdvTable_RU);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void  init()
    {
        if (! element.args() || element.args().dataset() != tableNum(EmplAdvTable_RU))
            throw error("@SYS22539");

        advTable = element.args().record();

        super();

        transSource = new EmplTransSource_RU();
    }

]]></Source>
			</Method>
