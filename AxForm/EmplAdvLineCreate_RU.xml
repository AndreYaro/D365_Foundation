				<Name>EmplTrans</Name>
				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            QueryBuildDataSource    qbdsTransCredit, qbdsSettlement, qbdsTransDebit;
            SysDictEnum             sysDictEnum = new SysDictEnum(enumNum(EmplTransSourceType_RU));
            Range                   sourceRange;
            int                     i;
            super();
            for (i = 0; i <= sysDictEnum.values() -1 ; i += 1)
            {
                try
                {
                    EmplAdvanceLineType_RU::sourceType2LineType(sysDictEnum.index2Value(i));
                    sourceRange = sourceRange ? queryRangeConcat(sourceRange,sysDictEnum.index2Value(i)) : queryValue(sysDictEnum.index2Value(i));
                }
                catch (Exception::Error)
                {
                    infolog.clear(0);
                }
            }
            qbdsTransCredit = emplTrans_ds.query().dataSourceTable(tableNum(EmplTrans_RU));
            qbdsTransCredit.clearDynalinks();
            qbdsTransCredit.addRange(fieldNum(EmplTrans_RU, EmplAccount)).value(advTable.EmplAccount);
            qbdsTransCredit.addRange(fieldNum(EmplTrans_RU, AmountCur)).value("@GLS111254");
            qbdsTransCredit.addRange(fieldNum(EmplTrans_RU, TransDate)).value(queryValue(advTable.TransDate));
            qbdsTransCredit.addRange(fieldNum(EmplTrans_RU, TransType)).value(sourceRange);

            qbdsTransCredit.addRange(fieldNum(EmplTrans_RU, AdvanceId)).value(SysQuery::valueEmptyString());

            qbdsSettlement = qbdsTransCredit.addDataSource(tableNum(EmplSettlement_RU));
            qbdsSettlement.clearDynalinks();
            qbdsSettlement.clearLinks();
            qbdsSettlement.addLink(fieldNum(EmplTrans_RU, RecId), fieldNum(EmplSettlement_RU, CreditRecId));
            qbdsSettlement.joinMode(JoinMode::NoExistsJoin);

            qbdsTransDebit = qbdsSettlement.addDataSource(tableNum(EmplTrans_RU));
            qbdsTransDebit.clearDynalinks();
            qbdsTransDebit.clearLinks();
            qbdsTransDebit.addLink(fieldNum(EmplSettlement_RU, DebitRecId), fieldNum(EmplTrans_RU, RecId));
            qbdsTransDebit.addDynalink(fieldNum(EmplTrans_RU, TransType), emplTrans, fieldNum(EmplTrans_RU, TransType));
            qbdsTransDebit.joinMode(JoinMode::ExistsJoin);
        }

]]></Source>
