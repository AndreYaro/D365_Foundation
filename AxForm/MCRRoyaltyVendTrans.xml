				<Source><![CDATA[
    /// <summary>
    /// Initializes the <c>MCRRoyaltyVendTrans</c> form.
    /// </summary>
    public void init()
    {
        super();

        if (element.args()
            && element.args().dataset() == tablenum(MCRRoyaltyVendTable))
        {
            MCRRoyaltyVendTrans_ds.object(fieldNum(MCRRoyaltyVendTrans, RoyaltyClaimId)).visible(false);
        }
    }

]]></Source>
