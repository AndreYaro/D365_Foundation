    /// <summary>
    /// Enables or disables form buttons based on <c>MCRRoyaltyTransactionType</c>.
    /// </summary>
    public void enableDisableButtons()
    {
        VendInvoiceJournalButton.enabled(MCRRoyaltyVendTrans.RoyaltyTransaction == MCRRoyaltyTransactionType::VendorPayment);
        LedgerJournalTableButton.enabled(MCRRoyaltyVendTrans.RoyaltyTransaction == MCRRoyaltyTransactionType::RoyaltyAccrual);
    }

]]></Source>
