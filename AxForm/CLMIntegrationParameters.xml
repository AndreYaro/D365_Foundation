				<Name>CLMIntegrationServiceOperation</Name>
				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();

            QueryBuildRange businessOperationRange = SysQuery::findOrCreateRange(CLMIntegrationServiceOperation_ds.queryBuildDataSource(), fieldNum(CLMIntegrationServiceOperation, BusinessOperation));
            businessOperationRange.value(SysQuery::valueNot(queryValue(CLMIntegrationBusinessOperation::InitiateConnection)));
            businessOperationRange.status(RangeStatus::Hidden);
        }

]]></Source>
