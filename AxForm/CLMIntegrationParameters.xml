		<DataControls xmlns="">
			<Control>
				<Name>ConfigureInitiationServiceOperation</Name>
				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            if (CLMIntegrationServiceOperation::findByBusinessOperation(CLMIntegrationBusinessOperation::InitiateConnection))
            {
                this.openMode(OpenMode::Edit);
            }
            else
            {
                this.openMode(OpenMode::New);
            }

            super();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>EnableIntegrationButton</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            str dialogText;

            if (CLMIntegrationServiceInstance.EstablishmentStatus != CLMIntegrationConnectionEstablishmentStatus::Success)
            {
                dialogText = "@CLMIntegration:CLMIntegrationParametersConnectionNotYetEstablished";
            }

            if (!dialogText || Box::yesNo(dialogText, DialogButton::No) == DialogButton::Yes)
            {
                CLMIntegrationServiceInstance::activateInstance(CLMIntegrationServiceInstance.RecId, true);
                CLMIntegrationServiceInstance_ds.research(true);
                element.setActivationControls();
            }
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>DisableIntegrationButton</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        public void clicked()
        {
            if (Box::yesNo("@CLMIntegration:CLMIntegrationParametersIntegrationDisableNotification", DialogButton::No) == DialogButton::Yes)
            {
                CLMIntegrationServiceInstance::activateInstance(CLMIntegrationServiceInstance.RecId, false);
                CLMIntegrationServiceInstance_ds.research(true);
                element.setActivationControls();
            }
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
