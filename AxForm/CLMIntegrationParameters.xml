[Form]
public class CLMIntegrationParameters extends FormRun
{
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        ttsbegin;
        CLMIntegrationServiceInstance::find();
        ttscommit;

        super();

        this.createPrivacyMenuItemButton();
    }

]]></Source>
			</Method>
			<Method>
				<Name>enableAuthenticationControls</Name>
				<Source><![CDATA[
    private void enableAuthenticationControls()
    {
        if (CLMIntegrationServiceInstance)
        {
            boolean isAuthenticationTypeOAuth2 = CLMIntegrationServiceInstance.AuthenticationType == CLMIntegrationAuthenticationType::OAuth2;
            CLMIntegrationServiceInstance_ds.object(fieldNum(CLMIntegrationServiceInstance, AuthorizationURL)).mandatory(isAuthenticationTypeOAuth2);
            ServiceInstanceAuthenticationParametersGroup_editClientId.mandatory(isAuthenticationTypeOAuth2);
            ServiceInstanceAuthenticationParametersGroup_editClientSecret.mandatory(isAuthenticationTypeOAuth2);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>setActivationControls</Name>
				<Source><![CDATA[
    private void setActivationControls()
    {
        boolean integrationEnabled = CLMIntegrationServiceInstance.InstanceEnabled;

        EnableIntegrationButton.visible(!integrationEnabled);
        DisableIntegrationButton.visible(integrationEnabled);
        IntegrationControlState.text(integrationEnabled ? "@CLMIntegration:CLMIntegrationParametersIntegrationEnabled" : "@CLMIntegration:CLMIntegrationParametersIntegrationDisabled");
    }

]]></Source>
			</Method>
			<Method>
				<Name>createPrivacyMenuItemButton</Name>
				<Source><![CDATA[
    private void createPrivacyMenuItemButton()
    {
        FormFunctionButtonControl privacyMenuItemButton = ServiceInstanceTitle.addControl(FormControlType::MenuFunctionButton, identifierStr(PrivacyStatementLink));
        privacyMenuItemButton.menuItemName(menuItemActionStr(SysMicrosoftPrivacyStatementNavigator));
        privacyMenuItemButton.menuItemType(MenuItemType::Action);
        privacyMenuItemButton.style(ButtonStyle::Link);
    }

]]></Source>
			</Method>
		</Methods>
