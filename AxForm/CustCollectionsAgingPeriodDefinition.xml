    public int task(int _taskId)
    {
        #Task
        if (_taskId == #taskEnter)
        {
            if (ok.enabled())
            {
                ok.clicked();
            }
            return 0;
        }
        else
        {
            return super(_taskId);
        }
    }

]]></Source>
