			<Method>
				<Name>getValidDefinitions</Name>
				<Source><![CDATA[
    private Set getValidDefinitions()
    {
        Set validDefinitionsLocal = new Set(Types::String);

        StatRepInterval statRepIntervalLocal;

        while select Name from statRepIntervalLocal
        {
            if (CustVendBalanceList::validateAgingPeriodDefinition(statRepIntervalLocal.Name, false, false))
            {
                validDefinitionsLocal.add(statRepIntervalLocal.Name);
            }
        }

        return validDefinitionsLocal;
    }

]]></Source>
