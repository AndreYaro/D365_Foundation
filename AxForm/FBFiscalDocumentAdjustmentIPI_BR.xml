				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class FBFiscalDocumentAdjustmentIPI_BR extends FormRun
{
    FBTaxAssessment_BR fbTaxAssessment;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        FiscalDocumentLine_BR fiscalDocumentLine;

        if (element.args().dataset() == tableNum(FiscalDocumentLine_BR) && element.args().record())
        {
            fiscalDocumentLine = element.args().record();

            FBBookingPeriod_BR fbBookingPeriod;
            FBBookingPeriodFiscalDocument_BR fbBookingPeriodFiscalDocument;
            TaxType_BR taxType = TaxType_BR::IPI;

            select firstOnly fbTaxAssessment
                where fbTaxAssessment.TaxType == taxType
                exists join fbBookingPeriod
                    where fbBookingPeriod.RecId == fbTaxAssessment.FBBookingPeriod_BR
                exists join fbBookingPeriodFiscalDocument
                    where fbBookingPeriodFiscalDocument.FBBookingPeriod_BR == fbBookingPeriod.RecId
                        && fbBookingPeriodFiscalDocument.FiscalDocument_BR == fiscalDocumentLine.FiscalDocument;

            if (!fbTaxAssessment)
            {
                throw error("@SYS22996");
            }
        }
        else
        {
            throw error("@SYS22996");
        }

        super();

        QueryBuildDataSource queryBuildDataSource = FiscalDocumentLine_BR_ds.query().dataSourceTable(tableNum(FiscalDocumentLine_BR));
        findOrCreateRange_W(queryBuildDataSource, fieldnum(FiscalDocumentLine_BR, RecId), queryValue(fiscalDocumentLine.RecId)).status(RangeStatus::Locked);

        queryBuildDataSource = FiscalDocumentTaxTrans_BR_ds.query().dataSourceTable(tableNum(FiscalDocumentTaxTrans_BR));
        findOrCreateRange_W(queryBuildDataSource, fieldnum(FiscalDocumentTaxTrans_BR, FiscalDocumentMiscCharge), queryValue(0)).status(RangeStatus::Locked);
        findOrCreateRange_W(queryBuildDataSource, fieldnum(FiscalDocumentTaxTrans_BR, Type), queryValue(fbTaxAssessment.TaxType)).status(RangeStatus::Locked);

        element.design().titleDatasource(FBTaxAssessment_BR_ds.id());

        this.updateDesign();
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    public void updateDesign()
    {
        boolean isLedgerIntegrationActivated = BrazilParameters::isFBLedgerIntegrationActivated();
        SecurityRights curUserRights = SecurityRights::newUser(curUserId());
        boolean hasUserRights = curUserRights.hasMenuItemAccess(SecurableType::MenuItemDisplay, menuitemDisplayStr(LedgerTransVoucher1));

        LedgerTransVoucher.visible(isLedgerIntegrationActivated);

        FBFiscalDocumentAdjustment_BR_ds.allowCreate(!isLedgerIntegrationActivated);
        FBFiscalDocumentAdjustment_BR_ds.allowEdit(!isLedgerIntegrationActivated && FBFiscalDocumentAdjustment_BR.Voucher == '');
        FBFiscalDocumentAdjustment_BR_ds.allowDelete(!isLedgerIntegrationActivated);

        LedgerTransVoucher.enabled(hasUserRights && FBFiscalDocumentAdjustment_BR.VoucherDataArea != '');
    }

]]></Source>
