				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            MenuFunction menuFunction = new menuFunction(menuitemDisplayStr(ledgerTransVoucher), MenuItemType::Display);

            changeCompany (FBFiscalDocumentAdjustment_BR.VoucherDataArea)
            {
                TaxTrans taxTrans;

                select firstOnly taxTrans
                    where taxTrans.Voucher == FBFiscalDocumentAdjustment_BR.Voucher
                        && taxTrans.TransDate == FBFiscalDocumentAdjustment_BR.TransDate;

                Args args = new args(this);
                args.record(taxTrans);
                menuFunction.run(args);
            }
        }

]]></Source>
