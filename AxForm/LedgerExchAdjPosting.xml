			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    [Hookable(false)]
    public void init()
    {
        super();

        this.initAccountTypes(AccountType_GL, LedgerTransSettlementAdvancedParameters::isPostRealizedGainLossParameterEnabled());
        this.initAccountTypes(AccountType_AP, true);
        this.initAccountTypes(AccountType_AR, true);
        this.initAccountTypes(AccountType_Bank, false);

        ledgerExchAdjPostingForm = new LedgerExchAdjPostingForm(element.args().record());

        ledgerExchAdjPostingForm.queryGL(GeneralLedger_ds, AccountType_GL.selectionAsEnumValue());
        ledgerExchAdjPostingForm.queryAP(AccountsPayable_ds, AccountType_AP.selectionAsEnumValue());
        ledgerExchAdjPostingForm.queryAR(AccountsReceivable_ds, AccountType_AR.selectionAsEnumValue());
        ledgerExchAdjPostingForm.queryBank(Bank_ds, AccountType_Bank.selectionAsEnumValue());
    }

]]></Source>
			</Method>
			<Method>
				<Name>setRelationFieldsEnabled</Name>
				<Source><![CDATA[
    private void setRelationFieldsEnabled(LedgerExchAdjPosting _ledgerExchAdjPosting)
    {
        if (FormDataUtil::isFormDataSource(_ledgerExchAdjPosting))
        {
            boolean allowEdit = _ledgerExchAdjPosting.isCurrencyRequired();
            FormDataObject formDataObject = FormDataUtil::getFormDataSource(_ledgerExchAdjPosting).object(fieldNum(LedgerExchAdjPosting, Currency));
            formDataObject.allowEdit(allowEdit);
            formDataObject.skip(!allowEdit);

            allowEdit = _ledgerExchAdjPosting.isAccountRelationRequired();
            formDataObject = FormDataUtil::getFormDataSource(_ledgerExchAdjPosting).object(fieldNum(LedgerExchAdjPosting, AccountRelation));
            formDataObject.allowEdit(allowEdit);
            formDataObject.skip(!allowEdit);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>initAccountTypes</Name>
				<Source><![CDATA[
    private void initAccountTypes(FormRadioControl _ctrl, boolean _addRealizedGainLoss)
    {
        _ctrl.clear();

        // If modify order, please update convertSelectionAsEnum accordingly
        _ctrl.add(enum2Str(CurrencyGainLossAccountType::UnrealizedGain));
        _ctrl.add(enum2Str(CurrencyGainLossAccountType::UnrealizedLoss));

        if (_addRealizedGainLoss)
        {
            _ctrl.add(enum2Str(CurrencyGainLossAccountType::RealizedGain));
            _ctrl.add(enum2Str(CurrencyGainLossAccountType::RealizedLoss));
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>convertSelectionAsEnum</Name>
				<Source><![CDATA[
    private CurrencyGainLossAccountType convertSelectionAsEnum(int _selection)
    {
        CurrencyGainLossAccountType converted = CurrencyGainLossAccountType::UnrealizedGain;

        // order should match with the setup order from initAccountTypes
        switch (_selection)
        {
            case 0:
                converted = CurrencyGainLossAccountType::UnrealizedGain;
                break;
            case 1:
                converted = CurrencyGainLossAccountType::UnrealizedLoss;
                break;
            case 2:
                converted = CurrencyGainLossAccountType::RealizedGain;
                break;
            case 3:
                converted = CurrencyGainLossAccountType::RealizedLoss;
                break;
        }

        return converted;
    }

]]></Source>
