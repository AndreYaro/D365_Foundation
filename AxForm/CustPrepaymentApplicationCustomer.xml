					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;

            ret = super();

            CustInvoiceJour selectedCustInvoiceJour = this.cursor();
            tmpSalesPrepaymentInvoice.clear();
            tmpSalesPrepaymentInvoice.setTmpData(custPrepaymentInvoice.populateJournalApplicablePrepaymentInvoice(selectedCustInvoiceJour));
            custInvoiceJour_ds.applyAmountSum();
            tmpSalesPrepaymentInvoice_ds.research(true);

            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>applyAmountSum</Name>
						<Source><![CDATA[
        public display AmountCur applyAmountSum()
        {
            return abs(CustPrepaymentCustInvoice::totalAdvanceAppliedAmountForInvoiceJour(this.cursor()));
        }

]]></Source>
					</Method>
					<Method>
						<Name>validateTotalAppliedAmount</Name>
						<Source><![CDATA[
        public boolean validateTotalAppliedAmount(AmountCur _amountApplied)
        {
            boolean ret = true;
            CustInvoiceJour selectedCustInvoiceJour = this.cursor();

            if (custInvoiceJour_ds.applyAmountSum() + _amountApplied > selectedCustInvoiceJour.InvoiceAmount)
            {
                ret = false;
            }

            return ret;
        }

]]></Source>
