				<Source><![CDATA[
[Form]
public class CustPrepaymentApplicationCustomer extends FormRun
{
    CustPrepaymentCustInvoice custPrepaymentCustInvoice;
    CustPrepaymentInvoice custPrepaymentInvoice; //class supporting business logic for prepayment application and its reversal
    boolean isPrepaymentApplication; //flag to identify if the form is for prepayment application or reversal
    AccountNum invoiceAccount;
    Common callerTable;
    boolean isInvoicePosted;

}
]]></Source>
			</Method>
			<Method>
				<Name>updateAdvanceApplicationAmount</Name>
				<Source><![CDATA[
    /// <summary>
    /// Updates amount against a respective <c>CustPrepaymentCustInvoice</c> record.
    /// </summary>
    /// <param name="_standardInvoice">
    /// The record ID of invoice on which advance is to be applied.
    /// </param>
    /// <param name="_appliedAdvanceInvoice">
    /// The record ID of the advance to be applied.
    /// </param>
    /// <param name="_appliedAmount">
    /// The applied advance amount to be updated.
    /// </param>
    public void updateAdvanceApplicationAmount(RecId _standardInvoice, RecId _appliedAdvanceInvoice, AmountCur _appliedAmount, RefTableId _TableId)
    {
        ttsBegin;

        CustPrepaymentCustInvoice::updateAdvanceAppliedAmount(_appliedAdvanceInvoice, _standardInvoice, _appliedAmount,_TableId , isInvoicePosted );

        ttscommit;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        if (!(element.args().caller() || element.args().record()))
        {
            throw error("@SYS22539");
        }

        if (element.args().caller() is FormRun)
        {
            callerTable = element.args().record();
            isPrepaymentApplication = true;
            custPrepaymentInvoice = CustPrepaymentInvoice::construct(isPrepaymentApplication);

            switch (callerTable.TableId)
            {
                case tableNum(CustTable):
                    
                    CustTable custTable = callerTable;
                    CustPrepaymentInvoiceTable custPrepaymentInvoiceTableBuff;
                    QuerybuildDatasource custTransAndInvoiceJourDS,custTransAndTransOpenDS;
                    Query query;

                    Query custPrepaymentInvoicequery;
                    QueryBuildDataSource qbdsCustPrepaymentInvoiceTable;
                    QueryBuildDataSource qbds_CustTable;
                    QueryBuildRange qbRange;

                    custPrepaymentInvoicequery = new Query();

                    qbdsCustPrepaymentInvoiceTable = custPrepaymentInvoicequery.addDataSource(tableNum(CustPrepaymentInvoiceTable));
                    
                    qbRange = qbdsCustPrepaymentInvoiceTable.addRange(fieldNum(CustPrepaymentInvoiceTable, InvoiceAccount));
                    qbRange.value(SysQuery::value(custTable.AccountNum));

                    qbdsCustPrepaymentInvoiceTable.addRange(fieldNum(CustPrepaymentInvoiceTable, Type)).value(queryValue(PrepaymentInvoiceType::Common));
                    qbdsCustPrepaymentInvoiceTable.addRange(fieldNum(CustPrepaymentInvoiceTable, RemainAmountCur)).value(queryValue('>0'));
                    qbdsCustPrepaymentInvoiceTable.addRange(fieldNum(CustPrepaymentInvoiceTable, SettleAmountCur)).value(queryValue('>0'));
                    qbdsCustPrepaymentInvoiceTable.addRange(fieldNum(CustPrepaymentInvoiceTable, SettleAmountCur)).
                        value(strFmt('(%1 == %2)', strFmt("%1.%2", qbdsCustPrepaymentInvoiceTable.name(), fieldStr(CustPrepaymentInvoiceTable, SettleAmountCur)),
                              strFmt("%1.%2", qbdsCustPrepaymentInvoiceTable.name(), fieldStr(CustPrepaymentInvoiceTable, InvoiceAmount))));
                    qbdsCustPrepaymentInvoiceTable.addRange(fieldNum(CustPrepaymentInvoiceTable, Status)).value(queryValue(CustPrepaymentInvoiceStatus::Received));

                    QueryRun queryRun;
                    queryRun = new QueryRun(custPrepaymentInvoicequery);

                    boolean prepaymentInvoiceFound;

                    while (queryRun.next())
                    {
                        prepaymentInvoiceFound = true;
                        custPrepaymentInvoiceTableBuff = queryRun.get(tableNum(CustPrepaymentInvoiceTable));
                        CustInvoiceJour_ds.query().dataSourceTable(tableNum(CustInvoiceJour)).addRange(fieldNum(CustInvoiceJour,SalesId)).value(custPrepaymentInvoiceTableBuff.SalesId);
                    }

                    if (prepaymentInvoiceFound)
                    {
                        custTransAndInvoiceJourDS = CustInvoiceJour_ds.query().dataSourceTable(tableNum(CustInvoiceJour)).addDataSource((tableNum(CustTrans)));
                        custTransAndInvoiceJourDS.addLink(fieldNum(CustInvoiceJour, InvoiceId), fieldNum(CustTrans,
                    Invoice));
                        custTransAndInvoiceJourDS.joinMode(JoinMode::InnerJoin);
                        custTransAndInvoiceJourDS.relations(true);
                        custTransAndTransOpenDS = CustInvoiceJour_ds.query().dataSourceTable(tableNum(CustTrans)).addDataSource((tableNum(CustTransOpen)));
                        custTransAndTransOpenDS.addLink(fieldNum(CustTrans,RecId), fieldNum(CustTransOpen,RefRecId));
                        custTransAndTransOpenDS.joinMode(JoinMode::ExistsJoin);
                        custTransAndTransOpenDS.relations(true);
                        isInvoicePosted = true;

                        break;
                    }
            }

            custPrepaymentInvoice.parmIsPosted(isInvoicePosted);
        }
        else
        {
            throw error(strFmt("@SYS105865","@SYS183647","@SYS8008"));
        }
    }

]]></Source>
