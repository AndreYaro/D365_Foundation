			<DataSource>
				<Name>tmpSalesPrepaymentInvoice</Name>
				<Methods>
					<Method>
						<Name>markTrans</Name>
						<Source><![CDATA[
        /// <summary>
        /// Mark the prepayment invoice with the selected invoicc journal record.
        /// </summary>
        /// <param name="_set">
        /// The flag to set the mark.
        /// </param>
        /// <param name="_tmpSalesPrepaymentInvoice">
        /// The selected prepayment invoice record.
        /// </param>
        /// <param name="_markTrans">
        /// The flag to mark the transaction.
        /// </param>
        /// <returns>
        /// The flag to mark the transaction.
        /// </returns>
        public edit NoYes markTrans(boolean _set, tmpSalesPrepaymentInvoice _tmpSalesPrepaymentInvoice, NoYes _markTrans)
        {
            NoYes returnMarkTrans;
            CustInvoiceJour custInvoiceJourLoc = CustInvoiceJour_ds.cursor();

            if (_set)
            {
                if (true)
                {
                    custPrepaymentInvoice.updateTransMarked(custInvoiceJourLoc.RecId, _tmpSalesPrepaymentInvoice.InvoiceRecId, _markTrans, custInvoiceJourLoc.TableId , _tmpSalesPrepaymentInvoice.InvoiceDate);

                    if (_markTrans)
                    {
                        AmountCur AppliedAmount = _tmpSalesPrepaymentInvoice.AppliedAmount;
                        AmountCur AdvanceAmount = _tmpSalesPrepaymentInvoice.AdvanceAmount;

                        if (AppliedAmount == 0)
                        {
                            AppliedAmount = AdvanceAmount;
                        }

                        if (!CustInvoiceJour_ds.validateTotalAppliedAmount(AppliedAmount))
                        {
                            throw warning("@SYS331457");
                        }

                        //sets advance apply amount to maximum possible value when selected a paid advance
                        if (AppliedAmount > 0 && AppliedAmount <= AdvanceAmount &&  AppliedAmount <= _tmpSalesPrepaymentInvoice.InvoiceAmount)
                        {
                            custPrepaymentInvoice.updateAdvanceApplicationAmount(custInvoiceJourLoc.RecId, _tmpSalesPrepaymentInvoice.InvoiceRecId, AppliedAmount, custInvoiceJourLoc.TableId);
                        }

                    }

                }

                CustInvoiceJour_ds.research(true);
                tmpSalesPrepaymentInvoice_ds.research(true);
            }

            CustPrepaymentCustInvoice custPrepaymentCustInvoiceLoc = CustPrepaymentCustInvoice::findByInvoiceJourIdPrepaymentInvoiceId(custInvoiceJour.RecId, _tmpSalesPrepaymentInvoice.InvoiceRecId);

            returnMarkTrans = (custPrepaymentCustInvoiceLoc)?NoYes::Yes:NoYes::No;
            return returnMarkTrans;
        }

]]></Source>
					</Method>
					<Method>
						<Name>updateApplicationAmount</Name>
						<Source><![CDATA[
        /// <summary>
        /// This method updates the applied amount against the selected prepayment invoice.
        /// </summary>
        /// <param name = "_set">
        /// The flag to set the applied amount.
        /// </param>
        /// <param name = "_tmpSalesPrepaymentInvoice">
        /// The selected prepayment invoice record.
        /// </param>
        /// <param name = "_appliedAmount">
        /// The applied amount to be updated.
        /// </param>
        /// <returns>
        /// The applied amount against the selected prepayment invoice.
        /// </returns>
        public edit AmountCur updateApplicationAmount(
            boolean _set,
            tmpSalesPrepaymentInvoice _tmpSalesPrepaymentInvoice,
            AmountCur _appliedAmount)
        {
            CustInvoiceJour custInvoiceJourLoc = CustInvoiceJour_ds.cursor();
            AmountCur applicationAmount = 0;

            if (_set)
            {
                if (_appliedAmount > 0 && _appliedAmount <= _tmpSalesPrepaymentInvoice.AdvanceAmount)
                {
                    if (CustInvoiceJour_ds.validateTotalAppliedAmount(_appliedAmount))
                    {
                        _tmpSalesPrepaymentInvoice.AppliedAmount = _appliedAmount;
                        _tmpSalesPrepaymentInvoice.AdvanceRemaining = _tmpSalesPrepaymentInvoice.AdvanceAmount - _appliedAmount;
                        custPrepaymentInvoice.updateAdvanceApplicationAmount(custInvoiceJourLoc.RecId, _tmpSalesPrepaymentInvoice.InvoiceRecId, _appliedAmount, custInvoiceJourLoc.TableId);
                        custInvoiceJour_ds.research(true);
                        tmpSalesPrepaymentInvoice_ds.research(true);
                    }
                    else
                    {
                        throw warning("@SYS331457");
                    }
                }
                else
                {
                    custPrepaymentInvoice.updateTransMarked(custInvoiceJourLoc.RecId, _tmpSalesPrepaymentInvoice.InvoiceRecId, NoYes::No, custInvoiceJourLoc.TableId, _tmpSalesPrepaymentInvoice.InvoiceDate);
                    throw warning("@SYS342088"); //throws an error message when prepayment amount being applied is zero
                }
            }

            CustPrepaymentCustInvoice custPrepaymentCustInvoiceLoc = CustPrepaymentCustInvoice::findByInvoiceJourIdPrepaymentInvoiceId(custInvoiceJour.RecId, _tmpSalesPrepaymentInvoice.InvoiceRecId);

            if (custPrepaymentCustInvoiceLoc)
            {
                applicationAmount = custPrepaymentCustInvoiceLoc.AmountCur;
            }

            return applicationAmount;
        }

]]></Source>
					</Method>
					<Method>
						<Name>ApplicationRemaining</Name>
						<Source><![CDATA[
        public display AmountCur ApplicationRemaining(tmpSalesPrepaymentInvoice _tmpSalesPrepaymentInvoiceLoc)
        {
            _tmpSalesPrepaymentInvoiceLoc.AdvanceRemaining = _tmpSalesPrepaymentInvoiceLoc.AdvanceAmount - _tmpSalesPrepaymentInvoiceLoc.AppliedAmount;
            return _tmpSalesPrepaymentInvoiceLoc.AdvanceRemaining;
        }

]]></Source>
