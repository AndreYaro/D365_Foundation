        {
            throw error ("@WAX3166");
        }

        waveTable = element.args().record();

        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>showLog</Name>
				<Source><![CDATA[
    /// <summary>
    /// Get infolog message for a given batch job and related tasks.
    /// </summary>
    /// <remarks>This is similar implementation as on the <c>BatchJob</c> form.</remarks>
    private void showLog()
    {
        InfologData data;

        if (batchJob && batchJob.RecId)
        {
            setPrefix(strFmt("@SYS113156", batchJob.Caption, int642str(batchJob.RecId)));
            data = BatchJob::showLog(batchJob.RecId, true);
            int countItems = conLen(data);

            boolean addedLog = false;
            for(int i = 1; i <= countItems; i += 3)
            {
                if (conLen(conPeek(data, i+2)) == 0)
                    continue;

                addedLog = true;

                setPrefix(strFmt("@SYS107863", conPeek(data, i), int642str(conPeek(data, i+1))));
                infolog.import(conPeek(data, i+2));
            }

            if (addedLog)
            {
                infolog.import([[1], [0, "@ApplicationFoundation:BatchJobLogEndMarker"]]);
            }
            else
            {
                Message::Add(MessageSeverity::Informational, "@ApplicationFoundation:BatchJobLogEmpty");
            }
        }
    }

]]></Source>
			</Method>
		</Methods>
