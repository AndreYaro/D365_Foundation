				<Source><![CDATA[
[Form]
public class BusinessDocumentSubmissionDetails extends FormRun
{
    BusinessDocumentInfo documentInfo;
    private boolean isExternal;
    private boolean resubmitFromFailedActionFeature;
    private BusinessDocumentSubmission submission;
    private BusinessDocumentSubmissionExternal submissionExternal;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (ERForInvoicingRouteSwitch::shouldUseV2())
        {
            new MenuFunction(menuitemDisplayStr(BusinessDocumentSubmissionDetailsV2), MenuItemType::Display).run(element.args());

            this.close();
        }
        else
        {
            super();

            str callerName = element.args().callerName();

            isExternal = callerName == formStr(BusinessDocumentSubmissionExternal);
            resubmitFromFailedActionFeature = FeatureStateProvider::isFeatureEnabled(BusinessDocumentResubmitFromFailedActionFeature::instance());

            if (isExternal)
            {
                submissionExternal =  BusinessDocumentSubmissionExternal::findByCommon(element.args().record());
            }
            else
            {
                submission = BusinessDocumentSubmission::findByCommon(element.args().record());
            }

            if (!submission && !submissionExternal)
            {
                throw Error(Error::wrongUseOfFunction(funcName()));
            }
       
            this.fillData();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    private void updateDesign()
    {
        ResubmitButton.visible(!isExternal);
        ResubmitButton.enabled(submission.RecId && conFind([
                BusinessDocumentState::Canceled,
                BusinessDocumentState::ValidationFailed,
                BusinessDocumentState::Failed], submission.State));

        ResubmitFromFailedActionButton.visible(resubmitFromFailedActionFeature && !isExternal);
        ResubmitFromFailedActionButton.enabled(resubmitFromFailedActionFeature && submission.RecId && submission.State == BusinessDocumentState::Failed);
    }

]]></Source>
			</Method>
			<Method>
				<Name>fillData</Name>
				<Source><![CDATA[
    private void fillData()
    {
        documentInfo = isExternal ?
            BusinessDocumentExternalController::newSubmission(submissionExternal).lookupDocumentInfo() :
            BusinessDocumentServiceController::newSubmission(submission).lookupDocumentInfo();

        if (!documentInfo)
        {
            return;
        }

        this.setTmpDataForDocument(documentInfo);

        boolean isSuccess;
        ERForInvoicingActivities activities = ERForInvoicingActivities::newFromCategory(formStr(BusinessDocumentSubmissionDetails));
        using (var activityContext = activities.updateDocumentStatus())
        {
            try
            {
                if (isExternal)
                {
                    this.updateBusinessDocumentExternalState();
                }
                else
                {
                    this.updateBusinessDocumentState();
                }
  
                isSuccess = true;
            }
            catch (Exception::CLRError)
            {
                str message = SRSProxy::getLastClrExceptionMessage();
                activityContext.addCustomProperty(ERForInvoicingInstrumentationConstants::CustomPropertyError, message);
                error(message);
            }
            finally
            {
                activityContext.addCustomProperty(ERForInvoicingInstrumentationConstants::CustomPropertyIsSuccess, any2Str(isSuccess));
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateBusinessDocumentState</Name>
				<Source><![CDATA[
    private void updateBusinessDocumentState()
    {
        BusinessDocumentSubmissionService::updateDocumentStatus(submission.getDocument(), documentInfo);
        this.updateData(submission.dataSource(), submission);
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateBusinessDocumentExternalState</Name>
				<Source><![CDATA[
    private void updateBusinessDocumentExternalState()
    {
        BusinessDocumentVendorImportService::updateDocumentState(submissionExternal, documentInfo);
        this.updateData(submissionExternal.dataSource(), submissionExternal);
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateData</Name>
				<Source><![CDATA[
    private void updateData(FormDataSource dataSource, Common document)
    {
        dataSource.reread();
        dataSource.refresh();
        dataSource.active();
        document.reread();
        this.updateDesign();
    }

]]></Source>
			</Method>
			<Method>
				<Name>setTmpDataForDocument</Name>
				<Source><![CDATA[
    private void setTmpDataForDocument(BusinessDocumentInfo docInfo)
    {
        BusinessDocumentExecutionTmp.setTmpData(docInfo.getExecutions());
        BusinessDocumentExecutionActionTmp.setTmpData(docInfo.getExecutionActions());
        BusinessDocumentActionLogTmp.setTmpData(docInfo.getExecutionLogs());
        BusinessDocumentActionOutputFileTmp.setTmpData(docInfo.getOutputFiles());
        BusinessDocumentOutgoingVariablesTmp.setTmpData(docInfo.getOutgoingVariables());
    }

]]></Source>
			</Method>
			<Method>
				<Name>task</Name>
				<Source><![CDATA[
    public int task(int _p1)
    {
        #Task
    
        switch (_p1)
        {
            case #taskRefresh:
                this.fillData();
                break;
        }
        
        return super(_p1);
    }

]]></Source>
