			<Method>
				<Name>outputFile</Name>
				<Source><![CDATA[
    private void outputFile(str _fileContents, str _fileName = '')
    {
        #File
        const str jsonExt = '.json';
        str fileName = _fileName ? _fileName : 'output';

        str fileExt;
        switch (true)
        {
            case strStartsWith(_fileContents, '<') : fileExt = #xml; break;
            case strStartsWith(_fileContents, '{') : fileExt = jsonExt; break;
            default: fileExt = #txt; break;
        }

        File::SendStringAsFileToUser(_fileContents, fileName + fileExt);
    }

]]></Source>
