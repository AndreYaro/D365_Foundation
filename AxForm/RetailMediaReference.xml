				<Source><![CDATA[
[Form]
public class RetailMediaReference extends FormRun
{
    int currentMediaId;
    RefRecId mediaResourceRecId;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
       RetailMediaResource currentRecord = element.args().record();
       if (currentRecord)
        {
            currentMediaId = currentRecord.ResourceId;
            mediaResourceRecId = currentRecord.RecId;
        }

        super();
        this.showHideGrids();
    }

]]></Source>
			</Method>
			<Method>
				<Name>showHideGrids</Name>
				<Source><![CDATA[
    /// <summary>
    /// Show or hide the respective Tabs based on the entities associated with the currently selected Media. If there are no entities associated with the currently selected Media then that Tab will be invisible.
    /// </summary>
    private void showHideGrids()
    {
        RetailMediaAndMasterEntityRelation mediaRelation;
        RetailMediaProductRelation mediaProductRelation;

        select * from mediaRelation where mediaRelation.MediaResourceId == currentMediaId && mediaRelation.MasterEntityType == RetailMediaMasterEntity::Catalog;
        TabPageCatalog.visible(mediaRelation != null);

        select * from mediaRelation where mediaRelation.MediaResourceId == currentMediaId && mediaRelation.MasterEntityType == RetailMediaMasterEntity::Category;
        TabPageCategory.visible(mediaRelation != null);

        select * from mediaRelation where mediaRelation.MediaResourceId == currentMediaId && mediaRelation.MasterEntityType == RetailMediaMasterEntity::Customer;
        TabPageCustomer.visible(mediaRelation != null);

        select * from mediaRelation where mediaRelation.MediaResourceId == currentMediaId && mediaRelation.MasterEntityType == RetailMediaMasterEntity::Worker;
        TabPageWorker.visible(mediaRelation != null);

        select * from mediaProductRelation where mediaProductRelation.MediaResource == mediaResourceRecId;
        TabPageProducts.visible(mediaProductRelation != null);
    }

]]></Source>
			</Method>
		</Methods>
