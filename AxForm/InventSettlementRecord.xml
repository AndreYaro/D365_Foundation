			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        // <GEERU>
        #ISOCountryRegionCodes
        // </GEERU>
        Query query;

        InventSettlement settlement;

        super();

        if (element.args().caller() && element.args().dataset() == tableNum(InventSettlement))
        {
            query = new Query();

            settlement = element.args().record();

            query.addDataSource(tableNum(InventSettlement));

            query.dataSourceNo(1).addDynalink(fieldNum(InventSettlement,TransRecId),
                                         settlement,
                                         fieldNum(InventSettlement,TransRecId));
            query.dataSourceNo(1).addDynalink(fieldNum(InventSettlement,InventTransId),
                                         settlement,
                                         fieldNum(InventSettlement,InventTransId));

            inventSettlement_ds.query(query);
        }

        // <GEERU>
        if (SysCountryRegionCode::isLegalEntityInCountryRegion([#isoRU]))
        {
            InventTurnoverReportDetail_RU::onFormInit(element, inventSettlement_DS);
        }
        // </GEERU>
    }

]]></Source>
