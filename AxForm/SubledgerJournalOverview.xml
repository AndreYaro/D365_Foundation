			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        QueryFilter qFilter;
        QueryBuildDataSource qbd;
        QueryBuildRange qbr;

        super();

        qbd = subledgerJournalEntry_ds.queryBuildDataSource();
        qFilter = subledgerJournalEntry_ds.query().addQueryFilter(qbd, fieldstr(SubledgerJournalEntry, Voucher));

        qbd = subledgerJournalAccountEntry_ds.queryBuildDataSource();
        qFilter = subledgerJournalAccountEntry_ds.query().addQueryFilter(qbd, fieldstr(SubledgerJournalAccountEntry, PostingType));
        qFilter = subledgerJournalAccountEntry_ds.query().addQueryFilter(qbd, fieldstr(SubledgerJournalAccountEntry, TransactionCurrency));

        // Initialize form view
        subledgerJournalOverviewFormView = SubledgerJournalOverviewFormView::newFromParameters(this);

        this.setFormControlsForFormViewClass();
        subledgerJournalOverviewFormView.init();

        // Bind accounting distribution view to accounting distribution view model
        subledgerJournalOverviewViewModel = subledgerJournalOverviewFormView.parmSubledgerJournalOverviewViewModel();

        //Initialize form view
        subledgerJournalOverviewFormView.initializeView();
    }

]]></Source>
