			<Method>
				<Name>setFormControlsForFormViewClass</Name>
				<Source><![CDATA[
    private void setFormControlsForFormViewClass()
    {
        subledgerJournalOverviewFormView.parmVoucherNumberControl(voucherNumber);

        subledgerJournalOverviewFormView.parmLedgerDimensionControl(ledgerDimension);

        subledgerJournalOverviewFormView.parmTransactionCurrencyTotalCreditControl(TransactionCurrencyTotalCredit);
        subledgerJournalOverviewFormView.parmTransactionCurrencyTotalDebitControl(TransactionCurrencyTotalDebit);
        subledgerJournalOverviewFormView.parmAccountingCurrencyCreditControl(accountingCurrencyCredit);
        subledgerJournalOverviewFormView.parmAccountingCurrencyDebitControl(accountingCurrencyDebit);
        subledgerJournalOverviewFormView.parmReportingCurrencyCreditControl(reportingCurrencyCredit);
        subledgerJournalOverviewFormView.parmReportingCurrencyDebitControl(reportingCurrencyDebit);

        subledgerJournalOverviewFormView.parmAccountingCurrencyTotalGroupControl(accountingCurrencyTotalGroup);
        subledgerJournalOverviewFormView.parmReportingCurrencyTotalGroupControl(reportingCurrencyTotalGroup);

        subledgerJournalOverviewFormView.parmViewOriginalDocumentControl(viewOriginalDocument);
        subledgerJournalOverviewFormView.parmTransactionLogControl(transactionLog);
        subledgerJournalOverviewFormView.parmPostedSalesTaxControl(postedSalesTax);

        subledgerJournalOverviewFormView.parmTransactionTotalCurrencyControl(transactionTotalCurrency);

        // <GEERU>
        if (showTransMode_RU)
        {
            if (subledgerJournalOverviewFormView.correspondenceEnabled_RU())
            {
                subledgerJournalOverviewFormView.parmShowTransModeControl_RU(showTransMode_RU);
                showTransMode_RU.selection(LedgerBondShowTransMode_RU::ShowDebitOnly);
            }
            else
            {
                showTransMode_RU.visible(false);
                postingTypeCorr_RU.visible(false);
                ledgerDimensionCorrDisplay_RU.visible(false);
            }
        }
        // </GEERU>
    }

]]></Source>
