<AxClass xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
	<Name>ReturnValueWrapper</Name>
	<SourceCode>
		<Declaration><![CDATA[
class ReturnValueWrapper
{
    anytype value;
    boolean valueIsValid;

}
]]></Declaration>
		<Methods>
			<Method>
				<Name>getValue</Name>
				<Source><![CDATA[
    anytype getValue()
    {
        return value;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setValue</Name>
				<Source><![CDATA[
    void setValue(anytype _value)
    {
        value = _value;
        valueIsValid = true;
    }

]]></Source>
			</Method>
			<Method>
				<Name>isValueValid</Name>
				<Source><![CDATA[
    boolean isValueValid()
    {
        return valueIsValid;
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
