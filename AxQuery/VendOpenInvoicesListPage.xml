				<Source><![CDATA[
    public void init()
    {
        QueryBuildDataSource vendTransDatabase;
        QueryBuildRange vendTransRange;

        super();

        if (Global::isConfigurationkeyEnabled(configurationkeynum(VendPromissoryNote)))
        {
            vendTransDatabase = this.query().dataSourceTable(tablenum(VendTrans));
            vendTransRange = SysQuery::findOrCreateRange(VendTransDatabase, fieldnum(VendTrans, PromissoryNoteStatus));
            vendTransRange.value(strFmt("%1, %2", SysQuery::value(CustVendNegInstStatus::None), SysQuery::value(CustVendNegInstStatus::Invoiced)));
        }
    }

]]></Source>
