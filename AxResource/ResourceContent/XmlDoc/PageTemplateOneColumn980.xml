<?xml version="1.0" encoding="utf-8" ?>
<PageTemplateSolutionComponent>
  <Id></Id>
  <DisplayName></DisplayName>
  <Description></Description>
  <BackgroundColor xmlns="http://schemas.microsoft.com/Dynamics/OnlineServices/WebService/Sites/V1">Ash</BackgroundColor>
  <WidthPreference xmlns="http://schemas.microsoft.com/Dynamics/OnlineServices/WebService/Sites/V1">Width980px</WidthPreference>
  <Forms xmlns="http://schemas.microsoft.com/Dynamics/OnlineServices/WebService/Sites/V1">
    <Form>
      <ViewId>View</ViewId>
      <RootControl>
        <Id>PageRoot</Id>
        <CssClassId>Page</CssClassId>
        <ContainerType>Generic</ContainerType>
        <Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="StackLayout"/>
        <ChildControls>

              <!-- Content control -->
              <Control xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ContainerControl">
                <Id>MainContent</Id>
                <CssClassId>PageContent</CssClassId>
                <Location xsi:type="StackLocation">
                  <StackOrder>0</StackOrder>
                </Location>
                <Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="StackLayout"/>
                <ContainerType>Generic</ContainerType>
                <ChildControls>

                  <!--Column control -->
                  <Control xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ContainerControl">
                    <Id>MainColumn</Id>
                    <CssClassId>OneColumn TitleOnLeft</CssClassId>
                    <ContainerType>Zone</ContainerType>
                    <Location xsi:type="StackLocation">
                      <StackOrder>0</StackOrder>
                    </Location>
                    <Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="StackLayout"/>
                  </Control>

                </ChildControls>
              </Control>
            </ChildControls>

      </RootControl>
    </Form>
  </Forms>
</PageTemplateSolutionComponent>
