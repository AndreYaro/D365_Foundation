<?xml version="1.0" encoding="utf-8"?>
<AxMacroDictionary xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
	<Name>WHSFiltersMatch</Name>
	<Source>/* Used in a where/if clause to find filter matches.             */
/* If filters are added to the array, they will need added here  */

/* %1 WHSFilterRequired                                          */
/* %2 is first array of filters, allows wildcards                */
/* %3 is the second array filters, no wildcards                  */
/* %4 WHSFilterWildcard                                          */

     (%1[1] == NoYes::No    ||
      %2[1] == %3[1]        ||
      %2[1] == %4)          &amp;&amp;
     (%1[2] == NoYes::No    ||
      %2[2] == %3[2]        ||
      %2[2] == %4)          &amp;&amp;
     (%1[3] == NoYes::No    ||
      %2[3] == %3[3]        ||
      %2[3] == %4)          &amp;&amp;
     (%1[4] == NoYes::No    ||
      %2[4] == %3[4]        ||
      %2[4] == %4)          &amp;&amp;
     (%1[5] == NoYes::No    ||
      %2[5] == %3[5]        ||
      %2[5] == %4)          &amp;&amp;
     (%1[6] == NoYes::No    ||
      %2[6] == %3[6]        ||
      %2[6] == %4)          &amp;&amp;
     (%1[7] == NoYes::No    ||
      %2[7] == %3[7]        ||
      %2[7] == %4)          &amp;&amp;
     (%1[8] == NoYes::No    ||
      %2[8] == %3[8]        ||
      %2[8] == %4)          &amp;&amp;
     (%1[9] == NoYes::No    ||
      %2[9] == %3[9]        ||
      %2[9] == %4)          &amp;&amp;
     (%1[10] == NoYes::No   ||
      %2[10] == %3[10]      ||
      %2[10] == %4)
</Source>
	<Macros />
</AxMacroDictionary>